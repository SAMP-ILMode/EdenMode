function UpdatePlayer(playerid)
{
	if(IsLevel(playerid) == 1 && DOF2_GetInt(PFile(playerid), "Kills") > Level2-1) UpLevel(playerid);
	if(IsLevel(playerid) == 2 && DOF2_GetInt(PFile(playerid), "Kills") > Level3-1) UpLevel(playerid);
