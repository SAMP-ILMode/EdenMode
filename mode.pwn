CMD:hangup(playerid, params[])
{
	if(InCall[gc]==0)return SendClientMessage(gc,-1,""#Red" .àúä ìà áùåí ùéçä ëòú");
	if(gc==playerid)
	{
		SendFormatMessage(gc,-1,""#NoN".%s "#White":ðú÷ú àú äùéçä ìùç÷ï",pName[sender]);
		SendFormatMessage(sender,-1,""#White".ðéú÷ àú äùéçä "#NoN"%s "#White":äùç÷ï",pName[gc]);
		SetPlayerSpecialAction(gc,SPECIAL_ACTION_STOPUSECELLPHONE);
		SetPlayerSpecialAction(sender,SPECIAL_ACTION_STOPUSECELLPHONE);
		InCall[gc]=0,GetCall[gc]=0; InCall[sender]=0,GetCall[sender]=0;
	}
	if(sender==playerid)
	{
		SendFormatMessage(gc,-1,""#NoN".%s "#White":ðú÷ú àú äùéçä ìùç÷ï",pName[gc]);
		SendFormatMessage(sender,-1,""#White".ðéú÷ àú äùéçä "#NoN"%s "#White":äùç÷ï",pName[sender]);
		SetPlayerSpecialAction(gc,SPECIAL_ACTION_STOPUSECELLPHONE);
