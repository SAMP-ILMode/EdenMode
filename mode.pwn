CMD:qsawn(playerid, params[])
{
	if(!InSawn[playerid]) return SendClientMessage(playerid, 0xff0000ff, ""#Red"  .àúä ìà áôòéìåú ñàåï");
	if(InSawn[playerid])
	{
		if(SawnEID[playerid] == -1) SendFormatMessageToAll(-1, ""#Red"!éöà îä÷øá ñàåï ùìå àéúê \"%s\" äùç÷ï", pName[playerid]);
		OnPlayerSpawn(playerid);
		SetPlayerVirtualWorld(playerid,0);
		SawnPID = 0;
		SawnPlayers = 0;
		if(SawnEID[playerid] != -1)
		{
			for(new i; i <= GetPlayerPoolSize(); i++) if(DOF2_GetBool(PFile(i),"SawnMessages"))
			{
				SendFormatMessage(i,-1, ""#TBlue" (/Settings: ëéáåé äåãòåú)!{%06x}%s "#TBlue"éöà î÷øá äñàåï åäôñéã ìùç÷ï {%06x} %s",GetPlayerColor(SawnEID[playerid]) >>> 8, pName[SawnEID[playerid]],GetPlayerColor(playerid) >>> 8, pName[playerid]);
			}
            SawnEID[playerid] = -1;
		}
		OnPlayerSpawn(playerid);
		OnPlayerSpawn(SawnEID[playerid]);
		ResetPlayerWeapons(SawnEID[playerid]);
		ResetPlayerWeapons(playerid);
		SetPlayerVirtualWorld(SawnEID[playerid],0);
	}
	return 1;
}
