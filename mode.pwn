CMD:warn(playerid, params[])
{
	if(IsAdmin(playerid) < 1) return 0;
	new id,reason[32];
    if(sscanf(params,"uz",id, reason)) return SendClientMessage(playerid, -1, ""#Red"/Warn [id] [reason]");
	if(!IsPlayerConnected(id)) return SendClientMessage(playerid,-1,""#Red"!äàééãé àéðå îçåáø");
    if(isnull(reason)) reason = "äàãîéï ìà ä÷ìéã ñéáä";
	DOF2_SetInt(PFile(id),"Warnings", DOF2_GetInt(PFile(id), "Warnings")+1);
	if(DOF2_GetInt(PFile(id), "Warnings") == 3)
	{
	    DOF2_SetInt(PFile(playerid),"Warnings", 0);
		TogglePlayerControllable(id,0);
		SendFormatMessage(playerid, -1, ""#Red"** Server Message: !äåøç÷ú îäùøú áùì äñéáä ùæä àæäøä îñôø %d îúåê 3, ëòú éåöâå ìôðéê ôøèé äáàï",DOF2_GetInt(PFile(id), "Warnings"));
		SendFormatMessageToAll(-1,""#Red"*** DM_Guard: %s has been banned by EDM_Bot.[Reason: 3 Warnings]",pName[id],pName[playerid]);
		new gStr[256];
		DOF2_CreateFile(BanFile(id));
		DOF2_SetString(BanFile(id),"Admin","EDM_Bot");
		DOF2_SetString(BanFile(id),"IP",getip(id));
		DOF2_SetString(BanFile(id),"Reason",reason);
		DOF2_SaveFile();
		format(gStr, sizeof(gStr), "\t\t"#White"ùìåí ìê îùúîù é÷ø,äåøç÷ú îï äùøú åìëï ìà úåëì ìäëðñ àìéå\n\n");
		format(gStr, sizeof(gStr), "%s"#White":ìäìï ôøèé äáàï åñéáú ääøç÷ä\n\n", gStr);
		format(gStr, sizeof(gStr), "%s"#Red"%s"#White":äåøç÷ú òì éãé äàãîéï\n", gStr,DOF2_GetString(BanFile(id),"Admin"));
		format(gStr, sizeof(gStr), "%s"#White"öéåï/ñéáú ääøç÷ä ùì äáàï:"#Red"%s\n", gStr,DOF2_GetString(BanFile(id),"Reason"));
		format(gStr, sizeof(gStr), "%s"#Red"%s"#White":úàøéê ääøç÷ä\n",gStr,DOF2_GetString(BanFile(id),"DateReg"));
		format(gStr, sizeof(gStr), "%s"#Red"%s"#White":ëúåáú àééôé\n",gStr,getip(id));
		format(gStr, sizeof(gStr), "%s"#White":úàøéê éøéãú ääøç÷ä"#Red"ìà öåééï\n\n", gStr);
		format(gStr, sizeof(gStr), "%s,áîéãä åàúä çåùá ùäàãîéï ðúï áàï ìà îåöã÷\n\n",gStr);
		format(gStr, sizeof(gStr), "%s"#Red"none.co.il"#White":àðà ôúç àùëåì îñåãø áôåøåí îùåá åòæøä/äåøãú áàðéí áôåøåí ùìðå\n\n", gStr);
		ShowPlayerDialog(id,DIALOG_BAN,DIALOG_STYLE_MSGBOX,""#Red"You have been banned!",gStr,"àéùåø", "");
		Kick2(id);
		return 1;
	}
	TogglePlayerControllable(id,0);
	SendFormatMessage(id, -1, ""#Red"** Server Message: !÷áìú àæäøä îäùøú, àæäøä æå äéðä %d îúåê 3, ëòú úçæåø ìùøú åú÷øà çå÷éí ùåá",DOF2_GetInt(PFile(id), "Warnings"));
	SendFormatMessageToAll(-1,""#Red"*** DM_Guard: %s has been warned by %s(%d/3).[Reason: %s]",pName[id],pName[playerid],DOF2_GetInt(PFile(id), "Warnings"),reason);
	Kick(id);
	return 1;
}
