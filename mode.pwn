	if(IsLevel(playerid) == 13 && DOF2_GetInt(PFile(playerid), "Kills") > Level14-1) UpLevel(playerid);
	if(IsLevel(playerid) == 14 && DOF2_GetInt(PFile(playerid), "Kills") > Level15-1) UpLevel(playerid);
	if(IsLevel(playerid) == 15 && DOF2_GetInt(PFile(playerid), "Kills") > Level16-1) UpLevel(playerid);
	if(IsLevel(playerid) == 16 && DOF2_GetInt(PFile(playerid), "Kills") > Level17-1) UpLevel(playerid);
	if(IsLevel(playerid) == 17 && DOF2_GetInt(PFile(playerid), "Kills") > Level18-1) UpLevel(playerid);
	if(IsLevel(playerid) == 18 && DOF2_GetInt(PFile(playerid), "Kills") > Level19-1) UpLevel(playerid);
	if(IsLevel(playerid) == 19 && DOF2_GetInt(PFile(playerid), "Kills") > Level20-1) UpLevel(playerid);
	if(IsPlayerInAnyVehicle(playerid))
	{
		format(String,150,"%s",VehNames[GetVehicleModel(GetPlayerVehicleID(playerid))-400]);
	    PlayerTextDrawSetString(playerid, TDSpeed1[playerid], String);
		new Float:X;
		new Float:Y;
		new Float:Z;
		GetVehicleVelocity(GetPlayerVehicleID(playerid),X,Y,Z);
		format(String,150,"Speed:     ~w~%d",floatround(floatsqroot(X * X + Y * Y + Z * Z) * 200.0000));
	    PlayerTextDrawSetString(playerid, TDSpeed2[playerid], String);
		new Float:Health;
		GetVehicleHealth(GetPlayerVehicleID(playerid),Health);
		if(Health != 100.0 && vHaveGM[GetPlayerVehicleID(playerid)] == true)
		{
			RepairVehicle(GetPlayerVehicleID(playerid));
	    	PlayerTextDrawSetString(playerid, TDSpeed3[playerid], "Health:     ~w~GM");
		}
		else
		{
			format(String,150,"Health:     ~w~%d.0",floatround(Health / 10));
	    	PlayerTextDrawSetString(playerid, TDSpeed3[playerid], String);
		}
	}
	new Float:Health;
	GetPlayerHealth(playerid, Health);
//	if(Health < 3.0) SetPlayerHealth(playerid, 0.0);
	new JP = GetPlayerSpecialAction(playerid);
    if(JP == SPECIAL_ACTION_USEJETPACK && IsJP[playerid] == false && !InAct[playerid])
	{
	    acTries[playerid]++;
	    if(acTries[playerid]== 2) for(new i; i <= GetPlayerPoolSize(); i++) if(IsAdmin(i) && CheatMessage) SendFormatMessage(i, -1, ""#Red"[DM_Guard]: Jet Pack :çùåã áùéîåù áö'éèéí %s(#%d) äùç÷ï",pName[playerid],playerid);
		if(acTries[playerid]== 3)
		{
			TogglePlayerControllable(playerid, 0);
			ClearAnimations(playerid);
	        SendFormatMessageToAll(-1, ""#Red"DM_Guard: %s has been kicked from the server by "SERVER_BOT". [Cheats: Jet Pack]",pName[playerid]);
	        Kick2(playerid);
		}
	}
    new Float:ST[4];
    if(IsPlayerInAnyVehicle(playerid)) GetVehicleVelocity(GetPlayerVehicleID(playerid),ST[0],ST[1],ST[2]);
	else GetPlayerVelocity(playerid,ST[0],ST[1],ST[2]);
    ST[3] = floatsqroot(floatpower(floatabs(ST[0]), 2.0) + floatpower(floatabs(ST[1]), 2.0) + floatpower(floatabs(ST[2]), 2.0)) * 179.28625;
	if(ST[3] > 290.0 && HaveSH[playerid] == false && !IsAdmin(playerid) && !FlyUse[playerid] && IsPlayerInAnyVehicle(playerid) && IsLevel(playerid) < 3)
	{
		if(HaveSH[playerid]) return 0;
	    acTries[playerid]++;
		new count =0 ;
	    if(acTries[playerid] == 2) for(new i; i <= GetPlayerPoolSize(); i++)
		{
			if(IsAdmin(i) && CheatMessage)
			{
				count++;
				SendFormatMessage(i, -1, ""#Red"[DM_Guard]: Speed Hack :çùåã áùéîåù áö'éèéí %s(#%d) äùç÷ï",pName[playerid],playerid);
			}
		}
		if(acTries[playerid]== 3 && count == 0)
