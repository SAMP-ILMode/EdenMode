CMD:votekick(playerid, params[])
{
	new Option[8];
	if(sscanf(params, "s[8] ", Option))
	{
		if(IsAdmin(playerid) > 2) return SendClientMessage(playerid, -1, ""#Red"/VoteKick [ Start / Send / Stop ]");
		if(IsLevel(playerid) < 7) return SendClientMessage(playerid,-1,""#HPurple"»"#TBlue" 7 "#TYellow":ô÷åãä æå äéðä îøîä");
		else return SendClientMessage(playerid, -1, ""#Red"/KickVote [Start/Send]");
	}
	if(!strcmp(Option, "Start", true))
	{
		if(IsLevel(playerid) < 7) return SendClientMessage(playerid,-1,""#HPurple"»"#TBlue" 7 "#TYellow":ô÷åãä æå äéðä îøîä");
		if(KickVotePlayer != -1) return SendClientMessage(playerid, -1, ""#Red".ëøâò ôåòì äöáòú ÷é÷ ðâã ùç÷ï,äîúï ùäéà úéâîø");
		new Reason[50];
		if(sscanf(params, "s[6]us[50]", Option, Player, Reason)) return SendClientMessage(playerid, -1, ""#Red" /VoteKick Start [ID] [REASON]");
		if(!IsPlayerConnected(Player)) return SendClientMessage(playerid, -1, ""#Red".äùç÷ï àéððå îçåáø");
		if(IsAdmin(Player) > 1 || IsActer(Player) > 1 || IsNighter(Player) > 1|| IsHelper(Player) > 1 || IsDJ(Player) > 1 || IsMClan(Player) > 1) return SendClientMessage(playerid, -1, ""#Red".ùç÷ï æä äéðå çáø öååú åìëï ìà úåëì ìòùåú òìéå äöáòä");
		KickVotePlayer = Player;
		KickVote = 0;
		for(new i=0; i<MAX_PLAYERS; i++) if(IsPlayerConnected(i) && InAFK[i]) KickVote++;
		KickVote = KickVote/3+1;
		KickPlayers = 1;
		IsKickVote[playerid] = true;
		KickVoteTimer = SetTimer("CancelKickVote", 2*60*1000, 0);
		SendFormatMessageToAll(-1,""#AsO"» "#TYellow":îäñéáä "#AsO"\"%s\""#TYellow" äúçéì äöáòú ÷é÷ òì äùç÷ï "#AsO"\"%s\""#TYellow" äùç÷ï",pName[Player],pName[playerid]);
		SendFormatMessageToAll(-1,""#AsO"» "#TYellow"["#AsO"%s"#TYellow"]",Reason);
		SendFormatMessageToAll(-1,""#AsO"» "#AsO"/VoteKick Send"#TYellow" ãøåù "#AsO"%d"#TYellow" äöáåòú ëãé ìáòåè ùç÷ï æä, éù ìëí ùúé ã÷åú ìäöéò ãøê äô÷åãä",KickVote);
	}
	else if(!strcmp(Option, "Send", true))
	{
		if(KickVotePlayer == -1) return SendClientMessage(playerid, -1, ""#Red".àéï äöáòä ôòéìä");
		if(IsKickVote[playerid] == true) return SendClientMessage(playerid, -1, ""#Red".ìà ðéúï ìäöáéò ôòîééí");
		IsKickVote[playerid] = true;
		KickPlayers++;
		SendFormatMessageToAll(-1,""#AsO"» "#AsO"/KickVote Send"#TYellow":áëãé ìäöéò "#AsO"\"%s\" äöáéò ìáòéèú äùç÷ï "#AsO"\"%s\""#TYellow" äùç÷ï",pName[KickVotePlayer],pName[playerid]);
		SendFormatMessageToAll(-1,""#AsO"» "#AsO"["#Red"%d:"#TYellow"îñôø ääöáòåú äãøåùåú ëøâò"#AsO"]",KickVote-KickPlayers);
		if(KickPlayers >= KickVote)
		{
			KillTimer(KickVoteTimer);
			SendFormatMessageToAll(-1,""#AsO"» %d/%d"#TYellow" îäùøú áùì äöáòú ÷é÷, ääöáòä òîãä òì îñôø ùç÷ðéí ùì "#AsO"\"%s\""#TYellow" äåçìè ìäåöéà àú äùç÷ï",KickVote,KickPlayers,pName[KickVotePlayer]);
			SendFormatMessageToAll(-1,""#Red"\"%s\" has been kicked by "SERVER_BOT". [Vote Kick]", pName[KickVotePlayer]);
			Kick2(KickVotePlayer);
			for(new i=0; i<MAX_PLAYERS; i++) if(IsPlayerConnected(i) && IsKickVote[i] == true) IsKickVote[i] = false;
			KickVotePlayer = -1;
		}
	}
	else if(!strcmp(Option, "Stop", true) && IsAdmin(playerid) > 2)
	{
		if(KickVotePlayer == -1) return SendClientMessage(playerid, -1, ""#Red".àéï äöáòä ôòéìä");
		KillTimer(KickVoteTimer);
		for(new i=0; i<MAX_PLAYERS; i++) if(IsPlayerConnected(i) && IsKickVote[i] == true) IsKickVote[i] = false;
		SendFormatMessageToAll(-1,""#Red"- \"%s\" ñâø àú ääöáòä ðâã äùç÷ï \"%s\" äàãîéï -", pName[KickVotePlayer], pName[playerid]);
		KickVotePlayer = -1;
	}
	else
	{
		if(IsAdmin(playerid) > 2) return SendClientMessage(playerid, -1, ""#Red"/VoteKick [Start/Send/Stop]");
		else SendClientMessage(playerid, -1, ""#Red"/KickVote [Start/Send]");
	}
	return 1;
}
