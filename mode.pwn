stock EnglishToHebrew(text[])
{
    new TextString[256];
    for(new iPos, iLen = strlen(text); iPos < iLen; iPos++)
    {
        switch(text[iPos])
        {
            case '.': strcat(TextString, "/");
            case 'ù': strcat(TextString, "a");
            case 'ð': strcat(TextString, "b");
            case 'á': strcat(TextString, "c");
            case 'â': strcat(TextString, "d");
            case '÷': strcat(TextString, "e");
            case 'ë': strcat(TextString, "f");
            case 'ò': strcat(TextString, "g");
            case 'é': strcat(TextString, "h");
            case 'ï': strcat(TextString, "i");
            case 'ç': strcat(TextString, "j");
            case 'ì': strcat(TextString, "k");
            case 'ê': strcat(TextString, "l");
            case 'ö': strcat(TextString, "m");
            case 'î': strcat(TextString, "n");
            case 'í': strcat(TextString, "o");
            case 'ô': strcat(TextString, "p");
            case '/': strcat(TextString, "q");
            case 'ø': strcat(TextString, "r");
            case 'ã': strcat(TextString, "s");
            case 'à': strcat(TextString, "t");
            case 'å': strcat(TextString, "u");
            case 'ä': strcat(TextString, "v");
            case ''': strcat(TextString, "w");
            case 'ñ': strcat(TextString, "x");
            case 'è': strcat(TextString, "y");
            case 'æ': strcat(TextString, "z");
            case ' ', 0..9: strcat(TextString, text[iPos]);
        }
    }
    return TextString;
}

stock PSIK(zsnumber)
{
    new num[50]; valstr(num, zsnumber);
    new revnum[50]; for(new i = 0; i < strlen(num); i++) format(revnum, sizeof(revnum), "%s%c",                   revnum, num[strlen(num)-(i+1)]);
    num=""; for(new n = 0; n < strlen(revnum); n++)
    {
        format(num, sizeof(num), "%s%c", num, revnum[n]);
        if((n+1)%3==0 && (n+1!=strlen(revnum))) format(num, sizeof(num), "%s,", num);
    }
    new newnum[50]; for(new c = 0; c < strlen(num); c++) format(newnum, sizeof(newnum), "%s%c",         newnum, num[strlen(num)-(c+1)]);
    return newnum;
}

stock Zero(num, slots = 13)
{
    new Zstring[256];
    format(Zstring, sizeof(Zstring), "%d", num);
    for(new i = 0; i <= slots - strlen(Zstring); i++)
    {
        strins(Zstring, "0", 0);
    }
    return Zstring;
}

