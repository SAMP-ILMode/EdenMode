stock GetXYInFrontOfPlayer(playerid, &Float:rcx, &Float:rcy, Float:rcdistance)
{
	new Float:rca;
	GetPlayerPos(playerid, rcx, rcy, rca);
	GetPlayerFacingAngle(playerid, rca);
	if (GetPlayerVehicleID(playerid))GetVehicleZAngle(GetPlayerVehicleID(playerid), rca);
	rcx += (rcdistance * floatsin(-rca, degrees));
	rcy += (rcdistance * floatcos(-rca, degrees));
}

stock IsPlayerInArea(playerid, Float:min_x, Float:min_y, Float:max_x, Float:max_y )
{
	new Float:PPos [ 3 ];
	GetPlayerPos ( playerid, PPos [ 0 ], PPos [ 1 ], PPos [ 2 ] );
	return PPos [ 0 ] >= min_x && PPos [ 0 ] <= max_x && PPos [ 1 ] >= min_y && PPos [ 1 ] <= max_y? 1:0;
}

stock IsVehicleInArea(vehicleid, Float:min_x, Float:min_y, Float:max_x, Float:max_y )
{
	new Float:PPos[3];
	GetVehiclePos ( vehicleid, PPos [ 0 ], PPos [ 1 ], PPos [ 2 ] );
	return PPos [ 0 ] >= min_x && PPos [ 0 ] <= max_x && PPos [ 1 ] >= min_y && PPos [ 1 ] <= max_y? 1:0;
}
