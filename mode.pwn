public OnVehicleMod(playerid, vehicleid, componentid)
{
    if(GetPlayerInterior(playerid) == 0)
	{
        acTries[playerid]++;
        if(acTries[playerid]== 2) for(new i; i <= GetPlayerPoolSize(); i++) if(IsAdmin(i) && CheatMessage) SendFormatMessage(i, -1, ""#Red"[DM_Guard]: Crash Update :çùåã áùéîåù áö'éèéí %s(#%d) äùç÷ï",pName[playerid],playerid);
		if(acTries[playerid]== 3)
		{
            SendFormatMessageToAll(-1, ""#Red"DM_Guard: %s has been kicked from the server. [Cheats: Crash Vehicle]",pName[playerid]);
            Kick2(playerid);
		}
	    return 0;
	}
	return 1;
}


