CMD:auction(playerid, params[])
{
    new Array[15];
	if(sscanf(params, "s[15] ", Array)) return SendClientMessage(playerid, -1, ""#Red"/Auction [ Start / Stop / End / Suggest / SetPrice]");
	if(!strcmp(Array, "Start", true))
	{
		if(DOF2_GetBool(PFile(playerid), "HaveCar") == false) return SendClientMessage(playerid, 0xFF0000FF, ".àéï áøëá ááòìåúê");
		if(HaveAuction == true) return SendClientMessage(playerid,-1,""#Red"!éù ëòú îëéøä ôåòìú");
		if(sscanf(params,"s[15]i",Array,params[0])) return SendClientMessage(playerid,-1,"/Auction Start [Price]");
		SendFormatMessageToAll(-1,""#TBlue"->  !"#AsO"%s"#TBlue" :äçì îëéøä ôåîáéú òì øëáå! ñåâ äøëá "#gmR"%s"#TBlue" äùç÷ï <-",DOF2_GetString(VFile(DOF2_GetInt(PFile(playerid),"VH")),"vName"),pName[playerid]);
		SendFormatMessageToAll(-1,""#TBlue"->  !áòåã 2 ã÷åú äîëéøä úéâîø "#Gold"/Auction Suggest"#TBlue" :äñëåí äðîåê áéåúø ìäöòä "#Brown"%s$"#TBlue" òì îðú ìäöéò ìîëéøä <-",PSIK(params[0]));
	    HaveAuction = true;
	    AuctionPrice = params[0];
	    AuctionStarter = playerid;
	    AuctionBestMoney = -1;
	    AuctionBestID = -1;
		AuctionTime = SetTimer("AuctionEnd", 2*60*1000, 0);
		for(new i; i <= GetPlayerPoolSize(); i++) if(IsMoneySuggest[i] != 0) IsMoneySuggest[i]=0;
    }
	if(!strcmp(Array, "SetPrice", true))
	{
		if(HaveAuction == false) return SendClientMessage(playerid,-1,""#Red"!àéï ëòú îëéøä ôåòìú");
		if(AuctionStarter != playerid) return SendClientMessage(playerid,-1,""#Red"!àúä ìà æä ùäúçéì àú äîëéøä");
		if(sscanf(params,"s[15]i",Array,params[0])) return SendClientMessage(playerid,-1,"/Auction SetPrice [Price]");
		SendFormatMessageToAll(-1,""#TBlue"-> !òøê àú äñëåí àì "#LGreen"%s$ "#TBlue" ìëï ëì ääöòåú àåôñå "#Orange"%s"#TBlue" äùç÷ï <-",PSIK(params[0]),pName[playerid]);
	    AuctionPrice = params[0];
		for(new i; i <= GetPlayerPoolSize(); i++) if(IsMoneySuggest[i] != 0) IsMoneySuggest[i]=0;
	}
	if(!strcmp(Array, "Stop", true))
	{
		if(HaveAuction == false) return SendClientMessage(playerid,-1,""#Red"!àéï ëòú îëéøä ôåòìú");
		if(AuctionStarter != playerid) return SendClientMessage(playerid,-1,""#Red"!àúä ìà æä ùäúçéì àú äîëéøä");
		SendFormatMessageToAll(-1,""#TBlue"-> "#LGreen"%s "#TBlue":òì äøëá "#AsO"%s$"#TBlue" :ñâø àú äîëéøä ôåîáéú! äîëéø ùäåöéò "#Orange"%s"#TBlue" äùç÷ï <-",DOF2_GetString(VFile(DOF2_GetInt(PFile(AuctionStarter),"VH")),"vName"),PSIK(AuctionPrice),pName[AuctionStarter]);
	    HaveAuction = false;
	    AuctionPrice = 0;
	    AuctionStarter = -1;
	    AuctionBestMoney = -1;
	    AuctionBestID = -1;
		KillTimer(AuctionTime);
		for(new i; i <= GetPlayerPoolSize(); i++) if(IsMoneySuggest[i] != 0) IsMoneySuggest[i]=0;
	}
	if(!strcmp(Array, "Suggest", true))
	{
		if(HaveAuction == false) return SendClientMessage(playerid,-1,""#Red"!àéï ëòú îëéøä ôåòìú");
		if(sscanf(params,"s[15]i",Array,params[0])) return SendClientMessage(playerid,-1,"/Auction Suggest [Price]");
		if(params[0] < AuctionPrice) return SendFormatMessage(playerid,-1,""#Red"!%s$ :äñëåí äîéðîàìé äéðå",PSIK(AuctionPrice));
	    IsMoneySuggest[playerid] = params[0];
		if(AuctionBestMoney < params[0]) SendFormatMessageToAll(-1,""#TBlue"-> !äöéò àú äñëåí: "#LYellow"%s$"#TBlue" åëòú æä äñëåí äëé âáåää ùäöéòå "#Orange"%s"#TBlue" äùç÷ï <-",PSIK(params[0]),pName[playerid]),AuctionBestMoney = params[0],AuctionBestID = playerid;
		else SendFormatMessageToAll(-1,""#TBlue"-> !äöéòä àú äñëåí: "#LYellow"%s$"#TBlue" "#Orange"%s"#TBlue" äùç÷ï <-",PSIK(params[0]),pName[playerid]);
	}
	if(!strcmp(Array, "End", true))
	{
		if(HaveAuction == false) return SendClientMessage(playerid,-1,""#Red"!àéï ëòú îëéøä ôåòìú");
		if(AuctionStarter != playerid) return SendClientMessage(playerid,-1,""#Red"!àúä ìà æä ùäúçéì àú äîëéøä");
		if(AuctionBestMoney != -1)
		{
			SendFormatMessageToAll(-1,""#TBlue"->  !"#Green"%s$"#TBlue" :áñëåí "#Purple"%s"#TBlue" :÷ðä àú äøëá "#gmR"%s"#TBlue" äùç÷ï <-",PSIK(AuctionBestMoney),DOF2_GetString(VFile(DOF2_GetInt(PFile(AuctionStarter),"VH")),"vName"),pName[AuctionBestID]);
			SendFormatMessageToAll(-1,""#TBlue"->  ."#Gold"%s$"#TBlue" :ñëåí îéðîàìé "#Brown"%s"#TBlue" äùç÷ï ùäöéò àú äøëá <-",PSIK(AuctionPrice),pName[AuctionStarter]);
		}
		else SendFormatMessageToAll(-1,""#TBlue"-> !àó àçã ìà ÷ðä àú äøëá"#LGreen"%s "#TBlue":òì äøëá "#AsO"%s$"#TBlue" :ñâø àú äîëéøä ôåîáéú! äîëéø ùäåöéò "#Orange"%s"#TBlue" äùç÷ï <-",DOF2_GetString(VFile(DOF2_GetInt(PFile(AuctionStarter),"VH")),"vName"),PSIK(AuctionPrice),pName[AuctionStarter]);
		Y_GivePlayerMoney(AuctionStarter,AuctionBestMoney);
		Y_GivePlayerMoney(AuctionBestID,-AuctionBestMoney);
		ParaseVehicle(playerid,AuctionBestID);
		HaveAuction = false;
	    AuctionPrice = 0;
	    AuctionStarter = -1;
	    AuctionBestMoney = -1;
	    AuctionBestID = -1;
		KillTimer(AuctionTime);
		for(new i; i <= GetPlayerPoolSize(); i++) if(IsMoneySuggest[i] != 0) IsMoneySuggest[i]=0;
	}
	return 1;
	}
