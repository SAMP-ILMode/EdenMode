	{
		if(IsInviteTL[playerid] == false) return SendClientMessage(playerid,-1,""#Red".àéðê îåæîï ìäùúâøåú");
		if(!IsPlayerConnected(IdToInviteGET[playerid])) return SendClientMessage(playerid,-1,""#Red".ùç÷ï æä àéðå îçåáø ìùøú");
