		SendFormatMessage(playerid, -1, ""#LGreen" ** ùéðéú àú äùòä áîùç÷ ùìê: %d",params[0]);
		GameTime[playerid] = params[0];
	}
	else if(!strcmp(Option, "del", true, 3))
	{
	    if(GameTime[playerid] == -1) return SendClientMessage(playerid, -1, ""#Red"/MyTime Set - ìà äâãøú ùòä, òì îðú ìäâãéø");
