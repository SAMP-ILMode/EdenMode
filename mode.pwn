CMD:mytime(playerid, params[])
{
	new Option[4];
	if(sscanf(params, "s[4] ", Option)) return SendClientMessage(playerid, -1, ""#Red"/MyTime [ Set / Del ]");
	if(!strcmp(Option, "set", true, 3))
	{
	    if(GameTime[playerid] != -1) return SendClientMessage(playerid, -1, ""#Red"/MyTime Del - äâãøú ëáø ùòä, òì îðú ìòøåê àåúä");
		if(sscanf(params, "s[4]i", Option, params[0])) return SendClientMessage(playerid, -1, ""#Red"/MyTime Set [hour]");
		if(params[0] > 24 || params[0] < 0) return SendClientMessage(playerid, -1, ""#Red"/MyTime Set [hour(24-00)]");
