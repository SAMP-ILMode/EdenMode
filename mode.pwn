CMD:join(playerid, params[])
{
    if(InAct[playerid]) return SendClientMessage(playerid, 0xff0000ff, ""#Red" .àúä ëáø áôòéìåú");
    if(!ActStatus || ActStatus == 2) return SendClientMessage(playerid, 0xff0000ff, !ActStatus ? (""#Red".àéï ôòéìåú òåáãú áùøú") : (""#Red" .äôòéìåú ëáø äçìä, ðñä áôòí äáàä"));
	if(Acts[IsActOn][Max_Act_Players] < ActPlayers) return SendClientMessage(playerid, 0xff0000ff, ""#Red".ëì äî÷åîåú áôòéìåú ðúôñå");
	if(SCarIn[playerid]) return SendClientMessage(playerid, 0xff0000ff, ""#Red".òìéê ìöàú îäøëá äîéåçã");
	if(GetPlayerInterior(playerid) != 0) return SendClientMessage(playerid, 0xff0000ff, ""#Red".ìà ðéúï ìáöò ô÷åãä æå áîöáê äðåëçé");
	if(Y_GetPlayerMoney(playerid) < 200) return SendClientMessage(playerid, -1, ""#Red"** $îçéø äöèøôåú ìôòéìåú: 200");
    if(ActPlayers == Acts[IsActOn][Max_Act_Players]) SendFormatMessage(playerid,-1, ""#Red"»  !ðúôñå äî÷åîåú ìôòéìåú %d",Acts[IsActOn][ActName]);
	Y_GivePlayerMoney(playerid, -200);
	InAct[playerid] = true;
	ActPlayers++;
	if(ActPlayers == Acts[IsActOn][Max_Act_Players]) SendFormatMessageToAll(-1, ""#HBrown"»  !ðúôñå äî÷åîåú ìôòéìåú "#Brown"%d",Acts[IsActOn][ActName]);
	if(!FJoin)
	{
		Y_GivePlayerMoney(playerid, 4000);
		SendFormatMessageToAll(-1, ""#LGreen"+ (%d/%d)/Join :äöèøó ìôòéìåú øàùåï åæëä áñëåí 4,000$. ìäöèøôåú %s",Acts[IsActOn][Max_Act_Players],ActPlayers,pName[playerid]);
        FJoin = true;
	}
	else if(FJoin)
	{
		switch(Acts[IsActOn][Type])
		{
			case 2..3:SendFormatMessageToAll(-1, ""#LGreen"[+] !äöèøó àì äîéøåõ %s",pName[playerid]);
		}
	}
	SendFormatMessage(playerid, -1, ".ðøùîú ìôòéìåú "#TBlue"%s "#White"àúä îùúúó îñôø "#Red"%d/%d",Acts[IsActOn][ActName], Acts[IsActOn][Max_Act_Players], ActPlayers);
	return 1;
}
