CMD:call(playerid, params[])
{
	if(GetPlayerState(playerid) == PLAYER_STATE_DRIVER )return SendClientMessage(playerid,-1,""#Red" .ùðåäâéí - ìà îèìôðéí");
	new id;
	if(sscanf(params,"u",id)) return SendClientMessage(playerid,-1,"/Call [ID]");
	if(!IsPlayerConnected(id)) return SendClientMessage(playerid,-1,""#Red" äàééãé àéðå îçåáø");
	if(id==playerid)return SendClientMessage(playerid,-1,""#Red" .ìà ðéúï ìùìåç ìòöîê äåãòä");
	if(InCall[playerid] == 1) return SendClientMessage(playerid,-1,""#Red" .àúä ëáø ðîöà áùéçä");
    if(InCall[id] == 1) return SendClientMessage(playerid,-1,""#Red" .ùç÷ï æä áùéçä,àðà äîúï ùúâîø äùéçä");
	if(DOF2_GetBool(PFile(id),"BlockCalls") == true) return SendClientMessage(playerid,-1,""#Red".ùç÷ï æä çñí ùéçåú ðëðñåú");
	SendFormatMessage(playerid,-1,""#Blue"...àðà äîúï ìîòðä \"%s\" îáöò ùéçä ìùç÷ï",pName[id]);
	SendFormatMessage(id,-1,""#Yellow",îú÷ùø àìéê \"%s\" äùç÷ï",pName[playerid]);
	SendClientMessage(id,-1,""#LGreen"/Anscall -"#White" áëãé ìòðåú ìùéçä ä÷ìã");
	SendClientMessage(id,-1,""#NoN"/Ignore -"#White" áëãé ìãçåú àú äùéçä ä÷ìã");
	InCall[playerid]=0,InCall[id]=0;
	GetCall[playerid]=0,GetCall[id]=1;
	gc=id; sender=playerid;
	return 1;
}
//==============================================================================
CMD:ignore(playerid, params[])
{
	if(GetCall[playerid]==0)return SendClientMessage(playerid,-1,""#Red" .ìà ÷áìú ùåí ùéçä");
	SendFormatMessage(gc,-1,""#NoN".%s "#White":ãçéú àú äùéçä ùì äùç÷ï",pName[sender]);
	SendFormatMessage(sender,-1,""#White" .ìà òðä ìùéçä "#LGreen"%s "#White":äùç÷ï",pName[gc]);
	InCall[sender]=0,InCall[gc]=0;
	GetCall[sender]=0,GetCall[gc]=0;
	return 1;
}
//==============================================================================
CMD:anscall(playerid, params[])
{
	if(GetCall[playerid]==0)return SendClientMessage(playerid,-1,""#Red" .ìà ÷áìú ùåí ùéçä");
	if(InCall[playerid]==1)return SendClientMessage(playerid,-1,""#Red" .àúä ëáø áùéçä");
	if(GetPlayerState(playerid) == PLAYER_STATE_DRIVER)return SendClientMessage(playerid,-1,""#Red" .ùðåäâéí - ìà îèìôðéí");
	SendFormatMessage(gc,-1,""#LGreen"%s "#White":òðéú ìùéçä ùì äùç÷ï",pName[sender]);
	SendFormatMessage(sender,-1,""#White" .òðä ìùéçä,ëòú úåëìì ãáø àéúå "#LGreen"%s "#White":äùç÷ï",pName[gc]);
	InCall[sender]=1;
	InCall[gc]=1;
	GetCall[sender]=0;
	GetCall[gc]=0;
	return 1;
}
