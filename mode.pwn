	    SendFormatMessage(playerid,-1,""#HRed"» "#Green".ìäùúâø àìéå "#Blue"\"%s\" "#Green" äæîðú àú äùç÷ï",pName[params[0]]);
	    SendFormatMessage(params[0],-1,""#HRed"» "#Green"/tl deny ìáéèåì • /tl accept îæîéï àåúê ìäùúâø àìéê, ìàéùåø "#Blue"\"%s\" "#Green" äùç÷ï",pName[params[0]]);
	    IsInviteGOTO[params[0]] = true;
	    IdToInviteGOTO[params[0]] = playerid;
	 	IsInviteTL[params[0]] = true;
    }
	else if(!strcmp(Array, "Get", true))
	{
		if(DOF2_GetBool(PFile(playerid),"HaveTL") == false) return SendClientMessage(playerid,-1,""#Red"! àéï áøùåúê îòøëú äùúâøåéåú");
		if(sscanf(params,"s[15]u",Array,params[0])) return SendClientMessage(playerid,-1,"/TL Get ID");
		if(IsInviteGOTO[params[0]] == true) return SendClientMessage(playerid,-1,""#Red".ùç÷ï æä îåæîï ìäùúâøåú");
	    SendFormatMessage(playerid,-1,""#HRed"» "#Green".ìäùúâø àìéê "#Blue"\"%s\" "#Green" äæîðú àú äùç÷ï",pName[playerid]);
	    SendFormatMessage(params[0],-1,""#HRed"» "#Green"/tl deny ìáéèåì • /tl accept îæîéï àåúê ìäùúâø àìéå, ìàéùåø "#Blue"\"%s\" "#Green" äùç÷ï",pName[params[0]]);
