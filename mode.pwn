	SendFormatMessageToAll(-1,""#Red":ñâø àú äôòéìéåú %s äàãîéï",pName[playerid]);
	SendFormatMessageToAll(-1,""#Red"(%s)",reason);
	KillTimer(ActTimer);
	KillTimer(ActTimer2);
	IsActOn = 0;
	ActStatus = 0;
	ActPlayers = 0;
	if(ActRezef != 0)
	{
	    if(ActRezef == 0) return 0;
		new best = -9999;
  		new id = -1;
		for(new i; i <= GetPlayerPoolSize(); i++)
		{
			if(RezefPoints[i] && RezefPoints[i] >= best)
			{
				best = RezefPoints[i];
				id = i;
			}
		}
		new money = random(5000);
		SendClientMessageToAll(-1,""#LOrange"---------- [ Activity Strike ] ----------");
		SendClientMessageToAll(-1,""#Orange"• !øöó äôòéìéåú äñúééí ëé äàãîéï ñâø àú äôòéìéåú");
		SendFormatMessageToAll(-1,""#Yellow"• %d :îñôø äôòéìéåú ùäåôòìå",ActRezef);
		if(id != -1)
		{
			SendFormatMessageToAll(-1,""#zYellow"• %s :äùç÷ï àùø òí äëé äøáä ð÷åãåú",pName[id]);
			SendFormatMessageToAll(-1,""#zYellow"• %d :ëîåú ôòéìéåú àùø äùç÷ï æëä",best);
			SendFormatMessageToAll(-1,""#zYellow"• $%s :ìëï äåà æëä áñëåí",PSIK(money));
		}
		else SendClientMessageToAll(-1,""#zYellow"• - àéï ùç÷ï àùø æëä áøöó -");
		SendClientMessageToAll(-1,""#LYellow"----------------------------------------------");
	    ActRezef = 0;
		ActRAmount = 0;
		foreach(new i: Player) RezefPoints[i] = 0;
		SendRconCommand("hostname " SERVER_HOST"");
	}
	for(new i; i <= GetPlayerPoolSize(); i++)
	{
		if(InAct[i])
		{
			InAct[i] = false;
			if(ActStatus == 2)
			{
				OnPlayerSpawn(i);
				InAct[i] = false;
				DisablePlayerRaceCheckpoint(i);
				SetPlayerVirtualWorld(i,0);
				InActCP[i] = 0;
				KillTimer(ActTimer3[i]);
				RaceTime[i] = 0;
			}
		}
	}
	for(new i; i<sizeof(ActVehicle); i++) DestroyVehicle(ActVehicle[i]), RemoveVehicleMissiles(ActVehicle[i]);
	return 1;
}
