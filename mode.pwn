CMD:stopacts(playerid, params[])
{
	if(IsAdmin(playerid) < 1 && IsActer(playerid) < 1) return 0;
	if(!ActStatus) return SendClientMessage(playerid, 0xff0000ff, "àéï ôòéìåéåú òåáãåú ëøâò");
