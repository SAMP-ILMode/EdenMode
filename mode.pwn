stock fwrite_utf8(File:handle, str[])
{
    if(!handle) return 0;
    new logx;
    while(str[logx] != EOS) fputchar(handle, str[logx++],false);
    return 1;
}

stock sWriteLog(sz_fileName[], sz_text[])
{
    new sz_logEntry[156], File:fhandle;
    fhandle = fexist(sz_fileName)? fopen(sz_fileName, io_append) : fopen(sz_fileName, io_write);
    format(sz_logEntry, sizeof(sz_logEntry), "[Date: %s | Time: %s]: %s\r\n", GetDateEx(), GetTimeEx(), sz_text);
    fwrite_utf82(fhandle,sz_logEntry,false);
    return fclose(fhandle);
}
