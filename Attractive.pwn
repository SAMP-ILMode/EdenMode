		INI_WriteString( File , "IPBanned",GetIp( ID ));
		INI_WriteString( File , "Date", GetDateAsString());
		INI_WriteString( File , "Time", GetTimeAsString());
		INI_WriteString(File, "Days", StrDays);
		INI_Close ( File );
     	BanIp( GetIp( ID ) ,false,Reason_ba);
		S_SendMsgToAll(Red, "*** \"%s\" Has been banned by \"%s\" for %d days. (Reason: %s)",GetName( ID ),GetName(playerid),Days, Reason_ba);
		new catstr[2500];
		format(catstr, sizeof(catstr), "");
		strcat(catstr, "{"#C_White"}~~ {"#C_Red"}%s{"#C_White"} Ban Info ~~\n");
		strcat(catstr, "{"#C_White"}• {"#C_Red"}Admin: {"#C_White"}%s\n• {"#C_Red"}Reason:{"#C_White"} %s\n• {"#C_Red"}Days:{"#C_White"} %d\n• {"#C_Red"}IP:{"#C_White"} %s\n• {"#C_Red"}Date:{"#C_White"} %s\n• {"#C_Red"}Time:{"#C_White"} %s");
		format(catstr, sizeof(catstr), catstr, GetName( ID ),GetName(playerid),Reason_ba,Days,GetIp( ID ),GetDateAsString(),GetTimeAsString());
		strcat(catstr, "\n{"#C_Red"}~~~~~~~~~~~~~~~~~\n"WebUrl" - להורדת הבאן פנה לפורום \"הורדת באנים\" וצלם תמונה זו");
		ShowPlayerDialog( ID , 1000, DIALOG_STYLE_MSGBOX, "{"#C_Red"}You Are Banned !", catstr, "אישור", "");
		KickEx( ID );
	}
	return 1;
}

