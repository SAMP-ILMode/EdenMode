   	new File:file_ptr,line[256], Object, SpawnX, SpawnY, SpawnZ,
   	SpawnRX, SpawnRY, SpawnRZ, draw, Objects_loaded;
   	file_ptr = fopen(filename,filemode:io_read);
   	if(!file_ptr) return 0;
   	while(fread(file_ptr, line, 256))
   	{
   	    sscanf(line, "p<,>iffffffi", Object, SpawnX, SpawnY, SpawnZ, SpawnRX, SpawnRY, SpawnRZ, draw);
