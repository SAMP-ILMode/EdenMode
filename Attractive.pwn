CMD:ahelp(playerid, params[])
{
	if(LevelAdmin(playerid) < 1) return 0;
	if(AdmLog[playerid] == false)return SendClientMessage(playerid,Red,"ERROR: אינך מחובר למערכת אדמינים");
	if(sscanf(params,"i",params[0])) return S_SendMsg(playerid,White,"Usage: /AHelp [Level 1-%d]",LevelAdmin(playerid));
	if(LevelAdmin(playerid) < params[0]) return 0;
	new gStr[2000],coutc;
 	for ( new i, j = sizeof ( CommandAdminInfo ); i < j; i++ )
	{
	 	if(CommandAdminInfo [ i ] [ levelcommandadmin ] == params[0])
		{
            coutc++;
		 	format ( gStr, sizeof ( gStr ), "%s{"#C_Blue"}%d.{"#C_lGrey"} %s\n", gStr,coutc,CommandAdminInfo [ i ] [ CommandAdmin ]);
		}
	}
	new zsStr[24];
    format (zsStr, sizeof ( zsStr ),"{"#C_Orange"}Admin Help %d",params[0]);
    ShowPlayerDialog(playerid, 98211, DIALOG_STYLE_MSGBOX,zsStr,gStr, "אישור", "");
	return 1;
}
