CMD:act(playerid, params[])
{
	new command[50];
	if(sscanf(params,"s[50]{s[50]}",command))
	{
		SendClientMessage(playerid,lGreen,"• /Act Join{"#C_White"} - הצטרפות לפעילות");
		SendClientMessage(playerid,lGreen,"• /Act Check{"#C_White"}  - בדיקת פעילות פעילה");
		SendClientMessage(playerid,lGreen,"• /Act Quit{"#C_White"} - יציאה מהפעילות");
        if(pInfo [ playerid ] [ Staff ] == true) SendClientMessage(playerid,Red,"• /Staffhelp - תפריט לצוותים");
        return 1;
	}
	if(!strcmp(command,"join",true))
	{
	    if(InActAuto[playerid] == true && ActStatus >= 2) return SendClientMessage(playerid,Red, "./QAct - אינך יכול להקיש פקודות שאתה בפעילות. ליציאה, הקש");
 		if(InDm[playerid] == true) return SendClientMessage(playerid,Red,".DM-אינך יכול להשתמש בפקודה זו שאתה נימצא באזור");
        if(InAct[playerid] == true) return SendClientMessage(playerid,Red,".אתה כבר בפעילות");
        if(ActOn == false) return SendClientMessage(playerid,Red,".אין פעילות מופעלת");
        if(ActTP == false || PlayersInAct == MAX_Players) return SendClientMessage(playerid,Red,".לא ניתן להשתגר לפעילות זו יותר");
        SetPlayerTeam(playerid,NO_TEAM);
        new Float:x,Float:y,Float:z,Float:a,Interior,VirtualW;
        GetPlayerPos(starterid,x,y,z,a);
		Interior = GetPlayerInterior(starterid);
		VirtualW = GetPlayerVirtualWorld(starterid);
		QuitGAll(playerid);
		if(IsPlayerInAnyVehicle(playerid))
		{
			if(JoinWithCar == false) SetPlayerPosExAct(playerid, x+random(2),y+random(2),z,a,Interior,VirtualW);
    		else if(JoinWithCar == true) SetPlayerCarPosEx(playerid, x+random(2),y+random(2),z,a,Interior,VirtualW);
		}
		else SetPlayerPosExAct(playerid, x+random(2),y+random(2),z,a,Interior,VirtualW);
    	ResetPlayerWeapons(playerid);
    	InAct[playerid] = true;
    	PlayersInAct++;
    	if(AntiTK == true) SetPlayerTeam(playerid,4);
    	SendClientMessage(playerid,lGreen,"!הצטרפת לפעילות בהצלחה");
    	return S_SendMsg(playerid,lGreen,"• {"#C_White"}#%d{"#C_lGreen"} הינך משתתף מספר",PlayersInAct);
	}
	if(!strcmp(command,"quit",true))
	{
		if(InAct[playerid] == false) return SendClientMessage(playerid,Red,".אתה לא בפעילות");
  		return ExitAct(playerid,".יצאת מהפעילות");
	}
	if(!strcmp(command,"check",true))
	{
		if(!ActOn)return SendClientMessage(playerid,Red,".אין פעילות מופעלת עכשיו");
        SendClientMessage(playerid,lGrey,":פרטים על הפעילות");
		S_SendMsg(playerid,llllGreen,":הפעיל את הפעילות %s הצוות",GetName(starterid));
  		S_SendMsg(playerid,llllGreen,"(%s)",Act_Name);
   		S_SendMsg(playerid,llllGreen,"%d/%d :שחקנים שהצתרפו לפעילות",PlayersInAct,MAX_Players);
     	if(ActTP == false || MAX_Players == PlayersInAct) SendClientMessage(playerid,Red,".לא ניתן להצטרף לפעילות זו יותר");
		else
		{
			SendClientMessage(playerid,Red,JoinWithCar == false?(".לא ניתן להצטרף עם מכונית"):("{"#C_lGreen"}.ניתן להצטרף עם מכונית"));
        	SendClientMessage(playerid,lGrey, "./Act Join בשביל להצטרף לפעילות, רשום");
     	}
      	return 1;
	}
	if(pInfo [ playerid ] [ Staff ] == true)
	{
		if(!strcmp(command,"checkall",true))
		{
		    if(ActOn == false)return SendClientMessage(playerid,Red, ".אין פעילות מופעלת");
			S_SendMsgToAll(llllGreen,":מפרסם כעת את הפעילות הקיימת %s צוות הפעילות",GetName(playerid));
    	    SendClientMessageToAll(llllGreen,Act_Name);
			SendClientMessageToAll(llllGreen, "/Act Join בשביל להצטרף לפעילות, רשום");
			return 1;
		}
		if(!strcmp(command,"menu",true))
		{
			return ShowPlayerDialog(playerid,DIALOG_ActStartOFF,DIALOG_STYLE_LIST,"מערכת פעילות ידנית","{"#C_lGreen"}»{"#C_White"} פתח פעילות\n{"#C_Red"}»{"#C_White"} סגור פעילות","אישור","ביטול");
		}
		if(!strcmp(command,"gm",true))
		{
			if(TimeGMact - gettime() > 0) return S_SendMsg(playerid,Red,".אינך יכול לקחת כסף מבנק הפעילויות מכיוון שמשכו ממנו כסף בחצי שעה הארחונה, תוכל למשוך עוד %d שניות",TimeGMact - gettime());
