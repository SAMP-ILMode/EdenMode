		return SendClientMessage(playerid,White,"Usage: /CreateCode <Code> [Day] [CodeType]");
	}
	for(new i; i < MAX_CODES; i++) if(CodeNumber [ i ] != -1 && !strcmp(Code[i],Code_C, true)) return S_SendMsg(playerid,Red, ".%s יש כבר קוד בשם זה",Code_C);
