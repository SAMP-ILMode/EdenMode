			if(IsPlayerInRangeOfPoint(playerid, 2.5, HouseInfo [ HouseID ] [ HX ],HouseInfo [ HouseID ] [ HY ],HouseInfo [ HouseID ] [ HZ ]) )
			{
				if(HouseInfo [ HouseID ] [ Owned ] == false)
				{
					if(Y_GetPlayerMoney(playerid) < HouseInfo [ HouseID ] [ Cost ]) return S_SendMsg(playerid,Red,".$%s אין לך מספיק כסף מחיר הבית הוא", GetNum(HouseInfo [ HouseID ] [ Cost ]));
