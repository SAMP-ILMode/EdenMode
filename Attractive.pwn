    if(ActStatus >= 2)
    {
        if(InPlayerClovGold == false)
        {
			if(ClovGoldWin != -1)
			{
				if(InActAuto[ClovGoldWin] == true) SendClientMessage(ClovGoldWin,llPurple,".בגלל שהפעילות כלוב הזהב בחרה באקראיות שחקן אחר, אינך ראשי להשתמש בפרס שהפעילות הביאה לך. והפרס נמחק");
   			}
            InPlayerClovGold = true;
			new Iterator:RandP<MAX_PLAYERS>;
			for(new i, j = GetPlayerPoolSize(); i <= j; i++)
			{
				if(!IsPlayerConnected(i) || InActAuto[i] == false) continue;
