CMD:qact(playerid, params[])
{
	if(!InActAuto[playerid] && !InActTeams(playerid)) return SendClientMessage(playerid,Red, ".אתה לא בפעילות");
	if(InActTeams(playerid))
	{
		if(ActStatusTeams == 2) return LozerTeamsActivity(playerid,".יצאת מהפעילות האוטומטית");
		else
		{
			ActPlayersTeams--;
			InActTeamA[playerid] = false;
  			InActTeamB[playerid] = false;
			ActPriceTeams -= 150;
			SendClientMessage(playerid,Red, ".יצאת מהפעילות האוטומטית");
		}
	}
	else if(InActAuto[playerid] == true)
	{
		if(ActStatus >= 2)
		{
        	LozerActivity(playerid,".יצאת מהפעילות האוטומטית");
			return 1;
		}
		else
		{
			ActPlayers--;
			InActAuto[playerid] = false;
			DisablePlayerRaceCheckpoint(playerid);
			InActCP[playerid] = 0;
			ActPrice -= 150;
			SendClientMessage(playerid,Red, ".יצאת מהפעילות האוטומטית");
			new gStr [ 20 ];
			format ( gStr, sizeof ( gStr ), "players:~g~~h~ %d/%d", ActPlayers, Acts [ IsActOn ] [ Max_Act_Players ] );
			TextDrawSetString ( ActTD [ 4 ], gStr );
		}
	}
	return 1;
}

CMD:rr(playerid, params[])
{
    SendClientMessage(playerid,Red,"~~~ :טבלת שיאים ~~~");
	for(new i; i < sizeof(Acts); i++)
	{
    	if(Acts [ i ] [ Type ] == ACT_RACE_TYPE)
    	{
    	    if(WhoWinR [ i ] != 200000) S_SendMsg(playerid, Yellow , "%s :זמן | %s :רכב | %s :שם השחקן | %s :שם הפעילות",GetTimeN(WhoWinR [ i ]),WhoWinRCar [ i ],RNameRecord [ i ],Acts [ i ] [ ActName ]);
			else S_SendMsg(playerid, Yellow , "Unknown :זמן | Unknown :רכב | Unknown :שם השחקן | %s :שם הפעילות",Acts [ i ] [ ActName ]);
  		}
	}
	return 1;
}

CMD:join(playerid, params[])
{
	if(InActAuto[playerid] == true && ActStatus >= 2) return SendClientMessage(playerid,Red, "./QAct - אינך יכול להקיש פקודות שאתה בפעילות. ליציאה, הקש");
	if(InDm[playerid] == true) return SendClientMessage(playerid,Red,".DM-אינך יכול להשתמש בפקודה זו שאתה נימצא באזור");
	if(InActAuto[playerid] || InActTeams(playerid)) return SendClientMessage(playerid,Red, ".אתה כבר בפעילות");
	if(!ActStatus && !ActStatusTeams) return SendClientMessage(playerid,Red, ".אין פעילות מופעלת בשרת");
    if(ActStatus)
    {
		if(ActStatus == 2 || ActStatus == 3) return SendClientMessage(playerid,Red, !ActStatus ? (".אין פעילות עובדת בשרת") : (".הפעילות כבר החלה, נסה בפעם הבאה"));
		if(Acts[IsActOn][Max_Act_Players] <= ActPlayers) return SendClientMessage(playerid,Red, ".כל המקומות בפעילות נתפסו");
		if(Y_GetPlayerMoney(playerid) < 200) return S_SendMsg(playerid, Red, "* .$%d עלות השתתפות בפעילות זו",200);
		Y_GivePlayerMoney(playerid, -200);
		InActAuto[playerid]  = true;
		ActPlayers++;
		new gStr[40];
		format ( gStr, sizeof ( gStr ), "players:~g~~h~ %d/%d", ActPlayers,Acts [ IsActOn ] [ Max_Act_Players ]);
		TextDrawSetString ( ActTD [ 4 ], gStr );
		ActPrice += 150;
		if(Acts[IsActOn][Max_Act_Players] <= ActPlayers) S_SendMsgToAll(Red,".נתפסו {"#C_llllGreen"}%s{"#C_Red"} כל המקומות בפעילות",Acts [ IsActOn ] [ ActName ]);
		S_SendMsg(playerid,Yellow, "- - - - - - [ %s ] - - - - - -",Acts [ IsActOn ] [ ActName ]);
		S_SendMsg(playerid,White, "!בהצלחה %s הצטרפת לפעילות",Acts [ IsActOn ] [ ActName ]);
    	S_SendMsg(playerid,White, ".הפעילות תתחיל בעוד %d שניות",StartActTime);
    	S_SendMsg(playerid,White, ".{"#C_llllGreen"}#%d{"#C_White"} הינך משתתף מספר",ActPlayers);
		foreach(new i: Player)
		{
			if(i != playerid && InActAuto[i]  == true)
			{
				S_SendMsg(i, Grey,  "MSG Activity:{"#C_White"} (%02d# הצטרף לפעילות. (משתתף מספר {"#C_Pink"}%s{"#C_White"}({"#C_Pink"}ID: %03d{"#C_White"}) השחקן",ActPlayers,GetName(playerid),playerid);
			}
		}
	}
	else if(ActStatusTeams)
	{
   		if(!ActStatusTeams || ActStatusTeams == 2) return SendClientMessage(playerid,Red, !ActStatusTeams ? (".אין פעילות עובדת בשרת") : (".הפעילות כבר החלה, נסה בפעם הבאה"));
		if(ActsTeams[ IsActOnTeams ][Max_Act_Players] <= ActPlayersTeams) return SendClientMessage(playerid,Red, ".כל המקומות בפעילות נתפסו");
		if(Y_GetPlayerMoney(playerid) < 200) return S_SendMsg(playerid, Red, "* .$%d עלות השתתפות בפעילות זו",200);
		Y_GivePlayerMoney(playerid, -200);
		ActPriceTeams += 150;
		ActPlayersTeams++;
		InActTeamA[playerid] = true;
		foreach(new b: Player)
		{
			if(b != playerid && InActTeams(b))
			{
				format(String, sizeof(String), "MSG Activity:{"#C_White"} (%02d# הצטרף לפעילות. (משתתף מספר {"#C_Pink"}%s{"#C_White"}({"#C_Pink"}ID: %03d{"#C_White"}) השחקן",ActPlayersTeams,GetName(playerid),playerid);
        	   	SendClientMessage(b, Grey , String );
			}
		}
		S_SendMsg(playerid,Yellow, "- - - - - - [ %s ] - - - - - -",ActsTeams[ IsActOnTeams ] [ ActNameTeams ]);
		S_SendMsg(playerid,White, "!בהצלחה %s הצטרפת לפעילות",ActsTeams[ IsActOnTeams ] [ ActNameTeams ]);
    	S_SendMsg(playerid,White, ".הפעילות תתחיל בעוד %d שניות",StartActTimeTeams);
    	S_SendMsg(playerid,White, ".{"#C_llllGreen"}#%d{"#C_White"} הינך משתתף מספר",ActPlayersTeams);
	}
	return 1;
}

CMD:actstats( playerid, params [ ] )
{
	if ( !ActStatus ) return SendClientMessage ( playerid, Red , ".אין פעילות פועלת" );
	SendClientMessage ( playerid,llllGreen,"~~~~~ סטטיסטיקות פעילות ~~~~~~" );
	S_SendMsg ( playerid,lBlue, "» .%s :שם הפעילות",Acts[ IsActOn ][ ActName ]);
    S_SendMsg ( playerid,lBlue, "» .%d/%d :כמות משתתפים", ActPlayers, Acts [ IsActOn ] [ Max_Act_Players ] );
    S_SendMsg ( playerid,lBlue, "» .מצב הפעילות: %s", ActStatus > 1? ( "החלה" ) : ( "ניתן להצטרף" ) );
    return SendClientMessage ( playerid, llllGreen,"~~~~~~~~~~~~~~~~~~~~~~~~~~" );
}

CMD:astats(playerid, params[])
{
	if(ActStatus == 0 && ActOn == false && ActStatusTeams == 0) return SendClientMessage(playerid,Red, ".אין פעילות פעילה");
	new count;
	SendClientMessage(playerid, lGrey, "~~ שחקנים הנמצאים בפעילות ~~");
	foreach(new i: Player)
	{
		if(InActAuto[i] == true || InAct[i] == true || InActTeams(i))
		{
			count++;
			S_SendMsg(playerid,White, "%d. %s [id: %03d]",count,GetName(i), i);
		}
	}
	if(!count) SendClientMessage(playerid,Red, ".אין שחקנים בפעילות");
	return 1;
}

