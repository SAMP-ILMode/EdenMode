	{
	    format(TD_szStr, sizeof(TD_szStr),"BANK: %08d",pInfo [ playerid ] [ Bank ]);
		PlayerTextDrawSetString(playerid, PlayerTDStartPlayer[2][playerid], TD_szStr);
	}
	else if(!strcmp("time", type , false))
	{
		new Times[2];
	  	gettime(Times[0],Times[1]);
		format(TD_szStr, sizeof(TD_szStr),"date: %s - %02d:%02d",GetDateAsString(),Times[0],Times[1]);
		TextDrawSetString(PlayerTDStartGlobal[10],TD_szStr);
	}
	else if(!strcmp("name", type , false))
	{
		format(TD_szStr, sizeof(TD_szStr),"NICK: %s",GetName(playerid));
		PlayerTextDrawSetString(playerid, PlayerTDStartPlayer[4][playerid],TD_szStr);
	}
	else if(!strcmp("level", type , false))
	{
	    format(TD_szStr, sizeof(TD_szStr),"LEVEL: %02d",pInfo [ playerid ] [ Level ]);
		PlayerTextDrawSetString(playerid, PlayerTDStartPlayer[1][playerid], TD_szStr);
	}
	else if(!strcmp("skin", type , false))
	{
	    format(TD_szStr, sizeof(TD_szStr),"SKIN: %03d",GetPlayerSkin(playerid));
		PlayerTextDrawSetString(playerid, PlayerTDStartPlayer[3][playerid], TD_szStr);
	}
	else if(!strcmp("all", type , false))
	{
	    format(TD_szStr, sizeof(TD_szStr),"BANK: %08d",pInfo [ playerid ] [ Bank ]);
		PlayerTextDrawSetString(playerid, PlayerTDStartPlayer[2][playerid], TD_szStr);
		format(TD_szStr, sizeof(TD_szStr),"NICK: %s",GetName(playerid));
		PlayerTextDrawSetString(playerid, PlayerTDStartPlayer[4][playerid],TD_szStr);
		format(TD_szStr, sizeof(TD_szStr),"LEVEL: %02d",pInfo [ playerid ] [ Level ]);
		PlayerTextDrawSetString(playerid, PlayerTDStartPlayer[1][playerid], TD_szStr);
		format(TD_szStr, sizeof(TD_szStr),"SKIN: %03d",GetPlayerSkin(playerid));
		PlayerTextDrawSetString(playerid, PlayerTDStartPlayer[3][playerid], TD_szStr);
        for(new i; i < sizeof(PlayerTDStartGlobal); i++) TextDrawShowForPlayer(playerid, PlayerTDStartGlobal[i]);
        for(new i; i < sizeof(PlayerTDStartPlayer); i++) PlayerTextDrawShow(playerid, PlayerTDStartPlayer[i][playerid]);
	}
	return 1;
