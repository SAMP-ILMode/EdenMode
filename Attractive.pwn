			S_SendMsg(playerid,Green, "%d. Point Type: [ Tag choice to 3 Days ] [Date Start: %s] - Point End: %d Days",Cout,pInfo [ playerid ] [ DateTag ],pInfo [ playerid ] [ HaveDayTag ]-DaysBetweenDates( pInfo [ playerid ] [ DateTag ] ,GetDateAsStringPoints()));
