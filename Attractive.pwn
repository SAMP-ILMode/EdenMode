CMD:battle(playerid, params[])
{
	new command[20];
	if(sscanf(params,"s[20]{u}",command)) return SendClientMessage(playerid,White,"Usage: /Battle [Invite/Accept/Ignore/commands]");
    if(!strcmp(command, "commands", true))
	{
		SendClientMessage(playerid,Grey,":פקודות המערכת");
		SendClientMessage(playerid,White,"/Battle Invite [id] [Weapon id] :הזמנה לקרב");
		SendClientMessage(playerid,White,"/Battle Accept :הצטרפות לקרב");
		SendClientMessage(playerid,White,"/Battle Ignore :סירוב הזמנה לקרב");
		return 1;
	}
	if(!strcmp(command, "invite", true))
	{
	    if(InActAuto[playerid] == true && ActStatus >= 2) return SendClientMessage(playerid,Red, "./QAct - אינך יכול להקיש פקודות שאתה בפעילות. ליציאה, הקש");
	    if(InDm[playerid] == true) return SendClientMessage(playerid,Red,".DM-אינך יכול להשתמש בפקודה זו שאתה נימצא באזור");
		new ID, Weaponid_S[40];
  		if(sscanf(params[6],"is[40]",ID,Weaponid_S)) return SendClientMessage(playerid,White,"Usage: /Battle Invite [ID] [War / Heavy / Weapon ID]");
		if(!strval(Weaponid_S) && strcmp(Weaponid_S, "War", true) && strcmp(Weaponid_S, "heavy", true)) return SendClientMessage(playerid,White,"Usage: /Battle Invite [ID] [War / Heavy / Weapon ID]");
		if(strval(Weaponid_S) < 1 && strval(Weaponid_S) > 38) return SendClientMessage(playerid, Red, ".Heavy / War / Weapon ID סוג הבאטל אמור להיות");
		if(battle[playerid][InBattle] == 1) return SendClientMessage(playerid,Red, ".אתה כבר נמצא בקרב עם משהו");
		if(!IsPlayerConnected(ID)) return SendClientMessage(playerid,Red, ".המשתמש אינו מחובר");
		if(IsPlayerNPC( ID )) return SendClientMessage(playerid,Red, ".אין באפשרותך להשתמש בפקודה זו על בוט");
		if(ID == playerid) return SendClientMessage(playerid,Red, ".אין באפשרותך להזמין את עצמך לקרב סאון");
		if(battle[ID][InBattle] == 1) return SendClientMessage(playerid,Red, ".השחקן נמצא כבר בקרב");
		if(battle[playerid][InviteAnyOne] == 1) return SendClientMessage(playerid,Red, ".הזמנת כבר שחקן מסוים לקרב");
		if(battle[ID][InviteMe] == 1) return SendClientMessage(playerid,Red, ".משהו כבר הזמין שחקן זה לקרב");
		battle[playerid][WeaponBattle] = strval(Weaponid_S);
		battle[ID][WeaponBattle] = strval(Weaponid_S);
		if(!strcmp(Weaponid_S, "Heavy", true))
		{
			battle[playerid][WeaponBattle] = 50;
			battle[ID][WeaponBattle] = 50;
		}
 		else if(!strcmp(Weaponid_S, "War", true))
		{
			battle[playerid][WeaponBattle] = 51;
			battle[ID][WeaponBattle] = 51;
		}
		battle[ID][InviteMe] = 1;
		battle[playerid][InviteAnyOne] = 1;
		battle[ID][IDInvite] = playerid;
		battle[playerid][IDJoin] = ID;
		battle[playerid][Inviter] = 1;
		new WeaponLocel[24];
  		if(battle[ID][WeaponBattle] == 51) WeaponLocel = "War";
  		else if(battle[ID][WeaponBattle] == 50) WeaponLocel = "Heavy";
  		else GetWeaponName(battle[ID][WeaponBattle],WeaponLocel,24);
		format(String, sizeof(String), "%s :לקרב מסוג %s הזמנת את השחקן",WeaponLocel,GetName(ID));
		SendClientMessage(playerid,0x00EC00FF,String);
		format(String, sizeof(String), "%s :הזמין אותך לקרב מסוג %s השחקן",WeaponLocel,GetName(playerid));
		SendClientMessage(ID,0x00EC00FF,String);
		SendClientMessage(ID,0x00EC00FF,"/Battle Accept - לאישור ,/Battle Ignore - לסירוב");
		battle[playerid][TimerBattle] = SetTimerEx("NoAcceptBattle",20*1000, 0, "%d", playerid);
		return 1;
	}
	if(!strcmp(command, "accept", true))
	{
	    if(InActAuto[playerid] == true && ActStatus >= 2) return SendClientMessage(playerid,Red, "./QAct - אינך יכול להקיש פקודות שאתה בפעילות. ליציאה, הקש");
        if(InDm[playerid] == true) return SendClientMessage(playerid,Red,".DM-אינך יכול להשתמש בפקודה זו שאתה נימצא באזור");
		new IdInvite;
		IdInvite = battle[playerid][IDInvite];
		if(battle[playerid][InviteMe] == 0) return SendClientMessage(playerid,Red, ".אף אחד לא הזמין אותך לקרב");
		if(InDm[IdInvite] == true) return SendClientMessage(playerid,Red,".לכן לא יוכל להתחיל הקרב DM השחקן השני נמצא באזור");
		QuitGAll(playerid);
		QuitGAll(IdInvite);
		KillTimerEx(battle[IdInvite][TimerBattle]);
		battle[playerid][InviteMe] = 0;
   		battle[IdInvite][InviteAnyOne] = 0;
   		ResetPlayerWeapons( playerid );
   		ResetPlayerWeapons( IdInvite );
   		SetPlayerVirtualWorld(IdInvite, playerid + 7 );
   		SetPlayerVirtualWorld(playerid, playerid + 7 );
		if(battle[playerid][WeaponBattle] == 50)
		{
  			GivePlayerWeapon(playerid,33,9999);
     		SetPlayerPosEx(playerid,-1130.0553,1057.7872,1346.4141,269.7668);
       		SetPlayerInterior(playerid,10);
		}
		else if(battle[playerid][WeaponBattle] == 51)
   		{
			SetPlayerInterior(playerid,16);
			SetPlayerPosEx(playerid,-1417.9404,1245.9352,1039.8672,271.5372);
			GivePlayerWeapon(playerid, 26,9999);
		}
		else
		{
			SetPlayerInterior(playerid,16);
			SetPlayerPosEx(playerid,-1417.9404,1245.9352,1039.8672,271.5372);
			GivePlayerWeapon(playerid, battle[playerid][WeaponBattle] ,9999);
		}
		TogglePlayerControllable(playerid, 0);
		SetPlayerHealth(playerid, 99);
		SetPlayerArmour(playerid,99);
		//GameTextForPlayer(playerid,"~W~Battle",1500,1);
		if(battle[IdInvite][WeaponBattle] == 50)
 		{
			SetPlayerPosEx(IdInvite,-974.7115,1061.0691,1345.6774,90.1350);
   			GivePlayerWeapon(IdInvite,33,9999);
      		SetPlayerInterior(IdInvite,10);
		}
		else if(battle[IdInvite][WeaponBattle] == 51)
       	{
        	SetPlayerPosEx(IdInvite,-1373.5332,1246.7544,1039.8741,90.7421);
		    GivePlayerWeapon(IdInvite, 26,9999);
      		SetPlayerInterior(IdInvite,16);
		}
		else
		{
			SetPlayerInterior(IdInvite,16);
			SetPlayerPosEx(IdInvite,-1373.5332,1246.7544,1039.8741,90.7421);
			GivePlayerWeapon(IdInvite, battle[IdInvite][WeaponBattle] ,9999);
		}
		TogglePlayerControllable(IdInvite, 0);
		SetPlayerHealth(IdInvite, 99);
		SetPlayerArmour(IdInvite,99);
		//
		battle[playerid][joiner] = 1;
		battle[playerid][InBattle] = 1;
		battle[IdInvite][InBattle] = 1;
		battle[playerid][CountBattle] = 5;
		battle[IdInvite][CountBattle] = 5;
		battle[playerid][TimerCountBattle] = SetTimerEx("CountBattlE",1*1000, 1, "%d", playerid,1);
		battle[IdInvite][TimerCountBattle] = SetTimerEx("CountBattlE",1*1000, 1, "%d", IdInvite);
		SetCameraBehindPlayer(playerid);
		SetCameraBehindPlayer(IdInvite);
		return 1;
	}
	if(!strcmp(command, "Ignore", true))
	{
		if(battle[playerid][InviteMe] == 0) return SendClientMessage(playerid , Red , ".אף אחד לא הזמין אותך לקרב");
		format(String, sizeof(String), ">> [Battle System] .לקרב סוואן %s סירבת בהצלחה להזמנה של השחקן", GetName(battle[playerid][IDInvite]));
		SendClientMessage(playerid,White,String);
		format(String, sizeof(String), ">> [Battle System] .סירב להזמנתך לקרב %s השחקן",GetName(playerid));
		SendClientMessage(battle[playerid][IDInvite],-1,String);
		battle[battle[playerid][IDInvite]][WeaponBattle] = 0;
		battle[playerid][WeaponBattle] = 0;
		battle[playerid][InviteMe] = 0;
		battle[battle[playerid][IDInvite]][InviteAnyOne] = 0;
		KillTimerEx(battle[battle[playerid][IDInvite]][TimerBattle]);
		return 1;
	}
	return SendClientMessage(playerid,Red, ".פקודה לא קיימת במערכת");
}
//------------------------- System Props -------------------------
CMD:createprop(playerid, params[])
{
	if(!IsPlayerAdmin(playerid)) return 0;
	if(IsPlayerInAnyVehicle(playerid)) return SendClientMessage(playerid,Red, ".לא ניתן לצור נכסים כאשר אתה ברכב");
	new Cost_P,
		Profit_P,
		NameProp_P[21];
	if(sscanf(params,"iis[40]",Cost_P,Profit_P,NameProp_P)) return SendClientMessage(playerid,White,"Usage: /CreateProp [Cost] [Profit] [NameProp] :אופן השימוש");
 	if(Profit_P < 1 || Profit_P > 10000) return SendClientMessage(playerid, Red, ".רווח הנכס יכול להיות בין 1$ ל 10,000$");
	if(IsNickValid(NameProp_P)) return SendClientMessage(playerid, Red, ".שם זה אינו חוקי לנכס , שם הנכס חייב להיות בעברית");
	if(strlen(NameProp_P) < 3 || strlen(NameProp_P) > 20) return SendClientMessage(playerid, Red, ".שם הנכס יכול להיות מ-2 תווים עד 20 תווים");
	for(new PropID = 1; PropID < MAX_PropS; PropID++)
	{
		if(!IsValidDynamicPickup(PropPickup[PropID]))
		{
		    if(PropID == 31) return SendClientMessage(playerid, Red, ".אי אפשר ליצור יותר מ 30 נכסים");
			new Float:X,Float:Y,Float:Z;
			GetPlayerPos(playerid, X, Y, Z);
			if(!fexist(PropsFile(PropID)))
			{
				PropInfo [ PropID ] [ Cost ] = Cost_P;
				PropInfo [ PropID ] [ Profit ] = Profit_P;
				PropInfo [ PropID ] [ HaveOwner ] = false;
                format (PropInfo [ PropID ] [ OwnerP ],5,"N/A");
				PropInfo [ PropID ] [ PX ] = X;
				PropInfo [ PropID ] [ PY ] = Y;
				PropInfo [ PropID ] [ PZ ] = Z;
				PropInfo [ PropID ] [ NameProp ] = NameProp_P;
				PropInfo [ PropID ] [ HaveReferralOwner ] = false;
				format (PropInfo [ PropID ] [ Referral_Owner ],5,"N/A");
				PropInfo [ PropID ] [ Referral_Cost ] = 0;
    			new INI:File = INI_Open(PropsFile(PropID));
 				INI_WriteString(File,"NameProp",PropInfo [ PropID ] [ NameProp ]);
				INI_WriteString(File,"Owner",PropInfo [ PropID ] [ OwnerP ]);
				INI_WriteBool ( File,"HaveOwner",PropInfo [ PropID ] [ HaveOwner ]);
				INI_WriteInt(File,"Cost",PropInfo [ PropID ] [ Cost ]);
				INI_WriteInt(File,"Profit",PropInfo [ PropID ] [ Profit ]);
				INI_WriteFloat(File,"X",PropInfo [ PropID ] [ PX ]);
				INI_WriteFloat(File,"Y",PropInfo [ PropID ] [ PY ]);
				INI_WriteFloat(File,"Z",PropInfo [ PropID ] [ PZ ]);
				INI_WriteBool ( File,"HaveReferralOwner",PropInfo [ PropID ] [ HaveReferralOwner ]);
				INI_WriteString(File,"Referral_Owner",PropInfo [ PropID ] [ Referral_Owner ]);
				INI_WriteInt(File,"Referral_Cost",PropInfo [ PropID ] [ Referral_Cost ]);
            	INI_Close ( File );
			}
			Prop_UpdateEntrance(PropID);
			format(String, sizeof(String), "(%d :יצרת נכס, בהצלחה (אידי הנכס", PropID);
			SendClientMessage(playerid, Grey,String);
			return 1;
		}
	}
	return 1;
}

CMD:resetprop(playerid, params[])
{
	if(!IsPlayerAdmin(playerid)) return 0;
