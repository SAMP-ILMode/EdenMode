			S_SendMsgToAll(Red, "*** \"%s\" Has been banned by \"%s\" for %d days. (Reason: %s)",Name_ba,GetName(playerid),Days, Reason_ba);
		}
	}
	else
	{
        new ID = strval(Name_ba);
		if(!IsPlayerConnected( ID )) return SendClientMessage(playerid,Red, ".המשתמש אינו מחובר");
		new INI:File = INI_Open(PBan( ID ));
		INI_WriteString( File , "Admin",GetName(playerid));
