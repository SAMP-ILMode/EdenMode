function CemeteryClan(playerid)
{
	Occupying_time[playerid] ++;
	//-------------- כמה שחקנים מהקלאן שמשטלת יש באזור -----------------
	new PlayerClanCemetery;
 	foreach(new i: Player)
	{
		if(Occupying_Clan[playerid] == InClan(i))
		{
	    	if(IsPlayerInDynamicArea(i, AreaHq[ StartClanArea[playerid] ])) PlayerClanCemetery++;
		}
	}
	if(Occupying_time[playerid] % 2 == 0)
	{
	    if(ClanArea[StartClanArea[playerid]] == 1) GangZoneShowForAll(GangZone[StartClanArea[playerid]],rgba2hex(ClanInfo [ StartClanArea[playerid] ] [ ClanR ] , ClanInfo [ StartClanArea[playerid] ] [ ClanG ] , ClanInfo [ StartClanArea[playerid] ] [ ClanB ] ,150));
     	if(ClanArea[StartClanArea[playerid]] == 3) GangZoneShowForAll(GangZone[StartClanArea[playerid]],rgba2hex(ClanInfo [ Clanlozer [ StartClanArea[playerid] ] ] [ ClanR ] ,ClanInfo [ Clanlozer [ StartClanArea[playerid] ] ] [ ClanG ],ClanInfo [ Clanlozer [ StartClanArea[playerid] ] ] [ ClanB ],150));
	}
	else GangZoneShowForAll(GangZone[StartClanArea[playerid]],rgba2hex( ClanInfo [ Occupying_Clan[playerid] ] [ ClanR ],ClanInfo [ Occupying_Clan[playerid] ] [ ClanG ],ClanInfo [ Occupying_Clan[playerid] ] [ ClanB ],150));

 	if(PlayerClanCemetery >= 0 && PlayerClanCemetery < MorePlayerInArea || !fexist(FileClan(StartClanArea[playerid])) || !fexist(FileClan( Occupying_Clan[playerid] )))
	{
	    S_SendMsgToAll(0xA5EBACFF, ">• בוטלה מכיוון שלא נמצאו 4 שחקני קלאן באזור %s%s {A5EBAC}על מפקדת הקלאן %s%s {A5EBAC}ההשתלטות של הקלאן",ColorClan( StartClanArea[playerid] ),NameClan( StartClanArea[playerid] ),ColorClan( Occupying_Clan[playerid] ),NameClan( Occupying_Clan[playerid] ));
	    if(ClanArea[StartClanArea[playerid]] == 1)
	    {
 			GangZoneShowForAll(GangZone[StartClanArea[playerid]],rgba2hex(ClanInfo [ StartClanArea[playerid] ] [ ClanR ],ClanInfo [ StartClanArea[playerid] ] [ ClanG ], ClanInfo [ StartClanArea[playerid] ] [ ClanB ],150));
			ClanArea[ StartClanArea[playerid] ] = 0;
		}
		if(ClanArea[StartClanArea[playerid]] == 3)
	    {
			//S_SendMsgToAll(Red, ">• בוטלה בגלל שנמצאו במקום פחות מ 4 שחקנים %s%s{"#C_Red"} שנמצת בשליטת הקלאן %s%s{"#C_Red"} על מפקדת הקלאן %s%s{"#C_Red"} השתלטות הקלאן",ColorClan( Clanlozer[ StartClanArea[playerid] ] ),ColorClan( Clanlozer[ StartClanArea[playerid] ] ),  ColorClan( StartClanArea[playerid] ),NameClan( StartClanArea[playerid] ),ColorClan( Occupying_Clan[playerid] ),NameClan( Occupying_Clan[playerid] ));
			GangZoneShowForAll(GangZone[StartClanArea[playerid]],rgba2hex(ClanInfo [ Clanlozer [ StartClanArea[playerid] ] ] [ ClanR ],ClanInfo [ Clanlozer [ StartClanArea[playerid] ] ] [ ClanG ],ClanInfo [ Clanlozer [ StartClanArea[playerid] ] ] [ ClanB ],150));
        	ClanArea[ StartClanArea[playerid] ] = 2;
		}
		Occupying_Clan[playerid] = 0;
		StartClanArea[playerid] = 0;
		KillTimerEx(Timer_ClanArea[playerid]);
		Occupying_time[playerid] = 0;
		return 1;
	}
	if(Occupying_time[playerid] == 60*howemach_AreaWar)
	{
 		if(ClanArea[StartClanArea[playerid]] == 3)
		{
   			if(StartClanArea[playerid] == Occupying_Clan[playerid])
   			{
				S_SendMsgToAll(Yellow, ">• הצליח להשתלט בחזרה על מפקדת הקלאן שלו %s%s{"#C_Yellow"} הקלאן",ColorClan( Occupying_Clan[playerid] ),NameClan( Occupying_Clan[playerid] ));
				Clanlozer[ StartClanArea[playerid] ] = 0;
				ClanArea[ StartClanArea[playerid] ] = 0;
			}
			else
			{
				S_SendMsgToAll( Yellow , ">• %s%s{"#C_Yellow"} הצליח להשתלט על מפקדת הקלאן %s%s{"#C_Yellow"} הקלאן",ColorClan( StartClanArea[playerid] ),NameClan( StartClanArea[playerid] ),ColorClan( Occupying_Clan[playerid] ),NameClan( Occupying_Clan[playerid] ));
				Clanlozer [ StartClanArea[playerid] ] = Occupying_Clan[playerid];
				ClanArea [ StartClanArea[playerid] ] = 2;
			}
		}
  		else if(ClanArea[ StartClanArea [ playerid ] ] == 1)
  		{
   			S_SendMsgToAll( Yellow , ">• %s%s{"#C_Yellow"} הצליח להשתלט על מפקדת הקלאן %s%s{"#C_Yellow"} הקלאן",ColorClan( StartClanArea[playerid] ),NameClan( StartClanArea[playerid] ),ColorClan( Occupying_Clan[playerid] ),NameClan( Occupying_Clan[playerid] ));
 			Clanlozer[ StartClanArea [ playerid ] ] = Occupying_Clan[playerid];
			ClanArea[ StartClanArea [ playerid ] ] = 2;
		}
		GangZoneShowForAll(GangZone[ StartClanArea [ playerid ] ],rgba2hex(ClanInfo [ Occupying_Clan[playerid] ] [ ClanR ],ClanInfo [ Occupying_Clan[playerid] ] [ ClanG ],ClanInfo [ Occupying_Clan[playerid] ] [ ClanB ],150));
        Occupying_time[playerid] = 0;
        StartClanArea [ playerid ] = 0;
        Occupying_Clan[playerid] = 0;
		KillTimerEx( Timer_ClanArea [ playerid ] );
		return 1;
	}
	if(Occupying_time[playerid] % 60 == 0)
	{
		SendClanMessageEx(Occupying_Clan[playerid],Yellow, "%s%s{"#C_Yellow"} - %s%s{"#C_Yellow"} נשארו עוד {"#C_White"}%d {"#C_Yellow"}דקות לסיום ההשתלטות על מפקדת",ColorClan( Occupying_Clan[playerid] ),NameClan( Occupying_Clan[playerid] ),ColorClan( StartClanArea[playerid] ),NameClan(StartClanArea[playerid]),howemach_AreaWar-Occupying_time[playerid]/60);
	}
    return 1;
}

public OnDynamicObjectMoved(objectid)
{
	if(objectid == GateClovGold) return 1;
 	for( new i, j = GetMaxPlayers(); i < j; i ++)
	{
		if(objectid == tilnontet[0][i])
		{
			if(IsValidDynamicObject( tilnontet[0][i] ))
