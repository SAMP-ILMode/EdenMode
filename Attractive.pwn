 		else S_SendMsgToAll(Red, "*** %s has been Warning %d/3 before Banned by %s %s. (Reason: %s)", GetName( ID_wa ),pInfo [ ID_wa ] [ Warning ],AdminLevelName(LevelAdmin(playerid)),GetName(playerid), Reason_wa );
