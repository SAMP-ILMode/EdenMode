CMD:call(playerid, params[])
{
 	new Player;
	if(sscanf(params,"i",Player)) return SendClientMessage(playerid,White,"Usage: /Call [ID]");
	if(Tallking[playerid] == 1) return SendClientMessage(playerid, Red,".אתה כבר בשיחה");
	if(Player == playerid) return SendClientMessage(playerid,Red,".אינך יכול להתקשר לעצמך");
	if(!IsPlayerConnected(Player)) return SendClientMessage(playerid, Red, ".המשתמש אינו מחובר");
	if(Ignore[playerid][Player])return SendClientMessage(playerid,Red,".חסמת את השחקן הזה");
	if(Ignore[Player][playerid])return SendClientMessage(playerid,Red,".השחקן חסם אותך");
	if(InCall[Player] == 1)
	{
		SendClientMessage(playerid,Red,".השחקן כבר בשיחה... הודעה על כך שניסית לצור איתו קשר נשלחה אליו");
		return S_SendMsg(Player,lGrey, ".ניסה להתקשר אליך %s(%d)",GetName(playerid),playerid);
	}
	S_SendMsg(playerid,lGreen, "... אנא המתן %s מחייג אל",GetName(Player));
 	S_SendMsg(Player,lGreen, ".מחייג אליך %s השחקן",GetName(playerid));
	SendClientMessage(Player,Green,"/Answer{"#C_lGreen"} :בכדי לענות הקש/י | {"#C_Red"}/IgnoreCall{"#C_lGreen"} :בכדי להתעלם מהשיחה הקש/י");
	EndTimer[Player] = SetTimerEx("EndCall",30000,0,"iii",playerid,Player,1);
	PlayerPlaySound(Player,1056,0.0,0.0,0.0);
	TallkingID[playerid] = Player;
	TallkingID[Player] = playerid;
	IsCalling[playerid] = 1;
	InCall[playerid] = 1;
	return InCall[Player] = 1;
}
CMD:answer(playerid, params[])
{
	if(IsCalling[playerid] == 1) return SendClientMessage(playerid, Red,".אתה מתקשר, אינך יכול לענות");
	if(Tallking[playerid] == 1) return SendClientMessage(playerid, Red,".אתה כבר בשיחה");
	if(InCall[playerid] == 0) return SendClientMessage(playerid, Red,".אף אחד לא מתקשר אליך");
	KillTimerEx(EndTimer[playerid]);
 	S_SendMsg(playerid,lGreen, ".כעת אתם יכולים לדבר ,%s ענית לשיחה מאת",GetName(TallkingID[playerid]));
	S_SendMsg(TallkingID[playerid],lGreen,".ענה לשיחתך, כעת אתם יכולים לדבר %s",GetName(playerid));
	SendClientMessage(TallkingID[playerid],Red,"/Hangup{"#C_lGreen"} - בכדי לנתק את השיחה, הקש/י");
	Tallking[TallkingID[playerid]] = 1;
	Tallking[playerid] = 1;
	return InCall[playerid] = 1;
}
CMD:hangup(playerid, params[])
{
	if(Tallking[playerid] == 0) return SendClientMessage(playerid, Red,".אתה לא בשיחה");
	if(IsPlayerConnected(TallkingID[playerid]) && Tallking[playerid] == 1 && TallkingID[playerid] > -1 && Tallking[TallkingID[playerid]] == 1)
	{
