CMD:hq(playerid, params[])
{
    if(InActAuto[playerid] == true && ActStatus >= 2) return SendClientMessage(playerid,Red, "./QAct - אינך יכול להקיש פקודות שאתה בפעילות. ליציאה, הקש");
	if(!InClan(playerid)) return SendClientMessage(playerid, Red,".אינך נמצא בשום קלאן");
	if(ClanInfo [ InClan(playerid) ] [ HasHQ ] == false) return SendClientMessage(playerid, Red, ".אין לקלאן שלך מפקדה");
	if(!MyClanReload(playerid)) return SendClientMessage(playerid, Red, ".מפקדת הקלאן שלך לא טעונה");
    QuitGAll(playerid);
	if(GetPlayerState ( playerid ) == 2)
	{
    	if(!ClanInfo [ InClan(playerid) ] [ VCpX ] || !ClanInfo [ InClan(playerid) ] [ VCpY ] || !ClanInfo [ InClan(playerid) ] [ VCpZ ]) return SendClientMessage(playerid,Red, ".אין לקלאן שלך מיקום עם רכב למפקדה");
		SetVehiclePos(GetPlayerVehicleID(playerid),ClanInfo [ InClan(playerid) ] [ VCpX ],ClanInfo [ InClan(playerid) ] [ VCpY ],ClanInfo [ InClan(playerid) ] [ VCpZ ]);
	}
	else
	{
	    if(!ClanInfo [ InClan(playerid) ] [ CpX ]  || ! ClanInfo [ InClan(playerid) ] [ CpY ] || !ClanInfo [ InClan(playerid) ] [ CpZ ]) return SendClientMessage(playerid,Red, ".אין לקלאן שלך מיקום למפקדה");
	    SetPlayerPos(playerid, ClanInfo [ InClan(playerid) ] [ CpX ] , ClanInfo [ InClan(playerid) ] [ CpY ] , ClanInfo [ InClan(playerid) ] [ CpZ ]);
	}
	SetPlayerVirtualWorld ( playerid, 0 );
	SetPlayerInterior ( playerid, 0 );
	format(String,sizeof(String),"~b~Welcome To Hq~N~ ~b~~H~~H~%s!",ClanPlayer(playerid));
	GameTextForPlayer(playerid,String,2500,4);
	S_SendMsg(playerid, White ,"%s.%s השתגרת למפקדת הקלאן שלך",ColorClan(InClan(playerid)),ClanPlayer(playerid));
	return 1;
}
