public OnPlayerClickMap(playerid, Float:fX, Float:fY, Float:fZ)
{
	if(InTTmap == true)
	{
        if(AFKTime[playerid] == 1000) return SendClientMessage(playerid,Red,"./AFK - אתה לא יכול לבצע פקודות שאתה במצב לא נמצא, ליציאה ממצב לא נמצא הקש/י");
		if(SMPInfo [ playerid ] [ InMissions ] == true && SMPInfo [ playerid ] [ MissionsingNumber ] != 5) return SendClientMessage(playerid,Red,".אין אפשרות לעשות פקודות באמצע המשימה");
		if(Ob_Cage[playerid] > 0) return SendClientMessage(playerid,Red, ".אינך יכול להקיש פקודות בזמן שאתה נימצא בכלוב");
		if(InActAuto[playerid] == true)
 		{
			if(ActStatus == 2 || ActStatus == 3) return SendClientMessage(playerid,Red, "./QAct - אינך יכול להקיש פקודות שאתה בפעילות. ליציאה, הקש");
		}
		if(PWarBattle[playerid][InPWar] == true) return SendClientMessage(playerid,Red, ".אתה בקרב אישי, ולכן אין באפשרותך לבצע פקודות");
		if(InActTeams(playerid) && ActStatusTeams == 2) return SendClientMessage(playerid,Red, "./QAct - אינך יכול להקיש פקודות שאתה בפעילות. ליציאה, הקש");
		if(GetPVarInt(playerid,"Spec")) return SendClientMessage(playerid,Red, "/unSpec /SpecCaroff - הקש/י Spec אתה צופה בשחקן / ברכב כעת, ואינך יכול לבצע פקודות בשביל לצאת ממצב");
		if(InAct[playerid] == true && pInfo [ playerid ] [ Staff ] == false) return SendClientMessage(playerid,Red,"/Act Quit -לא ניתן לבצע פקודות בפעילות חוץ מ");
 		if(InRules[playerid] == true) return SendClientMessage(playerid, Red, ".אינך יכול לבצע פקודות כאשר אתה בקריאת חוקים");
    	if(InClanWar[playerid] == 1) return SendClientMessage(playerid,Red, ".אין באפשרותך לבצע פקודות באמצע קרב קלאנים");
    	if(SRampPInfo [ playerid ] [ InStunt ] == true) return SendClientMessage(playerid,Red,".אין באפשרותך לעשות פקודות באמצע אתגר הסטאנט ");
		if(GetPlayerVirtualWorld(playerid) == 355) return SendClientMessage(playerid,Red,".על מנת לבצע פקודות הינך צריך להיכנס למשחק");
		if(logged[playerid] == false) return SendClientMessage(playerid,Red, ".על מנת להתחיל לשחק הינך, מחוייב להתחבר לחשבונך");
		if(fexist(Jail(playerid))) return S_SendMsg(playerid,Red, ".לא ניתן לבצע פקודות כאשר אתה בכלא, תוכל לכתוב שוב בעוד %d שניות",TimeJail[playerid]);
        if(LightHouseSpector[playerid] != -1) return SendClientMessage(playerid,Red,"./lhoff אנה המתן 30 שניות עד שתיגמר הצפייה, ואם אינך רוצה לצאת השתמש בפקודה");
		if(GetPlayerState(playerid) == PLAYER_STATE_DRIVER) SetVehiclePos(GetPlayerVehicleID(playerid),fX,fY,fZ + 0.5);
		else SetPlayerPos(playerid, fX,fY,fZ);
        SendClientMessage(playerid,lGreen, ".\"Teleports through the map\" השתגרת דרך המפה לשיגור מסוים על ידי האופציה המופעלת");
	}
	if(!IsPlayerInRangeOfPoint(playerid, 30.0, fX, fY, fZ) && InTTmap == false)
	{
 		MapPosX[playerid] = fX;
		MapPosY[playerid] = fY;
		MapPosZ[playerid] = fZ;
		ResetMapVars(15, playerid);
	}
    return 1;
}

forward ResetMapVars(cd, playerid);
public ResetMapVars(cd, playerid)
{
	if(cd > 0)
	{
		if(MapPosX[playerid] != 0.0 || MapPosY[playerid] != 0.0)
		{
		    if(IsPlayerInRangeOfPoint(playerid, 30.0, MapPosX[playerid], MapPosY[playerid], MapPosZ[playerid]))
			{
				if(timeleftcheats[playerid]  - gettime() > 0 && Report_DMGuard == true) return LogAntiCheat(playerid,"Cheats: Teleports through the map (#08)",true,true);
    			timeleftcheats[playerid] = gettime() + 6;
				DMGuardMsgToAdmin("[DM Guard] .Teleports through the map חשוד שהשתמש בצ'יט %s[ID: %d] השחקן",GetName(playerid),playerid);
			}
		}
		cd--;
		return SetTimerEx("ResetMapVars",1000,false,"dd",cd, playerid);
	}
	else
	{
	   	MapPosX[playerid] = 0.0;
		MapPosY[playerid] = 0.0;
		MapPosZ[playerid] = 0.0;
	}
	return 1;
}

function TimeNight()
{
    gettime(GetTime[0],GetTime[1],GetTime[2]);
    if(GetTime[0] == 00 && GetTime[1] == 00 && GetTime[2] == 00)
    {
    	SendClientMessageToAll(Blue, "Time UpDate:{"#C_White"} .השעה כעת היא 12:00 בלילה");
		printf("===================== Date: %s ============================",GetDateAsString());
    	SetTimer("TimeNight",(24 * 3600) * 1000,false);
    	CloseServerDate();
   		Server [ Taodc ] = 0;
    	Server [ MaxPlayersDay ] = 0;
		for(new i, j = GetPlayerPoolSize(); i <= j; i++)
		{
			if(!IsPlayerConnected(i) || logged[i] == false) continue;
			format(pInfo [ i ] [ LastLogin ], 15, "%s",GetDateAsStringPoints());
			new INI:File = INI_Open(PFile(i));
			INI_WriteString ( File, "LastLogin",pInfo [ i ] [ LastLogin ]);
			INI_Close ( File );
 			Server [ Taodc ]++;
    	    Server [ MaxPlayersDay ]++;
		}
 		if(!strcmp("שבת",getDayName(true), true))
		{
			for ( new i; i < MAX_Staff; i ++)
			{
 				if(fexist(PStaff(i)))
				{
					inline LoadS(string:name[], string:value[]) INI_Int("UserID",id);
					INI_ParseFile(PStaff(i), using inline "LoadS");
  					inline Loadusersu(string:name[], string:value[]) INI_String("NameUser",NameUserID,25);
					INI_ParseFile(GetUserID(id), using inline "Loadusersu");
  					if(fexist(PlayerNameFile(NameUserID)))
    				{
						new INI:File = INI_Open(PlayerNameFile(NameUserID));
						INI_WriteString ( File, "ClockStaff","0");
						INI_Close ( File );
					}
				}
			}
			for(new i, j = GetPlayerPoolSize(); i <= j; i++)
			{
				if(!IsPlayerConnected(i) || logged[i] == false) continue;
				format(pInfo [ i ] [ ClockStaff ], 2 , "0");
				ClockStaff1[ i ] = gettime();
			}
		}
	}
	else SetTimer("TimeNight",1000,false);
	return 1;
}
