			}
			else if(ClanInviteWar[ClanWarTeam[1]][2] == playerid || ClanInviteWar[ClanWarTeam[1]][3] == playerid || ClanInviteWar[ClanWarTeam[1]][4] == playerid)
			{
				PlayersCw_Team[0]--;
				InClanWar[playerid] = 0;
				SpawnPlayer(playerid);
				if(PlayersCw_Team[0]) S_SendMsgToAll(Orange, "»» [Clan War] .נכנס למצב אסקייפ, וכעת נשארו במלחמת הקלאנים: %d שחקנים %s%s {"#C_Orange"}מקלאן %s",PlayersCw_Team[0],ColorClan( ClanWarTeam[1] ),ClanInfo [ ClanWarTeam[1] ] [ ClanName ], GetName(playerid));
			}
