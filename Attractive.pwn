CMD:hitman(playerid, params[])
{
	Tolevel(playerid,4);
    if(InActAuto[playerid] == true && ActStatus >= 2) return SendClientMessage(playerid,Red, "./QAct - אינך יכול להקיש פקודות שאתה בפעילות. ליציאה, הקש");
	new Player,
		Amount;
	if(sscanf(params,"ii",Player,Amount)) return SendClientMessage(playerid,White,"Usage: /Hitman [playerid] [amount]");
	if(!IsPlayerConnected(Player))return SendClientMessage(playerid,Red, ".המשתמש אינו מחובר");
	if(Amount > Y_GetPlayerMoney(playerid))return SendClientMessage(playerid, Red, ".אין בידך סכום זה");
	if(Amount < 5000 || Amount > 1000000)return SendClientMessage(playerid, Red, ".הסכום חייב להיות בין 5,000 ל1,000,000");
	if(Player == playerid)return SendClientMessage(playerid, Red, ".לא ניתן לשים פרס על עצמך");
    if(IsPlayerNPC( Player )) return SendClientMessage(playerid,Red, ".אין באפשרותך להשתמש בפקודה זו על בוט");
	bounty[Player] += Amount;
	Y_GivePlayerMoney(playerid,-Amount);
	S_SendMsgToAll(Orange, "» {"#C_UserRegiser"}%d${"#C_Orange"} :על סך {"#C_llBlue"}%s(%d){"#C_Orange"} שם פרס על ראשו של השחקן {"#C_llBlue"}%s(%d) {"#C_Orange"}השחקן «",Amount,GetName(Player),Player,GetName(playerid),playerid);
	return S_SendMsg(Player,Green, ".%d$ :שם פרס על ראשך בסך %s(%d) השחקן",Amount,GetName(playerid),playerid);
}
