{
	if(LevelAdmin(playerid) < 1 && pInfo [ playerid ] [ ClanAccess ] == false && pInfo [ playerid ] [ Staff ] == false && pInfo [ playerid ] [ NightTeam ] < 1) return 0;
	if(!AdmLog[playerid] && LevelAdmin(playerid))return SendClientMessage(playerid,Red,"ERROR: אינך מחובר למערכת אדמינים");
	if(pInfo [ playerid ] [ NightTeam ] && !GetTNT() && pInfo [ playerid ] [ NightTeam ] != 3) return SendClientMessage(playerid,Red,".הגישות של הצוות לילה ניתנות מהשעה 7 עד לשעה 6 בבוקר");
	new ID,
	    Time_ja,
	    Reason_ja[128];
	sscanf(params,"iis[128]",ID,Time_ja,Reason_ja);
	if(sscanf(params,"ii",ID,Time_ja)) return SendClientMessage(playerid,White,"Usage: /Jail [id] [Time] [Reason]");
	if(!IsPlayerConnected( ID )) return SendClientMessage(playerid,Red, ".המשתמש אינו מחובר");
	if(fexist( Jail( ID ) )) return SendClientMessage(playerid,Red, ".שחקן זה כבר נימצא בכלא");
    if(Time_ja < 1 || Time_ja > 1800) return SendClientMessage(playerid,Red, ".אין באפשרותך לתת כלא ליותר מ-1800 שניות");
	if(LevelAdmin( ID ) > LevelAdmin(playerid))return SendClientMessage(playerid, Red, ".אתה לא יכול לעשות פקודה זו על אדמין זה מכיוון שהוא רמה יותר גבוהה ממך");
    if(IsPlayerNPC( ID )) return SendClientMessage(playerid,Red, ".אין באפשרותך להשתמש בפקודה זו על בוט");
	new Ntime[16];
	FormatTime(Time_ja, Ntime);
	if(strlen( Reason_ja )) S_SendMsgToAll(Red, "*** \"%s\" has been jailed for %s by %s \"%s\". (Reason: %s)",GetName( ID ),Ntime,AdminLevelName(LevelAdmin(playerid)) ,GetName(playerid), Reason_ja);
	else S_SendMsgToAll(Red, "*** \"%s\" has been jailed for %s by %s \"%s\".",GetName( ID ),Ntime,AdminLevelName(LevelAdmin(playerid)) ,GetName(playerid));
    GiveJail(ID,Time_ja,Reason_ja);
	return 1;
}

stock GiveJail(playerid,Time_ja,Reason_ja[])
{
	new INI:File = INI_Open(Jail( playerid ));
	INI_WriteInt(File, "Time",Time_ja);
	INI_WriteString(File, "Reason", strlen( Reason_ja )?( Reason_ja ):("אין סיבה"));
	INI_Close ( File );
	TimeJail[ playerid ] = Time_ja;
	QuitGAll( playerid );
	SetPlayerPos(playerid, 266.7516 - 1.5, 77.8246, 1001.0391);
  	ResetPlayerWeapons(playerid);
	SetPlayerInterior(playerid, 6);
	SetPlayerVirtualWorld(playerid,playerid+2);
	UnJailTimer [ playerid ] = SetTimerEx ( "UnJail", 1000 , true, "i" , playerid );
    TextDrawCreateJail(playerid,Time_ja);
}
