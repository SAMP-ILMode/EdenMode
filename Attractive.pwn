function EndCall(id1,id2,reason)
{
	if(reason == 1) // no answer.
	{
		if(InCall[id1] == 1)
		{
			InCall[id1] = 0;
			SetPlayerSpecialAction(id1,SPECIAL_ACTION_STOPUSECELLPHONE);
			S_SendMsg(id1,Red,".לא ענה לשיחתך \"%s\"",GetName(id2));
		}
		S_SendMsg(id2,Red,".\"%s\" לא ענית לשיחה",GetName(id1));
		InCall[id1] = 0;
		InCall[id2] = 0;
		InCall[id1] = 0;
		InCall[id2] = 0;
		Tallking[id1] = 0;
		Tallking[id2] = 0;
		TallkingID[id1] = -1;
		TallkingID[id2] = -1;
		IsCalling[id1] = 0;
	}
	else if(reason == 2) // no money.
	{
		SendClientMessage(id1, Red, ".השיחה נותקה, אין לך מספיק כסף");
		SendClientMessage(id2, Red, ".השיחה נותקה מאחר לבעל השיחה אין מספיק כסף");
		InCall[id1] = 0;
		InCall[id2] = 0;
		InCall[id1] = 0;
		InCall[id2] = 0;
		Tallking[id1] = 0;
		Tallking[id2] = 0;
		TallkingID[id1] = -1;
		TallkingID[id2] = -1;
		IsCalling[id1] = 0;
	}
	else if(reason == 3)
	{
		InCall[id1] = 0;
		InCall[id2] = 0;
		Tallking[id1] = 0;
		Tallking[id2] = 0;
		TallkingID[id1] = -1;
		TallkingID[id2] = -1;
		IsCalling[id1] = 0;
	}
	return 1;
}

