            SMPInfo [ playerid ] [ TD_M ] = CreatePlayerTextDraw(playerid,294.000000, 426.000000, "Started");
			PlayerTextDrawFont(playerid,SMPInfo [ playerid ] [ TD_M ], 3);
			PlayerTextDrawLetterSize(playerid,SMPInfo [ playerid ] [ TD_M ], 0.500000, 1.400000);
			PlayerTextDrawColor(playerid,SMPInfo [ playerid ] [ TD_M ], -1);
            PlayerTextDrawShow(playerid,SMPInfo [ playerid ] [ TD_M ]);
			SetCameraBehindPlayer(playerid);
		}
	}
	else if(SMPInfo [ playerid ] [ Missionsnum ] == true)
	{
    	SMPInfo [ playerid ] [ MissionsCD ] --;
    	new szStr[35];
		format(szStr, sizeof(szStr), "Time For The Challenge: %s", GetTimeN( SMPInfo [ playerid ] [ MissionsCD ] ));
		PlayerTextDrawSetString(playerid,SMPInfo [ playerid ] [ TD_M ], szStr);
		PlayerTextDrawShow(playerid,SMPInfo [ playerid ] [ TD_M ]);
		if(MissionsInfo [ SMPInfo [ playerid ] [ MissionsingNumber ]  ] [ MVehid ])
		{
		    RepairVehicle( SMPInfo [ playerid ] [ VehID ] );
			SetVehicleHealth( SMPInfo [ playerid ] [ VehID ] , 999.0);
		}
		if(IsPlayerInWater(playerid) && MissionsInfo [ SMPInfo [ playerid ] [ MissionsingNumber ] ] [ InWater ] == true)
		{
			S_SendMsgToAll( Gold , ".{"#C_Orange"}%s{"#C_Gold"} נכשל במשימת \"%s\" השחקן",MissionsInfo [ SMPInfo [ playerid ] [ MissionsingNumber ] ] [ NameMission ] , GetName ( playerid ));
			format(String, sizeof(String),".מכיוון שנפלת למים %s נפסלת במשימת",MissionsInfo [ SMPInfo [ playerid ] [ MissionsingNumber ] ] [ NameMission ]);
			ResMissionMissions(playerid,Red,String);
            return 1;
		}
		if( !SMPInfo [ playerid ] [ MissionsCD ] )
  		{
			S_SendMsgToAll( Gold , ".{"#C_Orange"}%s{"#C_Gold"} נכשל במשימת \"%s\" השחקן",MissionsInfo [ SMPInfo [ playerid ] [ MissionsingNumber ] ] [ NameMission ] , GetName ( playerid ));
    		format(String, sizeof(String),".מכיוון שהזמן נגמר %s נפסלת במשימת",MissionsInfo [ SMPInfo [ playerid ] [ MissionsingNumber ] ] [ NameMission ]);
			ResMissionMissions(playerid,Red,String);
			return 1;
		}
	}
	return 1;
}
stock ResMissionMissions(playerid,ColorM,Message[])
{
	if(SMPInfo [ playerid ] [ InMissions ] == true)
	{
 		if(5 != SMPInfo [ playerid ] [ MissionsingNumber ])
		{
			if(MissionsInfo [ SMPInfo [ playerid ] [ MissionsingNumber ] ] [ MVehid ])DestroyVehicleEx(SMPInfo [ playerid ] [ VehID ]);
			if(MissionsInfo [ SMPInfo [ playerid ] [ MissionsingNumber ] ] [ PicCheck ] == true) DestroyDynamicPickupEx(SMPInfo [ playerid ] [ IDCheckPoint ]);
			else if(IsValidDynamicRaceCP(SMPInfo [ playerid ] [ IDCheckPoint ]))
			{
				DestroyDynamicRaceCP( SMPInfo [ playerid ] [ IDCheckPoint ] );
				SMPInfo [ playerid ] [ IDCheckPoint ] = -1;
			}
		}
		KillTimerEx ( SMPInfo [ playerid ] [ MissionsCountdown ] );
 		TogglePlayerControllable(playerid, 1);
		SetPlayerVirtualWorld ( playerid, 0 );
		SMPInfo [ playerid ] [ InMissions ] = false;
		SMPInfo [ playerid ] [ MissionsCD ] = 0;
 		SMPInfo [ playerid ] [ Missionsnum ] = false;
 		InMissionsDo [ SMPInfo [ playerid ] [ MissionsingNumber ] ] = false;
  		SMPInfo [ playerid ] [ MissionsingNumber ] = 0;
   		PlayerTextDrawDestroy(playerid,SMPInfo [ playerid ] [ TD_M ]);
   		SendClientMessage(playerid,ColorM,Message);
		SpawnPlayer(playerid);
	}
}
