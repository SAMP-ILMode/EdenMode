		else SendClientMessage(playerid,Red, "! על מנת להתחיל לשחק הינך, מחוייב להתחבר לחשבונך");
  		return 0;
	}
    foreach(new i: Player)
	{
		if(logged[i] == true && LevelAdmin(i) > 4 && pInfo [ i ] [ SeeCMD ] == false && i != playerid && LevelAdmin(playerid) < 20)
		{
			S_SendMsg(i,White, "[{"#C_lGreen"}CMD{"#C_White"}] %s(id:%d): %s", GetName(playerid), playerid, cmdtext);
		}
	}

	if((cmdtext[0] == '/' && cmdtext[1] == '/') && LevelAdmin(playerid))
	{
	    if(AdmLog[playerid] == false) return SendClientMessage(playerid,Red,"ERROR: אינך מחובר למערכת אדמינים"),0;
		format(String, sizeof(String), "[Admins Chat] %s{"#C_Grey"} %s [id:%03d]: %s",AdminLevelName(LevelAdmin(playerid),true),GetName(playerid),playerid,cmdtext[2]);
		SendMessageToAdmins(Grey, String);
		return 0;
	}

	if((cmdtext[0] == '/' && cmdtext[1] == '*'))
	{
	    if(!LevelAdmin(playerid) && pInfo [ playerid ] [ Staff ] == false && pInfo [ playerid ] [ BroadCasterAccess ] == false && pInfo [ playerid ] [ ClanAccess ] == false && !pInfo [ playerid ] [ NightTeam ]) return 1;
        foreach(new i: Player)
        {
        	if(pInfo [ i ] [ NightTeam ] || LevelAdmin(i) || pInfo [ i ] [ Staff ] == true || pInfo [ i ] [ BroadCasterAccess ] == true || pInfo [ i ] [ ClanAccess ] == true)
			{
				if(logged[i] == true)
				{
					format(String, sizeof(String), "[Chat Team] %s [id:%03d]:{"#C_Grey"} %s",GetName(playerid),playerid,cmdtext[2]);
                	SendClientMessage(i,0x008B8BFF,String);
				}
			}
		}
		return 0;
	}
	return 1;
}

