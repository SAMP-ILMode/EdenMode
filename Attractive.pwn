//------------------------- public's act teams system --------------------------
function ActTeamsStartCd(CDTeams)
{
	new ctd[50];
	format(ctd, sizeof(ctd), "~g~The activity start in ~w~%d ~g~seconds",CDTeams);
	for(new i, j = GetPlayerPoolSize(); i <= j; i++)
	{
		if(!IsPlayerConnected(i) || !InActTeams(i)) continue;
		GameTextForPlayer(i, ctd, 1000, 3);
	}
	if(CDTeams > 0)
	{
		CDTeams--;
		ActTimerTeams = SetTimerEx("ActTeamsStartCd",1000, false, "d", CDTeams);
		return 1;
	}
	for(new i, j = GetPlayerPoolSize(); i <= j; i++)
	{
		if(!IsPlayerConnected(i)) continue;
		if(InActTeams(i))
		{
		    S_SendMsg(i,lGreen, "!%s בהצלחה בפעילות",ActsTeams[ IsActOnTeams ] [ ActNameTeams ]);
			TogglePlayerControllable(i, 1);
		}
		else
