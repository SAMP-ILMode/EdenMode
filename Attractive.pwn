		}
		if(!Cout) SendClientMessage(playerid,Red, ".אין לך דברים שקנית מנקודות הזהב שצברת");
		return 1;
	}
	if(!IsPlayerConnected( ID ))return SendClientMessage(playerid,Red, ".המשתמש אינו מחובר");
	if(IsPlayerNPC( ID )) return SendClientMessage(playerid,Red, ".אין באפשרותך להשתמש בפקודה זו על בוט");
	S_SendMsg(playerid,Green, "|_____________________| %s |_____________________|",GetName( ID ));
	new Cout;
	if(pInfo [ ID ] [ HaveDayTag ])
	{
     	Cout++;
		S_SendMsg(playerid,Green, "%d. Point Type: [ Tag choice to 3 Days ] [Date Start: %s] - Point End: %d Days",Cout, pInfo [ ID ] [ DateTag ] ,pInfo [ ID ] [ HaveDayTag ]-DaysBetweenDates( pInfo [ ID ] [ DateTag ] ,GetDateAsStringPoints()));
	}
	if(pInfo [ ID ] [ HaveBazooka ])
	{
    	Cout++;
		S_SendMsg(playerid,Green, "%d. Point Type: [ 50 Bazookot to 7 Days ] [Date Start: %s] - Point End: %d Days",Cout,pInfo [ ID ] [ DateBazooka ],pInfo [ ID ] [ HaveBazooka ]-DaysBetweenDates(pInfo [ ID ] [ DateBazooka ],GetDateAsStringPoints()));
	}
	if(!Cout) SendClientMessage(playerid,Red, ".אין למשתמש דברים שקנה מנקודות הזהב שחסך");
	return 1;
}
