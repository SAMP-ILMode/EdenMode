function PlayerLozzeBattle(playerid)
{
	new IdWinner, idLoozer;
	if(battle[playerid][Inviter] == 1)
	{
		IdWinner = battle[playerid][IDJoin];
		idLoozer = battle[IdWinner][IDInvite];
	}
	else if(battle[playerid][joiner] == 1)
	{
		IdWinner = battle[playerid][IDInvite];
		idLoozer = battle[IdWinner][IDJoin];
	}
	new WeaponLocel[25];
	if(battle[playerid][WeaponBattle] == 51) WeaponLocel = "War";
	else if(battle[playerid][WeaponBattle] == 50) WeaponLocel = "Heavy";
	else GetWeaponName(battle[playerid][WeaponBattle],WeaponLocel,24);
	S_SendMsgToAll(0x00CEC4FF,">> [Battle System] %s בבאטל %s ניצח את השחקן %s",WeaponLocel,GetName(idLoozer),GetName(IdWinner));
    SetPlayerInterior(idLoozer,0);
    SetPlayerInterior(IdWinner,0);
    SpawnPlayer(idLoozer);
    SpawnPlayer(IdWinner);
	battle[IdWinner][InBattle] = 0;
    battle[idLoozer][InBattle] = 0;
	SetPlayerHealth(IdWinner, 99);
	SetPlayerArmour(IdWinner,0);
	battle[IdWinner][Inviter] = 0;
	battle[IdWinner][joiner] = 0;
	battle[idLoozer][Inviter] = 0;
	battle[idLoozer][joiner] = 0;
	return 1;
}
function CountBattlE(playerid,playerstart)
{
	if(battle[playerid][CountBattle] == 0)
	{
		if(playerstart == 1)
		{
			new WeaponLocel[25];
  			if(battle[playerid][WeaponBattle] == 51) WeaponLocel = "War";
  			else if(battle[playerid][WeaponBattle] == 50) WeaponLocel = "Heavy";
  			else GetWeaponName(battle[playerid][WeaponBattle],WeaponLocel,24);
		  	S_SendMsgToAll(0x00CEC4FF,">> [Battle System] %s :בבאטל מסוג %s התחיל באטל עם השחקן %s השחקן",WeaponLocel,GetName(battle[playerid][IDInvite]),GetName(playerid));
  		}
		KillTimerEx(battle[playerid][TimerCountBattle]);
		GameTextForPlayer(playerid,"~r~Good Luck",1000,6);
		TogglePlayerControllable(playerid, 1);
	}
	else if(battle[playerid][CountBattle] > 0)
	{
		format(String,10, "~g~%d", battle[playerid][CountBattle]);
		GameTextForPlayer(playerid,String,1000,6);
	}
	battle[playerid][CountBattle] --;
	return 1;
}

