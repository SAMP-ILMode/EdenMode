		{
			S_SendMsgToAll(Pink,"! לא עבר את הטסט, בהצלחה בפעם הבאה \"%s\" השחקן",GetName(playerid));
            SendClientMessage(playerid,Red,".לא עברת את הטסט, תנסה שוב מאוחר יותר");
		}
	}
	return 1;
}
//------------------------- PWar System ------------------------------------
#define pwar_c0 FF5555
#define pwar_c1 FFFF80
#define pwar_cc1 0xFFFF8000
CMD:pwar(playerid, params[])
{
    if(InActAuto[playerid] == true && ActStatus >= 2) return SendClientMessage(playerid,Red, "./QAct - אינך יכול להקיש פקודות שאתה בפעילות. ליציאה, הקש");
    if(InDm[playerid] == true) return SendClientMessage(playerid,Red,".DM-אינך יכול להשתמש בפקודה זו שאתה נימצא באזור");
	new command[30];
	if(sscanf(params,"s[10]{s[30]}",command)) return SendClientMessage(playerid,White,"Usage: /PWar [Invite / Accept / Deny]");
    if(!strcmp(command, "Help", true))
	{
	    SendClientMessage(playerid,Grey,":פקודות המערכת");
		SendClientMessage(playerid,White,"• /pWar Invite [id] :הזמנה לקרב");
		SendClientMessage(playerid,White,"• /pWar Accept :הצטרפות לקרב");
		SendClientMessage(playerid,White,"• /pWar Deny :סירוב הזמנה לקרב");
		return 1;
	}
	if(!strcmp(command, "invite", true))
	{
		new ID, DriftBattle_S[40],Price_w;
  		if(sscanf(params[6],"is[40]i",ID,DriftBattle_S,Price_w)) return SendClientMessage(playerid,White,"Usage: /pWar Invite [ID] [Drift / Race / Derby / Tank / Boat] [סכום הימור]");
		if(strcmp(DriftBattle_S, "Drift", true) && strcmp(DriftBattle_S, "Race", true) && strcmp(DriftBattle_S, "Derby", true) && strcmp(DriftBattle_S, "Tank", true) && strcmp(DriftBattle_S, "Boat", true)) return SendClientMessage(playerid,White,"Usage: /Battle Invite [ID] [Drift / Race / Derby / Tank / Boat] [סכום הימור]");
		if(!IsPlayerConnected(ID)) return SendClientMessage(playerid,Red, ".המשתמש אינו מחובר");
		if(IsPlayerNPC( ID )) return SendClientMessage(playerid,Red, ".אין באפשרותך להשתמש בפקודה זו על בוט");
		if(ID == playerid) return SendClientMessage(playerid,Red, ".אין באפשרותך להזמין את עצמך");
		if(PWarBattle[playerid][InviteAnyOne] == true) return SendClientMessage(playerid,Red, ".הזמנת כבר שחקן מסוים לקרב");
 		if(PWarBattle[playerid][InviteMe] == true) return SendClientMessage(playerid,Red, "./Pwar Deny הזמינו אותך לקרב, בשביל להזמין מישהו בטל את ההזמנה זו");
        if(PWarBattle[playerid][InPWar]  == true) return SendClientMessage(playerid,Red, ".אתה כבר נמצא בקרב עם משהו");
		if(PWarBattle[ID][InviteAnyOne] == true) return SendClientMessage(playerid,Red, ".שחקן זה הזמין שחקן מסוים לקרב");
		if(PWarBattle[ID][InviteMe] == true) return SendClientMessage(playerid,Red, ".מישהו כבר הזמין שחקן זה לקרב");
		if(PWarBattle[ID][InPWar]  == true) return SendClientMessage(playerid,Red, ".השחקן נמצא כבר בקרב");
		if(Price_w > Y_GetPlayerMoney(playerid)) return SendClientMessage(playerid, Red, ".אין בידך את סכום ההימור שהקשת");
        if(Price_w < 5000 || Price_w > 500000) return SendClientMessage(playerid, Red, ".סכום ההימור יכול להיות בין $5,000 ל$500,000");
		PWarBattle [ playerid ] [ MoneyWin ] = Price_w;
		Y_GivePlayerMoney(playerid, -PWarBattle [ playerid ] [ MoneyWin ]);
		PWarBattle [ ID ] [ MoneyWin ] = Price_w;
		PWarBattle[playerid][DriftBattle] = strval(DriftBattle_S);
		PWarBattle[ID][DriftBattle] = strval(DriftBattle_S);
		if(!strcmp(DriftBattle_S, "Drift", true))
		{
			PWarBattle[playerid][DriftBattle] = 1;
			PWarBattle[ID][DriftBattle] = 1;
		}
 		else if(!strcmp(DriftBattle_S, "Race", true))
		{
			PWarBattle[playerid][DriftBattle] = 2;
			PWarBattle[ID][DriftBattle] = 2;
		}
		else if(!strcmp(DriftBattle_S, "Derby", true))
		{
			PWarBattle[playerid][DriftBattle] = 3;
			PWarBattle[ID][DriftBattle] = 3;
		}
		else if(!strcmp(DriftBattle_S, "Tank", true))
		{
			PWarBattle[playerid][DriftBattle] = 4;
			PWarBattle[ID][DriftBattle] = 4;
		}
		else if(!strcmp(DriftBattle_S, "Boat", true))
		{
			PWarBattle[playerid][DriftBattle] = 5;
			PWarBattle[ID][DriftBattle] = 5;
		}
		PWarBattle[ID][InviteMe] = true;
		PWarBattle[playerid][InviteAnyOne] = true;
		PWarBattle[ID][IDInvite] = playerid;
		PWarBattle[playerid][IDJoin] = ID;
		PWarBattle[playerid][Inviter] = true;
		new DriftLocel[20];
  		if(PWarBattle[ID][DriftBattle] == 1) DriftLocel = "לדריפט אישי";
  		else if(PWarBattle[ID][DriftBattle] == 2) DriftLocel = "למירוץ אישי";
		else if(PWarBattle[ID][DriftBattle] == 3) DriftLocel = "לקרב דרבי אישי";
		else if(PWarBattle[ID][DriftBattle] == 4) DriftLocel = "לקרב טנקים אישי";
