		strcat(catstr, "{"#C_White"}• {"#C_Red"}Admin: {"#C_White"}%s\n• {"#C_Red"}Reason:{"#C_White"} %s\n• {"#C_Red"}Days:{"#C_White"} 3\n• {"#C_Red"}IP:{"#C_White"} %s\n• {"#C_Red"}Date:{"#C_White"} %s\n• {"#C_Red"}Time:{"#C_White"} %s");
		format(catstr, sizeof(catstr), catstr, GetName( ID_wa ),GetName(playerid), Reason_wa ,GetIp( ID_wa ), GetDateAsString(),GetTimeAsString());
   		strcat(catstr, "\n{"#C_Red"}*! קיבלת באן על כך שיש לך 3 אזהרות");
		strcat(catstr, "\n{"#C_Red"}~~~~~~~~~~~~~~~~~\n"WebUrl" - משוב ותמיכה וצלם תמונה זו GTA להורדת הבאן פנה לפורום ");
		ShowPlayerDialog( ID_wa , 1000, DIALOG_STYLE_MSGBOX, "{"#C_Red"}You Are Banned !", catstr, "אישור", "");
        pInfo [ ID_wa ] [ Warning ] = 0;
		new INI:File3 = INI_Open(PFile(ID_wa));
	    INI_WriteInt ( File3, "Warning",0);
	    INI_Close ( File3 );
		KickEx( ID_wa );
		return 1;
   	}
