 	if(InRules[playerid] == true) return SendClientMessage(playerid, Red, ".אינך יכול לבצע פקודות כאשר אתה בקריאת חוקים"),0;
    if(InClanWar[playerid] == 1 && strcmp(cmdtext,"/kill",true)) return SendClientMessage(playerid,Red, ".אין באפשרותך לבצע פקודות באמצע קרב קלאנים"),0;
