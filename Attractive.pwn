function WolEnd()
{
	if(WolOn == false) return 1;
	if(MoneyWol / JoinWal_Money >= minWolPlayers)
	{
		new Iterator:RandP<MAX_PLAYERS>;
		for(new i, j = GetPlayerPoolSize(); i <= j; i++)
		{
			if(!IsPlayerConnected(i) || InWol[i] == false) continue;
			Iter_Add(RandP, i);
			InWol[i] = false;
		}
		new WinWol = Iter_Random(RandP);
		S_SendMsgToAll(lGreen,"!%s$ זכה בהגרלת גלגל המזל וקיבל %s",GetNum(MoneyWol),GetName( WinWol ));
		SendClientMessage(WinWol,lGreen, "!זכית בהגרלת גלגל המזל");
		Y_GivePlayerMoney( WinWol , MoneyWol );
	}
	else
	{
		for(new i, j = GetPlayerPoolSize(); i <= j; i++) InWol[i] = false;
		S_SendMsgToAll(Red,"!פעילות גלגל המזל בוטלה כיוון שהשתתפו פחות מ %d שחקנים",minWolPlayers);
	}
	WolOn = false;
	MoneyWol = 0;
	return 1;
}

