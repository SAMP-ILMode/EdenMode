CMD:totto(playerid, params[])
{
    if(!TMoney) return SendClientMessage(playerid,Red, ".הטוטו אינו פעיל");
    if(InTotto[playerid]) return SendClientMessage(playerid,Red, ".כבר אתה נימצא בהגרלת הטוטו");
	new TNumber;
	if(sscanf(params,"i",TNumber)) return SendClientMessage(playerid,White, "Usage: /totto [קבוצה מספר]");
    if(TNumber < 1 || TNumber > 6) return SendClientMessage(playerid, Red, ".מספר הקבוצה יכול להיות בין 1 ל 6");
	if(Y_GetPlayerMoney(playerid) < TMoney) return S_SendMsg(playerid, Red, ".אין בידך %s$ להשתתפות בטוטו",GetNum(TMoney));
	Y_GivePlayerMoney(playerid, -TMoney);
    S_SendMsg(playerid,lGreen, "! בחרת את קבוצה מספר %d בהצלחה",TNumber);
    InTotto[playerid] = TNumber;
	return 1;
}
function TottoCount()
{
	new Pc,cto;
	for(new i, j = GetPlayerPoolSize(); i <= j; i++)
	{
		if(!IsPlayerConnected(i) || InTotto[i] == 0) continue;
		cto++;
		Pc++;
	}
	if(Pc < 2)
	{
		SendClientMessageToAll(Red, ".{"#C_Red"}פעילות הטוטו בוטלה מהסיבה: {"#C_White"}חוסר משתתפים");
		for(new i, j = GetPlayerPoolSize(); i <= j; i++)
		{
			if(!IsPlayerConnected(i) || InTotto[i] == 0) continue;
			Y_GivePlayerMoney(i,TMoney);
   			S_SendMsg(i,lGreen, "! %s$ :בגלל שפעילות הטוטו בוטלה עקב חוסר משתתפים כספך הוחזר בחזרה אליך",GetNum(TMoney));
		 	InTotto[ i ] = 0;
		}
	}
	else
	{
		new Iterator:RandP<MAX_PLAYERS>;
		for(new i, j = GetPlayerPoolSize(); i <= j; i++)
		{
			if(!IsPlayerConnected(i) || InTotto[i] == 0) continue;
			Iter_Add(RandP, i);
		}
		Pc = Iter_Random(RandP);
		new Cout;
		for(new i, j = GetPlayerPoolSize(); i <= j; i++)
		{
			if(!IsPlayerConnected(i) || InTotto[ Pc ] != InTotto[ i ]) continue;
			Cout++;
		}
		SendClientMessageToAll(llllGreen, "• • • • • • • [ פעילות צ'אט ] • • • • • • •");
		if(Cout == 1)
		{
            SendClientMessageToAll(lBlue, "!הסתיימה Totto-פעילות ה");
            S_SendMsgToAll(lBlue,".מספר הקבוצה שזכתה היא: %d",InTotto[ Pc ]);
            S_SendMsgToAll(lBlue,".%s$ :הכמות כסף של הקבוצות",GetNum(TMoney*cto));
            SendClientMessageToAll(lBlue, ".כמות המנצחים: 1");
            Y_GivePlayerMoney( Pc , TMoney*cto);
		}
		else
		{
            SendClientMessageToAll(lBlue, "!הסתיימה Totto-פעילות ה");
            S_SendMsgToAll(lBlue,".מספר הקבוצה שזכתה היא: %d",InTotto[ Pc ]);
            S_SendMsgToAll(lBlue,".כמות המנצחים: %d",Cout);
            S_SendMsgToAll(lBlue,".סכום הכסף %s$ חולק ל%d ולכן כל אחד קיבל %s$",GetNum(TMoney*cto),Cout,GetNum(TMoney*cto/Cout));
			for(new i, j = GetPlayerPoolSize(); i <= j; i++)
			{
				if(!IsPlayerConnected(i) || InTotto[ Pc ] != InTotto[ i ]) continue;
				Y_GivePlayerMoney( i , TMoney*cto/Cout);
			}
		}
		SendClientMessageToAll(llllGreen, "• • • • • • • • [ הסתיימה ] • • • • • • • •");
	}
	for(new i, j = GetPlayerPoolSize(); i <= j; i++) InTotto[ i ] = 0;
 	TTimer = -1;
    TMoney = 0;
    return 1;
}
