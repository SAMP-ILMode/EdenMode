	if(AdmLog[playerid] == false) return SendClientMessage(playerid,Red,"ERROR: אינך מחובר למערכת אדמינים");
    if(!TftsStatus) return SendClientMessage(playerid, Red,".פעילות צ'אט זו לא פועלת");
    S_SendMsgToAll(Red, "!ביטל את הפעילות הראשון ששולח \"%s\" האדמין",GetName(playerid));
	foreach(new i: Player) InTftsWinners[i] = 0;
	TftsStatus = 0;
	TftsMoney = 0;
	TftsTimer = 0;
	TftsNumberM = 0;
	KillTimerEx(TftsTimer);
	return 1;
}

CMD:send(playerid, params[])
{
    if(TftsStatus != 2) return SendClientMessage(playerid, Red,".הפעילות השולח הראשון לא פועלת כעת או שעדיין לא התחילה");
	if(InTftsWinners[playerid]) return SendClientMessage(playerid, Red,".נכנסת כבר לפעילות זו");
	new Pay_S;
	if(sscanf(params,"i",Pay_S)) return S_SendMsg(playerid,White,"Usage: /Send %d",TftsNumberM);
	if(Pay_S != TftsNumberM) return SendClientMessage(playerid,Red,".הסכום שרשמת אינו זהה לסכום שצריך לשלוח");
    if(Y_GetPlayerMoney(playerid) < Pay_S)return SendClientMessage(playerid,Red,".אין לך את הסכום שרשמת");
    Y_GivePlayerMoney(playerid, - Pay_S );
    TftsMoney += Pay_S;
    InTftsWinners[playerid] = TftsMoney/Pay_S;
    SendClientMessage(playerid,lGreen,"!נכנסת לפעילות הראשון ששולח. בעוד מספר שניות המערכת תודיע מי המנצח");
	return 1;
}

function OnTfts(Tfts_i)
{
	if(Tfts_i == 0)
	{
	    TftsNumberM = RandomEx(25000,100000);
	    TftsStatus = 2;
        S_SendMsgToAll(lYellow, "*** יזכה בכל הסכום של כל המשתתפים שהשתתפו בפעילות זו /Send הראשון שישלח את הסכום %d בפקודה",TftsNumberM);
        SendClientMessageToAll(lYellow,".המערכת תודיע בעוד כחצי דקה על המנצח בפעילות הראשון ששולח");
        TftsTimer = SetTimerEx("OnTfts", 30000, false, "i", 1 );
	}
	else
	{
		new InnFtfts;
	    foreach(new i: Player) if(InTftsWinners[i]) InnFtfts++;
		if(!InnFtfts)
		{
            SendClientMessageToAll(Red,"!הפעילות הראשון ששולח בוטלה בגלל חוסר משתתפים");
			foreach(new i: Player) InTftsWinners[i] = 0;
			TftsStatus = 0;
			TftsMoney = 0;
			TftsTimer = 0;
			TftsNumberM = 0;
		}
		else
		{
			new lastid;
			new OldVar = 1000;
			foreach(new i: Player)
			{
				if(!InTftsWinners[i]) continue;
				if(InTftsWinners[i] <= OldVar)
				{
					lastid = i;
					OldVar = InTftsWinners[i];
				}
			}
        	S_SendMsgToAll(lYellow, "!%s ניצח בפעילות בראשון ששולח וקיבל {"#C_White"}\"%s\"{"#C_lYellow"} השחקן",GetNum(TftsMoney),GetName(lastid));
            Y_GivePlayerMoney(lastid, TftsMoney );
			foreach(new i: Player) InTftsWinners[i] = 0;
			TftsStatus = 0;
			TftsMoney = 0;
			TftsTimer = 0;
			TftsNumberM = 0;
		}
	}
    return 1;
}
