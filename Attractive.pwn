public OnPlayerWeaponShot(playerid, weaponid, hittype, hitid, Float:fX, Float:fY, Float:fZ)
{
    if(weaponid == 0) return LogAntiCheat(playerid,"Cheats: Crasher (Aim) (#01)",true,true);
	if(Prohibited_guns[playerid] != -1 && !IsPlayerInAnyVehicle(playerid))
    {
		new AreaPgID = Prohibited_guns[playerid];
		if(PgInfo [ AreaPgID ] [ AfterShot ] == true && GetPlayerVirtualWorld(playerid) == PgInfo [ AreaPgID ] [ Worldid ] && GetPlayerInterior(playerid) == PgInfo [ AreaPgID ] [ Interiorid ])
		{
			if(PgInfo [ AreaPgID ] [ PgKills ] == true)
			{
			    GameTextForPlayer(playerid,"~W~>~R~ DOnt Shoot~W~ <", 2000, 4);
				SetPlayerHealth(playerid,0);
				SetPlayerArmedWeapon(playerid, 0);
				format(String,sizeof(String),"{"#C_Red"}!בגלל שניסת לירות או לאיים באזור %s מתת, להבא לא לנסות לתקוף באזור זה",PgInfo [ AreaPgID ] [ AreaNamePg ]);
				ShowPlayerDialog(playerid, 98211, DIALOG_STYLE_MSGBOX,"אזהרה",String, "סגור", "");
    			S_SendMsg(playerid,Red, ".אסור להרוג/לירות באזור %s",PgInfo [ AreaPgID ] [ AreaNamePg ]);
			}
			else
			{
			    GameTextForPlayer(playerid,"~W~>~R~ DOnt Shoot~W~ <", 2000, 4);
			    SetPlayerArmedWeapon(playerid, 0);
				format(String,sizeof(String),"{"#C_Red"}.אסור לתקוף/לאיים באזור %s",PgInfo [ AreaPgID ] [ AreaNamePg ]);
				ShowPlayerDialog(playerid, 98211, DIALOG_STYLE_MSGBOX,"אזהרה",String, "סגור", "");
   				S_SendMsg(playerid,Red, ".אסור לתקוף/לאיים באזור %s",PgInfo [ AreaPgID ] [ AreaNamePg ]);
			}
		}
	}
	if(Chicken [ SChicken ] == true)
	{
		if(InChicken[playerid] == true)
		{
			if(hitid == Chicken [ Ob_Ch ] [4])
        	{
                Chicken [ HealthOb ] [4] -= random(20);
                format(String,sizeof(String),"אחוז חיי התרנגולת: %d",Chicken [ HealthOb ] [4]);
                UpdateDynamic3DTextLabelText(Chicken [ TD_ObCh ] [ 4 ],llBlue,String);
				if(Chicken [ HealthOb ][4] < 0)
				{
				    DestroyObjectEx(Chicken [ Ob_Ch ] [4]);
				    DestroyDynamic3DTextLabel(Chicken [ TD_ObCh ] [ 4 ]);
				    new payment = RandomEx(5000,10000);
					S_SendMsgToAll(Purple,"• הרג את התרנגול בפעילות וקיבל %s$ \"%s\" השחקן",GetNum(payment),GetName(playerid));
                    Y_GivePlayerMoney( playerid ,payment);
				}
			}
			else if(Chicken [ Ob_Ch ] [ 0 ] == -1 && Chicken [ Ob_Ch ] [ 1 ] == -1 && Chicken [ Ob_Ch ] [ 2 ] == -1 && Chicken [ Ob_Ch ] [ 3 ] == -1 && Chicken [ Ob_Ch ] [ 4 ] == -1)
            {
				for(new i, j = GetPlayerPoolSize(); i <= j; i++)
				{
					if(!IsPlayerConnected(i) || InChicken[i] == false) continue;
					OnPlayerSpawn(i);
     				InChicken[i] = false;
				}
				Chicken [ SChicken ] = false;
			}
			else
			{
		    	for(new i; i < 4; i++)
		    	{
					if(hitid == Chicken [ Ob_Ch ] [ i ] )
            		{
            		    Chicken [ HealthOb ] [ i ] -= random(10);
            		    format(String,sizeof(String),"אחוז חיי הביצה: %d",Chicken [ HealthOb ] [ i ]);
            		    UpdateDynamic3DTextLabelText(Chicken [ TD_ObCh ] [ i ],Orange,String);
						if(Chicken [ HealthOb ][ i ] < 0)
						{
						    GetObjectPos(Chicken [ Ob_Ch ] [ i ] ,GPos[0],GPos[1],GPos[2]);
							CreateExplosion(GPos[0],GPos[1],GPos[2],12,2);
						    DestroyObjectEx(Chicken [ Ob_Ch ] [ i ]);
						    DestroyDynamic3DTextLabel(Chicken [ TD_ObCh ] [ i ]);
						    new payment = RandomEx(2000,3500);
							S_SendMsgToAll(Pink,"• פיצץ את הביצה בפעילות וקיבל %s$ \"%s\" השחקן",payment,GetName(playerid));
            		        Y_GivePlayerMoney( playerid , payment);
						}
					}
				}
			}
		}
	}
	if(InNoReload == true) GivePlayerWeapon( playerid , GetPlayerWeapon(playerid) , 1 );
	if(InActAuto[playerid]  == true)
	{
	    if(Acts[IsActOn][Type] == ACT_WAR_TYPE && Acts[IsActOn][Map] == 14)
	    {
			if(weaponid == 34 || weaponid == 33)
			{
				switch(random(4))
				{
					case 0: CreateExplosion(fX,fY,fZ,0,2);
					case 1: CreateExplosion(fX,fY,fZ,5,2);
    		        case 2: CreateExplosion(fX,fY,fZ,4,2);
    		        case 3: CreateExplosion(fX,fY,fZ,11,2);
    		        case 4: CreateExplosion(fX,fY,fZ,13,2);
				}
			}
		}
  		if(Acts[IsActOn][Type] == ACT_WAR_TYPE && Acts[IsActOn][Map] == 13)
	    {
	    	if(hitid == ActPurpose)
			{
				InActCP[playerid]++;
 				if(InActCP[playerid] == 15)
				{
				    S_SendMsg(playerid,Pink, ".מכיוון שהצלחת לטווח 15 מטרות ראשון %s ניצחת בפעילות",Acts[IsActOn][ActName]);
				 	WinnerActivity(playerid);
				}
  				else S_SendMsg(playerid,Blue, ".{"#C_Blue"}הצלחת לפגוע במטרה, פגיעה {"#C_White"}%d/15",InActCP[playerid]);
        		PurposeRange();
			}
  		}
	}
	if(weaponid != 38 && weaponid != 28 && weaponid != 32 && weaponid != 31)
	{
		if((gettime() - shotTime[playerid]) < 1) shoot[playerid] += 1;
		else shoot[playerid] = 0;
		if(shoot[playerid] > 10)
		{
			LogAntiCheat(playerid,"Cheats: Rapid",true,true);
			DMGuardMsgToAdmin("[DM Guard] .Rapid חשוד שהשתמש בצ'יט %s[ID: %d] השחקן",GetName(playerid),playerid);
		}
		shotTime[playerid] = gettime();
	}
	if(IsWeaponWithAmmo(weaponid) && weaponid != 38 && weaponid != 38 && weaponid != 28 && !IsPlayerInAnyVehicle(playerid) && InNoReload == false)
	{
    	new count;
    	if(weaponid != CurrentWeapon[playerid])
		{
			CurrentWeapon[playerid] = weaponid;
			CurrentAmmo[playerid] = GetPlayerWeaponAmmo(playerid,weaponid);
			count++;
		}
		if(GetPlayerWeaponAmmo(playerid,weaponid) > CurrentAmmo[playerid] || GetPlayerWeaponAmmo(playerid,weaponid) < CurrentAmmo[playerid])
    	{
    	  	CurrentAmmo[playerid] = GetPlayerWeaponAmmo(playerid,weaponid);
    	  	NoReloading[playerid] = 0;
  			count++;
    	}
    	if(GetPlayerWeaponAmmo(playerid,weaponid) != 0 && GetPlayerWeaponAmmo(playerid,weaponid) == CurrentAmmo[playerid] && count == 0)
    	{
    	  	NoReloading[playerid]++;
  			if(NoReloading[playerid] >= 5)
    		{
     			NoReloading[playerid] = 0;
     			CurrentWeapon[playerid] = 0;
  				CurrentAmmo[playerid] = 0;
  				LogAntiCheat(playerid,"Cheats: No Reload (#02)",true,true);
  				return 0;
      		}
		}
	}
	if(Anti_SawnoffShutgam == true && weaponid == 26 && !IsPlayerInAnyVehicle(playerid))
	{
        new p_ammo = GetPlayerAmmo(playerid);
   	    if(Anti_Sawn[playerid] > p_ammo) Anti_Sawn[playerid] = p_ammo - 3;
   	    if(Anti_Sawn[playerid] == p_ammo)
   	    {
            if(GetPlayerWeaponState(playerid) != 1)
           	{
   	           	SetPlayerArmedWeapon(playerid, 0);
               	ApplyAnimation(playerid, "PED", "getup", 4.1, 0, 1, 1, 0, 0, 1);
   	       	}
   	       	Anti_Sawn[playerid] = 99999;
		}
	}
	return 1;
}

