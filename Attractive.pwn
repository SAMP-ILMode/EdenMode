    		SetPlayerHealth(playerid,99.0);
    		SetPlayerArmour(playerid,0);
    		SetPlayerSkin(playerid,293);
  			new TDM_Red_Random = random(sizeof(TDM_Red));
 			SetPlayerPos(playerid, TDM_Red[TDM_Red_Random][0], TDM_Red[TDM_Red_Random][1], TDM_Red[TDM_Red_Random][2]);
