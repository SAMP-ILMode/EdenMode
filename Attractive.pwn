stock FormatTime(sec, time_t[16])
{
    if (sec == 0)
	{
		time_t = "0 seconds";
		return time_t;
	}
    new fdecimal[16];
	new fmin[6];
	format(fmin, 6, "%.1f", floatdiv(sec,60));
	new minute = (sec/60);
	new sep = strfind(fmin, ".");
	strmid(fdecimal, fmin, sep+1, strlen(fmin), 16);
	if (sec < 60) format(time_t, 16, "%d second%s", sec, (sec != 1)?("s"):(""));
	else
	{
		if (strval(fdecimal) == 0) format(time_t, 16, "%d minute%s", minute, (minute > 1)?("s"):(""));
		else format(time_t, 16, "%s mintue%s", fmin, (floatstr(fmin) > 1.0) ? ("s"):(""));
	}
	return time_t;
}

