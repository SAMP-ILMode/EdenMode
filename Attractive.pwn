		PWarBattle[ idLoozer ][InPWar]  = false;
	    PWarBattle[ idLoozer ][DriftBattle] = 0;
	    PWarBattle[ idLoozer ] [ MoneyWin ] = 0;
	    PWarBattle[ idLoozer ][joiner] = false;
        SetPlayerInterior(idLoozer,0);
        OnPlayerSpawn(idLoozer);
        
		PWarBattle[ IdWinner ][IDJoin] = 0;
	 	PWarBattle[ IdWinner ][IDInvite] = 0;
	 	PWarBattle[ IdWinner ][Inviter] = false;
		PWarBattle[ IdWinner ][InviteAnyOne] = false;
		PWarBattle[ IdWinner ][InviteMe] = false;
		PWarBattle[ IdWinner ][InPWar]  = false;
	    PWarBattle[ IdWinner ][DriftBattle] = 0;
	    PWarBattle[ IdWinner ][joiner] = false;
        PWarBattle[ IdWinner ] [ MoneyWin ] = 0;
    	SetPlayerInterior(IdWinner,0);
    	OnPlayerSpawn(IdWinner);
		SetPlayerHealth(IdWinner, 99);
		SetPlayerArmour(IdWinner,0);
		DestroyVehicleEx(PWarBattle[ idLoozer ] [ VehicleWar ]);
		DestroyVehicleEx(PWarBattle[ IdWinner ] [ VehicleWar ]);
		DisablePlayerRaceCheckpoint(IdWinner);
		DisablePlayerRaceCheckpoint(idLoozer);
		TogglePlayerControllable(idLoozer, 1);
		TogglePlayerControllable(IdWinner, 1);
		KillTimerEx( PWarBattle[ idLoozer ] [ TimerCountBattle ] );
		KillTimerEx( PWarBattle[ IdWinner ] [ TimerCountBattle ] );
	}
