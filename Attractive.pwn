CMD:auction(playerid, params[])
{
	new command[20];
	if(sscanf(params,"s[20]{i}",command))
	{
		SendClientMessage(playerid,White,"Usage: /Auction [action]");
		SendClientMessage(playerid,White,"/Auction start [price] :התחלת מכירה פומבית על הרכב שלך");
  		SendClientMessage(playerid,White,"/Auction end :סיום המכירה הפומבית של הרכב שלך");
  		SendClientMessage(playerid,White,"/Auction setprice [price] :שינוי המחיר על הרכב");
		SendClientMessage(playerid,White,"/Auction suggest :הצעת מחיר על הרכב");
		SendClientMessage(playerid,White,"/Auction info :פרטים על רכב המוצע למכירה");
  		return 1;
	}
	if(!strcmp(command, "info", true))
	{
	    if(Auction_ID == -1) return SendClientMessage(playerid,Red, ".אין מכירה פומבית פעילה");
		new i;
		for(new b; b < 17; ++b)
		{
			if(VehicleInfo [ pInfo [ Auction_ID ] [ VehicleID ] ] [ VehEnhancements ] [ b ] == 0) continue;
			i++;
		}
		format(String, sizeof(String), "{"#C_lGrey"}~ ~ ~ :פרטי הרכב ~ ~ ~\n\n{"#C_Yellow"}•{"#C_lBlue"} Owner: %s\n{"#C_Yellow"}•{"#C_lBlue"} Vehicle Type: %s\n{"#C_Yellow"}•{"#C_lBlue"} Price: $%s\n{"#C_Yellow"}•{"#C_lBlue"} Vehicle Enhancements: %s\n\n{"#C_Grey"}~ ~ ~ ~ ~ ~ ~ ~ ~ ~ ~",GetName(Auction_ID),GetVehicleName(VehicleInfo [ pInfo [ Auction_ID ] [ VehicleID ] ] [ IDVeh ]),GetNum(Auc_Price),i >= 3?("כן"):("לא"));
		ShowPlayerDialog(playerid,681, DIALOG_STYLE_MSGBOX, "Auction info - פרטי המכירה", String, "סגור", "");
		return 1;
	}
	if(!strcmp(command, "setprice", true))
	{
	    if(Auction_ID == -1) return SendClientMessage(playerid,Red, ".אין מכירה פומבית פעילה");
	    if(Auction_ID != playerid) return SendClientMessage(playerid,Red, ".לא הוצאת את רכבך למכירה פומבית");
		new price_c;
		if(sscanf(params[8],"i",price_c)) return SendClientMessage(playerid,White,"Usage: /Auction setprice [price]");
        if(Auc_Price == price_c) return SendClientMessage(playerid, Red, ".סכום הכסף שכתבת זהה לסכום הכסף אשר במכירה");
        if(price_c < 1 || price_c > 20000000) return SendClientMessage(playerid, Red, ".הינך יכול לשנות את רכבך בין הסכומים 1$-20,000,000$");
		S_SendMsgToAll(COLOR_AUCTION, ">> $%s למחיר %s מחיר הרכב במכירה הפומבית שונה על ידי",GetNum(price_c),GetName(playerid));
		if(Auc_IDSuggest != -1)
		{
			S_SendMsg(Auc_IDSuggest,Red, ".שינה את סכום קניית הרכב והכסף שהצעת ($%s) חזר אליך %s בעל הרכב",GetNum(Auc_Price),GetName(playerid));
            Y_GivePlayerMoney(Auc_IDSuggest,Auc_Price);
		    Auc_IDSuggest = -1;
		}
		Auc_Price = price_c;
		return 1;
	}
	if(!strcmp(command, "start", true))
	{
	    if(InActAuto[playerid] == true && ActStatus >= 2) return SendClientMessage(playerid,Red, "./QAct - אינך יכול להקיש פקודות שאתה בפעילות. ליציאה, הקש");
	   	if(Auction_ID != -1) return SendClientMessage(playerid,Red, ".יש כבר רכב המוצע למכירה פומבית");
  		if(!IsPlayerInAnyVehicle(playerid) || !pInfo [ playerid ] [ VehicleID ]) return SendClientMessage(playerid,Red,".על מנת למכור את רכבך עליך להיות בתוכו");
		new b = pInfo [ playerid ] [ VehicleID ];
		if(GetPlayerVehicleID(playerid) != VehicleInfo [ b ] [ IDVeh ]) return SendClientMessage(playerid,Red,".רכב זה אינו נמצא בבעלותך");
		if(GetPlayerState(playerid) != PLAYER_STATE_DRIVER) return SendClientMessage(playerid,Red,".אינך הנהג ברכב ולכן אין באפשרותך למכור אותו");
		new price_c;
		if(sscanf(params[6],"i",price_c)) return SendClientMessage(playerid,White,"Usage: /Auction start [price]");
  		if(price_c < 1 || price_c > 20000000) return SendClientMessage(playerid, Red, ".20,000,000$-1$ הינך יכול להתחיל מכירה פומבית בין הסכומים");
		Auc_Price = price_c;
		Auction_ID = playerid;
  		S_SendMsgToAll(COLOR_AUCTION,">> (/Auction info לפרטים נוספים | %s התחיל מכירה פומבית על רכבו, (פרטים: מחיר מינימלי $%s | שם הרכב %s",GetVehicleName(VehicleInfo [ pInfo [ Auction_ID ] [ VehicleID ] ] [ IDVeh ]),GetNum(Auc_Price),GetName(playerid));
 		SendClientMessageToAll(COLOR_AUCTION, ">> המכירה הפומבית תסתיים בעוד 2 דקות | /Auction suggest מעוניינים לקנות את הרכב? השתמשו בפקודה");
		Auc_Timer = SetTimer("AuctionStop",120000, false);
		return 1;
	}
	if(!strcmp(command, "suggest", true))
	{
	    if(InActAuto[playerid] == true && ActStatus >= 2) return SendClientMessage(playerid,Red, "./QAct - אינך יכול להקיש פקודות שאתה בפעילות. ליציאה, הקש");
        if(pInfo [ playerid ] [ HaveLicense ] != 2) return SendClientMessage(playerid,Red, "/License אין לך רישיון נהיגה ולכן לא תוכל לקנות רכב, בשביל להוציא רישיון נהיגה");
        if(Auction_ID == -1) return SendClientMessage(playerid,Red, ".אין מכירה פומבית פעילה");
        if(Auc_IDSuggest == playerid) return SendClientMessage(playerid,Red, ".כבר הצעת על רכב זה , חכה ששחקן יציע ורק אחרי זה תוכל להציע");
	   	if(Auction_ID == playerid) return SendClientMessage(playerid,Red, ".הרכב שהוצא למכירה זה הרכב שלך ולכן לא תוכל להשתתף במכירה הפומבית");
        if(pInfo [ playerid ] [ VehicleID ]) return SendClientMessage(playerid,Red, ".יש לך רכב");
		new price_c;
		if(sscanf(params[7],"i",price_c)) return SendClientMessage(playerid,White,"Usage: /Auction suggest [price]");
		if(price_c > Y_GetPlayerMoney(playerid))return SendClientMessage(playerid, Red, ".אין בידך סכום זה");
	   	if(price_c < Auc_Price+1) return S_SendMsg(playerid,Red, "*! $%d לא ניתן להציע על הרכב פחות מהסכום",Auc_Price+1);
  		S_SendMsgToAll(COLOR_AUCTION, ">> במכירה פומבית %s הציע ($%s) על הרכב %s",GetVehicleName(VehicleInfo [ pInfo [ Auction_ID ] [ VehicleID ] ] [ IDVeh ]),GetNum(price_c),GetName(playerid));
		if(Auc_IDSuggest != -1)
		{
			Y_GivePlayerMoney(Auc_IDSuggest,Auc_Price);
			S_SendMsg(Auc_IDSuggest,Red, ".שמוצע כרגע במכירה פומבית , סכום הכסף שהצעת ($%s) חזר אליך %s שחקן אחר הציע סכום כסף על הרכב",GetNum(Auc_Price),GetVehicleName(VehicleInfo [ pInfo [ Auction_ID ] [ VehicleID ] ] [ IDVeh ]));
		}
		Auc_Price = price_c;
		Auc_IDSuggest = playerid;
		Y_GivePlayerMoney(playerid, - price_c);
		return 1;
	}
	if(!strcmp(command, "end", true))
	{
		if(!pInfo [ playerid ] [ VehicleID ]) return SendClientMessage(playerid,Red, ".אין לך רכב");
  		if(Auction_ID == -1) return SendClientMessage(playerid,Red, ".אין מכירה פומבית פעילה");
	    if(Auction_ID != playerid) return SendClientMessage(playerid,Red, ".לא הוצאת את רכבך למכירה פומבית");
		if(!IsPlayerInAnyVehicle(playerid) || GetPlayerState(playerid) != PLAYER_STATE_DRIVER) return SendClientMessage(playerid,Red,".(על מנת למכור את רכבך עליך להיות בתוכו (להיות הנהג");
		new b = pInfo [ playerid ] [ VehicleID ];
		if(GetPlayerVehicleID(playerid) != VehicleInfo [ b ] [ IDVeh ]) return SendClientMessage(playerid,Red,".רכב זה אינו נמצא בבעלותך");
		AuctionStop();
		return 1;
	}
	return SendClientMessage(playerid,Red,".הפקודה לא קיימת במערכת");
}
function AuctionStop()
{
	if(Auction_ID != -1)
	{
		S_SendMsgToAll(COLOR_AUCTION, ">> שהיה מוצע למכירה פומבית כבר לא מוצע למכירה מפני שהמכירה הפומבית הסתיימה %s הרכב",GetVehicleName(VehicleInfo [ pInfo [ Auction_ID ] [ VehicleID ] ] [ IDVeh ]));
        if(Auc_IDSuggest == -1) SendClientMessageToAll(COLOR_AUCTION, ".אף אחד לא קנה את הרכב");
		else
		{
