		S_SendMsgToAll(-1, ".{"#C_White"}%s{"#C_Yellow"}, {"#C_White"}%s{"#C_Yellow"},{"#C_White"} %s {"#C_Yellow"}:%s%s{"#C_Yellow"} הרכב הקלאן", GetName(ClanInviteWar[ClanWarTeam[1]][2]), GetName(ClanInviteWar[ClanWarTeam[1]][3]), GetName(ClanInviteWar[ClanWarTeam[1]][4]),ColorClan(ClanWarTeam[1]),ClanInfo [ ClanWarTeam[1] ] [ ClanName ] );
