	if(GetPlayerVehicleID(playerid) != VehicleInfo [ i ] [ IDVeh ]) return SendClientMessage(playerid,Red,".רכב זה אינו בבעלותך");
	GetVehiclePos(VehicleInfo [ i ] [ IDVeh ], GPos[0], GPos[1], GPos[2]);
	GetVehicleZAngle(VehicleInfo [ i ] [ IDVeh ], GPos[3]);
   	if(VehicleInfo [ i ] [ NeonNumber ])
	{
	  	DestroyDynamicObjectEx(VehicleInfo [ i ] [ ObjectNeon ] [ 0 ]);
	  	DestroyDynamicObjectEx(VehicleInfo [ i ] [ ObjectNeon ] [ 1 ]);
	}
    DestroyDynamic3DTextLabel(VehicleInfo [ i ] [ Veh3DText ]);
	DestroyVehicleEx(VehicleInfo [ i ] [ IDVeh ]);
	new INI:File = INI_Open(VehicleID_File(i));
	INI_WriteFloat(File,"X",GPos[0]);
	INI_WriteFloat(File,"Y",GPos[1]);
	INI_WriteFloat(File,"Z",GPos[2]);
	INI_WriteFloat(File,"A",GPos[3]);
	INI_Close ( File );
    VehicleInfo [ i ] [ IDVeh ] = CreateVehicle( VehicleInfo [ i ] [ ModelVeh ] ,GPos[0], GPos[1], GPos[2],GPos[3],VehicleInfo [ i ] [ ColorVeh ] [ 0 ],VehicleInfo [ i ] [ ColorVeh ] [ 1 ], 999,1,i);
	new zStr[128];
	format(zStr, sizeof(zStr), "{"#C_White"}\"%s\" {"#C_Pink"}:רכב בבעלות\n{"#C_White"}\"%s\" {"#C_Pink"}:סוג רכב",VehicleInfo [ i ] [ VehOwner ],GetVehicleName(VehicleInfo [ i ] [ IDVeh ]));
	VehicleInfo [ i ] [ Veh3DText ] = CreateDynamic3DTextLabel(zStr,Yellow, 0.0, 0.0, 0.0, 20.8, INVALID_VEHICLE_ID,VehicleInfo [ i ] [ IDVeh ],0, -1, -1, -1, 100.0);
    SetVehicleNumberPlate(VehicleInfo [ i ] [ IDVeh ],VehicleInfo [ i ] [ Plate ]);
	for(new b; b < 17; ++b)
	{
		if(VehicleInfo [ i ] [ VehEnhancements ] [ b ] == 0) continue;
		AddVehicleComponent(VehicleInfo [ i ] [ IDVeh ],VehicleInfo [ i ] [ VehEnhancements ] [ b ]);
	}
	if(VehicleInfo [ i ] [ NeonNumber ])
	{
	    VehicleInfo [ i ] [ ObjectNeon ] [ 0 ] = CreateDynamicObject(VehicleInfo [ i ] [ NeonNumber ],0,0,0,0,0,0);
	    VehicleInfo [ i ] [ ObjectNeon ] [ 1 ] = CreateDynamicObject(VehicleInfo [ i ] [ NeonNumber ],0,0,0,0,0,0);
     	AttachDynamicObjectToVehicle(VehicleInfo [ i ] [ ObjectNeon ] [ 0 ],VehicleInfo [ i ] [ IDVeh ], 1.0, 0.0, -0.70, 0.0, 0.0, 0.0);
      	AttachDynamicObjectToVehicle(VehicleInfo [ i ] [ ObjectNeon ] [ 1 ],VehicleInfo [ i ] [ IDVeh ], -1.0, 0.0, -0.70, 0.0, 0.0, 0.0);
	}
	if(VehicleInfo [ i ] [ VehEnhancements ] [ 17 ] != 4) ChangeVehiclePaintjob(VehicleInfo [ i ] [ IDVeh ],VehicleInfo [ i ] [ VehEnhancements ] [ 17 ]);
	PutPlayerInVehicle(playerid,VehicleInfo [ i ] [ IDVeh ],false);
    SendClientMessage(playerid,lGreen, ".החנת את רכבך ומעכשיו תמיד הוא יהיה נימצא כאן");
    return 1;
}
