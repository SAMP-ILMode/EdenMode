public OnPlayerEnterDynamicArea(playerid, areaid)
{
 	if(areaid == AreaVip)
	{
	    if(!pInfo [ playerid ] [ VIPLevel ])
		{
   			SetPlayerPosEx(playerid,-759.4534,1050.9178,37.9728,213.0522);
   			SendClientMessage(playerid,Red,"!V.I.P - אינך מורשה להיכנס למפקדת");
		}
		else SendClientMessage(playerid,Green,"! V.I.P ברוך הבא למפקדת");
	}
	else if(areaid == AreaPickup_p)
	{
        SendClientMessage(playerid,llllGreen,".הצטרפת לחיפוש אחר החבילה הסודית , בהצלחה בחיפוש");
  		new pickup_Players;
		foreach(new i: Player)
		{
			if(IsPlayerInDynamicArea(i,AreaPickup_p) && i != playerid)
			{
                S_SendMsg(i,lBlue, ".הצטרף בחיפוש אחר החבילה הסודית {"#C_White"}\"%s\"{"#C_lBlue"} השחקן",GetName(playerid));
				pickup_Players++;
			}
		}
		if(pickup_Players) S_SendMsg(playerid,lGreen, ".מספר השחקנים המחפשים את הפיקאפ באזור זה {"#C_White"}%d",pickup_Players);
	}
 	else if(areaid == MoneyShip_Area)
	{
		if(IsPlayerInAnyVehicle(playerid))
		{
			switch(GetVehicleModel(GetPlayerVehicleID(playerid)))
			{
				case 520,447,425,432,464,476:
				{
					SendClientMessage(playerid,Red,".אסור להיכנס עם רכב יורה זה לאזור ספינת הכסף");
					SetVehicleToRespawn(GetPlayerVehicleID(playerid));
				}
 			}
	 	}
	    TimerMoneyShip[playerid] = SetTimerEx("MoneyShipP",1200, false, "i", playerid );
	}
	else
	{
		for(new i; i < Basic; i++)
		{
			if(i < MAX_CLANS && i != 0)
			{
				if(InClanReload[i] == true && areaid == AreaHq[i])
				{
				    InHq[playerid] = i;
					format(String,sizeof(String),"~b~Welcome To Hq~N~ ~b~~H~~H~%s!",ClanInfo [ i ] [ ClanName ]);
					GameTextForPlayer(playerid,String,2500,4);
					if(ClanArea[ i ] == 0) S_SendMsg(playerid,White,"%s.%s ברוך הבא למפקדת הקלאן",ColorClan(i),ClanInfo [ i ] [ ClanName ]);
                    else if(ClanArea[ i ] == 2) S_SendMsg(playerid,Yellow, ".%s%s {"#C_Yellow"}שנמצאת כרגע בשליטת הקלאן %s%s {"#C_Yellow"}ברוך הבא למפקדת הקלאן",ColorClan(Clanlozer[ i ]),NameClan(Clanlozer[ i ]),ColorClan(i),ClanInfo [ i ] [ ClanName ]);
					if(i != InClan(playerid))
					{
						SendClanMessageEx(i,Yellow, ".%s%s {"#C_Yellow"}- פולש כרגע למפקדת הקלאן שלך %s(%d) השחקן",ColorClan(i),NameClan(i),GetName(playerid),playerid);
					}
					if(IsPlayerInAnyVehicle(playerid))
     				{
						switch(GetVehicleModel(GetPlayerVehicleID(playerid)))
						{
 							case 520,447,425,432,464,476: CarAreaError[playerid] = SetTimerEx("Hacking_HeadQuarters",2000, true , "i" ,playerid);
 						}
					}
					break;
				}
			}
			if(i < MAX_CAPTURE && i != 0)
			{
				if(areaid == CaptureInfo [ i ] [ Capture_Area ])
				{
				    InCapture[playerid] = i;
					S_SendMsg(playerid,White,".ברוך הבא לאזור %s%s",ColorCapture(i),NameCapture(i));
					if(Capturelozer [ i ]) S_SendMsg(playerid,White, "%s.%s אזור זה כרגע בשליטת הקלאן",ColorClan(Capturelozer [ i ]),NameClan(Capturelozer [ i ]));
   					if(IsPlayerInAnyVehicle(playerid))
    	    		{
						switch(GetVehicleModel(GetPlayerVehicleID(playerid)))
						{
 							case 520,447,425,432,464,476:
							{
								SendClientMessage(playerid,Red,".אסור להיכנס לאזור כבישה עם רכב יורה");
								SetVehicleToRespawn(GetPlayerVehicleID(playerid));
 							}
				 		}
				 	}
                    break;
				}
			}
			if(i < MAX_Wcache && i != 0)
			{
				if(areaid == WcacheInfo [ i ] [ Wcache_Area ])
				{
                    InWcache[playerid] = i;
					S_SendMsg(playerid,White,"{"#C_White"}.ברוך הבא לאזור החימוש %s%s",ColorWcache(i),NameWcache(i));
					if(Wcachelozer [ i ]) S_SendMsg(playerid,White,"%s.%s אזור חימוש זה כרגע בשליטת הקלאן",ColorClan(Wcachelozer [ i ]),NameClan(Wcachelozer [ i ]));
					break;
				}
			}
			if(i < sizeof PgInfo)
			{
				if( areaid == InAreaPg [ i ] )
				{
					Prohibited_guns[playerid] = i;
					break;
				}
			}
		}
	}
	return 1;
}
