stock GDamagePlayer(playerid, Float:damage)
{
    new Float:hp,Float:arm;
    GetPlayerHealth(playerid,hp);
	GetPlayerArmour(playerid,arm);
	if(damage >= 0)
	{
		if(arm >= 99 && hp >= 99) return true;
		if((hp + damage) >= 99)
		{
			SetPlayerHealth(playerid, 99);
    	    damage -= (99-hp);
			if((damage + arm) >= 99) SetPlayerArmour(playerid,99);
			else SetPlayerArmour(playerid,damage + arm);
		}
 		else SetPlayerHealth(playerid, hp + damage );
	}
 	return true;
}
stock DDamagePlayer(playerid, Float:damage)
{
    new Float:hp, Float:arm;
    GetPlayerHealth(playerid,hp);
	GetPlayerArmour(playerid,arm);
    if(damage < arm) SetPlayerArmour(playerid,arm-damage);
   	else
	{
		SetPlayerArmour(playerid,0);
  		SetPlayerHealth(playerid,hp - (damage - arm));
    }
    return true;
}
