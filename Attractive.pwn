CMD:search(playerid, params[])
{
    Tolevel (playerid,0);
	if(sscanf(params,"s[30]",params)) return SendClientMessage(playerid,White,"Usage: /(S)earch [id/name]");
	id = strval(params);
	SendClientMessage(playerid, Green, ":משתמשים שנמצאו");
	if(IsNumeric(params))
	{
		if(!IsPlayerConnected(id)) return SendClientMessage(playerid, Red, ".המשתמש אינו מחובר");
		S_SendMsg(playerid, Orange,  "%s", GetName(id));
	}
	else
	{
		new found;
		foreach(new i: Player)
		{
			if(strfind(GetName(i),params,true) != -1) S_SendMsg ( playerid , Orange , "%d. %s [id: %d]" , ++ found , GetName ( i ) , i );
		}
		if(!found) return SendClientMessage(playerid,Red,".לא נמצאו תוצאות");
		else S_SendMsg ( playerid , 0x17FF17FF , ".בסך הכל נמצאו %d תוצאות" , found );
	}
	return 1;
}
CMD:tag(playerid, params[])
{
    if(InActAuto[playerid] == true && ActStatus >= 2) return SendClientMessage(playerid,Red, "./QAct - אינך יכול להקיש פקודות שאתה בפעילות. ליציאה, הקש");
    Tolevel (playerid,10);
	new command[5];
	if(sscanf(params,"s[5]{s[12]}",command)) return SendClientMessage(playerid,White,"Usage: /Tag [ON / OFF]");
	if(!strcmp(command, "ON", true))
	{
	    if(pInfo [ playerid ] [ HaveTag ] == true) return SendClientMessage(playerid,Red,".כבר קיים אצלך התאג רמה");
		if(GetPlayerLevel(playerid) >= 25)
		{
		    new tvTag[12];
			if(sscanf(params[2],"s[12]",tvTag)) return SendClientMessage(playerid,White,"Usage: /Tag on [tag]");
			if(strlen(tvTag) > 9)return SendClientMessage(playerid,Red, ".תאג יכול להכיל עד 10 תווים");
            pInfo [ playerid ] [ HaveTag ] = true;
            format( pInfo [ playerid ] [ Tag_P ] , 10 , "%s", tvTag );
           	new INI:File = INI_Open(PFile(playerid));
    		INI_WriteBool ( File, "HaveTag",true);
    		INI_WriteString ( File, "Tag",pInfo [ playerid ] [ Tag_P ]);
		    INI_Close ( File );
   			S_SendMsg(playerid,0x17FF17FF, ".\"%s\" - ערכת את תאג הרמה שלך ל",tvTag);
		}
		else
		{
			S_SendMsg(playerid,0x17FF17FF, ".%d המותאם לרמתך - \"%s\" קיבלת את התאג",GetPlayerLevel(playerid),GetTagLevel(playerid));
  			format( pInfo [ playerid ] [ Tag_P ] , 30 , "%s", GetTagLevel(playerid));
	 		pInfo [ playerid ] [ HaveTag ] = true;
   			new INI:File = INI_Open(PFile(playerid));
    		INI_WriteBool ( File, "HaveTag",true);
    		INI_WriteString ( File, "Tag",pInfo [ playerid ] [ Tag_P ]);
		    INI_Close ( File );
		}
		return 1;
	}
	if(!strcmp(command, "OFF", true))
	{
		if(pInfo [ playerid ] [ HaveTag ] == false) return SendClientMessage(playerid,Red,".אין לך תאג שמור");
		format(pInfo [ playerid ] [ Tag_P ] , 4 , "אין");
		pInfo [ playerid ] [ HaveTag ] = false;
		new INI:File = INI_Open(PFile(playerid));
	    INI_WriteBool ( File, "HaveTag",false);
	    INI_WriteString ( File, "Tag","אין");
	    INI_Close ( File );
		return S_SendMsg(playerid,Red, ".%d המותאם לרמתך - \"%s\" הורדת את התאג",GetPlayerLevel(playerid),GetTagLevel(playerid));
	}
	return SendClientMessage(playerid,Red, "* הפקודה שכתבת אינה קיימת במערכת התאגים");
}
