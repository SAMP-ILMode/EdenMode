function CheckAntiSobiet(playerid,bool:num)
{
	if(num == false)
	{
		new Float:Pos[4],vehid;
		GetPlayerPos(playerid, Pos[0], Pos[1], Pos[2],Pos[3]);
		vehid = CreateVehicle(457, Pos[0], Pos[1], Pos[2]-1,Pos[3], -1, -1, 100);
		PutPlayerInVehicle(playerid, vehid, false);
    	DestroyVehicleEx(vehid);
    	SetTimerEx("CheckAntiSobiet",1500, false, "ii", playerid,true);
	}
	else
	{
		new gun,ammo;
		GetPlayerWeaponData(playerid, 1, gun, ammo);
		if(gun == 2)
		{
            ResetPlayerWeapons(playerid);
 			format (String, sizeof ( String ), "{"#C_Red"},%s שלום\n\n:השרת מזהה אצלך צ'יט מסוג\n(S0b31t)\n\n.נא צא מהשרת, תסיר את הצ'יטים וכנס נקי\n.תורחק מהשרת \"S0b31t\" אם תמשיך לנסות להכנס עם צ'יט מסוג",GetName(playerid));
    		ShowPlayerDialog(playerid, 98211, DIALOG_STYLE_MSGBOX,"DM Guard",String, "{"#C_llllGreen"}אישור", "");
            LogAntiCheat(playerid,"Cheats: S0b31t",true,true);
		}
		else SendClientMessage(playerid,Yellow,"» !משחק מהנה בשרת");
	}
 	return 1;
}

