			}
			else
			{
	  			if(!pInfo [ playerid ] [ VIPLevel ]) Y_GivePlayerMoney(playerid,RandomEx(2,6)*20);
				else Y_GivePlayerMoney( playerid , (pInfo [ playerid ] [ VIPLevel ] * 5) + RandomEx(2,6)*20);
			}
		}
	}
	return 1;
}

public OnPlayerLeaveDynamicArea(playerid, areaid)
{
	for(new i; i < Basic; i++)
	{
		if(i < MAX_CLANS && i != 0)
		{
			if(InClanReload[i] == true && areaid == AreaHq[i])
			{
			    InHq[playerid] = -1;
				if(ClanArea[ i ] == 0) S_SendMsg(playerid,White,"%s.%s יצאת מאזור מפקדת הקלאן",ColorClan(i),ClanInfo [ i ] [ ClanName ]);
    			else if(ClanArea[ i ] == 2) S_SendMsg(playerid,Yellow, ".%s%s {"#C_Yellow"}שנמצאת כרגע בשליטת הקלאן %s%s {"#C_Yellow"}יצאת מאזור מפקדת הקלאן",ColorClan(Clanlozer[ i ]),NameClan(Clanlozer[ i ]),ColorClan(i),ClanInfo [ i ] [ ClanName ]);
				if(IsPlayerInAnyVehicle(playerid))
        		{
					switch(GetVehicleModel(GetPlayerVehicleID(playerid)))
					{
 						case 520,447,425,432,464,476: KillTimerEx(CarAreaError[playerid]);
			 		}
			 	}
			}
		}
		if(i < MAX_CAPTURE && i != 0)
		{
			if(areaid == CaptureInfo [ i ] [ Capture_Area ])
			{
			    InCapture[playerid] = -1;
				S_SendMsg(playerid,White,".{"#C_White"}יצאת מאזור הכיבוש %s%s",ColorCapture(i),NameCapture(i));
				if(Capturelozer [ i ]) S_SendMsg(playerid,White, "%s.%s שנמצאת כרגע בשליטת הקלאן",ColorClan(Capturelozer [ i ]),NameClan(Capturelozer [ i ]));
				break;
			}
		}
		if(i < MAX_Wcache && i != 0)
		{
			if(areaid == WcacheInfo [ i ] [ Wcache_Area ])
			{
   				InWcache[playerid] = -1;
				S_SendMsg(playerid,White,".{"#C_White"}יצאת מאזור החימוש %s%s",ColorWcache(i),NameWcache(i));
                if(Wcachelozer [ i ]) S_SendMsg(playerid,White,"%s.%s שנמצאת כרגע בשליטת הקלאן",ColorClan(Wcachelozer [ i ]),NameClan(Wcachelozer [ i ]));
				break;
			}
		}
		if(i < sizeof PgInfo)
		{
		    if( areaid == InAreaPg [ i ] )
   			{
				Prohibited_guns[playerid] = -1;
				break;
   			}
  		}
	}
	if(areaid == AreaPickup_p)
	{
		foreach(new i: Player)
		{
			if(IsPlayerInDynamicArea(i,AreaPickup_p) && i != playerid) S_SendMsg(i,Red, ".עזב את אזור החיפוש אחר החבילה הסודית {"#C_White"}\"%s\"{"#C_Red"} השחקן",GetName(playerid));
		}
        SendClientMessage(playerid,Red,".עזבת את אזור החיפוש אחר החבילה הסודית");
	}
	else if(areaid == MoneyShip_Area)
	{
		KillTimerEx(TimerMoneyShip[playerid]);
	}
	return 1;
}

