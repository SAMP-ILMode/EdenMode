CMD:reloadobjects(playerid, params[])
{
    if(LevelAdmin(playerid) < 20) return 0;
    if(AdmLog[playerid] == false)return SendClientMessage(playerid,Red,"ERROR: אינך מחובר למערכת אדמינים");
	for(new i; i < 2000; i++)
	{
	    if(IsValidDynamicObject(ObjectsServer[i])) DestroyDynamicObjectEx(ObjectsServer[i]);
