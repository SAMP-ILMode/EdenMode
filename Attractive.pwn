function BombVs(num)
{
	num++;
	CreateExplosion(GetPosVs[0],GetPosVs[1],GetPosVs[2],10,0);
	if(num == 1) CreateExplosion(GetPosVs[0]+10,GetPosVs[1]+9,GetPosVs[2]+4,10,0);
	else if(num == 2) CreateExplosion(GetPosVs[0]+3,GetPosVs[1]+15,GetPosVs[2]+7,10,0);
	else if(num == 3) CreateExplosion(GetPosVs[0]+12,GetPosVs[1]+8,GetPosVs[2]+2,10,0);
	else if(num == 4) CreateExplosion(GetPosVs[0]+13,GetPosVs[1]+6,GetPosVs[2]+6,10,0);
	else if(num == 5)
	{
	    num = 0;
        CreateExplosion(GetPosVs[0]+10,GetPosVs[1]+9,GetPosVs[2]+4,10,0);
        CreateExplosion(GetPosVs[0]+3,GetPosVs[1]+15,GetPosVs[2]+7,10,0);
        CreateExplosion(GetPosVs[0]+12,GetPosVs[1]+8,GetPosVs[2]+2,10,0);
        CreateExplosion(GetPosVs[0]+13,GetPosVs[1]+6,GetPosVs[2]+6,10,0);
		KillTimerEx(TimerVs);
        TogglePlayerControllable( VSP , 1);
		SetPlayerHealth(VSP,0);
        VSP = -1;
	}
    return 1;
}
