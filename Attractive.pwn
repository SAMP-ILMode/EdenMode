CMD:spec(playerid, params[])
{
	if(LevelAdmin(playerid) < 3 && pInfo [ playerid ] [ ClanAccess ] == false && pInfo [ playerid ] [ NightTeam ] < 1) return 0;
	if(!AdmLog[playerid] && LevelAdmin(playerid))return SendClientMessage(playerid,Red,"ERROR: אינך מחובר למערכת אדמינים");
    if(pInfo [ playerid ] [ NightTeam ] && !GetTNT() && pInfo [ playerid ] [ NightTeam ] != 3) return SendClientMessage(playerid,Red,".הגישות של הצוות לילה ניתנות מהשעה 7 עד לשעה 6 בבוקר");
 	if(InDm[playerid] == true) return SendClientMessage(playerid,Red,".DM-אינך יכול להשתמש בפקודה זו שאתה נימצא באזור");
    if(InActAuto[playerid] == true && ActStatus >= 2) return SendClientMessage(playerid,Red, "./QAct - אינך יכול להקיש פקודות שאתה בפעילות. ליציאה, הקש");
	new ID;
	if(sscanf(params,"i",ID)) return SendClientMessage(playerid,White,"Usage: /Spec [id]");
	if(!IsPlayerConnected( ID )) return SendClientMessage(playerid, Red, ".המשתמש אינו מחובר");
	if(GetPVarInt(playerid,"Spec") == 1)return SendClientMessage(playerid, Red, ".אתה עוקב אחרי שחקן");
	if(ID == playerid) return SendClientMessage(playerid, Red, ".אינך יכול לעקוב אחרי עצמך");
	//if(GetPVarInt( ID ,"Spec") == 1)return SendClientMessage(playerid, Red, ".אתה לא יכול לעקוב אחרי שחקן שעוקב");
	if(LevelAdmin( ID ) > LevelAdmin(playerid))return SendClientMessage(playerid, Red, ".אתה לא יכול לעשות פקודה זו על אדמין זה מכיוון שהוא רמה יותר גבוהה ממך");
    if(GetPlayerVirtualWorld(ID) == 355) return SendClientMessage(playerid,Red, ".Spec שחקן זה נימצא בבחירת דמויות לכן לא תוכל לעשות על שחקן זה");
    SetPlayerVirtualWorld( playerid ,GetPlayerVirtualWorld( ID ));
    SetPlayerInterior(playerid,GetPlayerInterior( ID ));
    gSpectateID[playerid] = ID;
    //SetPVarInt(playerid,"Spec",1);
	if(!IsPlayerInAnyVehicle(ID))
	{
		TogglePlayerSpectating(playerid, 1);
		PlayerSpectatePlayer(playerid, ID);
	}
	else
	{
		TogglePlayerSpectating(playerid, 1);
		PlayerSpectateVehicle(playerid,GetPlayerVehicleID(ID));
	}
	return 1;
}
