		lotto[ Doublelotto ] = false;
		lotto[pay] = 0;
		KillTimerEx(lotto[LottoTimer]);
		return S_SendMsgToAll(Red, ".ביטל את הגרלת הלוטו %s האדמין",GetName(playerid));
	}
	if(!strcmp(command,"EditMoney",true))
	{
		if(lotto[on] == false || lotto[ Doublelotto ] == true) return SendClientMessage(playerid, Red, ".אין לוטו פעיל");
  		new EditMoney_L;
		if(sscanf(params[9],"i",EditMoney_L)) return SendClientMessage(playerid, White, "Usage: /ALotto EditMoney [1-50,000]");
		if(EditMoney_L > 50000 || EditMoney_L < 1) return SendClientMessage(playerid,Red, ".הסכום יכול להיות בין 1-50,000");
       	lotto[pay] = EditMoney_L;
       	S_SendMsg(playerid, lBlue, ".%d שינת את הסכום הזכייה בלוטו לסכום",EditMoney_L);
       	return 1;
	}
	if(!strcmp(command,"EditNumber",true))
	{
		if(lotto[on] == false || lotto[ Doublelotto ] == true) return SendClientMessage(playerid, Red, "! אין לוטו פעיל");
		new EditNumer_L;
		if(sscanf(params[10],"i",EditNumer_L)) return SendClientMessage(playerid, White, "Usage: /ALotto EditNumber [1-50]");
		if(EditNumer_L > 50 || EditNumer_L < 1) return SendClientMessage(playerid,Red, ".המספר יכול להיות בין 1-50");
		lotto[LottoA] = EditNumer_L;
		return S_SendMsg(playerid, lBlue, ".%d :המספר הוא",EditNumer_L);
	}
	return SendClientMessage(playerid,Red,".הפקודה לא קיימת במערכת");
}
