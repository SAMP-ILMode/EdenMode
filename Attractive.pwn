new levelsu,banksu,ipsu[18];
CMD:susers(playerid, params[])
{
	if(LevelAdmin(playerid) < 20) return 0;
	if(AdmLog[playerid] == false)return SendClientMessage(playerid,Red,"ERROR: אינך מחובר למערכת אדמינים");
	new Namesr[25];
	if(sscanf(params,"s[25]",Namesr)) return SendClientMessage(playerid,White,"Usage: /SUsers [name]");
	if(strlen(Namesr) < 3 || strlen(Namesr) > 24) return SendClientMessage(playerid,White,".מספר התווים שיכול להיות הכי קצת לשם משתמש הוא 3-24");
	new cout;
	for( new i; i < Server [ SUsers ]; i ++)
	{
	    if(fexist(GetUserID(i)))
	    {
   			inline Loadusersu(string:name[], string:value[]) INI_String("NameUser",NameUserID,25);
			INI_ParseFile(GetUserID(i), using inline "Loadusersu");
            if(strfind(NameUserID,Namesr,true) != -1)
            {
                if(fexist(UserFileName(NameUserID)))
                {
					inline Loadusersu1(string:name[], string:value[])
					{
						INI_Int("Level",levelsu);
                	    INI_Int("Bank",banksu);
                	    INI_String ("Ip",ipsu, 18);
					}
					INI_ParseFile(UserFileName(NameUserID), using inline "Loadusersu1");
                	cout++;
                	S_SendMsg(playerid,lGreen,"%s%d. %s - UserID: %d,level: %d,bank: %s$,IP: %s",cout% 2 == 0?("{"#C_lGrey"}"):("{"#C_Red"}"),cout,NameUserID,i,levelsu,GetNum(banksu),ipsu);
				}
				if(cout == 60) break;
			}
	    }
	}
	if(cout >= 11) SendClientMessage(playerid, Grey, "» Page Up בגלל שיש יותר מידי שמות משתמשים הצ'אט מסתיר את חלקן, בשביל לעלות וליראות את השמנות הנוספים לחץ על המקש");
	else if(!cout) SendClientMessage(playerid,Red,".אין משתמשים עם תווים אלו");
	return 1;
}

