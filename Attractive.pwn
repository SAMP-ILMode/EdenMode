       	LoadObjectsWcache(idLoad,WcacheFile_Objects(idLoad));
        LoadVehiclesWcache(idLoad,WcacheFile_Vehicles(idLoad));
        LoadPickupsWcache(idLoad,WcacheFile_Pickups(idLoad));
        if(WcacheInfo [ idLoad ] [ HasCheckWcache ] == true) WcacheInfo [ idLoad ] [ Check_Wcache ] = CreateDynamicCP(WcacheInfo [ idLoad ] [ Wcache_CheckX ],WcacheInfo [ idLoad ] [ Wcache_CheckY ],WcacheInfo [ idLoad ] [ Wcache_CheckZ ], 3.5);
