			{
                if(InActTeamA[i] == true)
				{
                    S_SendMsg(i,lGreen, ".ששדד את הבנק מת, ולכן הכסף חזר לבנק \"%s\" השחקן",GetName(i));
                    SendClientMessage(i,lGreen, ".לכן הגנבים יהיו צריכים לשדוד שוב את הבנק בשביל להפקיד אותו במפקדה שלהם");
