CMD:enterhouse(playerid, params[])
{
    if(InActAuto[playerid] == true && ActStatus >= 2) return SendClientMessage(playerid,Red, "./QAct - אינך יכול להקיש פקודות שאתה בפעילות. ליציאה, הקש");
	if(IsPlayerInAnyVehicle(playerid)) return SendClientMessage(playerid, Red, ".לא ניתן להיכנס לבית שלך כאשר אתה ברכב");
	new HouseCout = 0;
	for(new HouseID = 1; HouseID < MAX_HOUSES; HouseID++)
	{
		if(IsValidDynamicPickup(HousePickup[HouseID]))
		{
			if(IsPlayerInRangeOfPoint(playerid, 2.5, HouseInfo [ HouseID ] [ HX ],HouseInfo [ HouseID ] [ HY ],HouseInfo [ HouseID ] [ HZ ]) )
			{
			    HouseCout++;
				if(strcmp(HouseInfo [ HouseID ] [ Owner ], GetName(playerid), false) == 0)
				{
				    QuitGAll(playerid);
					new IntID = HouseInfo [ HouseID ] [ HLevel ];
					SetPlayerVirtualWorld(playerid, 5000 + HouseID);
					SetPlayerInterior(playerid, AHouseInteriors[IntID][InteriorID]);
					SetPlayerPos(playerid, AHouseInteriors[IntID][IntX], AHouseInteriors[IntID][IntY], AHouseInteriors[IntID][IntZ]);
					APlayerData[playerid][CurrentHouse] = HouseID;
					SendClientMessage(playerid,0x0099FFFF, "* /ExitHouse - ברוך הבא לביתך , על מנת לצאת מהבית הקש/י את הפקודה");
					return 1;
				}
				if(HouseInfo [ HouseID ] [ Opened ] == true)
				{
				    QuitGAll(playerid);
					new IntID = HouseInfo [ HouseID ] [ HLevel ];
					SetPlayerVirtualWorld(playerid, 5000 + HouseID);
					SetPlayerInterior(playerid, AHouseInteriors[IntID][InteriorID]);
					SetPlayerPos(playerid, AHouseInteriors[IntID][IntX], AHouseInteriors[IntID][IntY], AHouseInteriors[IntID][IntZ]);
					APlayerData[playerid][CurrentHouse] = HouseID;
					format(String, sizeof(String), ".%s ברוך הבא %s של",HouseInfo [ HouseID ] [ Owner ],AHouseInteriors[IntID][InteriorName]);
					SendClientMessage(playerid, 0x0099FFFF, String);
					return 1;
				}
				else return SendClientMessage(playerid,Red, ".הבית הזה נעול");
			}
		}
	}
	if(!HouseCout) return SendClientMessage(playerid,Red, ".על מנת להשתמש בפקודה זו עליך להיות על פיקאפ הבית שלך / בית פתוח");
	return 1;
}
