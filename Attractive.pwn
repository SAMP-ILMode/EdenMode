CMD:checkplayer(playerid, params[])
{
	if(LevelAdmin(playerid) < 21) return 0;
	if(AdmLog[playerid] == false)return SendClientMessage(playerid,Red,"ERROR: אינך מחובר למערכת אדמינים");
	new ID,Find[50];
	if(sscanf(params,"is[50]",ID,Find)) return SendClientMessage(playerid,White,"Usage: /CheckPlayer [ID] [Text]");
	if(!IsPlayerConnected( ID )) return SendClientMessage(playerid,Red, ".המשתמש אינו מחובר");
	if(IsPlayerNPC( ID )) return SendClientMessage(playerid,Red, ".אין באפשרותך להשתמש בפקודה זו על בוט");
	if(markedP[ ID ] != -1) return SendClientMessage(playerid,Red, ".המחשב של משתמש זה מסומן");
	for(new i; i < MAX_Findpy; i++)
	{
        if(i > MAX_Findpy) return S_SendMsg(playerid,Red, ".ישנם כבר %d מחשבים מסומנים, וזה המקסימום שאפשר",MAX_Findpy);
		if(!fexist(FindPermanently(i)))
		{
			new INI:File = INI_Open(FindPermanently(i));
			INI_WriteString( File , "Admin",GetName(playerid));
			INI_WriteString( File , "Player",GetName(ID));
			INI_WriteString( File , "Reason", Find);
			INI_WriteString( File , "gpci", GetGpci(ID));
			INI_Close ( File );
		    S_SendMsg(playerid,Red, ".ומעכשיו הוא מסומן במערכת \"%s\" סימנת את המחשב של השחקן",GetName(ID));
		    S_SendMsg(playerid,Red, "(%s)",Find);
		    markedP[ID] = i;
		    return 1;
 		}
 	}
	return 1;
}
