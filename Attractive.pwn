		if(EditMoney_L < 50000 || EditMoney_L > 200000) return SendClientMessage(playerid,Red, ".הסכום יכול להיות בין 50,000$-200,000$");
       	lotto[pay] = EditMoney_L;
       	S_SendMsg(playerid, lBlue, ".%d שינת את הסכום הזכייה בדאבל לוטו לסכום",EditMoney_L);
