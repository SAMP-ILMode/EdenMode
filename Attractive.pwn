CMD:walkwater(playerid, params[])
{
	if(WalkingOnWater[playerid] == false)
	{
		if(pInfo [ playerid ] [ InCode ] != 12) return SendClientMessage(playerid, Red, ".אין לך את הקוד הליכה / נסיעה על המים");
		WalkWaterObject[playerid] = CreatePlayerObject(playerid, 8661, 0.000000, 0.000000, 0.000000, 178.400314,0.000000,0.000000);
    	WalkingOnWater[playerid] = true;
		Walking_OnWater(playerid);
    	SendClientMessage(playerid, llllGreen, ".מצב ללכת / לנסוע על המים: מופעל");
	}
 	else
  	{
  	    WalkingOnWater[playerid] = false;
      	SendClientMessage(playerid, Red, ".מצב ללכת / לנסוע על המים: כבוי");
	}
 	return 1;
