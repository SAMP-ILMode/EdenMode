stock TeleFightArea(playerid,NumberArea,params[])
{
	if(InActAuto[playerid] == true && ActStatus >= 2) return SendClientMessage(playerid,Red, "./QAct - אינך יכול להקיש פקודות שאתה בפעילות. ליציאה, הקש");
    if(InFightArea[playerid] == NumberArea) return SendClientMessage(playerid,Red, ".אתה כבר נימצא באזור די-אם זה");
 	if(GetPlayerLevel(playerid) < FightArea[ NumberArea ][FA_Level]) return S_SendMsg(playerid,Red, ".שיגור זה זמין מרמה %d ומעלה", FightArea[ NumberArea ][FA_Level]);
	if(FightArea [ NumberArea ] [ FA_NumberWorlds ] != 0)
	{
	    new WorldFA;
        if(sscanf(params,"i", WorldFA ))
		{
			InVirtualDm[playerid] = 2;
			WorldFA = 1;
		}
		else
		{
		  	if(1 > WorldFA || WorldFA > FightArea [ NumberArea ] [ FA_NumberWorlds ]) return S_SendMsg(playerid,White, "Usage: %s [1-%d]",FightArea[ NumberArea ] [ FA_CMD ],FightArea[ NumberArea ] [ FA_NumberWorlds ]);
 			InVirtualDm[playerid] = WorldFA+1;
		}
		S_SendMsg(playerid,llllGreen, ".{"#C_llllGreen"}נכנסת לעולם מספר {"#C_Red"}%d# {"#C_llllGreen"},{"#C_Yellow"}\"%s\"{"#C_llllGreen"} - ברוך הבא לאזור ה",WorldFA,FightArea [ NumberArea ] [ FA_Name ]);
		SendClientMessage(playerid,llllGreen, "./Qdmz - בכדי לצאת מאזור זה הקש/י");
 	}
	else
	{
		S_SendMsg(playerid,llllGreen, ".{"#C_llllGreen"}נכנסת לעולם מספר {"#C_Red"}1# {"#C_llllGreen"},{"#C_Yellow"}\"%s\"{"#C_llllGreen"} - ברוך הבא לאזור ה",FightArea [ NumberArea ] [ FA_Name ]);
		SendClientMessage(playerid,llllGreen, "./Qdmz - בכדי לצאת מאזור זה הקש/י");
	}
	if(InFightArea[playerid] != -1) PDM[ InFightArea[playerid] ]--;
    InFightArea[playerid] = NumberArea;
    PDM[ NumberArea ]++;
	InDm[playerid] = true;
    KillsFightArea[playerid] = 0;
	PlayerSpawnFightArea(playerid);
	if(PlayersDM[ NumberArea ] - gettime() < 0 && PDM[ NumberArea ] % 5 == 0 && PDM[ NumberArea ] != 0)
	{
		PlayersDM[ NumberArea ] = gettime() + 600;
