CMD:markedslist(playerid, params[])
{
	if(LevelAdmin(playerid) < 21) return 0;
	if(AdmLog[playerid] == false)return SendClientMessage(playerid,Red,"ERROR: אינך מחובר למערכת אדמינים");
	new pys_cout;
	new StrHs[150] , catstr[1500],bool: mconnect[MAX_Findpy];
	for(new i, j = GetPlayerPoolSize(); i <= j; i++)
	{
		if(!IsPlayerConnected(i)) continue;
		if(markedP[i] != -1 && fexist(FindPermanently( markedP[i] )))
		{
		    mconnect[ markedP[i] ] = true;
			inline LoadPy(string:name[], string:value[]) INI_String("Reason",bInfo [ BReason ],128);
			INI_ParseFile(FindPermanently( markedP[i] ), using inline "LoadPy");
			pys_cout++;
			format(StrHs,sizeof(StrHs), "{"#C_Red"}%d. %s: {"#C_Orange"}%s{"#C_Red"} [ID: %03d] - {"#C_llllGreen"}מחובר\n", pys_cout, GetName(i),bInfo [ BReason ], i);
			strcat(catstr, StrHs ,1500);
		}
	}
	for(new i; i < MAX_Findpy; i++)
	{
		if(fexist(FindPermanently(i)) && mconnect[ i ] == false)
		{
			inline LoadPy(string:name[], string:value[])
			{
                INI_String("Player",bInfo [ BDate ],25);
                INI_String("Reason",bInfo [ BReason ],128);
			}
			INI_ParseFile(FindPermanently(i), using inline "LoadPy");
			pys_cout++;
			format(StrHs,sizeof(StrHs), "{"#C_Red"}%d. %s: {"#C_Orange"}%s{"#C_Red"} [ID: Unknow] - {"#C_Red"}מנותק\n", pys_cout,bInfo [ BDate ],bInfo [ BReason ]);
			strcat(catstr, StrHs ,1500);
		}
 	}
	if(!pys_cout) return SendClientMessage(playerid,Red,"! אין מסומנים כרגע");
    ShowPlayerDialog(playerid, 98211, DIALOG_STYLE_MSGBOX,"רשימת המסומנים בשרת", catstr, "סגור", "");
	return 1;
}

