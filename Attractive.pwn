function Walking_OnWater(playerid)
{
	if(!IsPlayerConnected(playerid) || WalkingOnWater[playerid] == false || GetPlayerVirtualWorld(playerid))
	{
        WalkingOnWater[playerid] = false;
        DestroyPlayerObjectEx(playerid, WalkWaterObject[playerid]);
        SendClientMessage(playerid, Red, ".הליכה על המים בוטל");
		return 1;
	}
	GetPlayerPos(playerid,GPos[0], GPos[1],GPos[2]);
	if(GPos[2] < 0)
	{
		if(GetPlayerState( playerid ) == PLAYER_STATE_DRIVER) SetVehiclePos(GetPlayerVehicleID( playerid ),GPos[0],GPos[1],4);
		else SetPlayerPos( playerid ,GPos[0],GPos[1],4);
		SendClientMessage(playerid,llllGreen, "• בגלל שנפלת למים / היית במים השרת הקפיץ אותך בכדי שתוכל ללכת על המים");
	}
	else if(GPos[2] < 2) SetPlayerObjectPos(playerid, WalkWaterObject[playerid], GPos[0], GPos[1], 0.2);
	SetTimerEx("Walking_OnWater",150,false,"i",playerid);
	return 1;
}
