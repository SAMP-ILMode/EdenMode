function SellWangExportVehicle(playerid)
{
	if(wantedWangExportVehicle == -1) return SendClientMessage(playerid,Red, ".המוסך לא מחפש כעת שום רכב");
	if(IsPlayerInAnyVehicle(playerid) && GetPlayerState(playerid)!=PLAYER_STATE_DRIVER) return SendClientMessage(playerid, White, ".אינך הנהג ברכב");
	if(!IsPlayerInAnyVehicle(playerid)) return SendClientMessage(playerid, Red, ".היכן הרכב שברצונך למכור");
	if(GetVehicleModel( GetPlayerVehicleID(playerid) ) == GetVehicleModel( wantedWangExportVehicle ))
	{
		if(SellWang[playerid] == true) return S_SendMsg(playerid, Red, ".%s כבר מכרת רכב מסוג זה",GetVehicleName(wantedWangExportVehicle));
		new payment = RandomEx(1500,4000);
		S_SendMsgToAll(lBlue, "*** !{"#C_lGreen"}%s${"#C_lBlue"} ותמורתו קיבל Wang למוסך {"#C_White"}\"%s\"{"#C_lBlue"} מכר את הרכב {"#C_White"}%s{"#C_lBlue"} השחקן",GetNum(payment),GetVehicleName(wantedWangExportVehicle),GetName(playerid));
		S_SendMsg(playerid,lBlue, ".ותמורתו קיבלת {"#C_lGreen"}%s${"#C_lBlue"}, צוות המוסך מודים לך על שירותך {"#C_White"}\"%s\"{"#C_lBlue"} את הרכב Wang מכרת למוסך",GetNum(payment),GetVehicleName(wantedWangExportVehicle));
		Y_GivePlayerMoney(playerid, payment);
		SetVehicleToRespawn(GetPlayerVehicleID(playerid));
		SellWang[playerid] = true;
	}
	else S_SendMsg(playerid,Red, ".%s איננו מחפשים רכב זה, אנו מחפשים",GetVehicleName(wantedWangExportVehicle));
	return 1;
}
function BankDeposit(playerid,amount)
{
	if(!IsPlayerConnected(playerid))return 0;
	if(amount > Y_GetPlayerMoney(playerid))return SendClientMessage(playerid, Red,".אין בידך סכום זה");
	if((pInfo [ playerid ] [ Bank ] + amount) >= (BankByLevel(GetPlayerLevel(playerid)) + 1)) return S_SendMsg(playerid,Red,".$%d מגבלת כמות הכסף בבנק לרמה %d היא",BankByLevel(GetPlayerLevel(playerid)),GetPlayerLevel(playerid));
	fSendBotBankMessage(playerid,".{"#C_lGreen"}$%d {"#C_White"}הפקדת לחשבונך {"#C_lGreen"}$%d{"#C_White"} מאזנך הוא",(pInfo [ playerid ] [ Bank ] + amount - amount/50),amount);
    pInfo [ playerid ] [ Bank ] = pInfo [ playerid ] [ Bank ] + amount - amount / 50;
    SaveBankFile(playerid);
 	fSendBotBankMessage(playerid,"! {"#C_Red"}%s${"#C_White"} בתוספת עמלה לבנק",GetNum(amount/50));
	Y_GivePlayerMoney(playerid,-amount);
	return 1;
}

function CountDown( )
{
	if(CD_SECONDS > 0)
	{
		new StrCD[19];
	 	if(CD_SECONDS > 100) format(StrCD, sizeof(StrCD), "~R~~H~%d", CD_SECONDS--);
		else if(CD_SECONDS > 70) format(StrCD, sizeof(StrCD), "~B~~H~~H~~H~%d", CD_SECONDS--);
		else if(CD_SECONDS > 40) format(StrCD, sizeof(StrCD), "~Y~~H~%d", CD_SECONDS--);
		else format(StrCD, sizeof(StrCD), "%d", CD_SECONDS--);
		TextDrawSetString(TextDraw[3],StrCD);
 		TextDrawShowForAll(TextDraw[3]);
	}
	else if(CD_SECONDS == 0)
 	{
		TextDrawSetString(TextDraw[3],"GO");
        CD_SECONDS--;
	}
	else if(CD_SECONDS == -1)
	{
        TextDrawHideForAll(TextDraw[3]);
	  	KillTimerEx(CD_TIMER);
 	  	CD = false;
	}
}

