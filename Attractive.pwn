function ClanWarTimer()
{
	if(0 < ClanWar[1])
	{
		format(String, 10, "%d", ClanWar[1]);
		GameTextForAll(String, 1000, 6);
		ClanWar[1]--;
	}
	else if(0 >= ClanWar[1])
	{
		GameTextForAll("GO!", 1000, 6);
		foreach(new i: Player)
		{
			if(InClanWar[i] == 1) TogglePlayerControllable(i, true);
		}
		ClanWar[0] = 1;
		SendClientMessageToAll(0x00FF9F00, "_______________________ [ {"#C_White"}Clan War{00FF9F} ] _______________________");
