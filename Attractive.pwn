			if(IsPlayerInRangeOfPoint(playerid, 2.5, HouseInfo [ HouseID ] [ HX ],HouseInfo [ HouseID ] [ HY ],HouseInfo [ HouseID ] [ HZ ]) )
			{
				if(HouseInfo [ HouseID ] [ Owned ] == true)
				{
				    foreach(new i: Player)
					{
						if(strcmp(HouseInfo [ HouseID ] [ Owner ], GetName(i), false) == 0)
						{
							SendClientMessage(i,Red, ".הבית שלך נמחק על ידי האדמין");
							for(new HouseSlot; HouseSlot < 1; HouseSlot++) if(APlayerData[i][Houses][HouseSlot] == HouseID) APlayerData[i][Houses][HouseSlot] = 0;
							break;
						}
					}
				}
				new INI:File = INI_Open(PlayerNameFile( HouseInfo [ HouseID ] [ Owner ] ));
                INI_WriteInt(File, "House",0);
    	    	INI_Close ( File );
				HouseInfo [ HouseID ] [ Owned ] = false;
				HouseInfo [ HouseID ] [ Owner ] = EOS;
				House_UpdateEntrance(HouseID);
				S_SendMsg(playerid,Grey, "(%d :הריסט התבצע לבית, בהצלחה (אידי הבית", HouseID);
				return 1;
			}
		}
		else return SendClientMessage(playerid,Red, ".אינך נימצא על פיקאפ בית");
	}
	return 1;
}

