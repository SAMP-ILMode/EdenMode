//head system
CMD:checkhead(playerid, params[])
{
    if(HeadID == -1) return SendClientMessage(playerid, Red, ".מערכת ראש השרת אינה פועלת כעת");
	S_SendMsg(playerid, Grey, "» .%s(ID: %03d) - ראש השרת כעת הינו",GetName(HeadID),HeadID);
	S_SendMsg(playerid, Grey, "» .%s - זמן שנותר לחלופת תפקיד",GetTimeN(1800 - (gettime()-HeadTime)));
	SendClientMessage(playerid, Grey, "» .{"#C_Grey"}צבעו במפה - {"#C_Yellow"}צהוב");
	return 1;
}
CMD:head(playerid, params[])
{
	if(LevelAdmin(playerid) < 20) return 0;
	if(AdmLog[playerid] == false)return SendClientMessage(playerid,Red,"ERROR: אינך מחובר למערכת אדמינים");
	new Array[6];
	if(sscanf(params, "s[6]", Array)) return SendClientMessage(playerid,White, "Usage: /Head [ Open / Close / Pass ]");
	if(!strcmp(Array, "open", true, 4))
	{
	    if(HeadID != -1) return SendClientMessage(playerid, Red, "./Head Close - המערכת פועלת, בשביל לסגור אותה");
		randomplayerhs();
		S_SendMsgToAll(lGreen, "» .נבחר לתפקיד ראש השרת {"#C_Purple"}%s",GetName(HeadID));
		S_SendMsg(HeadID, Grey, "** !הפעיל את מערכת ראש השרת ונבחרת באופן אקראי להיות ראש השרת %s האדמין",GetName(playerid));
		headserverstart(HeadID);
	}
	else if(!strcmp(Array, "close", true, 5))
	{
	    if(HeadID == -1) return SendClientMessage(playerid, Red, "./Head Open - המערכת לא פועלת, בשביל לפתוח אותה");
		S_SendMsgToAll(lGreen, "» .סגר את ראש השרת {"#C_Purple"}%s",GetName(playerid));
		HeadID = -1;
		HeadTime = 0;
	}
	else if(!strcmp(Array, "pass", true, 4))
	{
		if(sscanf(params, "s[6]u", Array, params[0])) return SendClientMessage(playerid,White, "Usage: /Head Pass [ID]");
		if(!IsPlayerConnected(params[0])) return SendClientMessage(playerid, Red, ".המשתמש אינו מחובר");
		if(HeadID == params[0]) return SendClientMessage(playerid, Red, ".שחקן זה כבר ראש השרת");
	   	S_SendMsgToAll(lGreen, ".על ידי האדמין {"#C_Purple"}%s {"#C_lGreen"}הוחלף בתפקיד ראש השרת עם השחקן {"#C_Purple"}%s",GetName(params[0]),GetName(HeadID));
        SetPlayerColor(HeadID, rgba2hex(random(255),random(255),random(255),100));
        S_SendMsg(HeadID, Grey, "» .%s - הזמן אשר שההת בו בתור ראש השרת",GetTimeN(gettime()-HeadTime));
		HeadID = params[0];
		S_SendMsg(HeadID, Grey, "** !העביר אליך את תפקיד ראש השרת %s האדמין",GetName(playerid));
        headserverstart(HeadID);
	}
	else return SendClientMessage(playerid,White, "Usage: /Head [ Open / Close / Pass ]");
	return 1;
}
stock randomplayerhs()
{
	new Iterator:RandP<MAX_PLAYERS>;
	for(new i, j = GetPlayerPoolSize(); i <= j; i++)
	{
		if(IsPlayerNPC(i) || !IsPlayerConnected(i) || GetPlayerVirtualWorld(i) || AFKTime[i] == 1000 || InActAuto[i]  == true || InActTeams(i) || SMPInfo [ i ] [ InMissions ] == true) continue;
		Iter_Add(RandP, i);
	}
	HeadID = Iter_Random(RandP);
	return 1;
}

stock headserverstart(playerid)
{
	SendClientMessage(playerid, Purple, "» :פרטים על המערכת");
	SendClientMessage(playerid, llPurple, "» .כעת אתה ראש השרת, זה אומר שתקבל תאג שכולם ידעו שזה התפקיד שלך");
	SendClientMessage(playerid, llPurple, "» .כעת אתה מוסמן בצבע {"#C_Yellow"}צהוב {"#C_llPurple"}במפה");
	SendClientMessage(playerid, llPurple, "» .בנוסף לכך, אתה תקבל כפול כסף מהנכסים + כפול כסף בספינת הכסף");
	SendClientMessage(playerid, Red, "» .שים לב: שחקן אשר יהרוג אותך יקבל את התפקיד שלך ותפקידך יוסר, או שכעבור חצי שעה תפקידך יוחלף");
    SetPlayerColor(playerid, Yellow);
    HeadTime = gettime();
}
//ranks system
CMD:setranks(playerid, params[])
{
	if(LevelAdmin(playerid) < 20 && pInfo [ playerid ] [ VIPLevel ] < 6 && pInfo [ playerid ] [ NightTeam ] < 3) return 0;
	if(AdmLog[playerid] == false && LevelAdmin(playerid) > 19)return SendClientMessage(playerid,Red,"ERROR: אינך מחובר למערכת אדמינים");
	DialogRanks(playerid);
	return 1;
}

CMD:ranks(playerid, params[])
{
	new players[7];
    foreach(new i: Player)
	{
		if(logged[i] == true && pInfo [ i ] [ dontshowme ] == false)
		{
		    if(pInfo [ i ] [ AdminLevel ] != 0) players[0]++;
			if(pInfo [ i ] [ ClanAccess ] == true) players[1]++;
			if(pInfo [ i ] [ BroadCasterAccess ] == true) players[2]++;
			if(pInfo [ i ] [ Staff ] == true) players[3]++;
			if(pInfo [ i ] [ NightTeam ] != 0) players[4]++;
			if(pInfo [ i ] [ VIPLevel ] != 0) players[5]++;
			if(pInfo [ i ] [ houseaccess ] == true) players[6]++;
		}
	}
	SendClientMessage(playerid,lGrey, "- - - Ranks: - - -");
    S_SendMsg(playerid, Yellow , "01# [%d :קבוצה: אדמינים - [משתמשים מחוברים מקבוצה זו", players[0]);
    S_SendMsg(playerid, Yellow , "02# [%d :קבוצה: מנהלי קלאנים - [משתמשים מחוברים מקבוצה זו", players[0]);
    S_SendMsg(playerid, Yellow , "03# [%d :קבוצה: שדרנים - [משתמשים מחוברים מקבוצה זו", players[0]);
    S_SendMsg(playerid, Yellow , "04# [%d :קבוצה: צוותי השרת - [משתמשים מחוברים מקבוצה זו", players[0]);
    S_SendMsg(playerid, Yellow , "05# [%d :קבוצה: צוותי לילה - [משתמשים מחוברים מקבוצה זו", players[0]);
    S_SendMsg(playerid, Yellow , "06# [%d :קבוצה: משתמשי כבוד - [משתמשים מחוברים מקבוצה זו", players[0]);
    S_SendMsg(playerid, Yellow , "07# [%d :קבוצה: מנהלי בתים - [משתמשים מחוברים מקבוצה זו", players[0]);
	return 1;
}


