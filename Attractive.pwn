CMD:cwithdraw(playerid, params[])
{
    if(InActAuto[playerid] == true && ActStatus >= 2) return SendClientMessage(playerid,Red, "./QAct - אינך יכול להקיש פקודות שאתה בפעילות. ליציאה, הקש");
 	if(pInfo [ playerid ] [ ClanLevel ] < 5)return LevelErrorClan(playerid,5);
	if(ClanInfo [ InClan ( playerid ) ] [ HasClanBank ] == false || !IsPlayerInDynamicCP (playerid, Bank_Clan[ InClan ( playerid ) ] ) )return SendClientMessage(playerid, Red, ".על מנת לבצע פקודה זו עליך להיות בבנק הקלאן שלך");
	if(ClanInfo [ InClan(playerid) ] [ ClanBank ] == false) return SendClientMessage(playerid, Red, ".בנק הקלאן נעול כרגע");
	new Amount_C;
	if(sscanf(params,"i",Amount_C)) return SendClientMessage(playerid, White, "Usage: /CWithdraw [amount]");
	if(ClanInfo [ InClan(playerid) ] [ Bank ] < Amount_C)return SendClientMessage(playerid, Red, ".הסכום שצויין שגוי");
	if(Amount_C < 1 || Amount_C > 5000000)return SendClientMessage(playerid, Red, ".הסכום שצויין שגוי");
	Y_GivePlayerMoney(playerid, Amount_C);
	ClanInfo [ InClan(playerid) ] [ Bank ] = ClanInfo [ InClan(playerid) ] [ Bank ] - Amount_C;
	SendClanMessageEx(InClan(playerid),0x4CA4E400, "(Clan %s%s{4CA4E4}) .משך מבנק הקלאן את הסכום %d$ %s",ColorClan(InClan(playerid)),ClanInfo [ InClan(playerid) ] [ ClanName ],Amount_C,GetName(playerid));
	S_SendMsg(playerid, 0x4CA4E400 ,".%d$ :משכת מחשבון הבנק קלאן את הסכום",Amount_C);
	return 1;
}
CMD:cdeposit(playerid, params[])
{
    if(InActAuto[playerid] == true && ActStatus >= 2) return SendClientMessage(playerid,Red, "./QAct - אינך יכול להקיש פקודות שאתה בפעילות. ליציאה, הקש");
	if(ClanInfo [ InClan ( playerid ) ] [ HasClanBank ] == false || !IsPlayerInDynamicCP(playerid, Bank_Clan[ InClan ( playerid ) ] ) )return SendClientMessage(playerid, Red, ".על מנת לבצע פקודה זו עליך להיות בבנק הקלאן שלך");
 	if(ClanInfo [ InClan(playerid) ] [ ClanBank ] == false)return SendClientMessage(playerid, Red, ".בנק הקלאן נעול כרגע");
	new Amount_C;
	if(sscanf(params,"i",Amount_C)) return SendClientMessage(playerid, White, "Usage: /CDeposit [amount]");
	if(Amount_C > Y_GetPlayerMoney(playerid))return SendClientMessage(playerid, Red, ".הסכום שצויין שגוי");
