public OnPlayerDisconnect(playerid, reason)
{
    if(IsPlayerNPC(playerid)) return 1;
   	if(reason == 0 || reason == 2 || Server [ Restart ] == 2) pInfo [ playerid ] [ CrashMoney ] = Y_GetPlayerMoney(playerid);
	//---------------------- savefile ------------------------
	if(fexist(PFile(playerid)) && logged[playerid] == true)
   	{
   	    if(GetPlayerVirtualWorld(playerid) != 355)
		{
  			format(pInfo [ playerid ] [ ClockStaff ], 20 , "%d",strval(pInfo [ playerid ] [ ClockStaff ]) + (gettime() - ClockStaff1[playerid]));
            ClockStaff1[playerid] = gettime();
		}
		else
		{
		    ClockStaff1[playerid] = gettime();
			format(pInfo [ playerid ] [ ClockStaff ], 20 , "%d",strval(pInfo [ playerid ] [ ClockStaff ]) + (gettime() - ClockStaff1[playerid]));
		}
		new INI:File = INI_Open(PFile(playerid));
	    INI_WriteInt ( File, "GoldPoints",pInfo [ playerid ] [ GoldPoints ]);
		INI_WriteInt ( File, "ClanID",pInfo [ playerid ] [ ClanID ]);
	 	INI_WriteInt ( File, "ClanLevel",pInfo [ playerid ] [ ClanLevel ]);
	 	INI_WriteInt ( File, "ClanSerial",pInfo [ playerid ] [ ClanSerial ]);
	    INI_WriteInt ( File, "HaveBazooka",pInfo [ playerid ] [ HaveBazooka ]);
	    INI_WriteString ( File, "DateBazooka",pInfo [ playerid ] [ DateBazooka ]);
	 	INI_WriteBool ( File, "CMute",pInfo [ playerid ] [ CMute ]);
	 	INI_WriteString ( File, "ClanTag",pInfo [ playerid ] [ ClanTag ]);
	 	INI_WriteInt ( File, "CrashMoney",pInfo [ playerid ] [ CrashMoney ]);
	 	INI_WriteString ( File, "ClockStaff",pInfo [ playerid ] [ ClockStaff ]);
	 	INI_WriteInt ( File, "Drugs",pInfo [ playerid ] [ pDrugs ]);
	    INI_Close ( File );

	    if(Server [ Restart ] == 3) return 1;
		if(Ob_Cage[playerid] > 0) DestroyDynamicObjectEx(Ob_Cage[playerid]);


		if(Server [ Restart ] == 2 && MessageCon_Dis == 0) S_SendMsgToAll(SeaGreen, "- [עזב את השרת [סיבה: אתחול מחדש {"#C_White"}%s(%d){"#C_SeaGreen"} השחקן",GetName(playerid),playerid);
		else if(MessageCon_Dis == 0)
		{
			switch(reason)
			{
				case 0:
				{
					for(new i, j = GetPlayerPoolSize(); i <= j; i++)
					{
						if(GetPlayerVirtualWorld(i) == 355 || !IsPlayerConnected(i)) continue;
						S_SendMsg(i,SeaGreen, "- [עזב את השרת [סיבה: תקלת מערכת {"#C_White"}%s(%d){"#C_SeaGreen"} השחקן",GetName(playerid),playerid);
					}
				}
				case 1:
				{
					for(new i, j = GetPlayerPoolSize(); i <= j; i++)
					{
						if(GetPlayerVirtualWorld(i) == 355 || !IsPlayerConnected(i)) continue;
						S_SendMsg(i,SeaGreen, "- [עזב את השרת [סיבה: התנתק {"#C_White"}%s(%d){"#C_SeaGreen"} השחקן",GetName(playerid),playerid);
					}
				}
				case 2:
				{
					for(new i, j = GetPlayerPoolSize(); i <= j; i++)
