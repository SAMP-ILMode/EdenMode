function MoneyShipP(playerid)
{
	if(AFKTime[playerid] != 1000 && IsPlayerInDynamicArea(playerid,MoneyShip_Area))
	{
	    TimerMoneyShip[playerid] = SetTimerEx("MoneyShipP",1200, false, "i", playerid );
        GetPlayerPos(playerid,GPos[0],GPos[1],GPos[2]);
		if(GPos[2] > 11.4996 && GPos[2] < 40.0238)
		{
            GDamagePlayer(playerid, 1.0);
			if(HeadID != playerid)
            {
	  			if(!pInfo [ playerid ] [ VIPLevel ]) Y_GivePlayerMoney(playerid,RandomEx(2,6)*10);
