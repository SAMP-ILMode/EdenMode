GetVehicleWithinDistance( playerid, Float:dist, &veh)
{
 	for(new i; i < sizeof(RCDM); i++)
	{
 		if(GetVehicleModel( RCDM [ i ] ) > 0)
 		{
   			if(GetPlayerVehicleID(playerid) != RCDM [ i ])
			{
            	new Float:x, Float:y, Float:z;
            	new Float:x2, Float:y2, Float:z2;
				new Float:x1, Float:y1, Float:z1;
                GetPlayerPos(playerid, x1,y1,z1 );
            	GetVehiclePos(RCDM[i], x, y, z);
            	x2 = x1 - x; y2 = y1 - y; z2 = z1 - z;
            	new Float:vDist = (x2 * x2 + y2 * y2 + z2 * z2);
            	if( vDist < dist)
				{
            		veh = RCDM[ i ];
            		dist = vDist;
				}
			}
		}
	}
}

IsVehicleRc( vehicleid )
{
	new model = GetVehicleModel(vehicleid);
  	switch(model)
  	{
  		case RC_GOBLIN, RC_BARON, RC_BANDIT, RC_RAIDER, RC_MINITANK: return 1;
  		default: return 0;
	}
	return 0;
}

stock ResetCarall()
{
    new bool: VehOnPlayer[MAX_VEHICLES];
	for(new i, j = GetPlayerPoolSize(); i <= j; i++)
	{
		if(!IsPlayerConnected(i) || !IsPlayerInAnyVehicle(i)) continue;
		VehOnPlayer [ GetPlayerVehicleID(i) ] = true;
  	}
	for(new i = 0; i < MAX_VEHICLES; i++)
	{
		if(i == Server [ MyFirstNPCVehicle ] || VehOnPlayer [ i ] == true) continue;
		SetVehicleToRespawn(i);
	}
	return 1;
}
