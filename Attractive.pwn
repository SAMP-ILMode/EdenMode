           	INI_WriteInt(File,"VehEnhancements8",0);
            INI_WriteInt(File,"VehEnhancements9",0);
            INI_WriteInt(File,"VehEnhancements10",0);
            INI_WriteInt(File,"VehEnhancements11",0);
            INI_WriteInt(File,"VehEnhancements12",0);
            INI_WriteInt(File,"VehEnhancements13",0);
