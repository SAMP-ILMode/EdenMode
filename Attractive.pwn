function ChangeSpeed(playerid)
{
    if(IsPlayerConnected(playerid) && IsPlayerInAnyVehicle(playerid))
	{
		new sp[4][100];
		new Float:health;
		new Float:xX;
		new Float:yY;
		new Float:zZ;
		new vehicleid = GetPlayerVehicleID(playerid);

		GetVehicleHealth(vehicleid,health);
	 	GetPlayerPos(playerid,xX,yY,zZ);
		if(GetPlayerState( playerid ) == PLAYER_STATE_DRIVER && NSh[playerid] == false && InSpeedHack == false)
		{
    		if(vehicleid != 578 || vehicleid != 520 || vehicleid != 578) //You can add more vehicle exceptions here
      		{
         		if(GetVehicleSpeed( vehicleid ) == 360)
         		{
          			GetPlayerPos(playerid, GPos[0], GPos[1], GPos[2]);
       	    		SetPlayerPos(playerid, GPos[0], GPos[1], GPos[2] + 5);
  					if(timeleftcheats[playerid]  - gettime() > 0 && Report_DMGuard == true) LogAntiCheat(playerid,"Cheats: Speed Hack (#09)",true,true);
					else
					{
    					timeleftcheats[playerid] = gettime() + 6;
						DMGuardMsgToAdmin("[DM Guard] .Speed hack חשוד שהשתמש בצ'יט %s[ID: %d] השחקן",GetName(playerid),playerid);
					}
				}
 		  	}
		}
        if(health == 1000.0)
		{
		    if(!IsPlayerInRangeOfPoint(playerid,25.0,487.3679,-1740.2372,11.1359) && !IsPlayerInRangeOfPoint(playerid,25.0,125.7892,-75.1295,1.5781) && !IsPlayerInRangeOfPoint(playerid,25.0,2065.2200,-1831.3635,13.5469) && !IsPlayerInRangeOfPoint(playerid,25.0,-1903.5262,288.6909,40.7706) && GetPlayerInterior(playerid) != 1 && GetPlayerInterior(playerid) != 3 && GetPlayerInterior(playerid) != 2)
   			{
				if(timeleftcheats[playerid]  - gettime() > 0 && Report_DMGuard == true) LogAntiCheat(playerid,"Cheats: Car Repair (#10)",true,true);
				else
				{
   					timeleftcheats[playerid] = gettime() + 6;
					DMGuardMsgToAdmin("[DM Guard] .Car Jump / Car Repair (Advance RP) חשוד שהשתמש בצ'יט %s[ID: %d] השחקן",GetName(playerid),playerid);
				}
	   		}
			SetVehicleHealth(vehicleid,999.0);
		}
        if(InTextDrawSpeedometer[playerid] == true)
        {
			if(health >= 1001)PlayerTextDrawSetString(playerid,Speedometer_HP[playerid],"~r~Health: ~r~~h~GM");
  			else
			{
				format(sp[2], 100, "~r~Health: ~r~~h~%.2f%",health + 10.0 / 10);
        		PlayerTextDrawSetString(playerid,Speedometer_HP[playerid],    sp[2]);
        	}
 			format(sp[0], 100, "~b~~h~~h~] ~w~%s ~b~~h~~h~]",GetVehicleName(vehicleid));
			format(sp[1], 100, "~b~~h~Speed: ~b~~h~~h~%d ~b~~h~km/h",GetVehicleSpeed(vehicleid));
			format(sp[3], 100, "~y~~h~Height: ~y~~h~~h~%.1f ~y~~h~m",zZ);
			PlayerTextDrawSetString(playerid,Speedometer_Name[playerid],  sp[0]);
			PlayerTextDrawSetString(playerid,Speedometer_Speed[playerid], sp[1]);
			PlayerTextDrawSetString(playerid,Speedometer_HG[playerid],    sp[3]);
		}
	}
	else
	{
		if(InTextDrawSpeedometer[playerid] == true)
		{
		    InTextDrawSpeedometer[playerid] = false;
			TextDrawHideForPlayer(playerid, Line_Speed[0]);
			TextDrawHideForPlayer(playerid, Line_Speed[1]);
			TextDrawHideForPlayer(playerid, Line_Speed[2]);
			TextDrawHideForPlayer(playerid, Line_Speed[3]);
			TextDrawHideForPlayer(playerid, Line_Speed[4]);
			PlayerTextDrawDestroy(playerid, Speedometer_Name[playerid]);
			PlayerTextDrawDestroy(playerid, Speedometer_Speed[playerid]);
			PlayerTextDrawDestroy(playerid, Speedometer_HP[playerid]);
			PlayerTextDrawDestroy(playerid, Speedometer_HG[playerid]);
		}
        if(!IsPlayerNPC(playerid)) KillTimerEx( VehicleSpeed_TD_Timer [ playerid ] );
	}
	return 1;
}
