CMD:ejectall(playerid, params[])
{
    Tolevel(playerid,3);
    if(InDm[playerid] == true) return SendClientMessage(playerid,Red,".DM-אינך יכול להשתמש בפקודה זו שאתה נימצא באזור");
	if(InActAuto[playerid] == true && ActStatus >= 2) return SendClientMessage(playerid,Red, "./QAct - אינך יכול להקיש פקודות שאתה בפעילות. ליציאה, הקש");
   	if(!IsPlayerInAnyVehicle(playerid) || GetPlayerState(playerid) != PLAYER_STATE_DRIVER) return SendClientMessage(playerid,Red,".על מנת להעיף את כל השחקנים מרכבך עליך להיות הנהג");
	new rcount;
	for(new i, j = GetPlayerPoolSize(); i <= j; i++)
	{
		if(!IsPlayerConnected(i) || !IsPlayerInVehicle(i,GetPlayerVehicleID(playerid)) || i == playerid) continue;
		{
			SendClientMessage(i,lGreen,".בעל הרכב הוציא אותך מרכבו");
			RemovePlayerFromVehicle(i);
			rcount++;
		}
	}
	if(!rcount) return SendClientMessage(playerid,Red,".אין ברכבך נוסעים");
	return S_SendMsg(playerid,lGreen, ".הוצאת מרכבך %d נוסעים",rcount);
}
