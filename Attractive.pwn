CMD:trivia(playerid, params[])
{
	if(TriviaOn == false)return SendClientMessage(playerid,Red,".אין חידון פעיל כרגע");
	if(WrongAns[playerid] == 3)return SendClientMessage(playerid, Red,".ענית כבר 3 תשובות שגויות , תוכל לנסות שוב בחידון הבא");
	if(sscanf(params,"s[132]",params)) return SendClientMessage(playerid,White,"Usage: /Trivia <answer>");
	if(!strcmp(params,Answer,true))
	{
		S_SendMsgToAll(llBlue, "!$%s זכה בסכום Trivia פתר את שאלת {"#C_White"}\"%s\"{"#C_llBlue"} השחקן",GetNum(TriviaMoney),GetName(playerid));
		S_SendMsgToAll(llBlue,  "%s :התשובה היא",Answer);
		TriviaOn = false;
		Y_GivePlayerMoney(playerid,TriviaMoney);
		KillTimerEx(EndTrivia);
  		UpdateTDServer(GetName(playerid),"is the winner in the~R~~H~~H~~H~~H~~H~ Trivia~h~!");
		return 1;
	}
	WrongAns[playerid]++;
	S_SendMsg(playerid,Red,".תשובה שגוי, נשאר לך עוד %d/3 הזדמנויות",WrongAns[playerid]);
	return 1;
}
