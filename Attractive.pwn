stock DeleteObjectsPart()
{
    if(InParty == false) return 1;
    for(new i; i < 141; i++) DestroyDynamicObjectEx(ObParty[i]);
    return 1;
}
stock ExitAct(playerid,reason[])
{
	if(InAct[playerid] == true)
	{
		SetPlayerInterior(playerid, 0);
		SetPlayerVirtualWorld(playerid, 0);
	    InAct[playerid] = false;
        PlayersInAct--;
		if(Freeze[ playerid ] == true)
		{
			TogglePlayerControllable(playerid, 1);
  			Freeze[ playerid ] = false;
		}
    	if(ClanInfo [ InClan(playerid) ] [ AntiTK_Clan ] == true && InClan(playerid)) SetPlayerTeam(playerid, InClan(playerid));
    	else SetPlayerTeam(playerid,NO_TEAM);
		if(IsPlayerInAnyVehicle(playerid))
		{
			SetVehicleToRespawn(GetPlayerVehicleID(playerid));
			SpawnPlayer(playerid);
		}
		else SpawnPlayer(playerid);
		SendClientMessage(playerid,Red,reason);
	}
	return 1;
}
function CountDown_Act()
{
	if(CD_SECONDS_Act > 0)
	{
		new StrCD[5];
	 	format(StrCD, sizeof(StrCD), "%d", CD_SECONDS_Act--);
		TextDrawSetString(TextDraw[3],StrCD);
 		TextDrawShowForAll(TextDraw[3]);
	}
	else if(CD_SECONDS_Act == 0)
 	{
		TextDrawSetString(TextDraw[3],"GO");
        CD_SECONDS_Act--;
	}
	else if(CD_SECONDS_Act == -1)
	{
        TextDrawHideForAll(TextDraw[3]);
	  	KillTimerEx(CD_TIMER_Act);
 	  	CD = false;
	  	if(CD_FREEZE_Act)
	  	{
	  		foreach(new i: Player)
			{
			  	if(InAct[i] == true) TogglePlayerControllable(i,1);
			}
		}
	}
	return 1;
}
