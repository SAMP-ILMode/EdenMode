    if(InDm[playerid] == true) return SendClientMessage(playerid,Red,".DM-אינך יכול להשתמש בפקודה זו שאתה נימצא באזור");
    if(InActAuto[playerid] == true && ActStatus >= 2) return SendClientMessage(playerid,Red, "./QAct - אינך יכול להקיש פקודות שאתה בפעילות. ליציאה, הקש");
	new ID;
	if(sscanf(params,"i",ID)) return SendClientMessage(playerid,White,"Usage: /Gmw [id]");
   	GetPlayerPos(ID, GPos[0], GPos[1], GPos[2]);
    if(!PlayerToPoint(5,playerid,GPos[0], GPos[1], GPos[2])) return SendClientMessage(playerid,Red,".שחקן זה לא נימצא קרוב אליך, לכן לא תוכל לבצע פקודה זו על שחקן זה");
	new SWeapons[13][2];
	for (new i; i < 13; i++)
	{
		GetPlayerWeaponData(playerid, i, SWeapons[i][0], SWeapons[i][1]);
        GivePlayerWeapon(ID, SWeapons[i][0], SWeapons[i][1]);
	}
	ResetPlayerWeapons(playerid);
	S_SendMsg(ID,lGreen,".שלח לך את הנשקים שלו \"%s\" השחקן",GetName(playerid));
	S_SendMsg(playerid,lGreen,".את הנשקים שלך \"%s\" שלחת לשחקן",GetName(ID));
    return 1;
}

CMD:selfie(playerid,params[])
{
    Tolevel(playerid,6);
 	if(takingselfie[playerid] == false)
 	{
        TogglePlayerControllable(playerid,0);
 		new Float:Degree;
		const Float: Radius = 1.4;
		const Float: Speed2  = 1.25;
		const Float: Height = 1.0;
		new Float:lPos[3];
     	GetPlayerPos(playerid,lPos[0],lPos[1],lPos[2]);
  		static Float: n1X, Float: n1Y;
  		if(Degree >= 360) Degree = 0;
  		Degree += Speed2;
  		n1X = lPos[0] + Radius * floatcos(Degree, degrees);
  		n1Y = lPos[1] + Radius * floatsin(Degree, degrees);
  		SetPlayerCameraPos(playerid, n1X, n1Y, lPos[2] + Height);
  		SetPlayerCameraLookAt(playerid, lPos[0], lPos[1], lPos[2]+1);
  		SetPlayerFacingAngle(playerid, Degree - 90.0);
  		takingselfie[playerid] = true;
  		ApplyAnimation(playerid, "PED", "gang_gunstand", 4.1, 1, 1, 1, 1, 1, 1);
  		return 1;
 	}
    else if(takingselfie[playerid] == true)
 	{
     	if(Freeze[ playerid ] == false) TogglePlayerControllable(playerid,1);
  		SetCameraBehindPlayer(playerid);
     	takingselfie[playerid] = false;
     	ApplyAnimation(playerid, "PED", "ATM", 4.1, 0, 1, 1, 0, 1, 1);
     	return 1;
 	}
    return 1;
}
