function CountBPWar(playerid,playerid2)
{
    if(!IsPlayerConnected(playerid) || !IsPlayerConnected(playerid2))
	{
		KillTimerEx( PWarBattle[ playerid ] [ TimerCountBattle ] );
		TogglePlayerControllable(playerid, 1);
		TogglePlayerControllable(playerid2, 1);
	}
	if(PWarBattle[playerid][CountBattle] == 0)
	{
		new DriftLocel[15];
		if(PWarBattle[playerid][DriftBattle] == 1) DriftLocel = "דריפט";
		else if(PWarBattle[playerid][DriftBattle] == 2) DriftLocel = "מירוץ";
		else if(PWarBattle[playerid][DriftBattle] == 3) DriftLocel = "דרבי";
        else if(PWarBattle[playerid][DriftBattle] == 4) DriftLocel = "קרב טנקים";
        else if(PWarBattle[playerid][DriftBattle] == 5) DriftLocel = "מירוץ סירות";
	  	S_SendMsgToAll(pwar_cc1,".%s$ על סכום הכסף {"#pwar_c0"}\"%s\"{"#pwar_c1"} לבין השחקן {"#pwar_c0"}\"%s\"{"#pwar_c1"} התחיל כעת %s בין השחקן",GetNum(PWarBattle[ playerid ] [ MoneyWin ]*2),GetName(playerid2),GetName(playerid),DriftLocel);
		KillTimerEx( PWarBattle[ playerid ] [ TimerCountBattle ] );
		TogglePlayerControllable(playerid, 1);
		TogglePlayerControllable(playerid2, 1);
	}
	else if(PWarBattle[playerid][CountBattle] > 0)
	{
		new StrPwar[20];
		format(StrPwar, 20, "~b~%d", PWarBattle[playerid][CountBattle]);
		GameTextForPlayer(playerid,StrPwar,1000,6);
		GameTextForPlayer(playerid2,StrPwar,1000,6);
	}
	PWarBattle[playerid][CountBattle] --;
	return 1;
}

