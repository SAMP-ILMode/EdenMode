	if(GetPlayerVirtualWorld(playerid) == 355) return SendClientMessage(playerid,Red,".על מנת לבצע פקודות הינך צריך להיכנס למשחק"),0;
	if(Ob_Cage[playerid] > 0 && LevelAdmin(playerid) < 20) return SendClientMessage(playerid,Red, ".אינך יכול להקיש פקודות בזמן שאתה נימצא בכלוב"),0;
    if(takingselfie[playerid] == true && strcmp(cmdtext,"/Selfie",true) != 0) return SendClientMessage(playerid,Red, "./Selfie - הקש/י את הפקודה Selfie כדי לסיים עם המצב ,Selfie אינך יכול לבצע פקודות כאשר אתה עושה"),0;
    if(fexist(Jail(playerid))) return S_SendMsg(playerid,Red, ".לא ניתן לבצע פקודות כאשר אתה בכלא, תוכל לכתוב שוב בעוד %d שניות",TimeJail[playerid]),0;
    
	if(logged[playerid] == false)
