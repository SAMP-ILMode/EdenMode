CMD:rad(playerid, params[])
{
	if(LevelAdmin(playerid) < 8) return 0;
	if(AdmLog[playerid] == false)return SendClientMessage(playerid,Red,"ERROR: אינך מחובר למערכת אדמינים");
	new command[20];
	if(sscanf(params,"s[20]{s[20]}",command)) return SendClientMessage(playerid, White, "Usage: /Rad [Command]");
	if(!strcmp(command, "Set", true) || !strcmp(command, "S", true))
	{
	    new SetRad_R;
		if(!strcmp(command, "Set", true) && sscanf(params[3],"i",SetRad_R) || !strcmp(command, "S", true) && sscanf(params[1],"i",SetRad_R)) return SendClientMessage(playerid, White, "Usage: /Rad Set [Radios]");
        if(SetRad_R < 1 || SetRad_R > 10000)return SendClientMessage(playerid,Red, ".מספר הרדיוס יכול להיות בין 1 ל 10,000");
		S_SendMsg(playerid,Yellow, "%d.000000 הגדרתה את הרדיוס שלך ל",SetRad_R);
		pInfo [ playerid ] [ Rad ] = SetRad_R;
		new INI:File = INI_Open(PFile(playerid));
		INI_WriteInt ( File, "Rad",pInfo [ playerid ] [ Rad ]);
	    INI_Close ( File );
		return 1;
	}
	if(!strcmp(command, "GM", true) || !strcmp(command, "GiveMoney", true))
	{
		new SetMoney_R;
		if(!strcmp(command, "GM", true) && sscanf(params[2],"i",SetMoney_R) || sscanf(params[9],"i",SetMoney_R) && !strcmp(command, "GiveMoney", true)) return SendClientMessage(playerid,White, "Usage: /Rad GM [Money 1-5,000]");
  		if(SetMoney_R < 1 || SetMoney_R > 5000) return SendClientMessage(playerid, Red, ".1-5,000 סכום הכסף שתוכל לתת");
		foreach(new i: Player)
		{
			if(i != playerid && IsPlayerNearPlayer(GetRad(playerid), playerid, i)) Y_GivePlayerMoney(i,SetMoney_R);
		}
  		S_SendMsgToAll(Brown, "%d.000000 הביא לכל השחקנים את הסכום כסף %d ברדיוס %s האדמין",GetRad(playerid),SetMoney_R, GetName(playerid));
        return 1;
	}
	if(!strcmp(command, "SetArmuor", true) || !strcmp(command, "Sar", true))
	{
		new Armuor_R;
		if(!strcmp(command, "SetArmuor", true) && sscanf(params[9],"i",Armuor_R) || !strcmp(command, "Sar", true) && sscanf(params[3],"i",Armuor_R)) return SendClientMessage(playerid,White, "Usage: /Rad sar [hp]");
  		if(Armuor_R < 0 || Armuor_R > 99) return SendClientMessage(playerid, Red, ".מספר החיים יכול להיות מ0 עד 99");
		foreach(new i: Player)
		{
			if(i != playerid && IsPlayerNearPlayer(GetRad(playerid), playerid, i)) SetPlayerArmour(i,Armuor_R);
		}
		S_SendMsgToAll(Brown,"%d.000000 שינה את אחוז המגן ל %d ברדיוס %s האדמין",GetRad(playerid), Armuor_R , GetName(playerid));
        return 1;
	}
	if(!strcmp(command, "vworld", true) || !strcmp(command, "VD", true))
	{
		new SetWorld_R;
		if(!strcmp(command, "vworld", true) && sscanf(params[6],"i",SetWorld_R) || !strcmp(command, "VD", true) && sscanf(params[2],"i",SetWorld_R)) return SendClientMessage(playerid,White, "Usage: /Rad Vworld [Number]");
		foreach(new i: Player)
		{
			if(i != playerid && IsPlayerNearPlayer(GetRad(playerid), playerid, i)) SetPlayerVirtualWorld(i, SetWorld_R);
		}
		S_SendMsgToAll(Brown,"%d.000000 שינה את העולם וירטואלי ל %d לכל השחקנים ברדיוס %s האדמין",GetRad(playerid),SetWorld_R, GetName(playerid));
        return 1;
	}
	if(!strcmp(command, "Shp", true) || !strcmp(command, "Sethp", true))
	{
		new SetHp_R;
		if(!strcmp(command, "Sethp", true) && sscanf(params[5],"i",SetHp_R) || !strcmp(command, "Shp", true) && sscanf(params[3],"i",SetHp_R)) return SendClientMessage(playerid,White, "Usage: /Rad shp [hp]");
  		if(SetHp_R < 1 || SetHp_R > 100) return SendClientMessage(playerid, Red, ".מספר החיים יכול להיות מ1 עד 99");
		foreach(new i: Player)
		{
			if(i != playerid && IsPlayerNearPlayer(GetRad(playerid), playerid, i)) SetPlayerHealth(i, SetHp_R);
		}
		S_SendMsgToAll(Brown,"%d.000000 שינה את אחוז החיים ל %d ברדיוס %s האדמין",GetRad(playerid), SetHp_R , GetName(playerid));
        return 1;
	}
	if(!strcmp(command, "GiveWeapon", true) || !strcmp(command, "Gw", true))
	{
		new weaponid,
			ammu;
		if(!strcmp(command, "GiveWeapon", true) && sscanf(params[10],"ii",weaponid,ammu) || !strcmp(command, "Gw", true) && sscanf(params[2],"ii",weaponid,ammu)) return SendClientMessage(playerid,White, "Usage: /Rad GiveWeapon [Weaponid] [Ammu]");
		foreach(new i: Player)
		{
			if(i != playerid && IsPlayerNearPlayer(GetRad(playerid), playerid, i)) GivePlayerWeapon(i, weaponid, ammu);
		}
		S_SendMsgToAll(Brown,"%d עם מספר הכדורים %s הביא לכל השחקנים ברדיוס %d.000000 את הנשק %s האדמין",ammu , WeaponName(weaponid), GetRad(playerid), GetName(playerid));
        return 1;
	}
	if(!strcmp(command, "Eject", true) || !strcmp(command, "Et", true))
	{
		foreach(new i: Player)
		{
			if(i != playerid && IsPlayerNearPlayer(GetRad(playerid), playerid, i)) RemovePlayerFromVehicle(i);
		}
		S_SendMsgToAll(Brown,"%d.000000 הוציא את כל השחקנים מרכבם ברדיוס %s האדמין", GetRad(playerid), GetName(playerid));
        return 1;
	}
	if(!strcmp(command, "Spawn", true) || !strcmp(command, "Sn", true))
	{
		foreach(new i: Player)
		{
			if(i != playerid && IsPlayerNearPlayer(GetRad(playerid), playerid, i)) SpawnPlayer(i);
		}
		S_SendMsgToAll(Brown,"%d.000000 עשה ספאון לכל השחקנים הנמצאים ברדיוס %s האדמין", GetRad(playerid), GetName(playerid));
        return 1;
	}
	if(!strcmp(command, "Jetpack", true) || !strcmp(command, "Jp", true))
	{
		foreach(new i: Player)
		{
			if(i != playerid && IsPlayerNearPlayer(GetRad(playerid), playerid, i))
			{
			    HaveJetpack[i] = true;
				SetPlayerSpecialAction(i,SPECIAL_ACTION_USEJETPACK);
			}
		}
		S_SendMsgToAll(Brown," %d.000000 לכל השחקנים הנמצאים ברדיוס Jetpack נתן %s האדמין", GetRad(playerid), GetName(playerid));
        return 1;
	}
	if(!strcmp(command, "Freeze", true) || !strcmp(command, "F", true))
	{
		foreach(new i: Player)
		{
			if(i != playerid && IsPlayerNearPlayer(GetRad(playerid), playerid, i) && Freeze[ i ] == false)
			{
				TogglePlayerControllable(i, 0);
                Freeze[ i ] = true;
			}
		}
		S_SendMsgToAll(Brown,"%d.000000 הקפיא את כל השחקנים ברדיוס %s האדמין", GetRad(playerid), GetName(playerid));
        return 1;
	}
	if(!strcmp(command, "UnFreeze", true) || !strcmp(command, "Unf", true))
	{
		foreach(new i: Player)
		{
			if(i != playerid && IsPlayerNearPlayer(GetRad(playerid), playerid, i)) TogglePlayerControllable(i, 1);
		}
		S_SendMsgToAll(Brown,"%d.000000 הפשיר את כל השחקנים ברדיוס %s האדמין", GetRad(playerid), GetName(playerid));
        return 1;
	}
	if(!strcmp(command, "Disarm", true) || !strcmp(command, "D", true))
	{
		foreach(new i: Player)
		{
			if(i != playerid && IsPlayerNearPlayer(GetRad(playerid), playerid, i)) ResetPlayerWeapons(i);
		}
		S_SendMsgToAll(Brown,"%d.000000 איפס נשקים לכל השחקנים ברדיוס %s האדמין", GetRad(playerid), GetName(playerid));
        return 1;
	}
	if(!strcmp(command, "ResetMoney", true) || !strcmp(command, "Rm", true))
	{
		foreach(new i: Player)
		{
			if(i != playerid && IsPlayerNearPlayer(GetRad(playerid), playerid, i)) Y_ResetPlayerMoney(i);
		}
		S_SendMsgToAll(Brown,"%d.000000 איפס את הכסף לכל השחקנים ברדיוס %s האדמין", GetRad(playerid), GetName(playerid));
        return 1;
	}
	if(!strcmp(command, "FixVehicle", true) || !strcmp(command, "Fv", true))
	{
		new Float:gPlayer_Pos[3];
		GetPlayerPos(playerid, gPlayer_Pos[0], gPlayer_Pos[1], gPlayer_Pos[2]);
        foreach(new i: Player)
        {
			if(!IsPlayerInAnyVehicle(i)) continue;
			if(!IsPlayerInRangeOfPoint(i, GetRad(playerid) , gPlayer_Pos[0], gPlayer_Pos[1], gPlayer_Pos[2])) continue;
			RepairVehicle(GetPlayerVehicleID(i));
			SetVehicleHealth(GetPlayerVehicleID(i),999.0);
		}
		S_SendMsgToAll(Brown,"%d.000000 תיקן את כל הרכבים ברדיוס %s האדמין",GetRad(playerid), GetName(playerid));
        return 1;
	}
	if(!strcmp(command, "SetSkin", true) || !strcmp(command, "Ss", true))
	{
		new SetSkin_R;
		if(!strcmp(command, "SetSkin", true) && sscanf(params[7],"i",SetSkin_R) || !strcmp(command, "Ss", true) && sscanf(params[2],"i",SetSkin_R)) return SendClientMessage(playerid, White, "/rad ss [1-299]");
		foreach(new i: Player)
		{
			if(i != playerid && IsPlayerNearPlayer(GetRad(playerid), playerid, i)) SetPlayerSkin(i, SetSkin_R );
		}
		S_SendMsgToAll(Brown,"%d.000000 שינה את הסקין לכל השחקנים ברדיוס %s האדמין", GetRad(playerid), GetName(playerid));
        return 1;
	}
	if(!strcmp(command, "GiveCar", true))
	{
	    new vidx;
		if(sscanf(params[7],"i",vidx)) return SendClientMessage(playerid,White, "Usage: /Rad givecar [car id]");
		if(vidx < 400 || vidx > 611) return SendClientMessage(playerid, Red, ".מודל איידי: 400-611");
		foreach(new i: Player)
		{
			if(i != playerid && IsPlayerNearPlayer(GetRad(playerid), playerid, i))
			{
  				GetPlayerPos(i, GPos[0], GPos[1], GPos[2],GPos[3]);
  				aPlayerV[playerid] = CreateVehicle(vidx,GPos[0], GPos[1], GPos[2],GPos[3],0,0,1);
  				PutPlayerInVehicle(i, aPlayerV[i], 0);
			}
		}
		S_SendMsgToAll(Brown,"%d נתן לכל השחקנים ברדיוס %d.000000 את מודל הרכב %s האדמין",vidx ,GetRad(playerid), GetName(playerid));
        return 1;
	}
	if(!strcmp(command, "Marker", true) || !strcmp(command, "Mar", true))
	{
		foreach(new i: Player)
		{
			if(i != playerid && IsPlayerNearPlayer(GetRad(playerid), playerid, i)) SetPlayerColor(i, 0x59FF50FF);
		}
 		S_SendMsgToAll(Brown,"%d.000000 הפך לזוהרים את כל השחקנים ברדיוס %s האדמין", GetRad(playerid), GetName(playerid));
        return 1;
	}
	if(!strcmp(command, "inv", true))
	{
		foreach(new i: Player)
		{
			if(i != playerid && IsPlayerNearPlayer(GetRad(playerid), playerid, i))
			{
				ShowPlayerNameTagForPlayer(i, i, false);
				SetPlayerColor(i, White);
			}
		}
   		S_SendMsgToAll(Brown,"%d.000000 הפך לבלתי נראים את כל השחקנים ברדיוס %s האדמין", GetRad(playerid), GetName(playerid));
        return 1;
	}
	if(!strcmp(command, "Dialog", true))
	{
		if(sscanf(params[6],"s[128]",params[6])) return SendClientMessage(playerid,White, "Usage: /Rad dialog [text]");
		foreach(new i: Player)
		{
			if(i != playerid && IsPlayerNearPlayer(GetRad(playerid), playerid, i))
			{
				format(String, sizeof(String), "{A3A3A3}Admin %s: %s", GetName(playerid),params[6]);
				ShowPlayerDialog(i,689,DIALOG_STYLE_MSGBOX,"{"#C_White"}Poke",String,"המשך","");
				if (InMusic == false) PlayAudioStreamForPlayer(i, "http://sa-mp.co.il/samp/sounds/you_were_poked.wav");
			}
		}
		S_SendMsgToAll(Brown,"%d.000000 שלח הודעה בדיאלוג לכל השחקנים הנמצאים ברדיוס %s האדמין", GetRad(playerid), GetName(playerid));
        return 1;
	}
	if(!strcmp(command, "explode", true))
	{
		new Float:X,
			Float:Y,
			Float:Z;
		foreach(new i: Player)
		{
			if(i != playerid && IsPlayerNearPlayer(GetRad(playerid), playerid, i))
			{
				if(!IsPlayerInAnyVehicle(i)) GetPlayerPos(i,X,Y,Z);
				else GetVehiclePos(GetPlayerVehicleID(i),X,Y,Z);
				for(new a = 1; a <= 5; a++) CreateExplosion(X,Y,Z,10,0);
			}
		}
		S_SendMsgToAll(Brown,"%d.000000 פוצץ את כל השחקנים בדריוס %s האדמין", GetRad(playerid), GetName(playerid));
        return 1;
	}
	if(!strcmp(command, "vis", true))
	{
		foreach(new i: Player)
		{
			if(i != playerid && IsPlayerNearPlayer(GetRad(playerid), playerid, i))
			{
				ShowPlayerNameTagForPlayer(i, i, true);
				SetPlayerColor(i, rgba2hex(random(256),random(256),random(256),255));
			}
		}
		S_SendMsgToAll(Brown,"%d.000000 הפך לנראים את כל השחקנים ברדיוס %s האדמין", GetRad(playerid), GetName(playerid));
        return 1;
	}
	return SendClientMessage(playerid,Red,".הפקודה לא קיימת במערכת");
}
