CMD:nighters(playerid, params[])
{
	new NTs,catstr[800];
	foreach(new i: Player)
	{
		if(pInfo [ i ] [ NightTeam ] && logged[i] == true && pInfo [ i ] [ dontshowme ] == false)
		{
			NTs++;
			format(catstr,sizeof(catstr), "%s{"#C_Pink"}%d. %s [ID: %03d | Level: %d | %s{"#C_Pink"}]\n",catstr, NTs, GetName(i), i, pInfo [ i ] [ NightTeam ],GetNameLNT(pInfo [ i ] [ NightTeam ]));
		}
	}
    format(String, sizeof(String),"{"#C_llBlue"}צוות לילה מחוברים: %d",NTs);
    if(!NTs) format(catstr,sizeof(catstr), "{"#C_Red"}.אין צוות לילה מחוברים");
    ShowPlayerDialog(playerid, 98211, DIALOG_STYLE_MSGBOX,String, catstr, "סגור", "");
	return 1;
}
