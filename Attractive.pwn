		}
		else if(PWarBattle[playerid][DriftBattle] == 5) DriftLocel = "במירוץ סירות";
		S_SendMsgToAll(pwar_cc1,".%s$ וזכה בסכום כסף העומד על {"#pwar_c0"}\"%s\"{"#pwar_c1"} ניצח %s את השחקן {"#pwar_c0"}\"%s\"{"#pwar_c1"} השחקן",GetNum(PWarBattle[ IdWinner ] [ MoneyWin ]*2),GetName(idLoozer),DriftLocel,GetName(IdWinner));
		Y_GivePlayerMoney(IdWinner, PWarBattle[ IdWinner ] [ MoneyWin ]*2);
 		PWarBattle[ idLoozer ][IDJoin] = 0;
	 	PWarBattle[ idLoozer ][IDInvite] = 0;
	 	PWarBattle[ idLoozer ][Inviter] = false;
		PWarBattle[ idLoozer ][InviteAnyOne] = false;
