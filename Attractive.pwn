function PoliceWinActTeams()
{
    if(!ActStatusTeams)
	{
		KillTimerEx(TimerPoliceWin);
        return 1;
	}
	timespw++;
	if(timespw != 10)
	{
		for(new i, j = GetPlayerPoolSize(); i <= j; i++)
		{
			if(!IsPlayerConnected(i) || InActTeamA[i] == false) continue;
			SendClientMessage(i,Blue, "• • • • • • • • • • • • • • • • • • • • • • • • •");
			S_SendMsg(i,Green, ".נשאר לקבוצתך עוד %d דקות בשביל לנצח",10-timespw);
			SendClientMessage(i,Blue, "• • • • • • • • • • • • • • • • • • • • • • • • •");
		}
        TimerPoliceWin = SetTimer("PoliceWinActTeams",60*1000,false);
	}
	else if(timespw == 10)
	{
		for(new i, j = GetPlayerPoolSize(); i <= j; i++)
		{
			if(!IsPlayerConnected(i)) continue;
			if(InActTeamA[i] == true)
			{
  				SendClientMessage(i,Blue, "• • • • • • • • • • • • • • • • • • • • • • • • •");
				SendClientMessage(i,lGreen, ".בגלל שקבוצתך החזיקה בבנק במשך 10 דקות ניצחתם");
   				SendClientMessage(i,Blue, "• • • • • • • • • • • • • • • • • • • • • • • • •");
			}
	  		else if(InActTeamB[i] == true)
			{
				SendClientMessage(i,Blue, "• • • • • • • • • • • • • • • • • • • • • • • • •");
			  	LozerTeamsActivity(i,".השוטרים הצליחו להחזיק בבנק במשך 10 דקות ולכן ניצחו בפעילות");
            	SendClientMessage(i,Blue, "• • • • • • • • • • • • • • • • • • • • • • • • •");
			}
		}
		KillTimerEx(TimerPoliceWin);
	}
    return 1;
}
