public OnPlayerCommandPerformed(playerid, cmdtext[], success)
{
	if(!success)
	{
		for ( new i, j = sizeof TeleportInfo; i < j; i ++ )
		{
			if ( !strcmp ( cmdtext, TeleportInfo [ i ] [ TeleportName ], true ) )
			{
			    if(InActAuto[playerid] == true && ActStatus >= 2) return SendClientMessage(playerid,Red, "./QAct - אינך יכול להקיש פקודות שאתה בפעילות. ליציאה, הקש");
			    if(GetPlayerLevel(playerid) < TeleportInfo [ i ] [ TeleportLevel ]) return LevelError(playerid,TeleportInfo [ i ] [ TeleportLevel ]);
			 	TogglePlayerControllable(playerid, 0);
			 	SetTimerEx("SpawnGod",800,false,"i",playerid);
	            QuitGAll(playerid);
				GameTextForPlayer(playerid,TeleportInfo [ i ] [ GameText ],2000,4);
				PlayerPlaySound(playerid,1132,0.0,0.0,0.0);
				if(GetPlayerState(playerid) == 2 && TeleportInfo [ i ] [ InCar_Tele ] == true)
				{
				    SetCameraBehindPlayer(playerid);
					SetVehiclePosEx( GetPlayerVehicleID(playerid), TeleportInfo [ i ] [ TeleportBX ], TeleportInfo [ i ] [ TeleportBY ], TeleportInfo [ i ] [ TeleportBZ ],TeleportInfo[i][TeleportBA]);
				}
				else
				{
					SetPlayerPosEx(playerid,TeleportInfo[i][TeleportX],TeleportInfo[i][TeleportY],TeleportInfo[i][TeleportZ],TeleportInfo[i][TeleportA]);
					SetPlayerVirtualWorld ( playerid, TeleportInfo [ i ] [ TeleportWorld ] );
					SetPlayerInterior ( playerid, TeleportInfo [ i ] [ TeleportInterior ] );
					SetCameraBehindPlayer(playerid);
					if(i == 13) SetPlayerArmedWeapon(playerid, 0);
				}
				S_SendMsg(playerid, llPink , ".{"#C_lllGreen"}%s{"#C_llPink"} - ברוך הבא לשיגור",TeleportInfo [ i ] [ TeleportName ][1]);
   				if(CommandOver[playerid] == -1) SendClientMessage(playerid,Grey, "(/tl :על מנת לבצע השתגרות חוזרת, הקלד את הפקודה המהירה)");
				CommandOver[playerid] = i;
				return 1;
			}
		}
		return S_SendMsg(playerid,0xB40000FF, "» /Help {"#C_White"}:לא נמצאה, לקבלת עזרה [{B40000}%s{"#C_White"}] הפקודה",cmdtext);
	}
    return 1;
}
