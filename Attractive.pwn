				if(!Capturelozer [ i ]) format(String, 256, "{"#C_White"}__________________________________________________________________________________________\n\n%s בשליטת הקלאן: שום קלאן | כמות הפעמים שהשתלטו על האזור: %d | רווח האזור: %d$ | איידי האזור: %d | שם האזור: %s\n",ColorCapture(i),CaptureInfo [ i  ] [ Number_of_takeovers ],CaptureInfo [ i ] [ ProfitCapture ],i,CaptureInfo [ i ] [ CaptureName ]);
				else format(String, 256, "{"#C_White"}__________________________________________________________________________________________\n\n%s כמות הפעמים שהשתלטו על האזור: %d | רווח האזור: %d$ | איידי האזור: %d | שם האזור: %s | %s%s%s :בשליטת הקלאן\n",ColorCapture(i),CaptureInfo [ i  ] [ Number_of_takeovers ],CaptureInfo [ i ] [ ProfitCapture ],i,CaptureInfo [ i ] [ CaptureName ],ColorClan(Capturelozer [ i ]),NameClan(Capturelozer [ i ]),ColorCapture(i));
				strcat(Str1, String);
				Captures++;
			}
		}
		if(!Captures) strcat(Str1, "{"#C_Red"}.אין אזורי כבישה בשרת\n", 200);
		ShowPlayerDialog(playerid, 142,DIALOG_STYLE_MSGBOX,"{00B2EE}Server Captures List - רשימת אזורי הכבישה בשרת", Str1,"אישור","");
		return 1;
	}
	if(!strcmp(command, "Help", true))
	{
		new Capture_System[][] =
		{
			{"{9E9E9E}בשרת זה קיימת מערכת השתלטויות על אזורים\n"},
			{"{"#C_White"}:הסבר על המערכת"},
			{"{"#C_White"}במערכת זו תוכלו להשתמש רק עם יש לכם קלאן אם אין לכם קלאן לא תוכלו להשתמש במערכת זו"},
			{"במערכת זו אתם והקלאן שלכם תכבשו אזורים בשרת ותקבלו על זה כסף"},
			{"בשרת קיימת הגבלה של אזורים וההגבלה היא עד 4 אזורים"},
			{"על כל אזור שתכבשו תקבלו סכום כסף שונה לבנק הקלאן שלכם"},
			{"אחראי הקלאנים ואירגונים יכול לשנות את הרווח של כל אזור שקיים בשרת וגם את המיקום"},
			{"?איך כובשים אזור"},
			{"כדי לכבוש אזור מסוים אתם צריכים ללכת לאזור שאתם רוצים לכבוש עם 4 שחקני קלאן"},
			{"אחרי שאתם באזור עם 4 שחקני קלאן אתם תתחילו בכבישת האזור"},
			{"כדי להצליח אתם צריכים להחזיק מעמד באזור 4 דקות בלי למות או לצאת מהאזור"},
			{"עם לא יהיו 4 שחקני קלאן באזור הכיבוש על האזור יכשל\n"},
			{"{BDFA05}:פקודות המערכת"},
			{"{0080C0}•{"#C_White"} /Capture Area - כבישת אזור"},
			{"{0080C0}•{"#C_White"} /Capture List - בדיקת האזורים בשרת"},
			{"{"#C_Red"}** הנהלת השרת מזהירה מאוד מניצולי באגים כל קלאן שייתפס מנצל באגים למיניהם ייענש בחומרה"}
		};
		format(string1, sizeof string1, "");
		for(new i = 0; i < sizeof Capture_System; i++) format(string1, sizeof string1, "%s%s\n",string1,Capture_System[i][0]);
		ShowPlayerDialog(playerid,98211, DIALOG_STYLE_MSGBOX, "{"#C_Yellow"}Captures{"#C_White"} System",string1,"סגור", "");
		return 1;
	}
	if(!strcmp(command, "aHelp", true))
	{
		if(pInfo [ playerid ] [ ClanAccess ] == false) return SendClientMessage(playerid,Red, "* הפקודה שכתבת אינה קיימת במערכת כבישה על אזורים");
		new StrAc[1000];
		strcat(StrAc, "{"#C_Pink"}» {"#C_White"}/Capture Create - יצירת אזור\n{"#C_Pink"}» {"#C_White"}/Capture Delete - מחיקת אזור\n{"#C_Pink"}» {"#C_White"}/Capture CreateCheckpoint - יצירת צק'פוינט לאזור\n{"#C_Pink"}» {"#C_White"}/Capture DeleteCheckpoint - מחיקת צק'פוינט לאזור\n{"#C_Pink"}» {"#C_White"}/Capture ChangeName - שינוי שם לאזור", 2000);
		strcat(StrAc, "\n{"#C_Pink"}» {"#C_White"}/Capture ChangeColor - שינוי צבע לאזור\n{"#C_Pink"}»{"#C_White"} /Capture ChangeProfit - שינוי רווח לאזור", 2000);
		return ShowPlayerDialog(playerid, 98211, DIALOG_STYLE_MSGBOX, "Captures System Command Admin",StrAc, "אישור", "");
	}
	return SendClientMessage(playerid,Red, "* הפקודה שכתבת אינה קיימת במערכת כבישה על אזורים");
}
//------------------------- Weapon Cache SYSTEM ----------------------------
CMD:wcache(playerid, params[])
{
	new command[50];
	if(sscanf(params,"s[50]{s[50]}",command)) return SendClientMessage(playerid, White, "Usage: /Wcache [Commands]");
	if(!strcmp(command, "CreateCheckpoint", true))
	{
	    if(InWcache[playerid] == -1) return SendClientMessage(playerid,Red, ".על מנת לשים צק'פוינט עליך להיות באזור חימוש");
        new i = InWcache[playerid];
		if(WcacheInfo [ i ] [ HasCheckWcache ] == true) return SendClientMessage(playerid,Red, ".לאזור חימוש יש כבר צק'פוינט");
		GetPlayerPos(playerid, GPos[0],GPos[1],GPos[2]);
		WcacheInfo [ i ] [ Check_Wcache ] = CreateDynamicCP(GPos[0],GPos[1],GPos[2], 3.5);
		WcacheInfo [ i ] [ HasCheckWcache ] = true;
		WcacheInfo [ i ] [ Wcache_CheckX ] = GPos[0];
		WcacheInfo [ i ] [ Wcache_CheckY ] = GPos[1];
		WcacheInfo [ i ] [ Wcache_CheckZ ] = GPos[2];
		SendClientMessage(playerid,lGreen, ".יצרת צק'פוינט לאזור חימוש זה");
		return 1;
	}
	if(!strcmp(command, "DeleteCheckpoint", true))
	{
		if(InWcache[playerid] == -1) return SendClientMessage(playerid,Red, ".על מנת למחוק צק'פוינט עליך להיות באזור חימוש");
        new i = InWcache[playerid];
	    if(WcacheInfo [ i ] [ HasCheckWcache ] == false) return SendClientMessage(playerid,Red, ".לאזור חימוש זה אין צק'פוינט");
		DestroyDynamicCP( WcacheInfo [ i ] [ Check_Wcache ] );
		WcacheInfo [ i ] [ Check_Wcache ] = -1;
		WcacheInfo [ i ] [ HasCheckWcache ] = false;
		WcacheInfo [ i ] [ Wcache_CheckX ] = 0;
		WcacheInfo [ i ] [ Wcache_CheckY ] = 0;
		WcacheInfo [ i ] [ Wcache_CheckZ ] = 0;
		SendClientMessage(playerid,lGreen, ".מחקת את הצק'פוינט של אזור חימוש זה");
        return 1;
	}
	if(!strcmp(command, "Create", true))
	{
		new Float:Xmin_W,
	    Float:Ymin_W,
	    Float:Xmax_W,
	    Float:Ymax_W,
		NameC_W[50];
		if(sscanf(params[6],"ffffs[50]",Xmin_W,Ymin_W,Xmax_W,Ymax_W,NameC_W)) return SendClientMessage(playerid,White, "Usage: /Wcache Create [Location] [Name]");
		for(new i = 1; i < MAX_Wcache; i++)
		{
			if(!fexist(FileWcache(i)))
			{
			    format(WcacheInfo [ i ] [ WcacheName ], 50,"%s",NameC_W);
			    WcacheInfo [ i ] [ XminWcache ] = Xmin_W;
				WcacheInfo [ i ] [ YminWcache ] = Ymin_W;
				WcacheInfo [ i ] [ XmaxWcache ] = Xmax_W;
				WcacheInfo [ i ] [ YmaxWcache ] = Ymax_W;
				WcacheInfo [ i ] [ RWcache ] = random(256);
				WcacheInfo [ i ] [ GWcache ] = random(256);
				WcacheInfo [ i ] [ BWcache ] = random(256);
				WcacheInfo [ i ] [ HasCheckWcache ] = false;
				WcacheInfo [ i ] [ Wcache_CheckX ] = 0;
				WcacheInfo [ i ] [ Wcache_CheckY ] = 0;
				WcacheInfo [ i ] [ Wcache_CheckZ ] = 0;
				WcacheInfo [ i ] [ Number_of_Wcache_takeovers ] = 0;
				new INI:File = INI_Open(FileWcache(i));
				INI_WriteString(File, "NameWcache",WcacheInfo [ i ] [ WcacheName ]);
				INI_WriteFloat(File,"XminWcache", WcacheInfo [ i ] [ XminWcache ] );
				INI_WriteFloat(File,"YminWcache", WcacheInfo [ i ] [ YminWcache ] );
				INI_WriteFloat(File,"XmaxWcache", WcacheInfo [ i ] [ XmaxWcache ] );
				INI_WriteFloat(File,"YmaxWcache", WcacheInfo [ i ] [ YmaxWcache ] );
				INI_WriteInt(File,"RWcache",WcacheInfo [ i ] [ RWcache ] );
				INI_WriteInt(File,"GWcache",WcacheInfo [ i ] [ GWcache ]);
				INI_WriteInt(File,"BWcache",WcacheInfo [ i ] [ BWcache ]);
				INI_WriteInt(File,"Number_of_Wtakeovers",WcacheInfo [ i ] [ Number_of_Wcache_takeovers ]);
				INI_WriteBool(File,"HasCheckWcache",WcacheInfo [ i ] [ HasCheckWcache ]);
				INI_WriteFloat(File,"Wcache_CheckX", WcacheInfo [ i ] [ Wcache_CheckX ]);
				INI_WriteFloat(File,"Wcache_CheckY", WcacheInfo [ i ] [ Wcache_CheckY ]);
				INI_WriteFloat(File,"Wcache_CheckZ", WcacheInfo [ i ] [ Wcache_CheckZ ]);
				INI_Close ( File );
				GangZoneWcache[i] = GangZoneCreate(Xmin_W,Ymin_W,Xmax_W,Ymax_W);
				GangZoneShowForAll(GangZoneWcache[i],rgba2hex(WcacheInfo [ i ] [ RWcache ],WcacheInfo [ i ] [ GWcache ],WcacheInfo [ i ] [ BWcache ],150));
				WcacheInfo [ i ] [ Wcache_Area ] = CreateDynamicRectangle(Xmin_W,Ymin_W,Xmax_W,Ymax_W);
				S_SendMsgToAll(0x00BFFFFF,"! {00BFFF}יצר אזור חימוש חדש בשם - %s%s {FAFAD2}%s {00BFFF}האדמין",ColorWcache(i),NameC_W, GetName(playerid));
				return 1;
			}
		}
		return 1;
	}
	if(!strcmp(command, "ChangeName", true))
	{
		new idWcache,
			NameC_W[50];
		if(sscanf(params[10],"is[50]",idWcache,NameC_W)) return SendClientMessage(playerid,White, "Usage: /Wcache Changename [id] [Name]");
		if(!fexist(FileWcache(idWcache))) return SendClientMessage(playerid,Red, ".איידי האזור לא קיים");
		S_SendMsgToAll(0x00BFFFFF,"»» שינה את שמו של אזור החימוש {FFD700}%s {00BFFF}לשם{FFD700} %s {FAFAD2}%s {00BFFF}האדמין",NameWcache(idWcache), NameC_W , GetName(playerid));
		format(WcacheInfo [ idWcache ] [ WcacheName ], 50, "%s",NameC_W);
		return 1;
	}
	if(!strcmp(command, "ChangeColor", true))
	{
		new idWcache,
			Color1_W,
			Color2_W,
			Color3_W;
		if(sscanf(params[11],"iiii",idWcache,Color1_W,Color2_W,Color3_W)) return SendClientMessage(playerid, White, "Usage: /Wcache ChangeColor [id] [R] [G] [B]");
		if(!fexist(FileWcache(idWcache))) return SendClientMessage(playerid,Red, ".איידי האזור לא קיים");
		WcacheInfo [ idWcache ] [ RWcache ] = Color1_W;
		WcacheInfo [ idWcache ] [ GWcache ] = Color2_W;
		WcacheInfo [ idWcache ] [ BWcache ] = Color3_W;
		S_SendMsgToAll(0x00BFFFFF,"»» {00BFFF}שינה את צבע האזור %s %sלצבע הודעה זו {FAFAD2}%s {00BFFF}האדמין",NameWcache(idWcache),ColorWcache(idWcache), GetName(playerid));
 		if(!WcacheArea[ idWcache ]) GangZoneShowForAll(GangZoneWcache[idWcache],rgba2hex(WcacheInfo [ idWcache ] [ RWcache ],WcacheInfo [ idWcache ] [ GWcache ],WcacheInfo [ idWcache ] [ BWcache ],150));
	    return 1;
	}
	if(!strcmp(command, "Delete", true))
	{
		new idWcache;
		if(sscanf(params[6],"i",idWcache)) return SendClientMessage(playerid, White, "Usage: /Wcache Delete [id]");
		if(!fexist(FileWcache(idWcache))) return SendClientMessage(playerid,Red, ".איידי האזור לא קיים");
		S_SendMsgToAll(0x00BFFFFF,"! {00BFFF}מחק את אזור החימוש %s%s {FAFAD2}%s {00BFFF}האדמין",ColorWcache( idWcache ),WcacheInfo [ idWcache ] [ WcacheName ], GetName(playerid));
		UnWcacheLoad(idWcache);
		fremove( FileWcache(idWcache) );
       	if(WcacheInfo [ idWcache ] [ HasCheckWcache ] == true) DestroyDynamicCP( WcacheInfo [ idWcache ] [ Check_Wcache ]);
       	if(IsValidDynamicArea(WcacheInfo [ idWcache ] [ Wcache_Area ])) DestroyDynamicArea(WcacheInfo [ idWcache ] [ Wcache_Area ]);
        WcacheInfo [ idWcache ] [ Check_Wcache ] = -1;
  		WcacheInfo [ idWcache ] [ Wcache_Area ] = -1;
		GangZoneDestroy(GangZoneWcache [ idWcache ] );
		format(WcacheInfo [ idWcache ] [ WcacheName ], 50, "%s","None");
		WcacheInfo [ idWcache ] [ XminWcache ] = 0;
		WcacheInfo [ idWcache ] [ YminWcache ] = 0;
		WcacheInfo [ idWcache ] [ XmaxWcache ] = 0;
		WcacheInfo [ idWcache ] [ YmaxWcache ] = 0;
		WcacheInfo [ idWcache ] [ RWcache ] = 0;
		WcacheInfo [ idWcache ] [ GWcache ] = 0;
		WcacheInfo [ idWcache ] [ BWcache ] = 0;
		WcacheInfo [ idWcache ] [ Number_of_Wcache_takeovers ] = 0;
		return 1;
	}
	if(!strcmp(command, "GiveWcache", true))
	{
	    if(pInfo [ playerid ] [ ClanAccess ] == false) return SendClientMessage(playerid,Red, "* הפקודה שכתבת אינה קיימת במערכת הקלאנים");
		new idWcache;
		if(sscanf(params[10],"i",idWcache)) return SendClientMessage(playerid, White, "Usage: /Clan GiveWcache [WcacheID]");
		if(!scriptWcaches(idWcache))return SendClientMessage(playerid, Red, ".מספר אזור חימוש זו לא קיימת");
		if(!fexist(FileWcache(idWcache))) return SendClientMessage(playerid, Red, ".מספר אזור חימוש לא נמצא");
		if(InWcacheHq[idWcache] == true) return SendClientMessage(playerid, Red, ".מספר אזור חימוש זו כבר טעונה");
		S_SendMsgToAll(llllGreen,"-> נתן לאזור חימוש %s%s מפקדה %s האדמין",ColorWcache(idWcache),NameWcache(idWcache), GetName(playerid));
 		WcacheLoad(idWcache);
		return 1;
	}
	if(!strcmp(command, "DelWcache", true))
	{
	    if(pInfo [ playerid ] [ ClanAccess ] == false) return SendClientMessage(playerid,Red, "* הפקודה שכתבת אינה קיימת במערכת הקלאנים");
		new idWcache;
		if(sscanf(params[9],"i",idWcache)) return SendClientMessage(playerid, White, "Usage: /Clan DelWcache [WcacheID]");
		if(!fexist(FileWcache(idWcache))) return SendClientMessage(playerid, Red, ".מספר אזור חימוש לא נמצא");
		if(InWcacheHq[idWcache] == false) return SendClientMessage(playerid,Red, ".מספר אזור חימוש זה אינו טעון");
		S_SendMsgToAll(llllGreen, "-> הוריד לאזור חימוש %s%s את מפקדה %s האדמין",ColorWcache(idWcache),NameWcache(idWcache), GetName(playerid));
		UnWcacheLoad(idWcache);
		return 1;
	}
	if(!strcmp(command, "Area", true))
	{
	    if(InActAuto[playerid] == true && ActStatus >= 2) return SendClientMessage(playerid,Red, "./QAct - אינך יכול להקיש פקודות שאתה בפעילות. ליציאה, הקש");
		if(!InClan(playerid))return SendClientMessage(playerid,Red,".אינך נמצא בשום קלאן");
 		if(StartClanArea[playerid]) return SendClientMessage(playerid,Red,".על מנת להתחיל השתלטות על קלאן / אזור זה עליך לחכות לסיום ההשתלטות שהתחלת");
 		if(InWcache[playerid] != -1) return SendClientMessage(playerid,Red,".אינך נימצא באזור חימוש");
        new AreaWcache = InWcache[playerid];
		if(Wcachelozer[ AreaWcache ] == InClan(playerid)) return SendClientMessage(playerid, Red,".אזור חימוש זה כבר בשליטת הקלאן שלך");
		if(WcacheArea[ AreaWcache ] == 1 || WcacheArea[ AreaWcache ] == 3) return SendClientMessage(playerid,Red, ".על מנת להתחיל להשתלט על אזור חימוש זה חכה לסיום ההשתלטות הקיימת");
		new PlayerClanCemetery;
 		foreach(new i: Player)
		{
		    if(InClan(playerid) == InClan(i) &&  IsPlayerInDynamicArea(i, WcacheInfo [ AreaWcache ] [ Wcache_Area ])) PlayerClanCemetery++;
		}
 		if(PlayerClanCemetery >= 0 && PlayerClanCemetery < MorePlayerInWcache) return SendClientMessage(playerid, Red,".אין באפשרותך להתחיל בכבישת אזור זה מכיוון שנמצאו פחות מ - 4 שחקני קלאן באזור");
   		S_SendMsgToAll(llllGreen, ">• התחיל בכבישת אזור החימוש {"#C_llllGreen"}%s%s %s%s{"#C_llllGreen"} מקלאן %s%s",ColorWcache(AreaWcache),NameWcache(AreaWcache),ColorClan(InClan(playerid)),ClanPlayer(playerid),ColorClan(InClan(playerid)),GetName(playerid));
		if(WcacheArea[AreaWcache] == 0) WcacheArea[AreaWcache] = 1;
		if(WcacheArea[AreaWcache] == 2)
		{
            S_SendMsgToAll(llllGreen, "%s%s{"#C_llllGreen"} שנכבש על ידי הקלאן",ColorClan( Wcachelozer[ AreaWcache ]),NameClan( Wcachelozer[ AreaWcache ] ));
			WcacheArea[AreaWcache] = 3;
		}
		GangZoneFlashForAll(GangZoneWcache[ AreaWcache ],rgba2hex(ClanInfo [ InClan(playerid) ] [ ClanR ] , ClanInfo [ InClan(playerid) ] [ ClanG ] , ClanInfo [ InClan(playerid) ] [ ClanB ],150));
		Occupying_Wcache[playerid] = InClan(playerid);
		StartClanArea[playerid] = AreaWcache;
		WcacheWClan[AreaWcache] = InClan(playerid);
		Occupying_time[playerid] = 0;
		Timer_Wcache[playerid] =  SetTimerEx ( "CemeteryWcache", 1000 , true, "i" , playerid);
		return 1;
	}
	if(!strcmp(command, "List", true))
	{
		new Str1[2000];
		new Wcaches;
		for(new i; i < MAX_Wcache; i++)
		{
			if(fexist(FileWcache(i)))
			{
				if(!Wcachelozer [ i ]) format(String, 256, "{"#C_White"}__________________________________________________________________________________________\n\n%s בשליטת הקלאן: שום קלאן | כמות הפעמים שהשתלטו על האזור: %d| איידי האזור: %d | שם האזור: %s\n",ColorWcache(i),WcacheInfo [ i  ] [ Number_of_Wcache_takeovers ],i,WcacheInfo [ i ] [ WcacheName ]);
				else format(String, 256, "{"#C_White"}__________________________________________________________________________________________\n\n%s כמות הפעמים שהשתלטו על האזור: %d | איידי האזור: %d | שם האזור: %s | %s%s%s :בשליטת הקלאן\n",ColorWcache(i),WcacheInfo [ i  ] [ Number_of_Wcache_takeovers ],i,WcacheInfo [ i ] [ WcacheName ],ColorClan(Wcachelozer [ i ]),NameClan(Wcachelozer [ i ]),ColorWcache(i));
				strcat(Str1, String);
				Wcaches++;
			}
		}
		if(!Wcaches) strcat(Str1, "{"#C_Red"}.אין אזורי חימוש בשרת\n", 200);
		ShowPlayerDialog(playerid, 142,DIALOG_STYLE_MSGBOX,"{00B2EE}Server Wcaches List - רשימת אזורי החימוש בשרת", Str1,"אישור","");
		return 1;
	}
	return SendClientMessage(playerid,Red, "* הפקודה שכתבת אינה קיימת במערכת חימוש על אזורים");
}
CMD:setclan(playerid, params[])
{
    if(pInfo [ playerid ] [ ClanAccess ] == false) return SendClientMessage(playerid,Red, "* הפקודה שכתבת אינה קיימת במערכת הקלאנים");
	new ID,ClanId,Level_c;
	if(sscanf(params,"iii",ID,ClanId,Level_c)) return SendClientMessage(playerid, White, "Usage: /SetClan [playerid] [ClanID] [level]");
    if(!fexist(FileClan( ClanId ))) return SendClientMessage(playerid,Red, ".איידי קלאן שגוי");
	pInfo [ ID ] [ ClanID ] = ClanId;
	pInfo [ ID ] [ ClanLevel ] = Level_c;
	pInfo [ ID ] [ ClanSerial ] = ClanInfo [ ClanId ] [ Clan_Serial_Number ];
	SendClanMessageEx(ClanId,lGreen, ".צורף לקלאן שלך %s",GetName(ID));
	S_SendMsg(playerid,lGreen, ".%s צורף לקלאן %s", NameClan ( ClanId ),GetName(ID));
	S_SendMsg(ID,lGreen, ".%s צורפת לקלאן", NameClan ( ClanId ) );
