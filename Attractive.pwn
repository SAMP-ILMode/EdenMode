public OnPlayerGiveDamage(playerid, damagedid, Float:amount, weaponid, bodypart)
{
    if(GetPlayerTeam(playerid) != GetPlayerTeam(damagedid)) godmodShot[damagedid]++;
	if(Powerlevel[playerid] != 0 && !GetPlayerInterior(damagedid))
	{
		new Float:hps[2];
		GetPlayerHealth(damagedid, hps[0]);
		GetPlayerArmour(damagedid, hps[1]);
		new Float:HP;
		if(Powerlevel[playerid] == 1) HP = 5;
		else if(Powerlevel[playerid] == 2) HP = 10;
		else if(Powerlevel[playerid] == 3) HP = 15;
		else if(Powerlevel[playerid] == 4) HP = 20;
		else if(Powerlevel[playerid] == 5) HP = 25;
		if((hps[1] - amount) + HP < 0) SetPlayerArmour(damagedid, 0.0);
		if((hps[1] - amount) + HP > 0 && hps[1] > 0) SetPlayerArmour(damagedid, hps[1] - HP);
		if(hps[1] < 1) SetPlayerHealth(damagedid, hps[0] - HP);
	}
	return 1;
}
public OnPlayerGiveDamageActor(playerid, damaged_actorid, Float:amount, weaponid, bodypart)
{
    for(new i; i < sizeof(newbots); i++)
    {
        if(newbots[i] == damaged_actorid)
        {
      		new Float:health;
	        GetActorHealth(newbots[i],health);
    		if(bodypart == 9) SetActorHealth(newbots[i],health-100);
			else if(bodypart == 8 || bodypart == 7 || bodypart == 6 || bodypart == 5) SetActorHealth(newbots[i],health-20);
			else if(bodypart == 4) SetActorHealth(newbots[i],health-50);
			GetActorHealth(newbots[i],health);
			if(health <= 0.0)
			{
			    ClearActorAnimations(newbots[i]);
       			SetActorHealth(newbots[i],0);
