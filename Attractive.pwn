CMD:dice(playerid, params[])
{
    if(InActAuto[playerid] == true && ActStatus >= 2) return SendClientMessage(playerid,Red, "./QAct - אינך יכול להקיש פקודות שאתה בפעילות. ליציאה, הקש");
    Tolevel(playerid,11);
	new Number , Money;
	if(sscanf(params,"ii",Number , Money)) return SendClientMessage(playerid,White,"Usage: /Dice [Number] [Money]");
	if(Number < 1 || Number > 6) return SendClientMessage(playerid,Red,".מספרים רק בין 6-1");
	if(Y_GetPlayerMoney(playerid) < Money) return SendClientMessage(playerid,Red,".אין לך מספיק כסף");
	if(Money > 2000000 || Money < 1000) return SendClientMessage(playerid,Red, ".סכום ההימור יכול להיות $1,000-$2,000,000");
	Y_GivePlayerMoney(playerid, -Money);
	new DiceCheck[][] = { {1} , {2} , {3} , {4} , {5} , {6} };
	new rand = random(sizeof(DiceCheck));
	if(DiceCheck[rand][0] != Number) return S_SendMsg(playerid, Red, "%d הפסדת, המספר הזוכה היה",DiceCheck[rand][0]);
	if(DiceCheck[rand][0] == Number)
	{
 		if(50000 <= Money)
		{
            foreach(new i: Player)
			{
				if(playerid != i) S_SendMsg(i,0x32EF36FF, ".$%s הימר במשחק הקוביה על הסכום $%s וזכה בסכום %s השחקן" ,GetNum(Money*2),GetNum(Money),GetName(playerid));
			}
		}
		S_SendMsg(playerid,0x32EF36FF, ".$%s-הימרת במשחק הקוביה על המספר %d וזכית, סכום ההימור הוכפל וזכית ב",GetNum(Money*2),DiceCheck[rand][0]);
		Y_GivePlayerMoney(playerid,Money*2);
	}
	return 1;
}
CMD:gamble(playerid, params[])
{
    if(InActAuto[playerid] == true && ActStatus >= 2) return SendClientMessage(playerid,Red, "./QAct - אינך יכול להקיש פקודות שאתה בפעילות. ליציאה, הקש");
    Tolevel(playerid,1);
	if(GetPlayerLevel(playerid) < 5)return LevelError(playerid,5);
	if(!IsPlayerInDynamicCP(playerid,CheckPoint[7])) return SendClientMessage(playerid,Red,"./Casino - על מנת להמר חובה להיות בקזינו");
	new Amount;
	if(sscanf(params,"i",Amount)) return SendClientMessage(playerid,White,"Usage: /Gamble [Amount]");
	if(Amount < 1000 || Amount > 2000000)return SendClientMessage(playerid,Red,".סכום ההימור חייב להיות בין $1,000-$2,000,000");
	if(Y_GetPlayerMoney(playerid) < Amount)return SendClientMessage(playerid,Red,".אין בידך סכום כסף זה");
	Y_GivePlayerMoney(playerid,-Amount);
	new Gamble1 = random(4);
	new Gamble2 = random(4);
	if(Gamble1 == Gamble2)
	{
		Y_GivePlayerMoney(playerid,Amount*2);
		if(50000 <= Amount)
		{
            foreach(new i: Player)
			{
				if(playerid != i) S_SendMsg(i,Orange, ".$%s-הימר על הסכום $%s וזכה, לכן כספו הוכפל הוא זכה ב %s השחקן" ,GetNum(Amount*2),GetNum(Amount),GetName(playerid));
			}
			S_SendMsg(playerid, 0x32EF36FF, ".$%s-הימרת על הסכום $%s וזכית ב",GetNum(Amount*2),GetNum(Amount));
		}
	}
	else S_SendMsg(playerid,Red, ".הימרת על הסכום $%s ולא זכית, נסה שוב בפעם הבאה",GetNum(Amount));
	return 1;
}

