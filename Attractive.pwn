//gps system
CMD:gps(playerid, params[])
{
    Tolevel(playerid,17);
	new IDS[4];
	if(sscanf(params,"s[4]",IDS)) return SendClientMessage(playerid,White,"Usage: /gps [id/off]");
    if(!strcmp("off",IDS, false))
	{
		if(GotoGps[playerid] == -1) return SendClientMessage(playerid,Red,".שלך לא מופעל GPS-ה");
  		SendClientMessage(playerid,Red, ".שלך GPS-כיבת את ה");
  		GotoGps[playerid] = -1;
	}
	else if(IsNumeric(IDS))
 	{
		if(GotoGps[playerid] != -1) return SendClientMessage(playerid,Red,".שלך מופעל GPS-ה");
        new ID = strval(IDS);
        if(!IsPlayerConnected( ID )) return SendClientMessage(playerid,Red, ".המשתמש אינו מחובר");
        if(IsPlayerNPC( ID )) return SendClientMessage(playerid,Red, ".אין באפשרותך להשתמש בפקודה זו על בוט");
        if(GetPlayerVirtualWorld( ID ) != GetPlayerVirtualWorld( playerid )) return SendClientMessage(playerid,Red,".שחקן זה לא נימצא בעולם שלך");
        if(playerid == ID) return SendClientMessage(playerid,Red, ".אין אפשרות להשתמש בפקודה זו על עצמך");
        GotoGps[playerid] = ID;
        SetTimerEx("GPSP",300, false, "i", playerid );
        S_SendMsg(playerid, llBlue , ".%s(ID: %d) :יעד ,GPS הפעלת",GetName(ID),ID);
	}
	else return SendClientMessage(playerid,White,"Usage: /gps [id/off]");
    return 1;
}
function GPSP(playerid)
{
	if(GotoGps[playerid] == -1 || !IsPlayerConnected( GotoGps[playerid] ) || !IsPlayerConnected( playerid ) || GetPlayerVirtualWorld( GotoGps[playerid] ) != GetPlayerVirtualWorld( playerid ))
	{
        if(!IsPlayerConnected( GotoGps[playerid] )) S_SendMsg(playerid, Red , ".התנתק %s(ID: %d) :GPS-היעד שהפעלת ב",GetName(GotoGps[playerid]),GotoGps[playerid]);
		else if(GetPlayerVirtualWorld( GotoGps[playerid] ) != GetPlayerVirtualWorld( playerid )) S_SendMsg(playerid, Red , ".בוטל, בגלל שהשחקן עבר לעולם אחר ולא נימצא בעולם שלך %s(ID: %d) :GPS-היעד שהפעלת ב",GetName(GotoGps[playerid]),GotoGps[playerid]);
		GotoGps[playerid] = -1;
		return 1;
	}
	GetPlayerPos (playerid, PosG [ 0 ] [ 0 ], PosG [ 0 ] [ 1 ],PosG [ 0 ] [ 2 ] );
    GetPlayerPos (GotoGps[playerid], PosG [ 1 ] [ 0 ], PosG [ 1 ] [ 1 ],PosG [ 1 ] [ 2 ] );
 	PosG [ 0 ] [ 0 ] = floatsqroot ( floatpower ( floatabs ( floatsub (PosG[1][0], PosG[0][0] ) ), 2) + floatpower ( floatabs ( floatsub ( PosG[1][1], PosG[0][1] ) ), 2 ) + floatpower ( floatabs ( floatsub ( PosG[1][2], PosG[0][2] ) ),2 ) );
	if(PosG [ 0 ] [ 0 ] < 5.0)
	{
	    S_SendMsg(playerid, lGreen , ".כובה אוטומטית GPS-והגעת אליו, ולכן ה %s(ID: %d) :GPS-היעד שהפעלת ב",GetName(GotoGps[playerid]),GotoGps[playerid]);
	    GotoGps[playerid] = -1;
        return 1;
	}
	SetTimerEx("GPSP", 300, false, "i", playerid );
    format(StrGPS, sizeof(StrGPS),"~N~~N~~N~~N~~N~~N~~N~~N~~N~~G~DISTANCE:~N~~R~%f ~Y~M",PosG [ 0 ] [ 0 ]);
    GameTextForPlayer(playerid,StrGPS,200,3);
    return 1;
}
