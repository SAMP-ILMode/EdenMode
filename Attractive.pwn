		    if(!IsNickValid(Name_ba)) return SendClientMessage(playerid,White,"חלק מהתווים שהקשת אינם נכונים");
		    new AllFiles[50];
   			format(AllFiles, 50, "Admin/Bans/%s.ini",Name_ba);
			if(fexist(AllFiles)) return SendClientMessage(playerid,Red,".משתמש זה כבר נימצא בבאן");
			if(fexist(PlayerNameFile(Name_ba)))
			{
				inline gIP(string:name[], string:value[]) INI_String("Ip",GIP,20);
				INI_ParseFile(PlayerNameFile(Name_ba), using inline "gIP");
			    new INI:File = INI_Open(AllFiles);
                INI_WriteString(File, "IPBanned",GIP);
			    INI_Close ( File );
                BanIp( GIP ,false,Reason_ba);
			}
			else format(GIP, 20, "00.00.00.00");
