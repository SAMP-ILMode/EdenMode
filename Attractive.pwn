function Hacking_HeadQuarters(playerid)
{
	if(IsPlayerInAnyVehicle(playerid) && IsPlayerConnected(playerid))
	{
		if(GetVehicleModel(GetPlayerVehicleID(playerid)) == 520 || GetVehicleModel(GetPlayerVehicleID(playerid)) == 447 || GetVehicleModel(GetPlayerVehicleID(playerid)) == 425 || GetVehicleModel(GetPlayerVehicleID(playerid)) == 432 || GetVehicleModel(GetPlayerVehicleID(playerid)) == 464 || GetVehicleModel(GetPlayerVehicleID(playerid)) == 476)
		{
		    if(InHq[playerid] != -1)
		    {
		        new b = InHq[playerid];
				if(b != InClan(playerid) && IsPlayerInDynamicArea(playerid, AreaHq[ b ]) && ClanInfo [ b ] [ HasKipatBarzel ] == true && IsValidDynamicObject( KipatBarzel[0][b] ))
				{
   					if(IsValidDynamicObject( tilnontet[0][playerid] ))
					{
						DestroyDynamicObjectEx(tilnontet[0][playerid]);
  						CreateExplosion(pPos[0][playerid]+5,pPos[1][playerid]+5,pPos[2][playerid]+10, 6, 50);
				   		CreateExplosion(pPos[0][playerid],pPos[1][playerid]+5,pPos[2][playerid]+10, 6, 50);
				   		CreateExplosion(pPos[0][playerid],pPos[1][playerid],pPos[2][playerid]+5, 6, 50);
						CreateExplosion(pPos[0][playerid],pPos[1][playerid],pPos[2][playerid], 6, 50);
				    }
					DestroyDynamicObjectEx(tilnontet[1][playerid]);
					GetPlayerPos(playerid,pPos[0][playerid],pPos[1][playerid],pPos[2][playerid]);
					tilnontet[0][playerid] = CreateDynamicObject(345,ClanInfo [ b ] [ KipatBarzelX ],ClanInfo [ b ] [ KipatBarzelY ],ClanInfo [ b ] [ KipatBarzelZ ],0.0 + 5, 0.0,0.0, 300);
					tilnontet[1][playerid] = CreateDynamicObject(2780,ClanInfo [ b ] [ KipatBarzelX ],ClanInfo [ b ] [ KipatBarzelY ],ClanInfo [ b ] [ KipatBarzelZ ],0.0 + 5, 0.0,0.0, 300);
					MoveDynamicObject(tilnontet[0][playerid], pPos[0][playerid],pPos[1][playerid],pPos[2][playerid], 120);
					S_SendMsg(playerid,Red,".שלחה לך טיל %s%s{"#C_Red"} מערכת כיפת הברזל של הקלאן",ColorClan(b),ClanInfo [ b ] [ ClanName ]);
				}
			}
			else if(InHq[playerid] == -1) KillTimerEx( CarAreaError [ playerid ] );
		}
		else KillTimerEx( CarAreaError [ playerid ] );
	}
	else KillTimerEx( CarAreaError [ playerid ] );
	return 1;
}

public OnVehicleSpawn(vehicleid)
{
    if(VehicleColor1(vehicleid) != -1 && VehicleColor1(vehicleid) != -1) ChangeVehicleColor(vehicleid, VehicleColor1(vehicleid),VehicleColor2(vehicleid));
	SetVehicleHealth(vehicleid, 999.0);
	if(Vehiclebelongsto[vehicleid] == 1)
	{
		new i = Townerotvehicle[vehicleid];
		if(VehicleInfo [ i ] [ IDVeh ] != -1)
		{
			for(new b; b < 17; ++b)
			{
				if(VehicleInfo [ i ] [ VehEnhancements ] [ b ] == 0) continue;
				AddVehicleComponent(vehicleid,VehicleInfo [ i ] [ VehEnhancements ] [ b ]);
			}
			if(VehicleInfo [ i ] [ VehEnhancements ] [ 17 ] != 4) ChangeVehiclePaintjob(vehicleid,VehicleInfo [ i ] [ VehEnhancements ] [ 17 ]);
		}
	}
	return 1;
}
