new lineobjects[256],Strloadg[14];
new Gate1Hq_ob;

function HqLoad(idLoad)
{
	if(InClanReload[idLoad] == false)
	{
	    LoadHq(idLoad);
        InClanReload[idLoad] = true;
        LoadObjectsFromFile(idLoad,HqFile_Objects(idLoad));
        LoadVehiclesFromFile(idLoad,HqFile_Vehicles(idLoad));
        LoadPickupsFromFile(idLoad,HqFile_Pickups(idLoad));
        HQInfo [ idLoad ] [ GGates ] = 0;
        for(new i; i < MAX_MoveObjectsHQ; i++)
        {
			Gate1Hq_ob = INVALID_OBJECT_ID;
			lineobjects[0] = EOS;
            format(Strloadg,sizeof(Strloadg), "CloseGate%d",i);
			inline GateHq(string:name[], string:value[]) INI_String(Strloadg,lineobjects,256);
			INI_ParseFile(HqFile_Main(idLoad), using inline "GateHq");
   			sscanf(lineobjects, "p<,>iffffff",Gate1Hq_ob,MoveObjectPos[idLoad][i][0],MoveObjectPos[idLoad][i][1],MoveObjectPos[idLoad][i][2],MoveObjectPos[idLoad][i][3],MoveObjectPos[idLoad][i][4],MoveObjectPos[idLoad][i][5]);
            lineobjects[0] = EOS;
            format(Strloadg,sizeof(Strloadg), "OpenGate%d",i);
			inline GateHq1(string:name[], string:value[]) INI_String(Strloadg,lineobjects,256);
			INI_ParseFile(HqFile_Main(idLoad), using inline "GateHq1");
   			sscanf(lineobjects, "p<,>fff",MoveObjectPos[idLoad][i][6],MoveObjectPos[idLoad][i][7],MoveObjectPos[idLoad][i][8]);
			if(Gate1Hq_ob != INVALID_OBJECT_ID)
			{
   				HQInfo [ idLoad ] [ MoveObjectsID ] [ i ] = CreateDynamicObject(Gate1Hq_ob,MoveObjectPos[idLoad][i][0],MoveObjectPos[idLoad][i][1],MoveObjectPos[idLoad][i][2],MoveObjectPos[idLoad][i][3],MoveObjectPos[idLoad][i][4],MoveObjectPos[idLoad][i][5],0,0);
                format(Strloadg,sizeof(Strloadg), "Gate [#%d]",i+1);
		 		gPlayerO_MoveLabelHq[idLoad][i] = Create3DTextLabel(Strloadg,lBlue,MoveObjectPos[idLoad][i][0],MoveObjectPos[idLoad][i][1],MoveObjectPos[idLoad][i][2],30.0,0);
                HQInfo [ idLoad ] [ GGates ]++;
			}
		}
		if(ClanInfo [ idLoad ] [ HasKipatBarzel ] == true)
  		{
            KipatBarzel[0][idLoad] = CreateDynamicObject(18848,ClanInfo [ idLoad ] [ KipatBarzelX ],ClanInfo [ idLoad ] [ KipatBarzelY ],ClanInfo [ idLoad ] [ KipatBarzelZ ]-0.30,0.0,0.0,0.0);
            KipatBarzel[1][idLoad] = CreateDynamicObject(3277,ClanInfo [ idLoad ] [ KipatBarzelX ],ClanInfo [ idLoad ] [ KipatBarzelY ],ClanInfo [ idLoad ] [ KipatBarzelZ ]-0.30,0.0,0.0,0.0);
		}
		if(ClanInfo [ idLoad ] [ Xmin ] != 0.0)
		{
			GangZone[idLoad] = GangZoneCreate(ClanInfo [ idLoad ] [ Xmin ],ClanInfo [ idLoad ] [ Ymin ] ,ClanInfo [ idLoad ] [ Xmax ],ClanInfo [ idLoad ] [ Ymax ]);
            GangZoneShowForAll(GangZone[idLoad],rgba2hex(ClanInfo [ idLoad ] [ ClanR ],ClanInfo [ idLoad ] [ ClanG ],ClanInfo [ idLoad ] [ ClanB ],150));
    		AreaHq[idLoad] = CreateDynamicRectangle(ClanInfo [ idLoad ] [ Xmin ],ClanInfo [ idLoad ] [ Ymin ] ,ClanInfo [ idLoad ] [ Xmax ],ClanInfo [ idLoad ] [ Ymax ]);
		}
		if(ClanInfo [ idLoad ] [ HasClanBank ] == true) Bank_Clan[idLoad] = CreateDynamicCP(ClanInfo [ idLoad ] [ BankX ] ,ClanInfo [ idLoad ] [ BankY ] ,ClanInfo [ idLoad ] [ BankZ ], 2.5);
        if(ClanInfo [ idLoad ] [ Type_Icon ] != 0) HQInfo [ idLoad ] [ Icon ] = CreateDynamicMapIcon(ClanInfo [ idLoad ] [ IconX ],ClanInfo [ idLoad ] [ IconY ],ClanInfo [ idLoad ] [ IconZ ],ClanInfo [ idLoad ] [ Type_Icon ],-1,-1,-1,-1,500.0);
	}
	return 1;
}
function HqUnLoad(idLoad)
{
	if(InClanReload[idLoad] == true)
	{
     	for(new i; i < MAX_CoutHqs; i++)
		{
		    if(i < MAX_OBJECTSHQ) DestroyDynamicObjectEx(HQInfo [ idLoad ] [ ObjectsHq ] [ i ]);
    	    if(i < MAX_VehiclesHQ)
			{
                if(IsValidDynamic3DTextLabel(gPlayerCarLabelHq[idLoad][ i ] )) DestroyDynamic3DTextLabel(gPlayerCarLabelHq[ idLoad ][ i ]);
				DestroyVehicleEx(HQInfo [ idLoad ] [ VehiclesHq ] [ i ]);
			}
 			if(i < MAX_PickupsHQ) DestroyDynamicPickupEx(HQInfo [ idLoad ] [ PickupsHq ] [ i ]);
			if(i < 5)
			{
				DestroyDynamicObjectEx( HQInfo [ idLoad ] [ MoveObjectsID ] [ i ]);
				Delete3DTextLabel( gPlayerO_MoveLabelHq[idLoad][i] );
			}
		}
  		if(ClanInfo [ idLoad ] [ HasKipatBarzel ] == true)
        {
			DestroyDynamicObjectEx( KipatBarzel[1][idLoad] );
			DestroyDynamicObjectEx( KipatBarzel[0][idLoad] );
		}
		if(ClanInfo [ idLoad ] [ HasClanBank ] == true)
		{
			if(IsValidDynamicCP(Bank_Clan[ idLoad ])) DestroyDynamicCP( Bank_Clan[ idLoad ] );
			Bank_Clan[ idLoad ] = -1;
		}
  		if(ClanInfo [ idLoad ] [ Xmin ] != 0.0)
		{
  			GangZoneDestroy(GangZone[idLoad]);
            GangZone[idLoad] = -1;
		}
  		if(IsValidDynamicArea(AreaHq[idLoad]))
		{
		  	DestroyDynamicArea(AreaHq[idLoad]);
       		AreaHq[idLoad] = -1;
       	}
		if(ClanInfo [ idLoad ] [ Type_Icon ] != 0)
		{
			DestroyDynamicMapIcon(HQInfo [ idLoad ] [ Icon ]);
			HQInfo [ idLoad ] [ Icon ] = -1;
		}
		MoreProfit[0][idLoad] = 0; //Objects
		MoreProfit[1][idLoad] = 0; //Vehicles
		MoreProfit[2][idLoad] = 0; //Pickups
		Clanlozer[idLoad] = 0;
		ClanArea[idLoad] = 0;
   		InClanReload[idLoad] = false;
   		UnLoadHq(idLoad);
	}
	return 1;
}
stock LoadObjectsFromFile(idclan,const filename[])
{
    new File:file_ptr,line[256], Object,Float: SpawnX,Float: SpawnY,Float: SpawnZ,
    Float: SpawnRX,Float: SpawnRY,Float: SpawnRZ,Objects_loaded;
    file_ptr = fopen(filename,filemode:io_read);
    if(!file_ptr) return 0;
    while(fread(file_ptr, line, 256))
    {
        sscanf(line, "p<,>iffffff",Object,SpawnX,SpawnY,SpawnZ,SpawnRX,SpawnRY,SpawnRZ);
		HQInfo [ idclan ] [ ObjectsHq ] [ Objects_loaded ] = CreateDynamicObject(Object,SpawnX,SpawnY,SpawnZ,SpawnRX,SpawnRY,SpawnRZ,0,0);
		Objects_loaded++;
		if(Objects_loaded >= MAX_OBJECTSHQ)
		{
			printf("Clan %s full objects (%d/%d).",ClanInfo [ idclan ] [ ClanName ],Objects_loaded,MAX_OBJECTSHQ);
			break;
		}
	}
    MoreProfit[0][idclan] = Objects_loaded;
    fclose(file_ptr);
    printf("Loaded %d objects from: %s",Objects_loaded, filename);
    return Objects_loaded;
}
stock LoadVehiclesFromFile(idclan,const filename[])
{
    new File:file_ptr,line[256], Vehicle, Float:SpawnX, Float:SpawnY, Float:SpawnZ,
    Float:SpawnRX, Color1, Color2, draw, Vehicles_loaded;
    file_ptr = fopen(filename,filemode:io_read);
    if(!file_ptr) return 0;
    while(fread(file_ptr, line, 256))
    {
    	sscanf(line, "p<,>iffffiii", Vehicle, SpawnX, SpawnY, SpawnZ, SpawnRX,Color1, Color2, draw);
        HQInfo [ idclan ] [ VehiclesHq ] [ Vehicles_loaded ] = CreateVehicle(Vehicle,SpawnX,SpawnY,SpawnZ,SpawnRX,ClanInfo [ idclan ] [ ColorCars2 ],ClanInfo [ idclan ] [ ColorCars1 ],draw,2,idclan);
        format(String, sizeof(String), "%s%s",ColorClan(idclan),ClanInfo [ idclan ] [ ClanName ] );
	    SetVehicleNumberPlate(HQInfo [ idclan ] [ VehiclesHq ] [ Vehicles_loaded ], String);
        format(String, sizeof(String), "{"#C_White"}%s\"%s\" {"#C_Pink"}:רכב מפקדת\n{"#C_White"}\"%s\" {"#C_Pink"}:סוג רכב",ColorClan(idclan),ClanInfo [ idclan ] [ ClanName ],GetVehicleName(HQInfo [ idclan ] [ VehiclesHq ] [ Vehicles_loaded ]));
		if(!IsValidDynamic3DTextLabel(gPlayerCarLabelHq[idclan][Vehicles_loaded ] ))
   		{
   	   		gPlayerCarLabelHq[idclan][Vehicles_loaded ] = CreateDynamic3DTextLabel(String,Yellow,Float: SpawnX,Float: SpawnY,Float: SpawnZ, 20.8, INVALID_VEHICLE_ID,HQInfo [ idclan ] [ VehiclesHq ] [ Vehicles_loaded ],0, -1, -1, -1, 100.0);
  		}
   		else UpdateDynamic3DTextLabelText(gPlayerCarLabelHq[idclan][Vehicles_loaded ],0xFF0000FF,String);
 	    Vehicles_loaded++;
 	    if(Vehicles_loaded >= MAX_VehiclesHQ)
  		{
	 		printf("Clan %s full vehicles (%d/%d).",ClanInfo [ idclan ] [ ClanName ],Vehicles_loaded,MAX_VehiclesHQ);
            break;
		}
	}
	MoreProfit[1][idclan] = Vehicles_loaded;
    fclose(file_ptr);
    printf("Loaded %d Vehicles from: %s",Vehicles_loaded, filename);
    return Vehicles_loaded;
}

