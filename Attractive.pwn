function TestCD(playerid, cd)
{
	if(GetPlayerVehicleID(playerid) == pTest[playerid][pTVehicle] && IsPlayerConnected(playerid) && cd >= 1)
	{
		format(String, sizeof(String), "~n~~n~~n~~n~~n~~n~  ~y~~h~Time ~y~Left: ~r~~h~%d", cd);
		GameTextForPlayer(playerid,String, 1000, 3);
		cd--;
		pTest [ playerid ] [ pTTime ] = SetTimerEx("TestCD", 1000, false, "ii", playerid, cd);
	}
	else ResetTestVehicle(playerid,false);
	return 1;
}
stock ResetTestVehicle(playerid,bool: num)
{
	if(pTest [ playerid ] [ pTVehicle ] != -1)
	{
        pTest [ playerid ] [ pTTime ] = -1;
 		pTest [ playerid ] [ pTCp ] = 0;
		pTest [ playerid ] [ pTVModel ] = 0;
		pTest [ playerid ] [ pTCity ] = -1;
		DestroyVehicleEx(pTest[playerid][pTVehicle]);
		OnPlayerSpawn(playerid);
		DisablePlayerRaceCheckpoint(playerid);
		if(num == true)
		{
			S_SendMsgToAll(Pink,"! עבר בהצלחה את הטסט ומעכשיו יוכל לקנות רכב, מזל טוב \"%s\" השחקן",GetName(playerid));
