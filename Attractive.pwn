public OnPlayerTakeDamage(playerid, issuerid, Float:amount, weaponid, bodypart)
{
	if(GetPlayerTeam(playerid) != GetPlayerTeam(issuerid) && GetPlayerTeam(playerid) != NO_TEAM)
	{
		if(InActAuto[playerid]  == true)
		{
			if(Acts[IsActOn][Type] == ACT_WAR_TYPE && Acts[IsActOn][Map] == 14)
 			{
				if(weaponid == 34 || weaponid == 33)
				{
					GetPlayerPos( playerid , GPos[0], GPos[1], GPos[2]);
					switch(random(4))
					{
						case 0: CreateExplosion(GPos[0], GPos[1], GPos[2],0,1);
						case 1: CreateExplosion(GPos[0], GPos[1], GPos[2],5,1);
    			        case 2: CreateExplosion(GPos[0], GPos[1], GPos[2],4,1);
    			        case 3: CreateExplosion(GPos[0], GPos[1], GPos[2],11,1);
    			        case 4: CreateExplosion(GPos[0], GPos[1], GPos[2],13,1);
					}
				}
			}
		}
		else if(issuerid >= 0 && issuerid <= 500 && HeadShoot == true && bodypart == 9)
		{
    	    if(InHeadShoot[playerid] - gettime() < 0)
    	    {
		    	InHeadShoot[playerid] = gettime() + 1;
				SetPlayerHealth(playerid, 0.0);
        	    S_SendMsg(playerid,lBlue, ".%s עם הנשק HeadShot עשה לך {"#C_Orange"}\"%s\"{"#C_lBlue"} השחקן",WeaponName(weaponid),GetName(issuerid));
        	    S_SendMsg(issuerid,lBlue, ".%s בנשק HeadShot (הד-שוט) {"#C_Orange"}\"%s\"{"#C_lBlue"} עשית לשחקן",WeaponName(weaponid),GetName(playerid));
				//S_SendMsgToAll(Brown,"[HeadShot]: ! %s בנשק %s לשחקן HeadShot עשה %s השחקן",WeaponName,GetName(playerid),GetName(issuerid));
			}
		}
		else if(issuerid >= 0 && issuerid <= 500 && SniperKill == true)
 		{
			if(weaponid == 34 || weaponid == 33) SetPlayerHealth(playerid, 0.0);
 		}
		else if(InCodeType( issuerid ) == 6 && InActAuto [ issuerid ] == false)
		{
		    DDamagePlayer(playerid,amount * 2);
		}
		else if((GetPlayerWeapon(issuerid) == 0 || GetPlayerWeapon(issuerid) == 1) && MegaPunch[issuerid] == true)
		{
    	    DDamagePlayer(playerid,amount + 20.0);
