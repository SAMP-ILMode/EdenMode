stock GetActorMoney(actorid,playerid)
{
	if(actorid == 1) pInfo [ playerid ] [ GoldPoints ] += GetBankBots[actorid];
    else Y_GivePlayerMoney(playerid,GetBankBots[actorid]);
    GetBankBots[actorid] = 0;
	new zStr[22];
    format(zStr,sizeof(zStr),"Actor%d_money",actorid);
	new INI:File = INI_Open(ActorMoney);
	INI_WriteInt(File, zStr,0);
	INI_Close ( File );
	return 1;
}
stock GiveActorMoney(actorid,amout)
{
    GetBankBots[actorid] += (amout * 2) / 150;
	new zStr[22];
    format(zStr,sizeof(zStr),"Actor%d_money",actorid);
	new INI:File = INI_Open(ActorMoney);
	INI_WriteInt(File, zStr,GetBankBots[actorid]);
	INI_Close ( File );
	return 1;
}
