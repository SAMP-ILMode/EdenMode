CMD:crane(playerid, params[])
{
	new command[10];
	if(sscanf(params,"s[10]",command)) return SendClientMessage(playerid, White, "/crane [state/enter/exit] :צורת השימוש");
	if(!strcmp(command, "state", true))
	{
        SendClientMessage(playerid,-1, Crane != -1?("{"#C_Red"}.המנוף תפוס כעת, בשביל לעלות על המנוף תהיה צריך להמתין עד שהמנוף יתפנה"):("{"#C_llllGreen"}/crane enter :אף שחקן לא משתמש כרגע במנוף. אתה מוזמן להיכנס אליו"));
        return 1;
	}
 	if(!strcmp(command, "enter", true))
	{
		if(Crane != -1) return SendClientMessage(playerid,Red, Crane != playerid?(".המנוף כבר תפוס, אנא המתן"):(".אתה כבר נימצא במנוף"));
        if(!IsPlayerInAnyVehicle(playerid)) return SendClientMessage(playerid,Red,".אתה לא נימצא ברכב");
	    new vmodel = GetVehicleModel(GetPlayerVehicleID(playerid));
		if(vmodel == 509 || vmodel == 481 || vmodel == 462 || vmodel == 522 || vmodel == 471 || vmodel == 586) return SendClientMessage ( playerid, Red, ".לא ניתן להיכנס עם סוג רכב זה למנוף" );
        Crane = playerid;
		SetVehiclePos(GetPlayerVehicleID(playerid), 680.0879,945.4099,-4.5404);
        SetVehicleHealth(GetPlayerVehicleID(playerid),999999999.9);
		TimerCrane = SetTimer("CraneStop",120*1000,false);
		SendClientMessage(playerid,llllGreen,".נכנסת למנוף");
        S_SendMsgToAll(Blue, "» נכנס אליו \"%s\" המנוף תפוס, השחקן",GetName(playerid));
		ShowPlayerDialog(playerid, 98211, DIALOG_STYLE_MSGBOX, "Crane - המנוף","{"#C_Orange"}!ברוך הבא למנוף\n\n{"#C_Yellow"}יש לך 120 שניות מעכשיו להנות מהמנוף, כשייגמר הזמן תוצא אוטומטית מהמנוף\n.שלוט ברכב שלך באמצעות החצים, לחץ מהר על המקש השמאלי בעכבר בשביל שהרכב יעוף מהר\n\n{"#C_lYellow"}• /Crane exit - ליציאה מהמנוף הקש/י", "אישור", "");
	    return 1;
	}
	if(!strcmp(command, "exit", true))
	{
        if(Crane != playerid) return SendClientMessage(playerid,Red, ".אתה לא נימצא במנוף");
		new IDC = GetPlayerVehicleID(playerid);
        OnPlayerSpawn(playerid);
        KillTimerEx(TimerCrane);
        SetVehicleToRespawn(IDC);
        SetVehicleHealth(IDC,999.0);
        S_SendMsgToAll(Blue, "» /Crane ?יצא ממנו, רוצה להיכנס למנוף \"%s\" המנוף פנוי, השחקן",GetName(playerid));
        SendClientMessage(playerid,Red,".יצאת מהמנוף");
        Crane = -1;
	    return 1;
	}
	return SendClientMessage(playerid, White, "/crane [state/enter/exit] :צורת השימוש");
}
function CraneStop()
{
	new IDC = GetPlayerVehicleID(Crane);
  	OnPlayerSpawn(Crane);
   	KillTimerEx(TimerCrane);
    SetVehicleToRespawn(IDC);
    SetVehicleHealth(IDC,999.0);
    S_SendMsgToAll(Blue, "» /Crane ?הוצא מהמנוף בגלל שתם זמנו, רוצים גם להיכנס למנוף \"%s\" המנוף פנוי, השחקן",GetName(Crane));
    SendClientMessage(Crane,Red,".הוצאת מהמנוף בגלל שעברו 120 שניות");
    Crane = -1;
    return 1;
}
