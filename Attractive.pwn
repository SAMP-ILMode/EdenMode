CMD:mute(playerid, params[])
{
	if(LevelAdmin(playerid) < 1 && pInfo [ playerid ] [ ClanAccess ] == false && pInfo [ playerid ] [ Staff ] == false && pInfo [ playerid ] [ NightTeam ] < 1) return 0;
	if(!AdmLog[playerid] && LevelAdmin(playerid))return SendClientMessage(playerid,Red,"ERROR: אינך מחובר למערכת אדמינים");
	if(pInfo [ playerid ] [ NightTeam ] && !GetTNT() && pInfo [ playerid ] [ NightTeam ] != 3 && pInfo [ playerid ] [ NightTeam ] != 3) return SendClientMessage(playerid,Red,".הגישות של הצוות לילה ניתנות מהשעה 7 עד לשעה 6 בבוקר");
    new ID,
		Time_Mu,
		Reason_Mu[128];
    sscanf(params,"iis[128]",ID,Time_Mu,Reason_Mu);
    if(sscanf(params,"ii",ID,Time_Mu)) return SendClientMessage(playerid,White,"Usage: /Mute [ID] [Time] [Reason]");
   	if(!IsPlayerConnected( ID )) return SendClientMessage(playerid,Red, ".המשתמש אינו מחובר");
    if(IsPlayerNPC( ID )) return SendClientMessage(playerid,Red, ".אין באפשרותך להשתמש בפקודה זו על בוט");
    if(fexist(Mute( ID ))) return SendClientMessage(playerid, Red, ".שחקן זה בהשתקה");
    if(Time_Mu < 1 || Time_Mu > 1800) return SendClientMessage(playerid,Red, ".אין באפשרותך לתת מיוט ליותר מ-1800 שניות");
	if(LevelAdmin( ID ) > LevelAdmin(playerid))return SendClientMessage(playerid, Red, ".אתה לא יכול לעשות פקודה זו על אדמין זה מכיוון שהוא רמה יותר גבוהה ממך");
	new Ntime[16];
	FormatTime(Time_Mu, Ntime);
	if(strlen( Reason_Mu )) S_SendMsgToAll(Red, "*** \"%s\" has been muted for %s by %s \"%s\". (%s)", GetName( ID ),Ntime,AdminLevelName(LevelAdmin(playerid)),GetName(playerid), Reason_Mu );
	else S_SendMsgToAll(Red, "*** \"%s\" has been muted for %s by %s \"%s\".", GetName( ID ),Ntime,AdminLevelName(LevelAdmin(playerid)),GetName(playerid));
    GiveMute(ID,Time_Mu,strlen( Reason_Mu )?( Reason_Mu ):("האדמין לא הקליד סיבה"));
	return 1;
}
