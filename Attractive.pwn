CMD:calc(playerid, params[])
{
    if(MathOn == true) return SendClientMessage(playerid, Red,".פעילות המתמטיקה פועלת, ולכן תהיה צריך לחכות לסיומה");
    if ( sscanf ( params, "dcd", params [ 0 ], params [ 1 ], params [ 2 ] ) ) return SendClientMessage ( playerid, White, "Usage: /Calc <Number> <+ | - | * | /> <Number>");
	if ( params [ 0 ] > 9999999 || params [ 2 ] > 9999999 || params [ 0 ] < -9999999 || params [ 2 ] < -9999999 ) return SendClientMessage ( playerid, White, "Usage: /Calc <number> <+ | - | * | /> <number>");
	if ( params [ 1 ] == '+' ) S_SendMsg ( playerid, Orange, "Answer: {"#C_lGrey"}%d + %d = %d", params [ 0 ], params [ 2 ], params [ 0 ]+ params [ 2 ] );
	else if ( params [ 1 ] == '-' ) S_SendMsg ( playerid, Orange, "Answer: {"#C_lGrey"}%d - %d = %d", params [ 0 ], params [ 2 ], params [ 0 ]-params [ 2 ] );
	else if ( params [ 1 ] == '*' ) S_SendMsg ( playerid,Orange, "Answer: {"#C_lGrey"}%d * %d = %d", params [ 0 ], params [ 2 ], params [ 0 ]*params [ 2 ] );
	else if ( params [ 1 ] == '/' ) S_SendMsg ( playerid,Orange, "Answer: {"#C_lGrey"}%d / %d = %d", params [ 0 ], params [ 2 ], params [ 0 ]/params [ 2 ] );
	return 1;
}

