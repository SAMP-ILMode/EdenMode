public OnVehicleMod(playerid,vehicleid, componentid)
{
	new b = pInfo [ playerid ] [ VehicleID ];
	if(GetPlayerState(playerid) == PLAYER_STATE_DRIVER && b != 0 && VehicleInfo [ b ] [ IDVeh ] == GetPlayerVehicleID(playerid))
	{
	    for(new i; i < 28; i++)
	    {
  			if(i < 23)
			{
 				if(componentid == fbumper[i][0])
				{
				    VSaveEnhancements(b,10,componentid);
					break;
				}
 				if(i < 22)
				{
	 				if(componentid == rbumper[i][0])
					{
					    VSaveEnhancements(b,11,componentid);
                        break;
					}
			  		if(i < 21)
					{
		 				if(componentid == rskirt[i][0])
						{
						    VSaveEnhancements(b,3,componentid);
                            break;
						}
						else if(componentid == lskirt[i][0])
						{
						    VSaveEnhancements(b,16,componentid);
							break;
						}
			  			if(i < 20)
						{
							if(componentid == spoiler[i][0])
							{
							    VSaveEnhancements(b,0,componentid);
								break;
							}
				  			if(i < 17)
							{
				 				if(componentid == rscoop[i][0])
				 				{
				 				    VSaveEnhancements(b,2,componentid);
								 	break;
								}
				 				else if(componentid == wheels[i][0])
				 				{
				 				    VSaveEnhancements(b,7,componentid);
								 	break;
								}
				  				if(i < 4)
								{
									if(componentid == bscoop[i][0])
									{
									    VSaveEnhancements(b,1,componentid);
                                        break;
									}
						 			else if(componentid == rbbars[i][0])
				 					{
				 					    VSaveEnhancements(b,14,componentid);
                                        break;
									}
							  		if(i < 3)
									{
										if(i < 2)
										{
						 					if(componentid == lights[i][0])
						 					{
						 					    VSaveEnhancements(b,4,componentid);
                                                break;
											}
								 			else if(componentid == bventr[i][0])
											{
											    VSaveEnhancements(b,12,componentid);
                                                break;
											}
											else if(componentid == bventl[i][0])
											{
											    VSaveEnhancements(b,13,componentid);
                                                break;
											}
								 			else if(componentid == fbbars[i][0])
											{
											    VSaveEnhancements(b,15,componentid);
                                                break;
											}
											if(i < 1)
											{
				 								if(componentid == basev[i][0])
				 								{
				 								    VSaveEnhancements(b,8,componentid);
              										break;
												}
									 			else if(componentid == hydraulics[i][0])
												{
												    VSaveEnhancements(b,9,componentid);
                                                    break;
                                                }
											}
										}
				 						if(componentid == nitro[i][0])
				 						{
				 						    VSaveEnhancements(b,5,componentid);
                                            break;
										}
									}
								}
							}
						}
					}
				}
			}
			if(componentid == exhaust[i][0])
			{
			    VSaveEnhancements(b,6,componentid);
                break;
			}
		}
	}
    if(GetPlayerInterior(playerid) == 0)
    {
        RemovePlayerFromVehicle(playerid);
  		if(timeleftcheats[playerid]  - gettime() > 0 && Report_DMGuard == true) LogAntiCheat(playerid,"Cheats: Tuning (#11)",true,true);
		else
		{
			timeleftcheats[playerid] = gettime() + 6;
			DMGuardMsgToAdmin("[DM Guard] .Tuning Hacks חשוד שהשתמש בצ'יט %s[ID: %d] השחקן",GetName(playerid),playerid);
		}
	}
	return 1;
}
new VDPlayer[MAX_PLAYERS], CVDPlayer[MAX_PLAYERS];
public OnVehicleDeath(vehicleid, killerid)
{
	if(killerid != INVALID_PLAYER_ID)
	{
		if((gettime() - VDPlayer[killerid]) < 1)
		{
			CVDPlayer[killerid] += 1;
   			if(CVDPlayer[killerid] == 5)
			{
				if(timeleftcheats[killerid]  - gettime() > 0 && Report_DMGuard == true) LogAntiCheat(killerid,"Cheats: Blow Up Cars (#12)",true,true);
				else
				{
					timeleftcheats[killerid] = gettime() + 6;
					DMGuardMsgToAdmin("[DM Guard] .Blow_Up_Cars חשוד שהשתמש בצ'יט %s[ID: %d] השחקן",GetName(killerid),killerid);
				}
			}
		}
		else CVDPlayer[killerid] = 0;
		VDPlayer[killerid] = gettime();
	}
	return 1;
}

