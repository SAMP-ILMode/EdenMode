function UnWcacheLoad(idLoad)
{
	if(InWcacheHq[idLoad] == true)
	{
		for(new i; i < MAX_CoutHqs; i++)
		{
		    if(i < MAX_OBJECTSHQ) DestroyDynamicObjectEx(ObjectsWcache[idLoad][i] );
    	    if(i < MAX_VehiclesHQ) DestroyVehicleEx( CarWcache[idLoad][i] );
         	if(i < MAX_PickupsHQ) DestroyDynamicPickupEx(PickupWcache[idLoad][ i ]);
		}
		if(WcacheInfo [ idLoad ] [ HasCheckWcache ] == true) DestroyDynamicCP( WcacheInfo [ idLoad ] [ Check_Wcache ]);
        WcacheInfo [ idLoad ] [ Check_Wcache ] = -1;
		InWcacheHq[idLoad] = false;
	}
	return 1;
}

