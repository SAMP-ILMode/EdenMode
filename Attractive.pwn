public OnPlayerExitVehicle(playerid, vehicleid)
{
    PLozzerPWar(playerid);
    RemovePlayerAttachedObject(playerid,7);
    if(InActAuto[playerid] == true && ActStatus == 3)
	{
		if(Acts[IsActOn][Type] == ACT_BCAR_TYPE || Acts[IsActOn][Type] == ACT_RACE_TYPE) LozerActivity(playerid,".נפסלת בפעילות מכיוון שיצאת מהרכב");
	}
	if(Crane == playerid)
	{
		new IDC = GetPlayerVehicleID(playerid);
 		OnPlayerSpawn(playerid);
 		KillTimerEx(TimerCrane);
 		SetVehicleToRespawn(IDC);
  		SetVehicleHealth(IDC,999.0);
   		S_SendMsgToAll(Blue, "» /Crane ?יצא ממנו, רוצה להיכנס למנוף \"%s\" המנוף פנוי, השחקן",GetName(playerid));
    	SendClientMessage(playerid,Red,".יצאת מהמנוף");
    	Crane = -1;
	}
    if(InCodeType(playerid) == 5) SetVehicleHealth(GetPlayerVehicleID(playerid),999.0);
    MyVehAFB[playerid] = -1;
	return 1;
}

