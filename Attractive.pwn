  		GangZoneStopFlashForAll(GangZoneCapture[ StartClanArea[ playerid ] ]);
		GangZoneShowForAll(GangZoneCapture[ StartClanArea[ playerid ] ],rgba2hex(ClanInfo [ Occupying_Capture[playerid] ] [ ClanR ] , ClanInfo [ Occupying_Capture[playerid] ] [ ClanG ] , ClanInfo [ Occupying_Capture[playerid] ] [ ClanB ],150));
		CaptureWClan[ StartClanArea[ playerid ] ] = 0;
		CaptureInfo [ StartClanArea[ playerid ]  ] [ Number_of_takeovers ] = CaptureInfo [ StartClanArea[ playerid ]  ] [ Number_of_takeovers ]+1;
   		Occupying_time[playerid] = 0;
        StartClanArea[ playerid ] = 0;
        Occupying_Capture[playerid] = 0;
		KillTimerEx( Timer_Capture [ playerid ] );
		return 1;
	}
	if(Occupying_time[playerid] % 60 == 0)
	{
		SendClanMessageEx(Occupying_Capture[playerid],Yellow, "• (Clan %s%s{"#C_Yellow"}) נשאר לקלאן שלך עוד {"#C_lYellow"}%d{"#C_Yellow"} דקות בשביל להשתלט על אזור %s%s",ColorClan(InClan(playerid)),ClanPlayer(playerid),howemach_AreaCapture-Occupying_time[playerid]/60,ColorCapture( StartClanArea[playerid] ),NameCapture( StartClanArea[playerid] ));
