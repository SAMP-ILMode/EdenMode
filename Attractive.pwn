	new ID;
	if(sscanf(params,"i",ID)) return SendClientMessage(playerid,White,"Usage: /TakeCall [ID]");
	if(!IsXTimePassed(ID,"HelpPlz")) return SendClientMessage(playerid,Red,"השחקן לא ביקש עזרה");
	if(Tallking[playerid] == 1) return SendClientMessage(playerid, Red,".אתה כבר בשיחה");
	if(Tallking[ID] == 1) return SendClientMessage(playerid, Red,".השחקן כבר בשיחה");
	if(ID == playerid) return SendClientMessage(playerid,Red,".אינך יכול להתקשר לעצמך");
	if(!IsPlayerConnected(ID)) return SendClientMessage(playerid,Red,".השחקנ/ית לא מחובר/ת");
	S_SendMsg(ID,lGreen,".(%d) איידי \"%s\" כעת אתה בשיחה עם התומך",playerid,GetName(playerid));
	S_SendMsg(ID, Yellow, "[Phone] >> From %s (id:%d) ?כיצד אוכל לעזור לך,%s שלום לך",GetName(playerid),playerid,GetName(ID));
	S_SendMsg(playerid, lGreen, ".\"%s\" - כעת את עוזר ל", GetName(ID));
	SendStaffMessage(lBlue,"[Supporters] >> .%s עוזר כעת לשחקן \"%s\"",GetName(ID),GetName(playerid));
	TallkingID[playerid] = ID;
	TallkingID[ID] = playerid;
	IsCalling[playerid] = 1;
	InCall[playerid] = 1;
	InCall[ID] = 1;
	Tallking[TallkingID[playerid]] = 1;
	Tallking[playerid] = 1;
	InCall[playerid] = 1;
	return 1;
}

