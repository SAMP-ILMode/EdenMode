	if(!strcmp(command, "players", true))
	{
		new ID_IRC;
	    if(sscanf(params[7],"i",ID_IRC)) return SendClientMessage(playerid,White,"Usage: /Irc Players [1-10000]");
        if(ID_IRC < 1 || ID_IRC > 10000) return SendClientMessage(playerid, Red, ".מספר החדרים בשרת הם בין 1-10000");
		new IRCS , StrIrc[150] , catstr[800] , bool: colorIrcs;
		for(new i, j = GetPlayerPoolSize(); i <= j; i++)
		{
			if(!IsPlayerConnected(i) || IRC[i] != ID_IRC) continue;
			if(colorIrcs == true) colorIrcs = false;
		    else colorIrcs = true;
			IRCS++;
			format(StrIrc,sizeof(StrIrc), "%s%d. %s [ID: %03d]\n",ReturnColor(colorIrcs?(Orange):(White)),IRCS, GetName(i),i);
			strcat(catstr, StrIrc ,800);
		}
	    format(StrIrc, sizeof(StrIrc),"{"#C_Yellow"}שחקנים הנמצאים בחדר: %d",IRCS);
	    if(!IRCS) format(catstr,sizeof(catstr), "{"#C_Red"}.אין שחקנים מחוברים בחדר זה");
	    ShowPlayerDialog(playerid, 98211, DIALOG_STYLE_MSGBOX,StrIrc, catstr, "סגור", "");
	}
	else if(!strcmp(command, "leave", true))
	{
	    if(IRC[playerid] == 0) return SendClientMessage(playerid, Red, ".אתה לא נימצא בחדר מסוים");
		for(new i, j = GetPlayerPoolSize(); i <= j; i++)
		{
			if(!IsPlayerConnected(i) || playerid == i || IRC[i] != IRC[playerid]) continue;
	        S_SendMsg(i,Red,"[IRC] ! עזב את החדר שאתה נימצא \"%s\"(ID:%d) השחקן",GetName(playerid),playerid);
		}
		S_SendMsg(playerid,Red,"[IRC] ! עזבת את החדר מספר %d וכעת אתה לא נימצא בשום חדר",IRC[playerid]);
	    IRC[playerid] = 0;
	}
	else if(!strcmp(command, "set", true))
	{
		new ID_IRC;
		if(sscanf(params[3],"i",ID_IRC)) return SendClientMessage(playerid,White,"Usage: /Irc Set [1-10000]");
		if(ID_IRC < 1 || ID_IRC > 10000) return SendClientMessage(playerid, Red, ".מספר החדרים בשרת הם בין 1-10000");
		if(ID_IRC == IRC[playerid]) return SendClientMessage(playerid, Red, ".אתה כבר נימצא בחדר זה");
		for(new i, j = GetPlayerPoolSize(); i <= j; i++)
		{
			if(!IsPlayerConnected(i)) continue;
    	    if(IRC[playerid] && playerid != i && IRC[i] == IRC[playerid]) S_SendMsg(i,Red,"[IRC] ! עזב את החדר שאתה נימצא ועבר לחדר אחר \"%s\"(ID:%d) השחקן",GetName(playerid),playerid);
			if(IRC[i] == ID_IRC) S_SendMsg(i,llllGreen,"[IRC] ! הצטרף לחדר שאתה נימצא \"%s\"(ID:%d) השחקן",GetName(playerid),playerid);
		}
    	IRC[playerid] = ID_IRC;
    	S_SendMsg(playerid,Orange,"[IRC] ! /i נכסת לחדר מספר %d בכדי לשוחח עם האנשים בחדר זה השתמש בפקודה",ID_IRC);
	}
	else if(!strcmp(command, "me", true))
	{
	    if(IRC[playerid] == 0) return SendClientMessage(playerid, Red, ".אינך נימצא בשום חדר");
    	S_SendMsg(playerid,lGreen,"[IRC] ! אתה נימצא בחדר מספר %d",IRC[playerid]);
	}
	else if(!strcmp(command, "help", true))
	{
		new StrIrc[700];
		strcat(StrIrc,"{"#C_White"}מערכת החדרים הינה מערכת צ'אטים בשרת, במערכת החדרים ישנם 10,000 חדרים, ולכן תוכלו\n.'להיכנס לכל חדר שתרצו ולדבר עם חברים שלכם וכו\n",700);
		strcat(StrIrc,"\n{"#C_Grey"}:פקודות המערכת\n{"#C_Blue"}•{"#C_White"} /irc players - בדיקת שחקנים בחדר מסוים\n{"#C_Blue"}•{"#C_White"} /irc leave - עזיבת חדר מסוים\n{"#C_Blue"}•{"#C_White"} /irc set - להיכנס לחדר מסוים\n{"#C_Blue"}•{"#C_White"} /irc me - לבדוק באיזה חדר אני נמצא",700);
		ShowPlayerDialog(playerid,98211, DIALOG_STYLE_MSGBOX, "{"#C_Yellow"}Irc System - מערכת צ'אטים",StrIrc, "סגור", "");
	}
	else SendClientMessage(playerid,Red, ".פקודה לא קיימת");
	return 1;
}
