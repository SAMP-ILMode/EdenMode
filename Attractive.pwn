CMD:sellhouse(playerid, params[])
{
    if(InActAuto[playerid] == true && ActStatus >= 2) return SendClientMessage(playerid,Red, "./QAct - אינך יכול להקיש פקודות שאתה בפעילות. ליציאה, הקש");
	if(IsPlayerInAnyVehicle(playerid)) return SendClientMessage(playerid, Red, ".לא ניתן להיכנס לבית שלך כאשר אתה ברכב");
	new bool: HouseCout;
	for(new HouseID = 1; HouseID < MAX_HOUSES; HouseID++)
	{
		if(IsValidDynamicPickup(HousePickup[HouseID]))
		{
			if(IsPlayerInRangeOfPoint(playerid, 2.5, HouseInfo [ HouseID ] [ HX ],HouseInfo [ HouseID ] [ HY ],HouseInfo [ HouseID ] [ HZ ]) )
			{
				if(!strcmp(HouseInfo [ HouseID ] [ Owner ], GetName(playerid), false) && HouseInfo [ HouseID ] [ Owned ] == true)
				{
				    HouseCout = true;
					Y_GivePlayerMoney(playerid, HouseInfo [ HouseID ] [ Cost ]/2);
					SendClientMessage(playerid, Green, "* מכרת את הבית שלך, וקיבלת חצי ממכירו");
					pInfo [ playerid ] [ House_f ] = 0;
	    			new INI:File = INI_Open(PFile(playerid));
    				INI_WriteInt ( File, "House",0);
    				INI_Close ( File );
					HouseInfo [ HouseID ] [ Owned ] = false;
					HouseInfo [ HouseID ] [ Owner ] = EOS;
					HouseInfo [ HouseID ] [ Opened ] = false;
					House_UpdateEntrance(HouseID );
					return 1;
				}
			}
		}
	}
	if(HouseCout == false) SendClientMessage(playerid,Red, ".על מנת להשתמש בפקודה זו עליך להיות על פיקאפ הבית שלך או שבית זה לא שלך");
