CMD:pm(playerid, params[])
{
    if(fexist(Mute(playerid)) && !LevelAdmin(playerid)) return S_SendMsg(playerid,Red, ".לא ניתן לבצע פקודה זו כאשר אתה בהשתקה, תוכל לכתוב שוב בעוד %d שניות",TimeMute[playerid]);
	new Player;
	if(sscanf(params,"is[128]",Player ,params)) return SendClientMessage(playerid,White,"Usage: /Pm [ID] [Text]");
	if(!IsPlayerConnected(Player)) return SendClientMessage(playerid,Red, ".המשתמש אינו מחובר");
 	if(AFKTime[Player] == 1000) return SendClientMessage(playerid, Red, ".AFK שחקן זה במצב");
 	if(fexist(Mute( Player ))) return SendClientMessage(playerid, Red, ".שחקן זה בהשתקה ולכן לא תוכל לדבר איתו עד תום ההשתקה");
  	if(Ignore[Player][playerid]) return SendClientMessage(playerid,Red,".שחקן זה חסם את ההודעות הפרטיות שלך");
   	if(Player == playerid) return SendClientMessage(playerid,Red, ".אין אפשרות לשלוח הודעה פרטית לעצמך");
   	if(pInfo [ Player ] [ blockpm ] == true && LevelAdmin(playerid) < 15) return SendClientMessage(playerid,Red, ".אדמין זה חסם הודעות פרטיות");
    if(IsPlayerNPC( Player )) return SendClientMessage(playerid,Red, ".אין באפשרותך להשתמש בפקודה זו על בוט");
	for(new i, l = sizeof AntiCurse; i != l; i++)
	{
		if(strfind(params, AntiCurse [ i ] [ Curse ] , true) != -1 && !LevelAdmin(playerid))
		{
