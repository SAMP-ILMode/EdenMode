CMD:stopacts(playerid, params[])
{
	if(LevelAdmin(playerid) < 1 && pInfo [ playerid ] [ Staff ] == false && pInfo [ playerid ] [ ClanAccess ] == false) return 0;
    if(!AdmLog[playerid] && LevelAdmin(playerid))return SendClientMessage(playerid,Red,"ERROR: אינך מחובר למערכת אדמינים");
    if(!ActStatus && !ActStatusTeams) return SendClientMessage(playerid,Red, ".אין פעילויות עובדות כרגע");
    if(ActStatusTeams)
    {
   		new Reason_sa[128];
		sscanf(params,"s[128]",Reason_sa);
		S_SendMsgToAll(Red,":מהסיבה %s בוטלה על ידי האדמין {"#C_lOrange"}%s{"#C_Red"} הפעילות",GetName(playerid),ActsTeams[ IsActOnTeams ] [ ActNameTeams ]);
    	if(strlen( Reason_sa )) S_SendMsgToAll(Red,"(%s)",Reason_sa);
    	else SendClientMessageToAll(Red,"(.האדמין לא הקליד סיבה)");
    	KillTimer(ActTimerTeams);
    	foreach(new i: Player)
		{
			if(InActTeams(i))
			{
				InActTeamA[i] = false;
  				InActTeamB[i] = false;
   				if(ActStatusTeams == 2)
   				{
					SpawnPlayer(i);
					SetPlayerHealth(i, 99);
					SetPlayerArmour(i, 0);
    			}
			}
		}
		IsActOnTeams = 0;
    	ActStatusTeams = 0;
		ActPlayersTeams = 0;
	}
	else if(ActStatus)
	{
   		new Reason_sa[128];
		sscanf(params,"s[128]",Reason_sa);
		S_SendMsgToAll(Red,":מהסיבה %s בוטלה על ידי האדמין %s הפעילות",GetName(playerid),Acts [ IsActOn ] [ ActName ]);
    	if(strlen( Reason_sa )) S_SendMsgToAll(Red,"(%s)",Reason_sa);
    	else SendClientMessageToAll(Red,"(.האדמין לא הקליד סיבה)");
    	KillTimerEx(ActTimer);
    	KillTimerEx(ActTimer2);
    	foreach(new i: Player)
		{
			if(InActAuto[i] == true)
			{
				InActAuto[i] = false;
   				if(ActStatus == 2 || ActStatus == 3)
   				{
					DisablePlayerRaceCheckpoint(i);
					InActCP[i] = 0;
					RemovePlayerFromVehicle(i);
					SetPlayerRandomSpawn(i);
					SetPlayerInterior(i, 0);
					SetPlayerHealth( i ,99);
					SetPlayerVirtualWorld(i, 0);
					ResetPlayerWeapons(i);
					GiveWeaponsMy(i);
					TogglePlayerControllable(i, 1);
 					if(ClanInfo [ InClan(i) ] [ AntiTK_Clan ] == true && InClan(i)) SetPlayerTeam(i, InClan(i));
					if(InClan(i)) SetPlayerColor(i, rgba2hex(ClanInfo [ InClan(i) ] [ ClanR ] ,ClanInfo [ InClan(i) ] [ ClanG ],ClanInfo [ InClan(i) ] [ ClanB ], 100));
					else SetPlayerColor(i,rgba2hex(random(255),random(255),random(255),100));
					if(pInfo [ i ] [ MyWeather ] != -1) SetPlayerWeather ( i , pInfo [ i ] [ MyWeather ] );
					else SetPlayerWeather ( i , Server_Weather );
 					if(pInfo [ i ] [ Mytime ] != -1) SetPlayerTime(i, pInfo [ i ] [ Mytime ],0);
					else SetPlayerTime(i,10,0);
    			}
  			}
		}
		IsActOn = 0;
    	ActStatus = 0;
		ActPlayers = 0;
		if(RAct == true) RActRandom();
		TextDrawHideForAll ( ActTD [ 0 ] );
		TextDrawHideForAll ( ActTD [ 1 ] );
		TextDrawHideForAll ( ActTD [ 3 ] );
		TextDrawHideForAll ( ActTD [ 4 ] );
		for(new i; i<sizeof(ActVehicle); i++)
		{
            if(IsValidDynamic3DTextLabel(TD_ActVehicle[i])) DestroyDynamic3DTextLabel(TD_ActVehicle[i]);
			DestroyVehicleEx(ActVehicle[i]);
		}
	}
	return 1;
}
