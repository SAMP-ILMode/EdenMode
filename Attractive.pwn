public OnPlayerClickTextDraw(playerid, Text:clickedid)
{
    if(clickedid == Text:INVALID_TEXT_DRAW)
    {
        CancelVShop(playerid,true);
	}
    if(clickedid == VehicleShopTD[1])
    {
        if(pInfo [ playerid ] [ HaveLicense ] != 2) return SendClientMessage(playerid,Red, "/License אין לך רישיון נהיגה ולכן לא תוכל לקנות רכב, בשביל להוציא רישיון נהיגה");
        if(pInfo [ playerid ] [ VehicleID ]) return SendClientMessage(playerid, Red, "./Sellv - יש רכב בבעלותך, בכדי לקנות חדש תיהיה צריך למכור את רכבך הקיים בפקודה");
        if(Auc_IDSuggest == playerid) return SendClientMessage(playerid,Red, ".בגלל שהצעת במכירה פומבית כסף רכב לא תוכל לקנות רכב מחנות הרכבים עד שהמכירה פומבית תסתיים");
        if(pInfo [ playerid ] [ GoldPoints ] < c_Shop[ NextBVehicle ][ Vehicle_POINT ]) return S_SendMsg(playerid,Red,".(אין לך מספיק נקודות זהב בכדי לקנות רכב זה, סכום נקודות הזהב של רכב זה הוא (%d נקודות זהב",c_Shop[ NextBVehicle ][ Vehicle_POINT ]);
        if(pInfo [ playerid ] [ InCode ] != 11)
		{
        	if(Y_GetPlayerMoney(playerid) < c_Shop[ NextBVehicle ] [ Vehicle_PRICE ]) return S_SendMsg(playerid,Red,".(אין לך מספיק כסף לקניית רכב זה, סכום רכב זה הוא (%s$",GetNum(c_Shop[ NextBVehicle ] [ Vehicle_PRICE ]));
        	Y_GivePlayerMoney(playerid, -c_Shop [ NextBVehicle ] [ Vehicle_PRICE ]);
			S_SendMsgToAll(Orange, ">• !במחיר {"#C_White"}\"$%s\"{"#C_Orange"} מזל טוב {"#C_Pink"}%s{"#C_Orange"} קנה את הרכב %s השחקן",GetNum( c_Shop[ NextBVehicle ][ Vehicle_PRICE ] ) , c_Shop [ NextBVehicle ][ Vehicle_NAME ] , GetName(playerid));
			S_SendMsg(playerid,lGrey, "~~~ :{00FFF2}[%s]{"#C_lGrey"} במחיר $%s באמצעות כספך שהיה בידך %s ,קנית את הרכב ~~~",GetDateAsString(),GetNum(c_Shop[ NextBVehicle ][ Vehicle_PRICE ]),c_Shop[ NextBVehicle ][ Vehicle_NAME ]);
		}
		else
		{
			new mc = (c_Shop[ NextBVehicle ] [ Vehicle_PRICE ] * 80) / 100;
        	if(Y_GetPlayerMoney(playerid) < mc) return S_SendMsg(playerid,Red,".(אין לך מספיק כסף לקניית רכב זה, סכום רכב זה הוא (%s$",GetNum(mc));
        	Y_GivePlayerMoney(playerid, -mc);
			S_SendMsgToAll(Orange, ">• !במחיר {"#C_White"}\"$%s\"{"#C_Orange"} מזל טוב {"#C_Pink"}%s{"#C_Orange"} קנה את הרכב %s השחקן",GetNum( mc ) , c_Shop [ NextBVehicle ][ Vehicle_NAME ] , GetName(playerid));
			S_SendMsg(playerid,lGrey, "~~~ :{00FFF2}[%s]{"#C_lGrey"} במחיר $%s באמצעות כספך שהיה בידך %s ,קנית את הרכב ~~~",GetDateAsString(),GetNum(mc),c_Shop[ NextBVehicle ][ Vehicle_NAME ]);
			foreach(new i: Player)
			{
				if(i != playerid) S_SendMsg(i,White, "[Code System] ! קיבל הנחה בחנות הרכבים של 20%s ({"#C_lGreen"}%s${"#C_White"}) על קניית הרכב {"#C_Yellow"}\"%s\"{"#C_White"} השחקן","%%",GetNum(c_Shop[ NextBVehicle ] [ Vehicle_PRICE ] - mc),GetName(playerid));
				else if(i == playerid) S_SendMsg(i,White, "[Code System] קיבלת הנחה של 20%s ({"#C_lGreen"}%s${"#C_White"}) על קניית הרכב","%%",GetNum(c_Shop[ NextBVehicle ] [ Vehicle_PRICE ] - mc));
			}
		}
		pInfo [ playerid ] [ GoldPoints ] = pInfo [ playerid ] [ GoldPoints ] - c_Shop[ NextBVehicle ] [ Vehicle_POINT ];
    	GiveSVehicle(playerid,c_Shop [ NextBVehicle ] [ Vehicle_ID ]);
		SendClientMessage(playerid,0x15D4EDFF, "* /Callv {37DB45}:לשיגור רכבך");
		SendClientMessage(playerid,0x15D4EDFF, "* /Sellv {37DB45}:למכירת רכבך");
		SendClientMessage(playerid,0x15D4EDFF, "* /CarHelp {37DB45}:להסבר מפורט על מערכת רכבים זו הקש/י");
	}
    else if(clickedid == VehicleShopTD[2])
    {
  		if(NextBVehicle == sizeof(c_Shop)) NextBVehicle = 0;
        else NextBVehicle++;
	    DCSHOP(playerid);
	}
    else if(clickedid == VehicleShopTD[3])
    {
		if(NextBVehicle == 0) NextBVehicle = sizeof(c_Shop) - 1;
        else NextBVehicle --;
        DCSHOP(playerid);
	}
	else if(clickedid == VehicleShopTD[12])
	{
        CancelVShop(playerid,true);
        SendClientMessage(playerid,Red, "! סגרת את הדיאלוג של הקניית רכבים");
	}
	else if(clickedid == ClicksSystem)
	{
		Clicks[playerid] ++;
		format(String,sizeof(String),"~R~] ~B~Click %d/50 ~R~]",Clicks[playerid]);
		GameTextForPlayer(playerid,String, 1000, 4);
		if(Clicks[playerid] == 50 && StartClick == true)
		{
			StartClick = false;
			KillTimerEx(TimerClick);
 			for(new i, j = GetPlayerPoolSize(); i <= j; i++)
			{
				if(!IsPlayerConnected(i)) continue;
				TextDrawHideForPlayer(i, ClicksSystem);
				CancelSelectTextDraw(i);
				Clicks[i] = 0;
			}
			new payment = RandomEx(1500,5000);
			S_SendMsgToAll(lPurple, ".%s$ ניצח בפעילות הקליקים וקיבל {"#C_White"}\"%s\"{"#C_lPurple"} השחקן",GetNum(payment),GetName(playerid));
            UpdateTDServer(GetName(playerid),"is the winner in the~B~ Click~h~!");
            Y_GivePlayerMoney(playerid,payment);
		}
	}
	return 1;
}

stock GiveSVehicle(playerid,VehID1)
{
	for(new b; b < sizeof(c_Shop); b++)
	{
   		if(c_Shop [ b ] [ Vehicle_ID ] == VehID1)
   		{
			Ownedsp[b]++;
			new INI:File1 = INI_Open("CarStats.ini");
			INI_WriteInt ( File1,c_Shop [ b ] [ Vehicle_NAME ] ,Ownedsp [ b ]);
			INI_Close ( File1 );
			break;
		}
   	}
	for(new i = 1; i < MAX_VEH; i++)
	{
		if(!fexist(VehicleID_File(i)))
		{
			pInfo [ playerid ] [ VehicleID ] = i;
			new INI:File1 = INI_Open(PFile(playerid));
			INI_WriteInt ( File1, "VehicleID",pInfo [ playerid ] [ VehicleID ]);
			INI_Close ( File1 );
			VehicleInfo [ i ] [ ModelVeh ] = VehID1;
   			format(VehicleInfo [ i ] [ VehOwner ],25,"%s",GetName(playerid));
      		VehicleInfo [ i ] [ NeonNumber ] = 0;
			format(VehicleInfo [ i ] [ Plate ],13,"Attractiv DM");
			format(VehicleInfo [ i ] [ VehicleLocked ],2,"0");
