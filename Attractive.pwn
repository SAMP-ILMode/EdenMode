stock OrgFile(orgid)
{
	new File[14];
	format(File, sizeof(File), "Orgs/%d.ini", orgid);
	return File;
}
stock GetOrgConnected(idOrg)
{
	new connected_count;
	foreach(new i: Player)
	{
   		if(fexist(PFile(i)) && InOrg(i) == idOrg) connected_count++;
	}
	return connected_count;
}
stock SendOrgMessage(orgid, color, MsgS[])
{
	foreach(new i: Player)
	{
		if(InOrg(i) == orgid) SendClientMessage(i,color, MsgS);
	}
	return 1;
}

CMD:setorg(playerid, params[])
{
    if(pInfo [ playerid ] [ ClanAccess ] == false) return SendClientMessage(playerid,Red, "* הפקודה שכתבת אינה קיימת במערכת הקלאנים");
	new ID,OrgId,Level_c;
	if(sscanf(params,"iii",ID,OrgId,Level_c)) return SendClientMessage(playerid, White, "Usage: /SetOrg [playerid] [OrgID] [level]");
    if(!fexist(OrgFile( OrgId ))) return SendClientMessage(playerid,Red, ".איידי ארגון שגוי");
	pInfo [ ID ] [ OrgID ] = OrgId;
	pInfo [ ID ] [ OrgLevel ] = Level_c;
	pInfo [ ID ] [ OrgSerial ] = SerialNumber_Org[ OrgId ];
	format(String, 70, ".צורף לארגון שלך %s",GetName(ID));
	SendOrgMessage( OrgId ,lGreen, String);
	S_SendMsg(playerid,lGreen, ".%s צורף לארגון %s", NameOrg[ OrgId ],GetName(ID));
	S_SendMsg(ID,lGreen, ".%s צורפת לארגון", NameOrg[ OrgId ]);
	return 1;
}
CMD:org(playerid, params[])
{
	new command[50];
	if(sscanf(params,"s[50]{s[50]}",command)) return SendClientMessage(playerid, White, "Usage: /Org [Invite/Kick/Quit/Join/SetLevel/Info/List/Help]");
	if(!strcmp(command, "tag", true))
	{
		if(!InOrg(playerid))return SendClientMessage(playerid, Red,".אינך נמצא בשום  ארגון");
		if(GetLevelOrg(playerid) < 2) return SendClientMessage(playerid, Red, ".(Tester) אתה לא נמצא בדרגת טסטר או גבוהה יותר");
		new ID,Tag_c[16];
		if(sscanf(params[3],"is[16]",ID,Tag_c)) return SendClientMessage(playerid,White, "Usage: /Org Tag [ID] [Tag]");
		if(!IsPlayerConnected(ID)) return SendClientMessage(playerid,Red, ".משתמש לא מחובר");
		if(InOrg(playerid) != InOrg(ID)) return SendClientMessage(playerid,Red, ".שחקן זה לא נמצא בארגון שלך");
        if(pInfo [ playerid ] [ OrgLevel ] <= pInfo [ ID ] [ OrgLevel ] && ID != playerid) return SendClientMessage(playerid,Red, ".אין באפשרותך לשנות תאג למישהו ברמה זהה או יותר גבוהה משלך");
		if(strlen(Tag_c) > 14 || strlen(Tag_c) < 0) return SendClientMessage(playerid,Red, ".אין באפשרותך לשים תאג יותר מ - 15 תווים");
		S_SendMsg(playerid,lGreen,":את התאג בארגון %sשמת ל", GetName(ID));
        S_SendMsg(playerid,lGreen, "(%s)",Tag_c);
        S_SendMsg(ID,lGreen, ":שם לך תאג בארגון %s", GetName(playerid));
        S_SendMsg(ID,lGreen, "(%s)",Tag_c);
        format( pInfo [ ID ] [ OrgTag ] , 15 , "%s",Tag_c);
       	new INI:File = INI_Open(PFile(ID));
 	   	INI_WriteString ( File , "OrgTag" , pInfo [ ID ] [ OrgTag ]);
 	   	INI_Close ( File );
		return 1;
	}
	if(!strcmp(command, "deltag", true))
	{
		if(!InOrg(playerid)) return SendClientMessage(playerid, Red,".אינך נמצא בשום ארגון");
		if(GetLevelOrg(playerid) < 2) return SendClientMessage(playerid, Red, ".(Tester) אתה לא נמצא בדרגת טסטר או גבוהה יותר");
		new ID;
		if(sscanf(params[6],"i",ID)) return SendClientMessage(playerid, White, "Usage: /Org DelTag [ID]");
  		if(!strcmp(pInfo [ ID ] [ OrgTag ],"None",true)) return SendClientMessage(playerid, Red, ".אין לשחקן זה תאג בארגון");
		if(!IsPlayerConnected(ID)) return SendClientMessage(playerid, Red, ".משתמש לא מחובר");
		if(InOrg(playerid) != InOrg(ID)) return SendClientMessage(playerid, Red, ".שחקן זה לא נמצא בארגון שלך");
		if(pInfo [ playerid ] [ OrgLevel ] <= pInfo [ ID ] [ OrgLevel ] && ID != playerid) return SendClientMessage(playerid,Red, ".אין באפשרותך למחוק תאג למישהו ברמה זהה או יותר גבוהה משלך");
		S_SendMsg(playerid,Red, ".את התאג בארגון %sמחקת ל", GetName(ID));
		S_SendMsg(ID,Red, ".מחק לך את התאג בארגון %s", GetName(playerid));
		format( pInfo [ ID ] [ OrgTag ] , 5 , "None");
		new INI:File = INI_Open(PFile(ID));
	    INI_WriteString ( File , "OrgTag" ,"None");
	    INI_Close ( File );
		return 1;
	}
	if(!strcmp(command, "invite", true))
	{
	    if(!InOrg(playerid)) return SendClientMessage(playerid, Red, ".אינך נמצא בשום ארגון");
		new ID;
    	if(sscanf(params[6],"i",ID)) return SendClientMessage(playerid, White, "Usage: /Org Invite [ID]");
		if(GetLevelOrg(playerid) < 2) return SendClientMessage(playerid, Red, ".(Tester) אתה לא נמצא בדרגת טסטר או גבוהה יותר");
		if(!IsPlayerConnected(ID)) return SendClientMessage(playerid, Red, ".המשתמש אינו מחובר");
		if(InOrg(playerid) == InOrg(ID))return SendClientMessage(playerid, Red, ".שחקן זה נמצא כבר בארגון שלך");
		InInviteOrg[ID] = 1;
		InviteOrg[ID] = InOrg(playerid);
		S_SendMsg(ID,0x80800000,"%s על ידי השחקן %s הוזמנת לארגון", GetName(playerid), OrgPlayer(playerid));
		SendClientMessage(ID,0x80800000, "/Org Join - להצטרפות לארגון רשום");
		S_SendMsg(playerid,0x80800000, ".לארגון שלך %s הזמנת את השחקן", GetName(ID));
		return 1;
	}
	if(!strcmp(command, "join", true))
	{
		if(InOrg(playerid)) return SendClientMessage(playerid,Red, ".אתה נמצא בארגון, עליך לעזוב את הארגון הנוכחי שלך");
		if(!InInviteOrg[playerid]) return SendClientMessage(playerid,Red, ".לא הוזמנת לשום ארגון");
  		pInfo [ playerid ] [ OrgID ] = InviteOrg[playerid];
		pInfo [ playerid ] [ OrgLevel ] = 1;
		pInfo [ playerid ] [ OrgSerial ] = SerialNumber_Org[ InviteOrg[playerid] ];
		format( pInfo [ playerid ] [ OrgTag ] , 5 , "None");
		InInviteOrg[playerid] = 0;
		InviteOrg[playerid] = 0;
		new INI:File = INI_Open(PFile(playerid));
    	INI_WriteInt ( File, "OrgID",pInfo [ playerid ] [ OrgID ]);
    	INI_WriteInt ( File, "OrgLevel",1);
    	INI_WriteInt ( File, "OrgSerial",pInfo [ playerid ] [ OrgSerial ]);
	    INI_WriteString ( File , "OrgTag" ,"None");
    	INI_Close ( File );
		format(String, sizeof(String), "%s - הצטרף לארגון שלך %s השחקן", OrgPlayer(playerid), GetName(playerid));
		SendOrgMessage(InOrg(playerid),lBlue, String);
		return 1;
	}
	if(!strcmp(command, "quit", true))
	{
		if(!InOrg(playerid)) return SendClientMessage(playerid, Red, ".אינך נמצא בשום ארגון");
		format(String, sizeof(String), "%s - עזב את הארגון שלך %s השחקן", OrgPlayer(playerid), GetName(playerid));
		SendOrgMessage(InOrg(playerid),Red, String);
		pInfo [ playerid ] [ OrgID ] = 0;
		pInfo [ playerid ] [ OrgLevel ] = 0;
		pInfo [ playerid ] [ OrgSerial ] = 0;
		format( pInfo [ playerid ] [ OrgTag ] , 5 , "None");
		new INI:File = INI_Open(PFile(playerid));
	    INI_WriteInt ( File, "OrgID",0);
    	INI_WriteInt ( File, "OrgLevel",0);
    	INI_WriteInt ( File, "OrgSerial",0);
	    INI_WriteString ( File , "OrgTag" ,"None");
	    INI_Close ( File );
		SetPlayerTeam(playerid, NO_TEAM);
		return 1;
	}
	if(!strcmp(command, "setlevel", true))
	{
		if(!InOrg(playerid))return SendClientMessage(playerid, Red, ".אינך נמצא בשום ארגון");
		if(GetLevelOrg(playerid) < 4)return SendClientMessage(playerid,Red, ".(Leader) אתה לא נמצא בדרגת מנהל או גבוהה יותר");
  		new levelorg,
			ID;
    	if(sscanf(params[8],"ii",ID,levelorg)) return SendClientMessage(playerid,White, "Usage: /Org SetLevel [ID] [1-4]");
		if(levelorg < 0 || levelorg > 4)return SendClientMessage(playerid,Red, ".דרגה זו לא קיימת");
		if(!IsPlayerConnected(ID))return SendClientMessage(playerid, Red, ".המשתמש אינו מחובר");
		if(InOrg(playerid) != InOrg(ID)) return SendClientMessage(playerid, Red, ".שחקן זה לא נמצא בארגון שלך");
		if(GetLevelOrg(playerid) == levelorg)return SendClientMessage(playerid, Red, ".אין באפשרותך לשים דרגה אשר זהה לשלך");
		if(GetLevelOrg(playerid) < levelorg)return SendClientMessage(playerid, Red, ".אתה לא יכול לערוך דרגה שיותר גבוהה מהדרגה הנוכחית שלך");
		if(GetLevelOrg(playerid) == GetLevelOrg(ID))return SendClientMessage(playerid,Red, ".אתה לא יכול לערוך דרגה לשחקן אשר נמצא בדרגה שלך");
		if(GetLevelOrg(ID) > GetLevelOrg(playerid)) return SendClientMessage(playerid,Red, ".אתה לא יכול לערוך דרגה לשחקן אשר בדרגה יותר גבוהה משלך");
		format(String, sizeof(String),"%s - %s על ידי %s שונתה לדרגה %s הדרגה של", OrgPlayer(playerid),GetName(playerid), NameLevelFile(levelorg), GetName(ID));
		SendOrgMessage(InOrg(playerid), llOrange, String);
  		pInfo [ ID ] [ OrgLevel ] = levelorg;
		new INI:File = INI_Open(PFile( ID ));
    	INI_WriteInt ( File, "OrgLevel",pInfo [ ID ] [ OrgLevel ]);
    	INI_Close ( File );
		return 1;
	}
	if(!strcmp(command, "kick", true))
	{
		if(!InOrg(playerid))return SendClientMessage(playerid, Red, ".אינך נמצא בשום ארגון");
		if(GetLevelOrg(playerid) < 3)return SendClientMessage(playerid, Red, ".(Tester) אתה לא נמצא בדרגת טסטר או גבוהה יותר");
		new ID;
    	if(sscanf(params[4],"i",ID)) return SendClientMessage(playerid, White, "Usage: /Org Kick [ID]");
		if(!IsPlayerConnected(ID))return SendClientMessage(playerid, Red, ".המשתמש אינו מחובר");
		if(InOrg(playerid) != InOrg(ID)) return SendClientMessage(playerid, Red, ".שחקן זה לא נמצא בארגון שלך");
		if(GetLevelOrg(playerid) == GetLevelOrg(ID)) return SendClientMessage(playerid, Red, ".אתה לא יכול להעיף שחקן אשר נמצא בדרגה שלך");
		if(GetLevelOrg(ID) > GetLevelOrg(playerid)) return SendClientMessage(playerid, Red, ".אתה לא יכול להעיף שחקן אשר נמצא בדרגה יותר גבוהה משלך");
		format(String, sizeof(String), "%s - %s הועף מהארגון על ידי %s", OrgPlayer(playerid), GetName(playerid), GetName(ID));
		SendOrgMessage(InOrg(playerid),Red, String);
		SetPlayerTeam(ID,NO_TEAM);
		pInfo [ ID ] [ OrgID ] = 0;
        pInfo [ ID ] [ OrgLevel ] = 0;
        pInfo [ ID ] [ OrgSerial ] = 0;
        format( pInfo [ ID ] [ OrgTag ] , 5 , "None");
       	new INI:File = INI_Open(PFile(ID));
    	INI_WriteInt ( File, "OrgID",0);
    	INI_WriteInt ( File, "OrgLevel",0);
    	INI_WriteInt ( File, "OrgSerial",0);
    	INI_WriteString ( File , "OrgTag" ,"None");
	    INI_Close ( File );
		return 1;
	}
	if(!strcmp(command, "info", true))
	{
		new NameOrg_O[50];
    	if(sscanf(params[4],"s[50]",NameOrg_O)) return SendClientMessage(playerid, White, "Usage: /Org Info [Org Name]");
		new OrgId;
		for(new i = 1; i < MAX_CLANS; i++) if(!strcmp(NameOrg[i],NameOrg_O, true) && fexist(OrgFile(i))) OrgId = i;
		if(!OrgId && !fexist(OrgFile(OrgId))) return SendClientMessage(playerid,Red, ".לא קיים ארגון בשם זה");
		new Str1[2000];
		format(String, sizeof(String), "{66FF99}Org Info [Name Org:{FF6600} %s{66FF99} - Org ID:{"#C_Red"} %d{66FF99}]\n\n",NameOrg[ OrgId ],OrgId);
		strcat(Str1, String, 2000);
		new PlayersConncect;
        foreach(new i: Player)
        {
			if(InOrg(i) == OrgId)
			{
			    PlayersConncect++;
				format(String, sizeof(String), "{66CCCC}%d. %s [ID: %d | {"#C_lllGreen"}%s{66CCCC}]\n",PlayersConncect,GetName(i),i,NameLevelFile( pInfo [ i ] [ OrgLevel ] ));
				strcat(Str1, String, 2000);
			}
		}
		if(!PlayersConncect) strcat(Str1, "{"#C_Red"}.אין שחקני ארגון מחוברים\n", 2000);
		else
		{
			format(String,sizeof(String), "\n{33CC00}%d :מחוברים",PlayersConncect);
			strcat(Str1,String, 2000);
		}
		ShowPlayerDialog(playerid, 98211,DIALOG_STYLE_MSGBOX,"{"#C_Yellow"}Org Info", Str1,"אישור", "");
		return 1;
	}
	if(!strcmp(command,"list", true))
	{
		new szStr[1500],Orgs;
		new bool: colorlist;
		for(new i = 1; i < 250; i++)
		{
			if(fexist(OrgFile(i)))
			{
    			if(colorlist == true) colorlist = false;
		    	else colorlist = true;
				Orgs++;
				format(String, sizeof(String), "%s %d. Org: %s - Org ID: %d - Connected: %d - Org Founder: %s\n",ReturnColor(colorlist?(lBlue):(White)),Orgs,NameOrg[ i ], i,GetOrgConnected(i),FounderOrg[ i ]);
				strcat(szStr,String,sizeof(szStr));
			}
		}
		if(!Orgs) strcat(szStr, "{"#C_Red"}.אין ארגונים בשרת\n",sizeof(szStr));
		ShowPlayerDialog(playerid, 1492, DIALOG_STYLE_MSGBOX ,"{00B2EE}Server Organizations List - רשימת הארגונים בשרת", szStr,"אישור","");
		return 1;
	}
	if(!strcmp(command,"Help", true))
	{
		new Organizations_System[][] =
		{
			{"{9E9E9E}:בשרת זה קיימת מערכת ארגונים\n"},
			{"{"#C_White"}הסבר על המערכת: במערכת הארגונים תוכלו לפתוח ארגון משלכם ולצרף חברים לארגון ולהיות ארגון בשרת"},
			{"{"#C_White"}מתי שיש לכם ארגון משלכם אתם תוכלו להוכיח לשאר הארגונים שאתם הארגון הכי טוב בשרת\n"},
