	SendClanMessageEx(InClan(playerid),0x4CA4E400, "(Clan %s%s{4CA4E4}) .הפקיד בבנק הקלאן את הסכום %d$ %s",ColorClan(InClan(playerid)),ClanInfo [ InClan(playerid) ] [ ClanName ],Amount_C,GetName(playerid));
	S_SendMsg(playerid, 0x4CA4E400 ,".%d$ :הפקדת בחשבון הבנק קלאן את הסכום", Amount_C);
	return 1;
}
