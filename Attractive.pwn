new CSClock;
CMD:cstaffs(playerid, params[])
{
	if(LevelAdmin(playerid) < 20) return 0;
 	if(AdmLog[playerid] == false)return SendClientMessage(playerid,Red,"ERROR: אינך מחובר למערכת אדמינים");
	for(new i, j = GetPlayerPoolSize(); i <= j; i++)
	{
		if(!IsPlayerConnected(i) || logged[i] == false || !fexist(PFile(i))) continue;
		format(pInfo [ i ] [ ClockStaff ], 20 , "%d",strval(pInfo [ i ] [ ClockStaff ]) + (gettime() - ClockStaff1[i]));
  		ClockStaff1[i] = gettime();
  		new INI:File = INI_Open(PFile(i));
  		INI_WriteString ( File , "ClockStaff" , pInfo [ i ] [ ClockStaff ]);
  		INI_Close ( File );
	}
	new CStaff,
		catstr[1000],
		bool: colorlist;
	for ( new i; i < MAX_Staff; i ++)
	{
 		if(fexist(PStaff(i)))
		{
			inline LoadS(string:name[], string:value[]) INI_Int("UserID",id);
			INI_ParseFile(PStaff(i), using inline "LoadS");
			inline Loadusersu(string:name[], string:value[]) INI_String("NameUser",NameUserID,25);
			INI_ParseFile(GetUserID(id), using inline "Loadusersu");
  			if(fexist(PlayerNameFile(NameUserID)))
    		{
				inline LoadSS(string:name[], string:value[]) INI_Int("ClockStaff",CSClock);
				INI_ParseFile(PlayerNameFile(NameUserID), using inline "LoadSS");
			}
			if(colorlist == true) colorlist = false;
  			else colorlist = true;
			CStaff++;
			format(String,sizeof(String), "%s%d. %s [Time: %s]\n",ReturnColor(colorlist?(Grey):(White)),CStaff,NameUserID,GetTimeN(CSClock));
            strcat(catstr, String);
		}
	}
    ShowPlayerDialog(playerid, 98211, DIALOG_STYLE_MSGBOX,"שעות משחק", catstr, "סגור", "");
	if(!CStaff) SendClientMessage(playerid,Red,".אין צוותים בשרת המוגדרים כצוותים רשמיים");
	return 1;
}
