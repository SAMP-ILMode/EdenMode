stock StaffServer(playerid)
{
	new StaffID = -1;
	for ( new i; i < MAX_Staff; i ++)
	{
 		if(fexist(PStaff(i)))
		{
			inline LoadS(string:name[], string:value[]) INI_Int("UserID",id);
			INI_ParseFile(PStaff(i), using inline "LoadS");
   			if(pInfo [ playerid ] [ UserID ] == id)
      		{
        		StaffID = i;
          		break;
			}
		}
	}
	if(pInfo [ playerid ] [ VIPLevel ] != 6 && pInfo [ playerid ] [ ClanAccess ] == false && pInfo [ playerid ] [ houseaccess ] == false && !LevelAdmin(playerid) && pInfo [ playerid ] [ Staff ] == false && pInfo [ playerid ] [ BroadCasterAccess ] == false && !pInfo [ playerid ] [ NightTeam ])
	{
 		if(StaffID != -1) fremove(PStaff(StaffID));
	}
	else if(pInfo [ playerid ] [ VIPLevel ] == 6 || pInfo [ playerid ] [ ClanAccess ] == true || pInfo [ playerid ] [ houseaccess ] == true || LevelAdmin(playerid) || pInfo [ playerid ] [ Staff ] == true || pInfo [ playerid ] [ BroadCasterAccess ] == true || pInfo [ playerid ] [ NightTeam ])
	{
		if(StaffID == -1)
		{
   			for (new i; i < MAX_Staff; i ++)
			{
    		    if(!fexist(PStaff(i)))
				{
					new INI:File = INI_Open(PStaff(i));
    		        INI_WriteInt(File, "UserID",pInfo [ playerid ] [ UserID ]);
					INI_Close ( File );
					format(pInfo [ playerid ] [ ClockStaff ], 2 , "0");
					ClockStaff1[ playerid ] = gettime();
					break;
				}
			}
		}
	}
	return 1;
}

