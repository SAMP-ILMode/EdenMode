function fBoxing(playerid,num)
{
	if(num == 0)
	{
        TextDrawShowForPlayer(playerid,Boxing[0]);
        SetTimerEx ( "fBoxing" , 100 , false, "ii" , playerid ,1);
        PlayerPlaySound(playerid,1190,0.0,0.0,0.0);
	}
	else if(num == 1)
	{
        TextDrawHideForPlayer(playerid,Boxing[0]);
	    TextDrawShowForPlayer(playerid,Boxing[1]);
		SetTimerEx ( "fBoxing" , 100 , false, "ii" , playerid ,2);
        PlayerPlaySound(playerid,1190,0.0,0.0,0.0);
	}
	else if(num == 2)
	{
	    TextDrawHideForPlayer(playerid,Boxing[1]);
	    TextDrawShowForPlayer(playerid,Boxing[2]);
        SetTimerEx ( "fBoxing" , 100 , false, "ii" , playerid ,3);
        PlayerPlaySound(playerid,1190,0.0,0.0,0.0);
	}
	else if(num == 3)
	{
        TextDrawHideForPlayer(playerid,Boxing[2]);
	    TextDrawShowForPlayer(playerid,Boxing[3]);
        SetTimerEx ( "fBoxing" , 100 , false, "ii" , playerid ,4);
        PlayerPlaySound(playerid,1190,0.0,0.0,0.0);
	}
	else if(num == 4)
	{
	    TextDrawHideForPlayer(playerid,Boxing[3]);
        TextDrawShowForPlayer(playerid,Boxing[4]);
        SetTimerEx ( "fBoxing" , 1500 , false, "ii" , playerid ,5);
        PlayerPlaySound(playerid,1190,0.0,0.0,0.0);
	}
	else if(num == 5) TextDrawHideForPlayer(playerid,Boxing[4]);
    return 1;
}
