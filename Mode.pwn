CMD:capture(playerid, params[])
{
	new Mera[5];
	if(sscanf(params,"s[5]",Mera)) return SendClientMessage(playerid, -1,"Usage: /Capture [ List / Area / Stop]");
	new str[256];
	if(!strcmp(Mera, "list", true))
	{
		new
			count = 0,
			ZsT[24]
		;
		for(new i; i < sizeof(caInfo); i++)
		{
			format(ZsT, 24, "Clans/%s.ini",caVar[i][caOwn]);
		    count++;
			if(!strcmp(caVar[i][caOwn], "n.u.l.l", false)) format(str, sizeof(str),"%s{%s}» %d. Name: • %s Earn: %s$ • Time: 02:00 • Clan Owner: שום קלאן\n",str, CAColor(i),i, caInfo[i][caName], PSIK(caInfo[i][caEarn]));
			else format(str, sizeof(str),"%s{%s}» %d. Name: • %s Earn: %s$ • Time: 02:00 • Clan Owner: {%s}%s\n",str, CAColor(i),i, caInfo[i][caName], PSIK(caInfo[i][caEarn]), GetHColor(rgba2hex(DOF2_GetInt(ZsT,"R"), DOF2_GetInt(ZsT, "G"), DOF2_GetInt(ZsT, "B"),100)),caVar[i][caOwn]);
		}
		new StR[30];
		format(StR, sizeof(StR),""#Blue":%d אזורי השתלטויות",count);
		ShowPlayerDialog(playerid, 313, DIALOG_STYLE_MSGBOX, StR, str, "אישור", "");
		if(!count) return ShowPlayerDialog(playerid, 313, DIALOG_STYLE_MSGBOX, String, ""#Red" - אין כעת אזור השתלטויות -", "אישור", "");
	}

    else if(!strcmp(Mera, "area", true))
	{
		if(DOF2_GetBool(PFile(playerid),"HaveClan") == false) return SendClientMessage(playerid,-1,""#Red".אינך נמצא בקלאן");
		if(DOF2_GetInt(PFile(playerid),"ClanLevel") < 4) return SendClientMessage(playerid,-1,""#Red".אינך לידר בקלאן");
		new ZsT[24];
		format(ZsT, 24, "Clans/%s.ini", GetClanName(playerid));
		if(DOF2_GetInt(ZsT, "InCapture")) return SendClientMessage(playerid, -1, ""#Red"!אתם כבר משתלטים על אזור");
		if(InArea[playerid] == -1) return SendClientMessage(playerid, -1, ""#Red"!אינך נמצא בשום אזור");
		if(!strcmp(caVar[InArea[playerid]][caOwn], GetClanName(playerid), false)) return SendClientMessage(playerid, -1, ""#Red".אזור זה נשלט כבר על ידי הקלאן שלך");
		new
			count,
			Float:Pos[3]
		;
		GetPlayerPos(playerid, Pos[0], Pos[1], Pos[2]);
		for(new i; i <= GetPlayerPoolSize(); i++)
		{
	        if(IsPlayerInArea(i, caInfo[InArea[playerid]][xMiny][0], caInfo[InArea[playerid]][xMiny][1], caInfo[InArea[playerid]][xMiny][2], caInfo[InArea[playerid]][xMiny][3]))
	        {
				if(!strcmp(GetClanName(playerid), GetClanName(i)))
				{
	            	count++;
				}
			}
		}
		if(count < 3) return SendClientMessage(playerid ,-1, ""#Red"כמות שחקנים דרושה לכבישה: 3#");
		SendFormatMessageToAll(-1, ""#Orange"->"#LGreen" .החל השתלטות על {%s}%s"#LGreen", מספר משתתפים: "#Red"%d {%s}%s "#LGreen"הקלאן",CAColor(InArea[playerid]),caInfo[InArea[playerid]][caName],count,GetHColor(rgba2hex(DOF2_GetInt(ZsT,"R"), DOF2_GetInt(ZsT, "G"), DOF2_GetInt(ZsT, "B"),100)),GetClanName(playerid));
		if(fexist(caVar[InArea[playerid]][caOwn]))
		{
			SendFormatMessageToAll(-1, ""#Orange"-> "#LGreen"!{%s}%s "#LGreen":שבבעלות הקלאן",GetHColor(rgba2hex(DOF2_GetInt(ZsT,"R"), DOF2_GetInt(ZsT, "G"), DOF2_GetInt(ZsT, "B"),100)), caVar[InArea[playerid]][caOwn]);
		}
        GangZoneFlashForAll(caVar[InArea[playerid]][caGangZone], rgba2hex(DOF2_GetInt(ZsT,"R"), DOF2_GetInt(ZsT, "G"), DOF2_GetInt(ZsT, "B"),100));
		DOF2_SetInt(ZsT, "CaptPlayers", count);
		DOF2_SetBool(ZsT, "InCapture", true);
		caVar[InArea[playerid]][caTimer] = SetTimerEx("CaptureArea", 1000*90, false, "ii", playerid, InArea[playerid]);
	}
	return 1;
}
