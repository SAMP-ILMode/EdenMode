CMD:tbans(playerid, params[])
{
	if(IsAdmin(playerid) < 2) return 0;
	new str[600],count;
	for(new i = 1; i< MAX_TBANS; i++) if(fexist(TBanFile(i)))
	{
	    count++;
		format(str, 600, "%s\n"#Grey2"» %s (Reason: "#White"%s "#Grey2"| Date: "#White"%s "#Grey2" By: "#White"%s"#Grey2")",str,tBan[i][tPlayer],tBan[i][tReason],tBan[i][tDate],tBan[i][tAdmin]);
	}
	if(!count) return SendClientMessage(playerid, -1, ""#Red"אין שחקנים בהרחקה");
	ShowPlayerDialog(playerid, DIALOG_TBANS, DIALOG_STYLE_LIST, ""#TYellow"Ban List", str, "בחר", "ביטול");
	return 1;
}
