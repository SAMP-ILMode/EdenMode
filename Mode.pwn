function countdown_speed(playerid)
{
	if(!gSeconds{playerid})
	{
		KillTimer(gTimer{playerid});
		TogglePlayerControllable(playerid, true);
		PlayerPlaySound(playerid, 1083, 0.0, 0.0, 0.0);
		GetVehicleVelocity(GetPlayerVehicleID(playerid), gPositions[0], gPositions[1], gPositions[2]);
		GetVehiclePos(GetPlayerVehicleID(playerid), gPositions[3], gPositions[4], gPositions[5]);
		GetVehicleZAngle(GetPlayerVehicleID(playerid), gPositions[6]);
		return SetVehicleVelocity(GetPlayerVehicleID(playerid), (floatsin(gPositions[6] = 360 - gPositions[6], degrees) * (2000/100) + floatcos(gPositions[6], degrees) * 0 + gPositions[3]) - gPositions[3], (floatcos(gPositions[6] = 360 - gPositions[6], degrees) * (2000/100) + floatsin(gPositions[6], degrees) * 0 + gPositions[4]) - gPositions[4], gPositions[2]);
	}
	PlayerPlaySound(playerid, 1085, 0.0, 0.0, 0.0);
	format(String, sizeof(String), "~b~%d", gSeconds{playerid});
	return GameTextForPlayer(playerid, String, 1000, 3), gSeconds{playerid} -= 1;
}
