CMD:paintjob(playerid, params[])
{
	if(DOF2_GetBool(PFile(playerid), "HaveCar") == false) return SendClientMessage(playerid, 0xFF0000FF, ""#Red".אין ברכב בבעלותך");
    new Array[4];
	if(sscanf(params, "s[4] ", Array)) return SendClientMessage(playerid, -1, ""#Red"/PaintJob [ Sell / Buy ]");
	if(!strcmp(Array, "Buy", true))
	{
	    if(sscanf(params, "s[4]i", Array, params[0])) return SendClientMessage(playerid, -1, ""#Red"/PaintJob Buy [1-3]" );
		if(Y_GetPlayerMoney(playerid) < 90000)return SendClientMessage(playerid,0xFF0000C8,"90,000$ - מכיר פיינטג'וב הינו");
		if(params[0] < 1 || params[0] > 4) return SendClientMessage(playerid, -1, ""#Red"/PaintJob Buy [1-3)]" );
		switch(params[0])
		{
		    case 1:
		    {
				DOF2_SetInt(VFile(DOF2_GetInt(PFile(playerid),"VH")),"PaintJob",1);
				ChangeVehiclePaintjob(GetPlayerVehicleID(playerid), 0);
				SendClientMessage(playerid, -1, ""#Orange"קנית פיינטג'וב ב"#LGreen"90,000$"#Orange" מסוג "#White"1");
			}
		    case 2:
		    {
				DOF2_SetInt(VFile(DOF2_GetInt(PFile(playerid),"VH")),"PaintJob",2);
				ChangeVehiclePaintjob(GetPlayerVehicleID(playerid), 1);
				SendClientMessage(playerid, -1, ""#Orange"קנית פיינטג'וב ב"#LGreen"90,000$"#Orange" מסוג "#White"2");
			}
		    case 3:
		    {
				DOF2_SetInt(VFile(DOF2_GetInt(PFile(playerid),"VH")),"PaintJob",3);
				ChangeVehiclePaintjob(GetPlayerVehicleID(playerid), 2);
				SendClientMessage(playerid, -1, ""#Orange"קנית פיינטג'וב ב"#LGreen"90,000$"#Orange" מסוג "#White"3");
			}
		}
		Y_GivePlayerMoney(playerid, -90000);
	}
	else if(!strcmp(Array, "Sell", true))
	{
	    if(DOF2_GetInt(VFile(DOF2_GetInt(PFile(playerid),"VH")),"PaintJob") == 0) return SendClientMessage(playerid, -1, ""#Red".אין לך פיינטג'וב");
		SendClientMessage(playerid, -1, ""#Red"!מכרת את הצבע של הרכב שלך בהצלחה");
		DOF2_SetInt(VFile(DOF2_GetInt(PFile(playerid),"VH")),"PaintJob",0);
		ChangeVehiclePaintjob(GetPlayerVehicleID(playerid), 0);
	}
	else return SendClientMessage(playerid, -1, ""#Red"/PaintJob [ Sell / Buy ]");
	return 1;
}
