		if(!IsPlayerConnected(Id2))  return SendFormatMessage(playerid, -1, ""#Red"!איידי %d אינו מחובר",Id2);
		if(!IsPlayerConnected(Id3))  return SendFormatMessage(playerid, -1, ""#Red"!איידי %d אינו מחובר",Id3);
