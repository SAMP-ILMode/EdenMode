CMD:freeze(playerid, params[])
{
	if(!IsNighter(playerid) && !IsHelper(playerid) && IsAdmin(playerid) < 3) return 0;
	new id, reason[30];
	if(sscanf(params,"uz",id,reason)) return SendClientMessage(playerid,-1,"/(f)reeze [id] [reason]");
    if(isnull(reason)) reason = "האדמין לא הקליד סיבה";
	if(!IsPlayerConnected(id)) return SendClientMessage(playerid,-1,""#Red"האיידי אינו מחובר");
	SendFormatMessageToAll(-1,"*** %s Has been freeze by %s [%s]",pName[id],pName[playerid],reason);
	TogglePlayerControllable(id,0);
	return 1;
}
//==============================================================================
CMD:unf(playerid, params[]) return cmd_unfreeze(playerid,params);
CMD:unfreeze(playerid, params[])
{
	if(!IsNighter(playerid) && !IsHelper(playerid) && IsAdmin(playerid) < 3) return 0;
	new id;
	if(sscanf(params,"u",id)) return SendClientMessage(playerid,-1,"/(unf)reeze [id]");
	if(!IsPlayerConnected(id)) return SendClientMessage(playerid,-1,""#Red"האיידי אינו מחובר");
	SendFormatMessageToAll(-1,"*** %s Has been unfreeze by %s.",pName[id],pName[playerid]);
	TogglePlayerControllable(id,1);
	return 1;
}
