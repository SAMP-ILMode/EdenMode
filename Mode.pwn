	DOF2_SetInt(SERVER_FILE, "CarsBuys", DOF2_GetInt(SERVER_FILE, "CarsBuys")-1);
	DestroyVehicle(Vehicles[DOF2_GetInt(PFile(playerid),"VH")]);
	Delete3DTextLabel(News[TextCars][DOF2_GetInt(PFile(playerid),"VH")]);
	if(IsValidObject(News[NeonObject1][DOF2_GetInt(PFile(playerid),"VH")]))
	{
		DestroyObject(News[NeonObject1][DOF2_GetInt(PFile(playerid),"VH")]);
		DestroyObject(News[NeonObject2][DOF2_GetInt(PFile(playerid),"VH")]);
	}
	DOF2_SetBool(PFile(playerid), "HaveCar",false);
	DOF2_SetInt(PFile(playerid),"VH",0);
	DOF2_SetInt(VFile(DOF2_GetInt(PFile(playerid),"VH")),"OnOff",0);
    DOF2_RemoveFile(file);
	return SendClientMessage(playerid,0xFF8000C8,""#LYellow"» "#Blue".מכרת את הרכב בבעלותך וקבלת וחצי מהמחיר");
