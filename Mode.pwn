		{
			if(DOF2_GetBool(PFile(i),"HaveClan") == true && !strcmp(cname,GetClanName(i),true))
			{
				DOF2_SetBool(PFile(i),"HaveClan",false);
				DOF2_SetString(PFile(i), "Clan","אין");
				DOF2_SetInt(PFile(i), "ClanLevel",0);
				DOF2_SetInt(PFile(i), "ClanWarns",0);
				SetPlayerRandomColor(i);
			}
		}
		DOF2_RemoveFile(cfile);
	}
