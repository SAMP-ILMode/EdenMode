			{
				SetPlayerInterior(playerid, 0);
				SetPlayerVirtualWorld(playerid, 0);
                ResetPlayerWeapons(playerid);
				OnPlayerSpawn(playerid);
				InCTraining[playerid] = false;
		        for(new i; i <= GetPlayerPoolSize(); i++) if(!strcmp(GetClanName(playerid), GetClanName(i), true))
				{
					SendFormatMessage(i, -1,""#Red"• (Clan: {%s}%s"#Red"): !יצא מאזור הטריינינג רום %s הלידר",GetHColor(rgba2hex(DOF2_GetInt(str,"R"), DOF2_GetInt(str, "G"), DOF2_GetInt(str, "B"),100)),GetClanName(playerid),pName[playerid]);
