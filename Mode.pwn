	if(newkeys & KEY_FIRE || newkeys == 128 && IsPlayerInAnyVehicle(playerid))
	{
		if(Vehicles[DOF2_GetInt(PFile(playerid),"VH")] == GetPlayerVehicleID(playerid) && DOF2_GetInt(VFile(DOF2_GetInt(PFile(playerid),"VH")),"Nitro") == 1)
		{
	    	AddVehicleComponent(GetPlayerVehicleID(playerid), 1010);
		}
	}
	if(newkeys & KEY_FIRE || newkeys == 128)
	{
		if(GetPlayerVirtualWorld(playerid) == 123123)
		{
			ShowPlayerDialog(playerid, 2358, DIALOG_STYLE_MSGBOX, ""#TBlue"!שגיאה", ""#Red".אין אפשרות לירות באזור זה", "סגור", "");
			SetCameraBehindPlayer(playerid);
			ClearAnimations(playerid);
			OnPlayerSpawn(playerid);
		}
		else if(IsPlayerInArea(playerid,-1969.787, 266.6297, -1923.435, 325.8042) || IsPlayerInArea(playerid,-2595.542, 617.7316, -2537.602, 708.4658) || IsPlayerInArea(playerid,904.3843, 2301.02, 966.8618, 2374.514))
		{
			ShowPlayerDialog(playerid, 2358, DIALOG_STYLE_MSGBOX, ""#TBlue"!שגיאה", ""#Red".אין אפשרות לירות באזור זה", "סגור", "");
			SetCameraBehindPlayer(playerid);
			ClearAnimations(playerid);
			OnPlayerSpawn(playerid);
		}
	}

	if(PRESSED(KEY_FIRE) && CanRock[playerid])
	{
		new Float:Pos[4];
		GetPlayerPos(playerid, Pos[0], Pos[1], Pos[2]);
		GetPlayerFacingAngle(playerid, Pos[3]);
		Pos[3] +=270;
	    GetXYInFrontOfPlayer(playerid, Pos[0], Pos[1], 3);
	    Rock[playerid] = CreateObject(905, Pos[0], Pos[1], Pos[2], 0.0, 0.0, 0.0);
		Pos[3] += 90;
	    Pos[0] += (30 * floatsin(-Pos[3], degrees));
	    Pos[1] += (30 * floatcos(-Pos[3], degrees));
	    MoveObject(Rock[playerid], Pos[0], Pos[1], Pos[2], 10);
	    CanRock[playerid] = false;
	    SetTimerEx("RockExplode", 1500, false, "i", playerid);
	}
	if(newkeys & KEY_HANDBRAKE && BoomUse[playerid] == 1)
	{
		new Float:X, Float:Y, Float:Z;
		GetPlayerPos(playerid, X, Y, Z);
		CreateExplosion(X, Y+9, Z, 10, 15);
		SetPlayerHealth(playerid, 100);
	}
	if(newkeys & KEY_FIRE && BoomUse[playerid] == 1)
	{
		new Float:X, Float:Y, Float:Z;
		GetPlayerPos(playerid, X, Y, Z);
		CreateExplosion(X, Y, Z, 10, 15);
		SetPlayerHealth(playerid, 100);
	}
	if(takingselfie[playerid] == 1)
	{
		if(PRESSED(KEY_ANALOG_RIGHT))
		{
			GetPlayerPos(playerid,lX[playerid],lY[playerid],lZ[playerid]);
			static Float: n1X, Float: n1Y;
			if(Degree[playerid] >= 360) Degree[playerid] = 0;
			Degree[playerid] += selfSpeed;
			n1X = lX[playerid] + selfRadius * floatcos(Degree[playerid], degrees);
			n1Y = lY[playerid] + selfRadius * floatsin(Degree[playerid], degrees);
			SetPlayerCameraPos(playerid, n1X, n1Y, lZ[playerid] + selfHeight);
			SetPlayerCameraLookAt(playerid, lX[playerid], lY[playerid], lZ[playerid]+1);
			SetPlayerFacingAngle(playerid, Degree[playerid] - 90.0);
		}
		if(PRESSED(KEY_ANALOG_LEFT))
		{
			GetPlayerPos(playerid,lX[playerid],lY[playerid],lZ[playerid]);
			static Float: n1X, Float: n1Y;
			if(Degree[playerid] >= 360) Degree[playerid] = 0;
			Degree[playerid] -= selfSpeed;
			n1X = lX[playerid] + selfRadius * floatcos(Degree[playerid], degrees);
			n1Y = lY[playerid] + selfRadius * floatsin(Degree[playerid], degrees);
			SetPlayerCameraPos(playerid, n1X, n1Y, lZ[playerid] + selfHeight);
			SetPlayerCameraLookAt(playerid, lX[playerid], lY[playerid], lZ[playerid]+1);
			SetPlayerFacingAngle(playerid, Degree[playerid] - 90.0);
		}
	}
	if(PRESSED(KEY_FIRE)  && IsPlayerInAnyVehicle(playerid) && !InAct[playerid] && GetPlayerVirtualWorld(playerid) == 0 && HaveAJ[playerid] == true)
	{
		new Float:ajx, Float:ajy, Float:ajz, Float:StA;
		GetVehicleVelocity(GetPlayerVehicleID(playerid), ajx, ajy, ajz);
		StA = floatsqroot(floatpower(floatabs(ajx), 2.0) + floatpower(floatabs(ajy), 2.0) + floatpower(floatabs(ajz), 2.0)) * 179.28625;
		if(StA > 200.0) return 0;
		SetVehicleVelocity(GetPlayerVehicleID(playerid) ,ajx ,ajy ,ajz+0.5);
	}
	if(PRESSED(KEY_FIRE) && IsPlayerInAnyVehicle(playerid) && !InAct[playerid] && GetPlayerVirtualWorld(playerid) == 0 && HaveSH[playerid] == true)
	{
		new Float:shvx, Float:shvy, Float:shvz, Float:StH;
		GetVehicleVelocity(GetPlayerVehicleID(playerid), shvx, shvy, shvz);
		StH = floatsqroot(floatpower(floatabs(shvy), 2.0) + floatpower(floatabs(shvy), 2.0) + floatpower(floatabs(shvz), 2.0)) * 179.28625;
		if(StH > 400.0) return 0;
		SetVehicleSpeed(GetPlayerVehicleID(playerid) ,390);
	}
    return 0;
}

