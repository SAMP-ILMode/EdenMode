CMD:callv(playerid, params[])
{
	if(InAct[playerid]) return SendClientMessage(playerid, -1, ""#Red".לא ניתן לבצע פקודה זו במצבך הנוכחי");
	if(DOF2_GetBool(PFile(playerid), "HaveCar") == false) return SendClientMessage(playerid, 0xFF0000FF, ".אין ברכב בבעלותך");
	if(IsPlayerInAnyVehicle(playerid)) return SendClientMessage(playerid, 0xFF0000FF, ".אתה כבר נמצא בתוך רכב");
	for(new i; i <= GetPlayerPoolSize(); i++)
	{
		if(IsPlayerInAnyVehicle(i) && GetPlayerVehicleID(i) == Vehicles[DOF2_GetInt(PFile(playerid),"VH")])
		{
		    RemovePlayerFromVehicle(i);
		    SendClientMessage(i, -1, ""#Blue":] בעל הרכב שיגר את רכבו, הליכה נעימה");
		}
	}
	TogglePlayerControllable(playerid,1);
    GetPlayerPos(playerid,pCInfo[playerid][PosP][0],pCInfo[playerid][PosP][1],pCInfo[playerid][PosP][2]);
    GetPlayerFacingAngle(playerid,pCInfo[playerid][PosP][3]);
    SetVehiclePos(Vehicles[DOF2_GetInt(PFile(playerid),"VH")],pCInfo[playerid][PosP][0],pCInfo[playerid][PosP][1],pCInfo[playerid][PosP][2]);
	SetVehicleZAngle(Vehicles[DOF2_GetInt(PFile(playerid),"VH")],pCInfo[playerid][PosP][3]);
	PutPlayerInVehicle(playerid,Vehicles[DOF2_GetInt(PFile(playerid),"VH")],0);
	switch(DOF2_GetInt(VFile(DOF2_GetInt(PFile(playerid),"VH")),"PaintJob"))
	{
	    case 1:ChangeVehiclePaintjob(GetPlayerVehicleID(playerid), 0);
	    case 2:ChangeVehiclePaintjob(GetPlayerVehicleID(playerid), 1);
	    case 3:ChangeVehiclePaintjob(GetPlayerVehicleID(playerid), 2);
	}
	switch(DOF2_GetInt(VFile(DOF2_GetInt(PFile(playerid),"VH")),"Wheels"))
	{
		case 1:AddVehicleComponent(Vehicles[DOF2_GetInt(PFile(playerid),"VH")], 1080);
		case 2:AddVehicleComponent(Vehicles[DOF2_GetInt(PFile(playerid),"VH")], 1074);
		case 3:AddVehicleComponent(Vehicles[DOF2_GetInt(PFile(playerid),"VH")], 1079);
		case 4:AddVehicleComponent(Vehicles[DOF2_GetInt(PFile(playerid),"VH")], 1025);
		case 5:AddVehicleComponent(Vehicles[DOF2_GetInt(PFile(playerid),"VH")], 1073);
		case 6:AddVehicleComponent(Vehicles[DOF2_GetInt(PFile(playerid),"VH")], 1075);
		case 7:AddVehicleComponent(Vehicles[DOF2_GetInt(PFile(playerid),"VH")], 1076);
		case 8:AddVehicleComponent(Vehicles[DOF2_GetInt(PFile(playerid),"VH")], 1077);
		case 9:AddVehicleComponent(Vehicles[DOF2_GetInt(PFile(playerid),"VH")], 1078);
		case 10:AddVehicleComponent(Vehicles[DOF2_GetInt(PFile(playerid),"VH")], 1081);
		case 11:AddVehicleComponent(Vehicles[DOF2_GetInt(PFile(playerid),"VH")], 1082);
		case 12:AddVehicleComponent(Vehicles[DOF2_GetInt(PFile(playerid),"VH")], 1083);
		case 13:AddVehicleComponent(Vehicles[DOF2_GetInt(PFile(playerid),"VH")], 1084);
		case 14:AddVehicleComponent(Vehicles[DOF2_GetInt(PFile(playerid),"VH")], 1085);
		case 15:AddVehicleComponent(Vehicles[DOF2_GetInt(PFile(playerid),"VH")], 1096);
		case 16:AddVehicleComponent(Vehicles[DOF2_GetInt(PFile(playerid),"VH")], 1097);
	}
    DOF2_SetString(VFile(DOF2_GetInt(PFile(playerid),"VH")),"Owner",pName[playerid]);
    if(DOF2_GetInt(VFile(DOF2_GetInt(PFile(playerid),"VH")),"HaveHyd") == 1) AddVehicleComponent(GetPlayerVehicleID(playerid), 1087);
	new str[190];
	format(str,sizeof(str),""#LYellow"• ["#White"%s"#LYellow"] :סוג הרכב\n"#LYellow"• [{%06x}%s"#LYellow"]: בבעלות השחקן",DOF2_GetString(VFile(DOF2_GetInt(PFile(playerid),"VH")),"vName"),GetPlayerColor(playerid) >>> 8,pName[playerid]);
	Update3DTextLabelText(News[TextCars][DOF2_GetInt(PFile(playerid),"VH")], 0x00FF00FF, str);
	return SendClientMessage(playerid, 0xFFFF00FF, ""#LYellow"» "#Blue".זמנת בהצלחה את רכבך"), 1;
