public OnPlayerShootPlayer(Shooter,Target,Float:HealthLost,Float:ArmourLost)
{
	if(!strcmp(GetClanName(Shooter),GetClanName(Target),true) && DOF2_GetBool(cfile,"TKOn") == true)
	{
		SendClientMessage(Shooter, -1, ""#Red"!אסור לירות על חברים קלאנים");
		if(Shooter != INVALID_PLAYER_ID)
		{
		    if(GetPlayerTeam(Shooter) == GetPlayerTeam(Target))
			{
				new Float:hp;
				GetPlayerHealth(Target, hp);
				SetPlayerHealth(Target, hp + HealthLost);
				SetPlayerHealth(Shooter, 0);
			}
		}
	}
	if(TK == 1 && InSAct[Shooter])
	{
		if(Shooter != INVALID_PLAYER_ID)
		{
		    if(GetPlayerTeam(Shooter) == GetPlayerTeam(Target))
			{
				new Float:hp;
				GetPlayerHealth(Target, hp);
				SetPlayerHealth(Target, hp + HealthLost);
				SetPlayerHealth(Shooter, 0);
			}
		}
	}
	PlayerShoot[Target] = Shooter;
	return 1;
}

