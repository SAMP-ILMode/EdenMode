		}
		else if(!strcmp(edit, "Sell", true))
		{
			new str[90];
			format(str,90,"Clans/%s.ini",GetClanName(playerid));
			if(DOF2_GetBool(str, "HaveTK") == false) return SendClientMessage(playerid,-1,""#Red".אין לך אנטי-טים קיל בקלאן");
            for(new i; i <= GetPlayerPoolSize(); i++) if(!strcmp(GetClanName(playerid), GetClanName(i), true)) SendFormatMessage(i, -1,""#Green"!מכר את מערכת האנטי-טים קיל %s הלידר",pName[playerid]);
			DOF2_SetBool(str, "HaveTK", false);
			Y_GivePlayerMoney(playerid, 125000/2);
		}
	}
    else if(!strcmp(Option, "Edit", true))
	{
		if(DOF2_GetBool(PFile(playerid),"HaveClan") == false) return SendClientMessage(playerid,-1,""#Red".אינך נמצא בקלאן");
		if(DOF2_GetInt(PFile(playerid),"ClanLevel") < 4) return SendClientMessage(playerid,-1,""#Red".אינך לידר בקלאן");
		new edit[10];
		if(sscanf(params,"s[7]s[6]",Option,edit)) return SendClientMessage(playerid, -1, "/Clan Edit [Color/Pos/VPos]");
		format(cfile,256,"Clans/%s.ini",GetClanName(playerid));
		if(!strcmp(edit, "Pos", true))
		{
			format(cfile, sizeof (cfile), "Clans/%s.ini", GetClanName(playerid));
			if(DOF2_GetBool(cfile,"HaveHQ") == false) return SendClientMessage(playerid, -1, ""#Red".אין לקלאן שלך מפקדה");
			new Float:HqPos[4];
			GetPlayerPos(playerid, HqPos[0], HqPos[1], HqPos[2]);
			GetPlayerFacingAngle(playerid, HqPos[3]);
			DOF2_SetFloat(cfile, "CpX", HqPos[0]);
			DOF2_SetFloat(cfile, "CpY", HqPos[1]);
			DOF2_SetFloat(cfile, "CpZ", HqPos[2]);
			DOF2_SetFloat(cfile, "CpA", HqPos[3]);
			DOF2_SetBool(cfile, "Cp",true);
			SendClientMessage(playerid, -1, ""#LGreen".שיניתה את מיקום ההישתגרות למפקדת הקלאן");
			DOF2_SaveFile();
		}
		else if(!strcmp(edit, "VPos", true))
		{
			if(DOF2_GetBool(PFile(playerid),"HaveClan") == false) return SendClientMessage(playerid,-1,""#Red".אינך נמצא בקלאן");
			if(DOF2_GetInt(PFile(playerid),"ClanLevel") < 4) return SendClientMessage(playerid,-1,""#Red".אינך לידר בקלאן");
			format(cfile, sizeof (cfile), "Clans/%s.ini", GetClanName(playerid));
			if(DOF2_GetBool(cfile,"HaveHQ") == false) return SendClientMessage(playerid, -1, ""#Red".אין לקלאן שלך מפקדה");
			new Float:VHqPos[3];
			GetPlayerPos(playerid, VHqPos[0], VHqPos[1], VHqPos[2]);
			DOF2_SetFloat(cfile, "VCpX", VHqPos[0]);
			DOF2_SetFloat(cfile, "VCpY", VHqPos[1]);
			DOF2_SetFloat(cfile, "VCpZ", VHqPos[2]);
			DOF2_SetBool(cfile, "VCp",true);
			SendClientMessage(playerid, -1, ""#LGreen".שיניתה את מיקום ההישתגרות למפקדת הקלאן עם רכב");
			DOF2_SaveFile();
		}
		else if(!strcmp(edit, "Color", true))
		{
		 	new r, g, b;
			if(sscanf(params,"s[7]s[6]iii",Option,edit,r, g, b)) return SendClientMessage(playerid, -1, ""#Red"/Clan Edit Color [R] [G] [B] ");
			format(cfile, sizeof (cfile), "Clans/%s.ini", GetClanName(playerid));
			DOF2_SetInt(cfile, "R", r);
			DOF2_SetInt(cfile, "G", g);
			DOF2_SetInt(cfile, "B", b);
            for(new i; i <= GetPlayerPoolSize(); i++) if(!strcmp(GetClanName(playerid), GetClanName(i), true))
            {
                SendFormatMessage(i, rgba2hex(r,g,b,0),".ערך את צבע הקלאן לצבע הודעה זאת %s הלידר",pName[playerid]);
				SetPlayerColor(i, rgba2hex(r,g,b,0));
			}
		}
	}
	else return SendClientMessage(playerid, -1, ""#Red".צורת שימוש שגויה");
	return 1;
}
