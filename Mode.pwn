				ResetPlayerWeapons(i);
				switch(ClanWarType)
				{
				    case 1:
				    {
						GivePlayerWeapon(i, 27, 2000);
						GivePlayerWeapon(i, 24, 2000);
						GivePlayerWeapon(i, 31, 2000);
					}
				    case 2:
				    {
						GivePlayerWeapon(i, 26, 2000);
						GivePlayerWeapon(i, 23, 2000);
