CMD:stopcw(playerid,params[])
{
	if(!HaveClanWar) return SendClientMessage(playerid, -1, ""#Red"!אין קלאן וואר");
	SendClientMessageToAll(-1, ""#Blue"!האדמין סגר את הקלאן וואר");
    for(new i; i <= GetPlayerPoolSize(); i++)
    {
		if(InCWSpec[playerid])
		{
		    InCWSpec[playerid] = false;
		    SetCameraBehindPlayer(playerid);
			OnPlayerSpawn(playerid);
		}
        if(InCW[i])
        {
	        new zfile[32];
	        format(zfile, 32, "Clans/%s.ini", GetClanName(i));
	        DOF2_SetInt(zfile, "CWPlayers", 0);
			HaveClanWar = false;
	        InCW[i] = false;
			ResetPlayerWeapons(i);
			OnPlayerSpawn(i);
			CWInvite1 = -1;
			CWInvite2 = -1;
			CWInvite3 = -1;
		}
	}
	return 1;
}

