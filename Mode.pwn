	HQGateInfo[gateid][ModelID] = -1;
	HQGateInfo[gateid][Ox] = 0.0;
	HQGateInfo[gateid][Oy] = 0.0;
	HQGateInfo[gateid][Oz] = 0.0;
	HQGateInfo[gateid][CUp] = 0.0;
	HQGateInfo[gateid][RotX] = 0.0;
	HQGateInfo[gateid][RotY] = 0.0;
	HQGateInfo[gateid][RotZ] = 0.0;
	fremove(file);
	SendFormatMessage(playerid, -1, ""#Red"gate %d has been removed succeesfully!", gateid);
	return 1;
}
