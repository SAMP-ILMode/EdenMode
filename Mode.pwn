		ClanWarType = 0;
		HaveClanWar = false;
		CWInvite1 = -1;
		CWInvite2 = -1;
		CWInvite3 = -1;
	}
	else if(!strcmp(Option, "Spec", true))
	{
		if(!HaveClanWar) return SendClientMessage(playerid, -1, ""#Red"!אין כעת קלאן וואר");
