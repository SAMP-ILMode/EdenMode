		}
		case 2:
		{
			TeamB--, InTDM[playerid]= 0;
			SetPlayerSkin(playerid, PlayerSkin[playerid]);
			SetPlayerColor ( playerid, rgba2hex ( random ( 255 ), random ( 255 ), random ( 255 ), 100 ) );
			OnPlayerSpawn(playerid);
			SetPlayerVirtualWorld(playerid,0);
			SendClientMessage(playerid, 0xEEEE00FF, ""#Green".TeamDM "#Blue"- יצאת מאזור ה");
		}
	}
	return 1;
}
