		{
		    SendClientMessage(playerid, -1, ""#Red"!יצאת ממצב צפייה בקלאן וואר");
		    InCWSpec[playerid] = false;
		    SetCameraBehindPlayer(playerid);
		    OnPlayerSpawn(playerid);
		}
		else
		{
			switch(random(2))
			{
			    case 0:
			    {
					SetPlayerCameraPos(playerid, -3019.218017, 3383.442871, 93.338371);
					SetPlayerCameraLookAt(playerid, -3018.166259, 3383.305908, 88.452178);
			    }
			    case 1:
			    {
					SetPlayerCameraPos(playerid, -3047.728027, 3322.411376, 16.341609);
					SetPlayerCameraLookAt(playerid, -3044.369140, 3326.065429, 15.737622);
			    }
			}
		    SendClientMessage(playerid, -1, ""#Red"!נכנס למצב צפייה בקלאן וואר, ליציאה הקש פקודה זו שוב");
		    InCWSpec[playerid] = true;
		}
	}
	else return SendClientMessage(playerid, -1, "/CW [ Invite / Accept / Cancel / Spec ]");
	return 1;
}
