CMD:afk(playerid, params[])
{
    if(InAFK[playerid]) return SendClientMessage(playerid, -1, ""#Red"אתה כבר נמצא במצב לא נמצא");
	InAFK[playerid] = true;
	TogglePlayerControllable(playerid, 0);
	SendFormatMessage(playerid,-1, ""#Red"» (AFK)"#White" .עזב את לוח המקשים "#Red"%s "#White"השחקן",pName[playerid]);
	TimeInAFK[playerid] = 121;
	return 1;
}
