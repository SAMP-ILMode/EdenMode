CMD:hq(playerid,params[])
{
	if(DOF2_GetBool(PFile(playerid),"HaveClan") == false) return SendClientMessage(playerid,-1,""#Red".אינך נמצא בקלאן");
	format(cfile ,sizeof(cfile), "/Clans/%s.ini", GetClanName(playerid));
	if(DOF2_GetBool(cfile,"HaveHQ") == false) return SendClientMessage(playerid,-1,""#Red".אין לקלאן שלך מפקדה כעת");
	if(GetPlayerState(playerid) == PLAYER_STATE_DRIVER)
	{
		if(DOF2_GetBool(cfile,"VCp") == false) return SendClientMessage(playerid,-1,""#Red".אין לקלאן שלך מיקום שמור ברכב");
		SetVehiclePos(GetPlayerVehicleID(playerid), DOF2_GetFloat(cfile,"VCpX") , DOF2_GetFloat(cfile,"VCpY"), DOF2_GetFloat(cfile,"VCpZ"));
		SetVehicleZAngle(GetPlayerVehicleID(playerid), DOF2_GetFloat(cfile,"VCpA"));
	}
	else
	{
		if(DOF2_GetBool(cfile,"Cp") == false) return SendClientMessage(playerid,-1,""#Red".אין לקלאן שלך מיקום שמור ברגל");
		SetPlayerPosEx(playerid, DOF2_GetFloat(cfile,"CpX") , DOF2_GetFloat(cfile,"CpY"), DOF2_GetFloat(cfile,"CpZ"));
		SetPlayerFacingAngle(playerid, DOF2_GetFloat(cfile,"CpZ"));
	}
	return SendClientMessage(playerid,-1,""#Yellow"!ברוך הבא למפקדת הקלאן שלך");
}
