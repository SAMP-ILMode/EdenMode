CMD:admlog(playerid, params[])
{
	if(IsAdmin(playerid) < 1) return 0;
	if(IsConnectedAdmin[playerid] == true) return SendClientMessage(playerid,-1,""#Red".אתה מחובר למערכת האדמינים");
	new sDialog[4096],str[128];
	format(str, sizeof(str),""#AsO"•"#White" .וברוך שובך אל שרתינו אדמין יקר "#AsO"%s"#White" שלום לך\n",pName[playerid]);
	strcat(sDialog, str, sizeof(sDialog));
	strcat(sDialog, ""#AsO"•"#White" .המערכת זיהתה אותך בתור אדמין בקהילתנו\n", sizeof(sDialog));
	strcat(sDialog, ""#AsO"•"#White" .מפני שאתה אדמין עליך להתחבר למערכת אדמינים על מנת להמשיך לשחק\n", sizeof(sDialog));
	strcat(sDialog, ""#AsO"•"#White" .מערכת זו נוצרה על מנת למנוע פריצות של האדמינים וביצוע חבלות בשרת\n", sizeof(sDialog));
	format(str, sizeof(str),""#AsO"»"#White" כרגע המערכת זיהתה שהינך אדמין רמה "#AsO"%d"#White" ולכן תצטרך להתחבר למערכת האדמינים.\n",IsAdmin(playerid));
	strcat(sDialog, str, sizeof(sDialog));
	strcat(sDialog, ""#AsO"»"#White" .בכדי להתחבר למערכת האדמינים אנא הקשר את סיסמת האדמינים לפי הסיסמא שמנהל השרת קבע\n", sizeof(sDialog));
	strcat(sDialog, ""#AsO"»"#White" .שים לב שאם לא תקיש סיסמא נכונה 3 פעמים תקבל קיק מהשרת ותצטרך להתחבר שנית אל המערכת\n", sizeof(sDialog));
	strcat(sDialog, ""#AsO"»"#White" :אנא הכנס את סיסמת האדמין\n", sizeof(sDialog));
	ShowPlayerDialog(playerid,DIALOG_ADMINPASS,DIALOG_STYLE_INPUT,""#AsO"(("#White" :מערכת אדמינים "#AsO"))",sDialog,"אישור", "ביטול");
	return 1;
}
