CMD:votekick(playerid, params[])
{
	new Option[8];
	if(sscanf(params, "s[8] ", Option))
	{
		if(IsAdmin(playerid) > 2) return SendClientMessage(playerid, -1, ""#Red"/VoteKick [ Start / Send / Stop ]");
		if(IsLevel(playerid) < 7) return SendClientMessage(playerid,-1,""#HPurple"»"#TBlue" 7 "#TYellow":פקודה זו הינה מרמה");
		else return SendClientMessage(playerid, -1, ""#Red"/KickVote [Start/Send]");
	}
	if(!strcmp(Option, "Start", true))
	{
		if(IsLevel(playerid) < 7) return SendClientMessage(playerid,-1,""#HPurple"»"#TBlue" 7 "#TYellow":פקודה זו הינה מרמה");
		if(KickVotePlayer != -1) return SendClientMessage(playerid, -1, ""#Red".כרגע פועל הצבעת קיק נגד שחקן,המתן שהיא תיגמר");
		new Reason[50];
		if(sscanf(params, "s[6]us[50]", Option, Player, Reason)) return SendClientMessage(playerid, -1, ""#Red" /VoteKick Start [ID] [REASON]");
		if(!IsPlayerConnected(Player)) return SendClientMessage(playerid, -1, ""#Red".השחקן איננו מחובר");
		if(IsAdmin(Player) > 1 || IsActer(Player) > 1 || IsNighter(Player) > 1|| IsHelper(Player) > 1 || IsDJ(Player) > 1 || IsMClan(Player) > 1) return SendClientMessage(playerid, -1, ""#Red".שחקן זה הינו חבר צוות ולכן לא תוכל לעשות עליו הצבעה");
		KickVotePlayer = Player;
		KickVote = 0;
		for(new i=0; i<MAX_PLAYERS; i++) if(IsPlayerConnected(i) && InAFK[i]) KickVote++;
		KickVote = KickVote/3+1;
		KickPlayers = 1;
		IsKickVote[playerid] = true;
		KickVoteTimer = SetTimer("CancelKickVote", 2*60*1000, 0);
		SendFormatMessageToAll(-1,""#AsO"» "#TYellow":מהסיבה "#AsO"\"%s\""#TYellow" התחיל הצבעת קיק על השחקן "#AsO"\"%s\""#TYellow" השחקן",pName[Player],pName[playerid]);
		SendFormatMessageToAll(-1,""#AsO"» "#TYellow"["#AsO"%s"#TYellow"]",Reason);
		SendFormatMessageToAll(-1,""#AsO"» "#AsO"/VoteKick Send"#TYellow" דרוש "#AsO"%d"#TYellow" הצבועת כדי לבעוט שחקן זה, יש לכם שתי דקות להציע דרך הפקודה",KickVote);
	}
	else if(!strcmp(Option, "Send", true))
	{
		if(KickVotePlayer == -1) return SendClientMessage(playerid, -1, ""#Red".אין הצבעה פעילה");
		if(IsKickVote[playerid] == true) return SendClientMessage(playerid, -1, ""#Red".לא ניתן להצביע פעמיים");
		IsKickVote[playerid] = true;
		KickPlayers++;
		SendFormatMessageToAll(-1,""#AsO"» "#AsO"/KickVote Send"#TYellow":בכדי להציע "#AsO"\"%s\" הצביע לבעיטת השחקן "#AsO"\"%s\""#TYellow" השחקן",pName[KickVotePlayer],pName[playerid]);
		SendFormatMessageToAll(-1,""#AsO"» "#AsO"["#Red"%d:"#TYellow"מספר ההצבעות הדרושות כרגע"#AsO"]",KickVote-KickPlayers);
		if(KickPlayers >= KickVote)
		{
			KillTimer(KickVoteTimer);
			SendFormatMessageToAll(-1,""#AsO"» %d/%d"#TYellow" מהשרת בשל הצבעת קיק, ההצבעה עמדה על מספר שחקנים של "#AsO"\"%s\""#TYellow" הוחלט להוציא את השחקן",KickVote,KickPlayers,pName[KickVotePlayer]);
			SendFormatMessageToAll(-1,""#Red"\"%s\" has been kicked by "SERVER_BOT". [Vote Kick]", pName[KickVotePlayer]);
			Kick2(KickVotePlayer);
			for(new i=0; i<MAX_PLAYERS; i++) if(IsPlayerConnected(i) && IsKickVote[i] == true) IsKickVote[i] = false;
			KickVotePlayer = -1;
		}
	}
	else if(!strcmp(Option, "Stop", true) && IsAdmin(playerid) > 2)
	{
		if(KickVotePlayer == -1) return SendClientMessage(playerid, -1, ""#Red".אין הצבעה פעילה");
		KillTimer(KickVoteTimer);
		for(new i=0; i<MAX_PLAYERS; i++) if(IsPlayerConnected(i) && IsKickVote[i] == true) IsKickVote[i] = false;
		SendFormatMessageToAll(-1,""#Red"- \"%s\" סגר את ההצבעה נגד השחקן \"%s\" האדמין -", pName[KickVotePlayer], pName[playerid]);
		KickVotePlayer = -1;
	}
	else
	{
		if(IsAdmin(playerid) > 2) return SendClientMessage(playerid, -1, ""#Red"/VoteKick [Start/Send/Stop]");
		else SendClientMessage(playerid, -1, ""#Red"/KickVote [Start/Send]");
	}
	return 1;
}
