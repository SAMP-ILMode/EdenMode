CMD:createhqgate(playerid,params[])
{
	if(!IsMClan(playerid)) return 0;
	new
	   clan[90],
	   Model,
       Float:zOx,
       Float:zOy,
       Float:zOz,
       Float:zCUp,
       Float:zRx,
       Float:zRy,
       Float:zRz
    ;
	if(sscanf(params,"p<,>s[90]iffffffff",clan,Model,zOx,zOy,zOz,zCUp,zRx,zRy,zRz)) return SendClientMessage(playerid,-1,"/CreateHQGate [Clan] [Obj] [X] [Y] [Z] [Up] [RX] [RY] [RZ]");
	for(new i; i<MAX_HQ_RESCUE; i++)
	{
		new file[35];
		format(file, sizeof(file), "Clans/HQ/Gates/%d.ini", i);
		if(!fexist(file))
		{
			format(HQGateInfo[i][HQName], 90, "%s", clan);
			HQGateInfo[i][ModelID] = Model;
			HQGateInfo[i][Ox] = zOx;
			HQGateInfo[i][Oy] = zOy;
			HQGateInfo[i][Oz] = zOz;
			HQGateInfo[i][CUp] = zCUp;
			HQGateInfo[i][RotX] = zRx;
			HQGateInfo[i][RotY] = zRy;
			HQGateInfo[i][RotZ] = zRz;
			HQGateInfo[i][GateObject] = CreateObject(Model, zOx, zOy, zOz, zRx, zRy ,zRz);
			new str[280];
			format(str,sizeof(str),""#Yellow"• Clan Gate:"#Red" %s \n"#Yellow"• Gate ID:#%d",clan,i);
			GateText[i] = CreateDynamic3DTextLabel(str,-1,zOx, zOy, zOz, 50.0, INVALID_PLAYER_ID, INVALID_VEHICLE_ID, 0, 0, 0, -1, 15.0 );
			DOF2_CreateFile(file);
			DOF2_SetString(file, "Clan", clan);
			DOF2_SetInt(file, "ModelID",Model);
			DOF2_SetFloat(file, "X", zOx);
			DOF2_SetFloat(file, "Y", zOy);
			DOF2_SetFloat(file, "Z", zOz);
			DOF2_SetFloat(file, "CUp", zCUp);
			DOF2_SetFloat(file, "RX", zRx);
			DOF2_SetFloat(file, "RY", zRy);
			DOF2_SetFloat(file, "RZ", zRz);
			DOF2_SetInt(file, "GateObject", HQGateInfo[i][GateObject]);
		}
		break;
	}
	return 1;
}
