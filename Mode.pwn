		\n"#Blue"» (גודל: "#TBlue"%s(%d \
		\n"#Blue"» מצב הבית: "#TBlue"%s\
		\n"#Blue"» מאזן בבנק: "#TBlue"%s",hInfo[DOF2_GetInt(PFile(playerid), "House ID")][hID],
		PSIK(hInfo[DOF2_GetInt(PFile(playerid), "House ID")][hPrice]),
		GetHType(hInfo[DOF2_GetInt(PFile(playerid), "House ID")][hSize]),
		hInfo[DOF2_GetInt(PFile(playerid), "House ID")][hSize],
		!hInfo[DOF2_GetInt(PFile(playerid), "House ID")][hLock]?(""#Red"סגור"):(""#Green"פתוח"),
		PSIK(hInfo[DOF2_GetInt(PFile(playerid), "House ID")][hBank]));
		UpdateDynamic3DTextLabelText(hInfo[DOF2_GetInt(PFile(playerid), "House ID")][hText],-1,str);
		DOF2_SetInt(PFile(playerid), "House ID", 0);
	}
	else if(!strcmp(Mera, "lock", true, 4))
	{
		if(!DOF2_GetInt(PFile(playerid), "House ID")) return SendClientMessage(playerid, -1, ""#Red"!אין לך בית");
		if(hInfo[DOF2_GetInt(PFile(playerid), "House ID")][hLock] == false)
		{
		    SendClientMessage(playerid, -1, ""#Yellow"- .נעלת את הבית, בשביל לפתוח אנא הקש פקודה זו שנית");
		    hInfo[DOF2_GetInt(PFile(playerid), "House ID")][hLock] = true;
		}
		else
		{
			SendClientMessage(playerid, -1, ""#Yellow"- .פתחת את הבית, בשביל לסגור אנא הקש פקודה זו שנית");
		    hInfo[DOF2_GetInt(PFile(playerid), "House ID")][hLock] = false;
		}
	}
	else if(!strcmp(Mera, "quit", true, 4))
	{
		if(!InHouse[playerid]) return SendClientMessage(playerid, -1, ""#Red".אתה לא נמצא בשום בית");
		SetPlayerInterior(playerid, 0);
		SetPlayerVirtualWorld(playerid, 0);
		OnPlayerSpawn(playerid);
		InHouse[playerid] = 0;
		SendClientMessage(playerid, -1, ""#sGreen".יצאת מביתך");
	}
	else return SendClientMessage(playerid, -1,"Usage: /House [help]");
	return 1;
}
