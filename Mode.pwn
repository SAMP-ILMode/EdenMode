//==================================[Gangs]=====================================
CMD:gang(playerid, params[])
{
	new Option[7];
	if(sscanf(params, "s[9] ", Option)) return SendClientMessage(playerid, -1, "/Gang [Help/Create/Delete/Kick/Invite/Accept/Deny/Leave]");
    if(!strcmp(Option, "Help", true))
	{
	new pDialog[3000];
	strcat(pDialog, "\n"#White".בשרת ישנה מערכת גאנגים", sizeof(pDialog));
	strcat(pDialog, "\n"#White".מערכת הגאנגים נוצרה על מנת לדבר עם חברים בצ'אט קבוצתי", sizeof(pDialog));
	strcat(pDialog, "\n"#White".המערכת אינה נשמרת אחרי שיוצאים מהשרת", sizeof(pDialog));
	strcat(pDialog, "\n"#Green2":פקודות מערכת\n", sizeof(pDialog));
	strcat(pDialog, ""#Blue"•"#White" /Gang Create - יצירת גאנג חדש בשרת\n", sizeof(pDialog));
	strcat(pDialog, ""#Blue"•"#White" /Gang Delete - מחיקת גאנג בבעלותך\n", sizeof(pDialog));
	strcat(pDialog, ""#Blue"•"#White" /Gang Invite - הזמנת שחקן לגאנג\n", sizeof(pDialog));
	strcat(pDialog, ""#Blue"•"#White" /Gang Kick - הוצאת שחקן מהגאנג שלך\n", sizeof(pDialog));
	strcat(pDialog, ""#Blue"•"#White" /Gang Accept - אישור הזמנה לגאנג\n", sizeof(pDialog));
	strcat(pDialog, ""#Blue"•"#White" /Gang Deny - ביטול הזמנה לגאנג\n", sizeof(pDialog));
	strcat(pDialog, ""#Blue"•"#White" /Gang List - מספר הארגונים בשרת\n", sizeof(pDialog));
	strcat(pDialog, ""#Orange"•"#White" `@` - דיבור בצ'אט הגאנג\n", sizeof(pDialog));
	ShowPlayerDialog(playerid,DIALOG_SYSTEM,DIALOG_STYLE_MSGBOX,""#HPurple"Mode 22  -  גאנגים",pDialog,"חזרה", "ביטול");
	}
	else if(!strcmp(Option, "Create", true))
	{
	if(IsLevel(playerid) < 7) return SendClientMessage(playerid,-1,""#HPurple"»"#TBlue" 7 "#TYellow":פקודה זו הינה מרמה");
	if(IsPlayerInGang[playerid] == true) return SendClientMessage(playerid,-1,""#Red"/Gang Leave - אתה כבר בגאנג, ליציאה מגאנג הקלד");
	if(sscanf(params,"s[9]s[40]",Option,params[0])) return SendClientMessage(playerid,-1,"/Gang Create [Gang]");
	if(Y_GetPlayerMoney(playerid) < 4500) return SendClientMessage(playerid,-1,""#Red"4,500$ :המחיר ליצירת גאנג הינו");
	SendFormatMessageToAll(-1,""#LGreen"» ."#Grey"%s "#LGreen"פתח את הגאנג "#Grey"%s"#LGreen" השחקן",params[0], pName[playerid]);
	IsPlayerInGang[playerid] = true;
	DOF2_SetString(PFile(playerid),"Gang",params[0]);
	GangsInServer++;
	GangLevel[playerid] = 2;
	}
	else if(!strcmp(Option, "Delete", true))
	{
	if(IsPlayerInGang[playerid] == false) return SendClientMessage(playerid,-1,""#Red".אינך נמצא כעת בגאנג");
	if(GangLevel[playerid] < 2) return SendClientMessage(playerid,-1,""#Red".אינך לידר הגאנג");
	SendClientMessage(playerid,-1,""#Red"2,500$ :מכרת את הגאנג שלך וקבלת חצי מהמחיר");
	Y_GivePlayerMoney(playerid, 2500);
	SendFormatMessageToAll(-1,""#LGreen"» ."#Grey"%s "#LGreen"מחק את הארגון "#Grey"%s"#LGreen" השחקן",params[0], pName[playerid]);
	IsPlayerInGang[playerid] = false;
	DOF2_SetString(PFile(playerid),"Gang","אין");
	GangsInServer--;
	GangLevel[playerid] = 0;
	for(new i; i <= GetPlayerPoolSize(); i++)
	{
	if(IsPlayerConnected(i) && IsPlayerInGang[i] == true && !strcmp(DOF2_GetString(PFile(playerid),"Gang"),DOF2_GetString(PFile(i),"Gang"),true))
	{
	SendFormatMessage(i,-1,""#Red".מחק את הארגון %s השחקן",pName[playerid]);
	IsPlayerInGang[i] = false;
	GangLevel[i] = 0;
	DOF2_SetString(PFile(i),"Gang","אין");
	}
	}
	}
	else if(!strcmp(Option, "Leave", true))
	{
	if(GangLevel[playerid] == 2) return SendClientMessage(playerid,-1,""#Red"/Gang Delete - מפני שאתה מייסד הגאנג לא תוכל למחוקו, על מנת לצאת מהארגון");
	if(IsPlayerInGang[playerid] == false) return SendClientMessage(playerid,-1,""#Red".אינך נמצא כעת בגאנג");
	IsPlayerInGang[playerid] = false;
	GangLevel[playerid] = 0;
	DOF2_SetString(PFile(playerid),"Gang","אין");
	for(new i; i <= GetPlayerPoolSize(); i++)
	{
	if(IsPlayerConnected(i) && IsPlayerInGang[i] == true && !strcmp(DOF2_GetString(PFile(playerid),"Gang"),DOF2_GetString(PFile(i),"Gang"),true))
	{
	SendFormatMessage(i,-1,""#Red"- .עזב את הארגון %s השחקן",pName[playerid]);
	}
	}
	}
	else if(!strcmp(Option, "List", true))
	{
	SendFormatMessage(playerid,-1,""#Green"%d :מספר הגאנגים בשרת",GangsInServer);
	}
	else if(!strcmp(Option, "Invite", true))
	{
	if(IsPlayerInGang[playerid] == false) return SendClientMessage(playerid,-1,""#Red".אינך נמצא כעת בגאנג");
	if(GangLevel[playerid] < 2) return SendClientMessage(playerid,-1,""#Red".אינך לידר הגאנג");
	if(sscanf(params,"s[9]u",Option,params[0])) return SendClientMessage(playerid,-1,"/Gang Invite [id]");
	if(!IsPlayerConnected(params[0])) return SendClientMessage(playerid,-1,""#Red".השחקן איננו מחובר");
	if(IsInvGang[params[0]]) return SendClientMessage(playerid,-1,""#Red".השחקן כבר מוזמן לגאנג");
	IsInvGang[params[0]] = 1;
	GangInvID[params[0]] = playerid;
    SendFormatMessage(playerid,-1,""#Orange"» .לגאנג שלך %s הזמנת את השחקן",pName[params[0]]);
	SendFormatMessage(params[0],-1,""#LGreen"• /Gang Accept - לאישור /Gang Deny - לביטול ."#Grey"%s"#LGreen" זימן אותך אל הגאנג "#Grey"%s"#LGreen" השחקן",DOF2_GetString(PFile(playerid),"Gang"),pName[playerid]);
	}
	else if(!strcmp(Option, "Accept", true))
	{
	if(IsPlayerInGang[playerid] == true) return SendClientMessage(playerid,-1,""#Red"/Gang Leave - אתה כבר בגאנג, ליציאה מגאנג הקלד");
	if(!IsInvGang[playerid]) return SendClientMessage(playerid,-1,""#Red".אינך מוזמן לגאנג מסוים");
	for(new i; i <= GetPlayerPoolSize(); i++)
	{
	if(IsPlayerConnected(i) && IsPlayerInGang[i] == true && !strcmp(IsInvGang[playerid],DOF2_GetString(PFile(i),"Gang"),true))
	{
	SendFormatMessage(i,-1,""#Green2"+ .הצטרף אל הגאנג %s השחקן",pName[playerid]);
	}
	}
	IsInvGang[playerid] = 0;
    GangInvID[playerid] = 0;
	GangLevel[playerid] = 1;
	IsPlayerInGang[playerid] = true;
	}
	else if(!strcmp(Option, "Deny", true))
	{
	if(!IsInvGang[playerid]) return SendClientMessage(playerid,-1,""#Red".אינך מוזמן לגאנג מסוים");
	for(new i; i <= GetPlayerPoolSize(); i++)
	{
	if(IsPlayerConnected(i) && IsPlayerInGang[i] == true && !strcmp(IsInvGang[playerid],DOF2_GetString(PFile(i),"Gang"),true))
	{
	SendFormatMessage(i,-1,""#Red"- .דחה את ההזמנה לגאנג %s השחקן",pName[playerid]);
	}
	}
	IsInvGang[playerid] = 0;
    GangInvID[playerid] = 0;
    SendClientMessage(playerid,-1,""#Red".דחית את ההזמנה");
	}

	return 1;
}
