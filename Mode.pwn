CMD:nban(playerid, params[])
{
	if(IsAdmin(playerid) < 3) return 0;
	new reason[30];
	if(sscanf(params,"s[30]z",params[0],reason)) return SendClientMessage(playerid,-1,""#Red"/NBan [username] [reason]");
    if(isnull(reason)) reason = "האדמין לא הקליד סיבה";
	new file[30];
	format(file,sizeof(file),"Bans/%s.ini",params[0]);
	if(fexist(file)) return SendClientMessage(playerid, -1, ""#Red".שחקן זה כבר בבאן");
	SendFormatMessageToAll(-1,""#Red"*** %s has been banned by %s. [Reason: %s]",params[0],pName[playerid],reason);
	DOF2_CreateFile(file);
	DOF2_SetString(file,"Admin",pName[playerid]);
	DOF2_SetString(file,"IP",getip(params[0]));
	DOF2_SetString(file,"Reason",reason);
	DOF2_SaveFile();
	return 1;
}
//==============================================================================
CMD:ban(playerid, params[])
{
	if(IsAdmin(playerid) < 3) return 0;
	new id,reason[30];
	if(sscanf(params,"uz",id,reason)) return SendClientMessage(playerid,-1,""#Red"/Ban [id] [reason]");
    if(isnull(reason)) reason = "האדמין לא הקליד סיבה";
	if(IsAdmin(playerid) < IsAdmin(id)) return SendClientMessage(playerid,-1,""#Red".אדמין זה ברמה יותר גבוה ממך"),SendFormatMessage(id,-1,""#Red".ניסה לתת באן %s(%d) השחקן",pName[playerid],playerid);
	if(!IsPlayerConnected(id)) return SendClientMessage(playerid,-1,""#Red"האיידי אינו מחובר");
	TogglePlayerControllable(id,0);
	SendFormatMessageToAll(-1,""#Red"*** %s has been banned by %s.[Reason: %s]",pName[id],pName[playerid],reason);
	new gStr[256];
	DOF2_CreateFile(BanFile(id));
	DOF2_SetString(BanFile(id),"Admin",pName[playerid]);
	DOF2_SetString(BanFile(id),"IP",getip(id));
	DOF2_SetString(BanFile(id),"Reason",reason);
	DOF2_SaveFile();
	format(gStr, sizeof(gStr), "\t\t"#White"שלום לך משתמש יקר,הורחקת מן השרת ולכן לא תוכל להכנס אליו\n\n");
	format(gStr, sizeof(gStr), "%s"#White":להלן פרטי הבאן וסיבת ההרחקה\n\n", gStr);
	format(gStr, sizeof(gStr), "%s"#Red"%s"#White":הורחקת על ידי האדמין\n", gStr,DOF2_GetString(BanFile(id),"Admin"));
	format(gStr, sizeof(gStr), "%s"#White"ציון/סיבת ההרחקה של הבאן:"#Red"%s\n", gStr,DOF2_GetString(BanFile(id),"Reason"));
	format(gStr, sizeof(gStr), "%s"#Red"%s"#White":תאריך ההרחקה\n",gStr,DOF2_GetString(BanFile(id),"DateReg"));
	format(gStr, sizeof(gStr), "%s"#Red"%s"#White":כתובת אייפי\n",gStr,getip(id));
	format(gStr, sizeof(gStr), "%s"#White":תאריך ירידת ההרחקה"#Red"לא צויין\n\n", gStr);
	format(gStr, sizeof(gStr), "%s,במידה ואתה חושב שהאדמין נתן באן לא מוצדק\n\n",gStr);
	format(gStr, sizeof(gStr), "%s"#Red"none.co.il"#White":אנא פתח אשכול מסודר בפורום משוב ועזרה/הורדת באנים בפורום שלנו\n\n", gStr);
	ShowPlayerDialog(id,DIALOG_BAN,DIALOG_STYLE_MSGBOX,""#Red"You have been banned!",gStr,"אישור", "");
	Kick2(id);
	return 1;
}
