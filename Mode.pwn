CMD:sellv2(playerid, params[])
{
	if(!IsPlayerInAnyVehicle(playerid)) return SendClientMessage(playerid, 0xFF0000FF, ""#Red".אתה לא נמצא ברכב");
	if(DOF2_GetBool(PFile(playerid), "SCarHave") == false) return SendClientMessage(playerid, 0xFF0000FF, ""#Red".אין לך רכב");
	Y_GivePlayerMoney(playerid, DOF2_GetInt(PFile(playerid), "SCarPrice") / 2);
	if(SCarIn[playerid] == true)
	{
		vHaveGM[SCarCreate[playerid]] = false;
		DestroyVehicle(SCarCreate[playerid]), SCarIn[playerid] = false;
		Delete3DTextLabel(SCarText3D[SCarCreate[playerid]]);
		DestroyObject(VObjects[playerid]);
	}
	DOF2_SetString(PFile(playerid), "SCarName", "אין");
	DOF2_SetInt(PFile(playerid), "SCarPrice", 0);
	DOF2_SetInt(PFile(playerid), "SCarModel", 0);
	DOF2_SetInt(PFile(playerid), "SCarObj", 0);
	DOF2_SetBool(PFile(playerid), "SCarHave", false);
	return SendClientMessage(playerid, 0xFFFF00FF, ""#LGreen".מכרת את רכבך המיוחד וקבלת חצי מהסכום"), 1;
}
