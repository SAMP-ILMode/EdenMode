CMD:tban(playerid, params[])
{
	if(IsAdmin(playerid) < 2) return 0;
	new reason[30];
	if(sscanf(params,"uiz",params[0],params[1],reason)) return SendClientMessage(playerid,-1,"/tban [playerid] [days] [reason]");
    if(isnull(reason)) reason = "האדמין לא הקליד סיבה";
	if(!IsPlayerConnected(params[0])) return SendClientMessage(playerid,-1,""#Red"האיידי אינו מחובר");
	if(params[1] < 1 || params[1] > 30) return SendClientMessage(playerid,-1,""#Red"/TBan [playerid] [days(1-50)] [reason]");
	SendFormatMessageToAll(-1,""#Red"*** %s has been temporary banned for %d days by %s. [Reason: %s]",pName[params[0]],params[1],pName[playerid],reason);
	new date[3], str[30];
	getdate(date[2], date[1], date[0]);
	format(str, 30, "%02d/%02d/%02d", date[0]+params[1], date[1], date[2]);
	if(date[0]+params[1] > 31)
	{
		format(str, 30, "%02d/%02d/%02d", date[2]+params[0]-31, date[1], date[0]);
	}
	for(new i = 1; i < MAX_TBANS; i++)
	{
	    if(fexist(TBanFile(i))) continue;
	    if(!fexist(TBanFile(i)))
	    {
			DOF2_CreateFile(TBanFile(i));
			DOF2_SetString(TBanFile(i),"Admin",pName[playerid]);
			DOF2_SetString(TBanFile(i),"Player",pName[params[0]]);
			DOF2_SetString(TBanFile(i),"Reason",reason);
			DOF2_SetString(TBanFile(i),"Date",str);
			DOF2_SaveFile();
			format(tBan[i][tAdmin], 24, "%s", pName[playerid]);
			format(tBan[i][tReason], 35, "%s", reason);
			format(tBan[i][tPlayer], 24, "%s", pName[params[0]]);
			format(tBan[i][tDate], 35, "%s", str);
		}
		break;
	}
	Kick2(params[0]);
	return 1;
}
