CMD:qsawn(playerid, params[])
{
	if(!InSawn[playerid]) return SendClientMessage(playerid, 0xff0000ff, ""#Red"  .אתה לא בפעילות סאון");
	if(InSawn[playerid])
	{
		if(SawnEID[playerid] == -1) SendFormatMessageToAll(-1, ""#Red"!יצא מהקרב סאון שלו איתך \"%s\" השחקן", pName[playerid]);
		OnPlayerSpawn(playerid);
		SetPlayerVirtualWorld(playerid,0);
		SawnPID = 0;
		SawnPlayers = 0;
		if(SawnEID[playerid] != -1)
		{
			for(new i; i <= GetPlayerPoolSize(); i++) if(DOF2_GetBool(PFile(i),"SawnMessages"))
			{
				SendFormatMessage(i,-1, ""#TBlue" (/Settings: כיבוי הודעות)!{%06x}%s "#TBlue"יצא מקרב הסאון והפסיד לשחקן {%06x} %s",GetPlayerColor(SawnEID[playerid]) >>> 8, pName[SawnEID[playerid]],GetPlayerColor(playerid) >>> 8, pName[playerid]);
			}
            SawnEID[playerid] = -1;
		}
		OnPlayerSpawn(playerid);
		OnPlayerSpawn(SawnEID[playerid]);
		ResetPlayerWeapons(SawnEID[playerid]);
		ResetPlayerWeapons(playerid);
		SetPlayerVirtualWorld(SawnEID[playerid],0);
	}
	return 1;
}
