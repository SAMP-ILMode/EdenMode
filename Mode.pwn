	SendFormatMessageToAll(-1,""#Purple"»»•"#Grey"'%s'",gSurveyVB[Question]);
	if(AllVotes != 0)
	{
		for(new s; s < gSurveyVB[NumberOfAnswers]; s ++)
		{
			if(strlen(gSurveyAnswers[s]) > 0)
			{
				if(Votes[s] > Winer){ Winer = s; }
				SendFormatMessageToAll(-1,""#Purple"»»•"#Grey"%s - %d הצבעות - %d אחוזים מכלל ההצבעות",gSurveyAnswers[s],Votes[s],((Votes[s] * 100) / AllVotes),"%");
			}
		}
		SendFormatMessageToAll(-1,""#Purple"»»•"#Grey"התשובה עם מרבית הקולות היא: %s",gSurveyAnswers[Winer]);
	}
	else SendClientMessageToAll(-1,""#Purple"»»•"#Grey".לא היו הצבעות בסקר");
	SendClientMessageToAll(-1,"	"#Grey"+_+_+_+["#Purple"מערכת סקרים"#Grey"]+_+_+_+");
	for(new s; s < gSurveyVB[NumberOfAnswers]; s ++)
	if(strlen(gSurveyAnswers[s]) > 0)
	gSurveyAnswers[s] = "";
	return 1;
}

stock ColouredText(text[])
{
	new tString[16], I = -1;
	strmid(String, text, 0, 128, sizeof(String));
	for(new C = 0; C != sizeof(ColorsTag); C ++)
	{
		format(tString, sizeof(tString), "(%s)", ColorsTag[C][ColorName]);
		while((I = strfind(String, tString, true, (I + 1))) != -1)
		{
			new tLen = strlen(tString);
			format(tString, sizeof(tString), "{%s}", ColorsTag[C][ColorID]);
			if(tLen < 8) for(new C2 = 0; C2 != (8 - tLen); C2 ++) strins(String, " ", I);
			for(new tVar; ((String[I] != 0) && (tVar != 8)); I ++, tVar ++) String[I] = tString[tVar];
			if(tLen > 8) strdel(String, I, (I + (tLen - 8)));
		}
	}
	return String;
}

