	new szstring[32];
	if(sscanf(params,"z",szstring)) return SendClientMessage(playerid,-1,""#Red"/ActStio [reason]");
	if(isnull(szstring)) szstring = "האדמין לא הקליד סיבה";
	SendClientMessageToAll(-1,""#Orange"•~•~•~•~•~•~•~•~•~•~•~•~•~•~•~•~•~•~•~•~•~•");
	SendFormatMessageToAll(-1,""#Blue":סגר את הפעילות "#TGreen"%s"#Blue" האדמין",pName[playerid]);
	SendFormatMessageToAll(-1,""#AsO"%s",pAct[SActName]);
	SendClientMessageToAll(-1,""#Purple2":הסיבה לסגירת הפעילותה");
	SendFormatMessageToAll(-1,""#LYellow"%s",szstring);
	SendClientMessageToAll(-1,""#Orange"•~•~•~•~•~•~•~•~•~•~•~•~•~•~•~•~•~•~•~•~•~•");
	SetPlayerVirtualWorld(playerid,0);
	SetPlayerInterior(playerid,0);
	SActOn = false;
	SActPlayers = 0;
	DestroyVehicle(SActVehicle);
	ActOwner = -1;
	for (new i = 0; i <= GetPlayerPoolSize(); i++)
	{
		if(InSAct[i] == 1)
		{
			InSAct[i] = 0;
			RemovePlayerFromVehicle(i);
			OnPlayerSpawn(i);
			SetPlayerVirtualWorld(i,0);
			SetPlayerInterior(i,0);
			SetPlayerRandomColor(i);
		}
	}
	return 1;
}
