CMD:unwarn(playerid, params[])
{
    new id;
    if(sscanf(params,"u",id)) return SendClientMessage(playerid, -1, ""#Red"/UnWarn [id]");
	if(!IsPlayerConnected(id)) return SendClientMessage(playerid,-1,""#Red"!האיידי אינו מחובר");
	if(!DOF2_GetInt(PFile(id), "Warnings"))  return SendClientMessage(playerid,-1,""#Red"!לשחקן זה אין אזהרות");
	DOF2_SetInt(PFile(id),"Warnings", DOF2_GetInt(PFile(id), "Warnings")-1);
	SendFormatMessageToAll(-1,""#Red"*** DM_Guard: %s has been unwarned by %s(%d/3).",pName[id],pName[playerid],DOF2_GetInt(PFile(id), "Warnings"));
	return 1;
}
