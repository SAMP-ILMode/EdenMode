CMD:createhqgang(playerid,params[])
{
	if(!IsMClan(playerid)) return 0;
	new clan[90],Float:Mines[4];
	if(sscanf(params,"s[90]ffff",clan,Mines[0],Mines[1],Mines[2],Mines[3])) return SendClientMessage(playerid,-1,"/CreateHQGang [clan] [X] [Y] [xX] [xY]");
	for(new i; i<MAX_HQ_RESCUE; i++)
	{
		new file[35];
		format(file, sizeof(file), "Clans/HQ/GangZones/%d.ini", i);
		if(!fexist(file))
		{
			format(HQGangInfo[i][gClan], 90, "%s", clan);
			HQGangInfo[i][minX] = Mines[0];
			HQGangInfo[i][minY] = Mines[1];
			HQGangInfo[i][maxX] = Mines[2];
			HQGangInfo[i][maxY] = Mines[3];
			HQGangInfo[i][gGang] = GangZoneCreate(Mines[0], Mines[1], Mines[2], Mines[3]);
			new str[45];
			format(str,sizeof(str),"Clans/%s.ini",clan);
			GangZoneShowForAll(HQGangInfo[i][gGang],rgba2hex(DOF2_GetInt(str,"R"), DOF2_GetInt(str, "G"), DOF2_GetInt(str, "B"),100));
			HQGangInfo[i][gRect] = CreateDynamicRectangle(HQGangInfo[i][minX], HQGangInfo[i][minY], HQGangInfo[i][maxX], HQGangInfo[i][maxY]);
			DOF2_CreateFile(file);
			DOF2_SetString(file, "Clan", clan);
			DOF2_SetFloat(file, "minX", Mines[0]);
			DOF2_SetFloat(file, "minY", Mines[1]);
			DOF2_SetFloat(file, "maxX", Mines[2]);
			DOF2_SetFloat(file, "maxY", Mines[3]);
		}
		break;
	}
	return 1;
}
