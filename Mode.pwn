CMD:auction(playerid, params[])
{
    new Array[15];
	if(sscanf(params, "s[15] ", Array)) return SendClientMessage(playerid, -1, ""#Red"/Auction [ Start / Stop / End / Suggest / SetPrice]");
	if(!strcmp(Array, "Start", true))
	{
		if(DOF2_GetBool(PFile(playerid), "HaveCar") == false) return SendClientMessage(playerid, 0xFF0000FF, ".אין ברכב בבעלותך");
		if(HaveAuction == true) return SendClientMessage(playerid,-1,""#Red"!יש כעת מכירה פועלת");
		if(sscanf(params,"s[15]i",Array,params[0])) return SendClientMessage(playerid,-1,"/Auction Start [Price]");
		SendFormatMessageToAll(-1,""#TBlue"->  !"#AsO"%s"#TBlue" :החל מכירה פומבית על רכבו! סוג הרכב "#gmR"%s"#TBlue" השחקן <-",DOF2_GetString(VFile(DOF2_GetInt(PFile(playerid),"VH")),"vName"),pName[playerid]);
		SendFormatMessageToAll(-1,""#TBlue"->  !בעוד 2 דקות המכירה תיגמר "#Gold"/Auction Suggest"#TBlue" :הסכום הנמוך ביותר להצעה "#Brown"%s$"#TBlue" על מנת להציע למכירה <-",PSIK(params[0]));
	    HaveAuction = true;
	    AuctionPrice = params[0];
	    AuctionStarter = playerid;
	    AuctionBestMoney = -1;
	    AuctionBestID = -1;
		AuctionTime = SetTimer("AuctionEnd", 2*60*1000, 0);
		for(new i; i <= GetPlayerPoolSize(); i++) if(IsMoneySuggest[i] != 0) IsMoneySuggest[i]=0;
    }
	if(!strcmp(Array, "SetPrice", true))
	{
		if(HaveAuction == false) return SendClientMessage(playerid,-1,""#Red"!אין כעת מכירה פועלת");
		if(AuctionStarter != playerid) return SendClientMessage(playerid,-1,""#Red"!אתה לא זה שהתחיל את המכירה");
		if(sscanf(params,"s[15]i",Array,params[0])) return SendClientMessage(playerid,-1,"/Auction SetPrice [Price]");
		SendFormatMessageToAll(-1,""#TBlue"-> !ערך את הסכום אל "#LGreen"%s$ "#TBlue" לכן כל ההצעות אופסו "#Orange"%s"#TBlue" השחקן <-",PSIK(params[0]),pName[playerid]);
	    AuctionPrice = params[0];
		for(new i; i <= GetPlayerPoolSize(); i++) if(IsMoneySuggest[i] != 0) IsMoneySuggest[i]=0;
	}
	if(!strcmp(Array, "Stop", true))
	{
		if(HaveAuction == false) return SendClientMessage(playerid,-1,""#Red"!אין כעת מכירה פועלת");
		if(AuctionStarter != playerid) return SendClientMessage(playerid,-1,""#Red"!אתה לא זה שהתחיל את המכירה");
		SendFormatMessageToAll(-1,""#TBlue"-> "#LGreen"%s "#TBlue":על הרכב "#AsO"%s$"#TBlue" :סגר את המכירה פומבית! המכיר שהוציע "#Orange"%s"#TBlue" השחקן <-",DOF2_GetString(VFile(DOF2_GetInt(PFile(AuctionStarter),"VH")),"vName"),PSIK(AuctionPrice),pName[AuctionStarter]);
	    HaveAuction = false;
	    AuctionPrice = 0;
	    AuctionStarter = -1;
	    AuctionBestMoney = -1;
	    AuctionBestID = -1;
		KillTimer(AuctionTime);
		for(new i; i <= GetPlayerPoolSize(); i++) if(IsMoneySuggest[i] != 0) IsMoneySuggest[i]=0;
	}
	if(!strcmp(Array, "Suggest", true))
	{
		if(HaveAuction == false) return SendClientMessage(playerid,-1,""#Red"!אין כעת מכירה פועלת");
		if(sscanf(params,"s[15]i",Array,params[0])) return SendClientMessage(playerid,-1,"/Auction Suggest [Price]");
		if(params[0] < AuctionPrice) return SendFormatMessage(playerid,-1,""#Red"!%s$ :הסכום המינמאלי הינו",PSIK(AuctionPrice));
	    IsMoneySuggest[playerid] = params[0];
		if(AuctionBestMoney < params[0]) SendFormatMessageToAll(-1,""#TBlue"-> !הציע את הסכום: "#LYellow"%s$"#TBlue" וכעת זה הסכום הכי גבוהה שהציעו "#Orange"%s"#TBlue" השחקן <-",PSIK(params[0]),pName[playerid]),AuctionBestMoney = params[0],AuctionBestID = playerid;
		else SendFormatMessageToAll(-1,""#TBlue"-> !הציעה את הסכום: "#LYellow"%s$"#TBlue" "#Orange"%s"#TBlue" השחקן <-",PSIK(params[0]),pName[playerid]);
	}
	if(!strcmp(Array, "End", true))
	{
		if(HaveAuction == false) return SendClientMessage(playerid,-1,""#Red"!אין כעת מכירה פועלת");
		if(AuctionStarter != playerid) return SendClientMessage(playerid,-1,""#Red"!אתה לא זה שהתחיל את המכירה");
		if(AuctionBestMoney != -1)
		{
			SendFormatMessageToAll(-1,""#TBlue"->  !"#Green"%s$"#TBlue" :בסכום "#Purple"%s"#TBlue" :קנה את הרכב "#gmR"%s"#TBlue" השחקן <-",PSIK(AuctionBestMoney),DOF2_GetString(VFile(DOF2_GetInt(PFile(AuctionStarter),"VH")),"vName"),pName[AuctionBestID]);
			SendFormatMessageToAll(-1,""#TBlue"->  ."#Gold"%s$"#TBlue" :סכום מינמאלי "#Brown"%s"#TBlue" השחקן שהציע את הרכב <-",PSIK(AuctionPrice),pName[AuctionStarter]);
		}
		else SendFormatMessageToAll(-1,""#TBlue"-> !אף אחד לא קנה את הרכב"#LGreen"%s "#TBlue":על הרכב "#AsO"%s$"#TBlue" :סגר את המכירה פומבית! המכיר שהוציע "#Orange"%s"#TBlue" השחקן <-",DOF2_GetString(VFile(DOF2_GetInt(PFile(AuctionStarter),"VH")),"vName"),PSIK(AuctionPrice),pName[AuctionStarter]);
		Y_GivePlayerMoney(AuctionStarter,AuctionBestMoney);
		Y_GivePlayerMoney(AuctionBestID,-AuctionBestMoney);
		ParaseVehicle(playerid,AuctionBestID);
		HaveAuction = false;
	    AuctionPrice = 0;
	    AuctionStarter = -1;
	    AuctionBestMoney = -1;
	    AuctionBestID = -1;
		KillTimer(AuctionTime);
		for(new i; i <= GetPlayerPoolSize(); i++) if(IsMoneySuggest[i] != 0) IsMoneySuggest[i]=0;
	}
	return 1;
	}
