CMD:cw(playerid, params[0]) return cmd_clanwar(playerid,params);
CMD:clanwar(playerid, params[])
{
	new Option[9];
	if(sscanf(params, "s[9] ", Option)) return SendClientMessage(playerid, -1, "/CW [ Invite / Accept / Cancel / Spec / Join ]");
	if(!strcmp(Option, "Invite", true))
	{
		if(DOF2_GetBool(PFile(playerid),"HaveClan") == false) return SendClientMessage(playerid,-1,""#Red".אינך נמצא בקלאן");
		if(DOF2_GetInt(PFile(playerid),"ClanLevel") < 4) return SendClientMessage(playerid,-1,""#Red".אינך לידר בקלאן");
	    new
			ClanName[12],
			Id1,
			Id2,
			Id3,
			CWType,
			zcfile[32],
			cwfile[32]
		;
		if(sscanf(params, "s[9]s[12]uuui", Option, ClanName, Id1, Id2, Id3, CWType)) return SendClientMessage(playerid, -1, ""#Red"/CW Invite [Clan] [id1] [id2] [id3] [type(1-Heavy | 2-Sawn)]");
		format(zcfile, 32, "Clans/%s.ini", GetClanName(playerid));
		format(cwfile, 32, "Clans/%s.ini", ClanName);
		if(!strcmp(GetClanName(playerid), ClanName,true)) return SendClientMessage(playerid,-1, ""#Red"!אי אפשר להזמין את עצמך");
		if(!fexist(cwfile)) return SendClientMessage(playerid, -1, ""#Red"!אין כזה קלאן");
		if(!IsPlayerConnected(Id1))  return SendFormatMessage(playerid, -1, ""#Red"!איידי %d אינו מחובר",Id1);
		if(!IsPlayerConnected(Id2))  return SendFormatMessage(playerid, -1, ""#Red"!איידי %d אינו מחובר",Id2);
		if(!IsPlayerConnected(Id3))  return SendFormatMessage(playerid, -1, ""#Red"!איידי %d אינו מחובר",Id3);
		if(strcmp(GetClanName(playerid), GetClanName(Id1),true)) return SendFormatMessage(playerid,-1, ""#Red"%s(#%03d).אחד השחקנים אינו חבר בקלאן שלך",pName[Id1],Id1);
		if(strcmp(GetClanName(playerid), GetClanName(Id2),true)) return SendFormatMessage(playerid,-1, ""#Red"%s(#%03d).אחד השחקנים אינו חבר בקלאן שלך",pName[Id2],Id2);
		if(strcmp(GetClanName(playerid), GetClanName(Id3),true)) return SendFormatMessage(playerid,-1, ""#Red"%s(#%03d).אחד השחקנים אינו חבר בקלאן שלך",pName[Id3],Id3);
		if(CWType < 1 || CWType > 2) return SendClientMessage(playerid, -1, ""#Red"!טייפ שגוי");
		if(Id1 == Id2 || Id2 == Id1 || Id3 == Id1 || Id3 == Id1 || Id3 == Id3 || Id1 == Id1 || Id2 == Id2 || Id3 == Id3) return SendClientMessage(playerid, -1, ""#Red"!לא ניתן לבחור אותו שחקן כמה פעמים");
		if(HaveClanWar) return SendClientMessage(playerid, -1, ""#Red".אנא המתן לסיום הקלאן וואר שיש כעת");
		if(strcmp(DOF2_GetString(cwfile, "CWInvite"), "n.u.l.l",true)) return SendClientMessage(playerid, -1, ""#Red".קלאן זה מוזמן כבר לקלאן וואר");
		InCW[Id1] = true;
		SendFormatMessage(Id1, -1, ""#Blue">• !{%s}%s "#Blue"קבע אותך בקלאן וואר נגד הקלאן %s הלידר",GetHColor(rgba2hex(DOF2_GetInt(cwfile,"R"), DOF2_GetInt(cwfile, "G"), DOF2_GetInt(cwfile, "B"),100)), ClanName, pName[playerid]);
		InCW[Id2] = true;
		SendFormatMessage(Id2, -1, ""#Blue">• !{%s}%s "#Blue"קבע אותך בקלאן וואר נגד הקלאן %s הלידר",GetHColor(rgba2hex(DOF2_GetInt(cwfile,"R"), DOF2_GetInt(cwfile, "G"), DOF2_GetInt(cwfile, "B"),100)), ClanName, pName[playerid]);
		InCW[Id3] = true;
		SendFormatMessage(Id3, -1, ""#Blue">• !{%s}%s "#Blue"קבע אותך בקלאן וואר נגד הקלאן %s הלידר",GetHColor(rgba2hex(DOF2_GetInt(cwfile,"R"), DOF2_GetInt(cwfile, "G"), DOF2_GetInt(cwfile, "B"),100)), ClanName, pName[playerid]);
		CWInvite1 = Id1;
		CWInvite2 = Id2;
		CWInvite3 = Id3;
		ClanWarType = CWType;
		SendFormatMessage(Id3, -1, ""#Blue">• !{%s}%s "#Blue"קבע אותך בקלאן וואר נגד הקלאן %s הלידר",GetHColor(rgba2hex(DOF2_GetInt(cwfile,"R"), DOF2_GetInt(cwfile, "G"), DOF2_GetInt(cwfile, "B"),100)), ClanName, pName[playerid]);
		SendFormatMessage(playerid, -1, ""#Blue">• :הרכב ,{%s}%s"#Blue" קבעת קלאן וואר נגד הקלאן ",GetHColor(rgba2hex(DOF2_GetInt(cwfile,"R"), DOF2_GetInt(cwfile, "G"), DOF2_GetInt(cwfile, "B"),100)), ClanName);
		SendFormatMessage(playerid, -1, ""#Blue">• %s(#%03d), %s(#%03d), %s(#%03d)",pName[Id1], Id1,pName[Id2], Id2,pName[Id3], Id3);
		if(CWType == 1) SendClientMessage(playerid, -1, ""#Blue">• "#White"Heavy "#Blue":סוג הקלאן וואר");
		else if(CWType == 2) SendClientMessage(playerid, -1, ""#Blue">• "#White"Sawn "#Blue":סוג הקלאן וואר");
		DOF2_SetString(cwfile, "CWInvite", GetClanName(playerid));
		for(new i; i <= GetPlayerPoolSize(); i++)
		{
		    if(!strcmp(GetClanName(i), ClanName,true))
		    {
				SendFormatMessage(i, -1, ""#Blue">• :הזמין אותכם לקלאן וואר, הרכב {%s}%s"#Blue" הקלאן",GetHColor(rgba2hex(DOF2_GetInt(zcfile,"R"), DOF2_GetInt(zcfile, "G"), DOF2_GetInt(zcfile, "B"),100)), GetClanName(playerid));
				SendFormatMessage(i, -1, ""#Blue">• %s(#%03d), %s(#%03d), %s(#%03d)",pName[Id1], Id1,pName[Id2], Id2,pName[Id3], Id3);
				if(CWType == 1) SendClientMessage(i, -1, ""#Blue">• "#White"Heavy "#Blue":סוג הקלאן וואר");
				else if(CWType == 2) SendClientMessage(i, -1, ""#Blue">• "#White"Sawn "#Blue":סוג הקלאן וואר");
                SendClientMessage(i, -1, ""#Blue">• /clanwar accept - לאישור • /clanwar cancel - לדחות");
			}
		}
	}
	else if(!strcmp(Option, "Accept", true))
	{
