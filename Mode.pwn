	else if(!strcmp(Option, "Delete", true))
	{
		SendFormatMessageToAll(-1,""#Blue"• ["#White"Room"#Blue"] » !כיבה את הרום שלו "#TBlue"%s "#Blue"השחקן",pName[playerid]);
		IsUseRoom[playerid] = true;
		HaveRoom[playerid] = false;
		OnPlayerSpawn(playerid);
