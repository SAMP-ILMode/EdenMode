	if(!fexist(file)) return SendFormatMessage(playerid, 0xFF9900FF, ""#Red".אינו בבאן %s שחקן זה", params[0]);
	SendFormatMessageToAll(-1,""#Red"*** %s has been unbanned by %s.",params[0],pName[playerid]);
	DOF2_RemoveFile(file);
	return 1;
}
