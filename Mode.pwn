CMD:setclanname(playerid,params[])
{
	if(!IsMClan(playerid)) return 0;
	new cName[40];
	if(sscanf(params,"s[40]s[40]",cName,params[0])) return SendClientMessage(playerid, -1, "/SetClanName [Clan] [NewName]");
	new file[50];
	format(file, sizeof(file), "Clans/%s.ini", cName);
	new Zfile[50];
	format(Zfile, sizeof(Zfile), "Clans/%s.ini", params[0]);
