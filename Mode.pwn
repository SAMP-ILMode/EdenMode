		SendFormatMessage(Id1, -1, ""#Blue">• !{%s}%s "#Blue"קבע אותך בקלאן וואר נגד הקלאן %s הלידר",GetHColor(rgba2hex(DOF2_GetInt(zfile,"R"), DOF2_GetInt(zfile, "G"), DOF2_GetInt(zfile, "B"),100)), DOF2_GetString(cwfile, "CWInvite"), pName[playerid]);
		InCW[Id2] = true;
		SendFormatMessage(Id2, -1, ""#Blue">• !{%s}%s "#Blue"קבע אותך בקלאן וואר נגד הקלאן %s הלידר",GetHColor(rgba2hex(DOF2_GetInt(zfile,"R"), DOF2_GetInt(zfile, "G"), DOF2_GetInt(zfile, "B"),100)), DOF2_GetString(cwfile, "CWInvite"), pName[playerid]);
