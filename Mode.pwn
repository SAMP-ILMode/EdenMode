CMD:cardisco(playerid, params[])
{
	if(DOF2_GetBool(PFile(playerid), "HaveCar") == false) return SendClientMessage(playerid, 0xFF0000FF, ""#Red".אין ברכב בבעלותך");
    new Array[4];
	if(sscanf(params, "s[4] ", Array)) return SendClientMessage ( playerid, -1, ""#Red"/CarDisco [ Sell / Buy / On / Off ]" );
	if(!strcmp(Array, "Buy", true))
	{
	    if(DOF2_GetBool(VFile(DOF2_GetInt(PFile(playerid),"VH")),"HaveDisco") == true) return SendClientMessage(playerid, -1, ""#Red".יש לך דיסקו לרכב");
		if(Y_GetPlayerMoney(playerid) < 250000)return SendClientMessage(playerid,0xFF0000C8,"250,000$ - מחיר מערכת דיסקו הינו");
		DOF2_SetBool(VFile(DOF2_GetInt(PFile(playerid),"VH")),"HaveDisco",true);
		SendFormatMessageToAll(-1,""#Blue"» !"#Orange"קנה "#Yellow"מערכת "#AsO"דיסקו "#Brown"לרכבו "#Green"%s",pName[playerid]);
		SendClientMessage(playerid,-1,""#LGreen"• /CarDisco On/Off - קנית מערכת דיסקו, על מנת להפעיל/לכבות");
		Y_GivePlayerMoney(playerid, -250000);
	}
	else if(!strcmp(Array, "On", true))
	{
		if(DOF2_GetBool(VFile(DOF2_GetInt(PFile(playerid),"VH")),"HaveDisco") == false) return SendClientMessage(playerid, -1, ""#Red".אין לך דיסקו לרכב");
		if(HaveCarDisco[playerid] == true) return SendClientMessage(playerid, -1, ""#Red".אתה במצב דיסקו כעת");
		HaveCarDisco[playerid] = true;
		CarDiscoTimer[playerid] = SetTimerEx("CarDisco",500,1,"dd",DOF2_GetInt(PFile(playerid),"VH"),playerid);
		SendClientMessage(playerid,-1,""#LGreen"• .הפעלת מצב דיסקו");
	}
	else if(!strcmp(Array, "Off", true))
	{
		if(DOF2_GetBool(VFile(DOF2_GetInt(PFile(playerid),"VH")),"HaveDisco") == false) return SendClientMessage(playerid, -1, ""#Red".אין לך דיסקו לרכב");
		if(HaveCarDisco[playerid] == false) return SendClientMessage(playerid, -1, ""#Red".אתה לא במצב דיסקו כעת");
		HaveCarDisco[playerid] = false;
		KillTimer(CarDiscoTimer[playerid]);
		SendClientMessage(playerid,-1,""#LGreen"• .כיבית מצב דיסקו");
		ChangeVehicleColor(Vehicles[DOF2_GetInt(PFile(playerid),"VH")],DOF2_GetInt(VFile(DOF2_GetInt(PFile(playerid),"VH")),"Color1"),DOF2_GetInt(VFile(DOF2_GetInt(PFile(playerid),"VH")),"Color2"));
	}
	else if(!strcmp(Array, "Sell", true))
	{
		if(DOF2_GetBool(VFile(DOF2_GetInt(PFile(playerid),"VH")),"HaveDisco") == false) return SendClientMessage(playerid, -1, ""#Red".אין לך דיסקו לרכב");
		SendClientMessage(playerid,-1,""#LGreen"• .מכרת את המערכת הדיסקו שלך וקבלת חצי מהסכום");
		Y_GivePlayerMoney(playerid, 250000/2);
		DOF2_SetBool(VFile(DOF2_GetInt(PFile(playerid),"VH")),"HaveDisco",false);
		KillTimer(CarDiscoTimer[playerid]);
		ChangeVehicleColor(Vehicles[DOF2_GetInt(PFile(playerid),"VH")],DOF2_GetInt(VFile(DOF2_GetInt(PFile(playerid),"VH")),"Color1"),DOF2_GetInt(VFile(DOF2_GetInt(PFile(playerid),"VH")),"Color2"));
	}
	return 1;
}
