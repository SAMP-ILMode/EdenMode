stock zSTime(time)
{
	new minutes, seconds, tstring[16];
	if(time > 59)
	{
		minutes = floatround(time/60);
		seconds = floatround(time - minutes*60);
	}
	else seconds = time;
	format(tstring, sizeof(tstring), "%02d:%02d", minutes, seconds);
	return tstring;
}

