CMD:setclancolor(playerid,params[])
{
	if(!IsMClan(playerid)) return 0;
	new cName[40],r, g, b;
	if(sscanf(params,"s[40]iii",cName,r, g, b)) return SendClientMessage(playerid, -1, "/SetClanColor [Clan] [R] [G] [B]");
	new file[50];
	format(file, sizeof(file), "/Clans/%s.ini", cName);
	if(!DOF2_FileExists(file)) return SendClientMessage(playerid, 0xFF0000C8, ""#Red".לא קיים קלאן כזה");
	DOF2_SetInt(file, "R", r);
	DOF2_SetInt(file, "G", g);
	DOF2_SetInt(file, "B", b);
	for(new i = 0; i <= GetPlayerPoolSize(); i++)
	{
		if(!strcmp(params[0], DOF2_GetString(PFile(i), "Clan"), true))
		{
			SetPlayerColor(i, rgba2hex(r,g,b,0));
			SendFormatMessage(i, rgba2hex(r,g,b,0),".ערך את צבע הקלאן שלכם לצבע הזה %s מנהל הקלאנים",pName[playerid]);
		}
	}
	return 1;
}
