CMD:ejectall(playerid, params[])
{
	if(!IsPlayerInAnyVehicle(playerid)) return SendClientMessage(playerid, 0xFF0000FF, ".אינך נמצא בתוך רכב");
	if(News[Check22][GetPlayerVehicleID(playerid)] != 1)return SendClientMessage(playerid,0xFF0000C8,"!רכב זה אינו בבעלותך");
	if(DOF2_GetBool(PFile(playerid), "HaveCar") == false) return SendClientMessage(playerid, 0xFF0000FF, ""#Red".אין ברכב בבעלותך");
	for(new i = 0; i <= GetPlayerPoolSize(); i++)
	{
	if(!IsPlayerConnected(playerid)) continue;
	if(IsPlayerInVehicle(i, DOF2_GetInt(PFile(playerid),"VH"))) RemovePlayerFromVehicle(i);
	}
	return SendClientMessage(playerid, -1, ""#LGreen"• .הוצאת כל השחקנים ברכב שלך");
}
