CMD:mleave(playerid, params[])
{
	if(!InMPilot[playerid] && !InMD[playerid] && !InMRace[playerid] && !InSP[playerid]) return SendClientMessage(playerid, -1, ""#Red"!אתה לא בפעילות");
	KillTimer(MissionTimer);
	MissionRestart[playerid] = SetTimerEx("MissionNewEnd", 1000*60*5, false, "i", playerid);
	MissionStart[playerid] = gettime()+300;
	if(InSP[playerid] == 1)
	{
		InSP[playerid] = 0;
		SMS = 0;
		SendFormatMessageToAll(-1,""#Gold"» ."#Orange"Super "#Gold"נכשל במשימת את משימת "#Red"\"%s\""#Gold" השחקן", pName[playerid]);
		DisablePlayerRaceCheckpoint(playerid);
		StopFly(playerid);
		OnPlayerSpawn(playerid);
	}
	if(InMRace[playerid] == 1)
	{
		InMRace[playerid] = 0;
		SMRace = 0;
		DestroyVehicle(VMRace);
		KillTimer(MissionTimer);
		SendFormatMessageToAll(-1,""#Gold"» ."#Orange"Race "#Gold"נכשל במשימת את משימת "#Red"\"%s\""#Gold" השחקן", pName[playerid]);
		DisablePlayerRaceCheckpoint(playerid);
		OnPlayerSpawn(playerid);
	}
	if(InMPilot[playerid] == 1)
	{
		InMPilot[playerid] = 0;
		SMPilot = 0;
		DestroyVehicle(VMPilot);
		KillTimer(MissionTimer);
		SendFormatMessageToAll(-1,""#Gold"» ."#Orange"Pilot "#Gold"נכשל במשימת את משימת "#Red"\"%s\""#Gold" השחקן", pName[playerid]);
		DisablePlayerRaceCheckpoint(playerid);
		OnPlayerSpawn(playerid);
	}
	if(InMD[playerid] == 1)
	{
		InMD[playerid] = 0;
		SMD = 0;
		DestroyVehicle(VMD);
		KillTimer(MissionTimer);
		SendFormatMessageToAll(-1,""#Gold"» ."#Orange"Drift "#Gold"נכשל במשימת את משימת "#Red"\"%s\""#Gold" השחקן", pName[playerid]);
		DisablePlayerRaceCheckpoint(playerid);
		OnPlayerSpawn(playerid);
	}
	return 1;
}
