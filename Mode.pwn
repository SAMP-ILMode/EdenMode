CMD:calc(playerid, params[])
{
    if(sscanf(params, "dcd", params[0], params[1], params[2])) return SendClientMessage ( playerid, 0xffffffFF, ""#Red"/Calc [number] [+|-|*|/] [number]" );
	if(params[0] > 9999999 || params[2] > 9999999 || params [0] < -9999999 || params[2] < -9999999) return SendClientMessage ( playerid, 0xffffffFF, ""#Red"/Calc [number] [+|-|*|/] [number]" );
	if(params[1] == '+') SendFormatMessage ( playerid, 0xffffffFF, ""#Orange"Answer: "#NoN"%d + %d = %d", params [ 0 ], params [ 2 ], params [ 0 ]+ params [ 2 ] );
	else if(params[1] == '-' ) SendFormatMessage ( playerid, 0xffffffFF, ""#Orange"Answer: "#NoN"%d - %d = %d", params [ 0 ], params [ 2 ], params [ 0 ]-params [ 2 ] );
	else if(params[1] == '*' ) SendFormatMessage ( playerid, 0xffffffFF, ""#Orange"Answer: "#NoN"%d * %d = %d", params [ 0 ], params [ 2 ], params [ 0 ]*params [ 2 ] );
