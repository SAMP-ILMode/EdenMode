	        format(str, 1400, "%s\n"#LGreen"Code ID: "#TGreen"#%d "#LGreen"Code: "#TGreen"%s "#LGreen"| Code Name: "#TGreen"%s "#LGreen"| Code Date: "#TGreen"%s"#LGreen" | Code Owner: "#TGreen"%s",str,i,codeInfo[i][cCode],codeInfo[i][codeName],codeInfo[i][cDate],codeInfo[i][cOwner]);
		}
		if(!count) SendClientMessage(playerid, -1, ""#Red".אין קודים כעת");
		ShowPlayerDialog(playerid, 322, DIALOG_STYLE_MSGBOX, ""#White"Code`s "#Blue"List", str, "אישור", "");
	}
    else if(!strcmp(Mera, "Create", true))
    {
		new CodeTAP[50];
		if(sscanf(params,"s[7]iis[50]",Mera,params[0], params[1], CodeTAP))
		{
		    SendClientMessage(playerid,-1,""#Yellow"1.Random Money");
		    SendClientMessage(playerid,-1,""#Yellow"2.Option Select");
		    SendClientMessage(playerid,-1,""#Yellow"3.Activity Select");
		    SendClientMessage(playerid,-1,""#Yellow"4.Random Points");
		    SendClientMessage(playerid,-1,""#Yellow"5.Nothing");
		    SendClientMessage(playerid,-1,""#Yellow"6.Speed Hack");
		    SendClientMessage(playerid,-1,""#Yellow"7.Auto Jump");
		    SendClientMessage(playerid,-1,""#Yellow"8.Tag User Level");
		    SendClientMessage(playerid,-1,""#Yellow"9.Tag");
		    SendClientMessage(playerid,-1,""#Yellow"10.Water Walk");
		    SendClientMessage(playerid,-1,""#Yellow"11.Mini Gun");
		    SendClientMessage(playerid,-1,""#Yellow"12.God Mod");
		    SendClientMessage(playerid,-1,""#Yellow"13.V.I.P Bronze");
		    SendClientMessage(playerid,-1,""#Yellow"14.Infinity Armour");
		    SendClientMessage(playerid,-1,""#Yellow"15.Sniper Boom");
		    SendClientMessage(playerid,-1,""#HPurple"Usage: /CreateCode [Days] [Type] [Code]");
			return 1;
		}
		if(params[1] < 1 || params[1] > 15) return SendClientMessage(playerid,-1,"Usage: /CreateCode [Days] [Type(1-13)] [Code]");
		if(CodePress) return SendClientMessage(playerid,-1,""#Red"!פועל כעת קוד");
	    for(new i = 1; i < MAX_CODES; i++)
	    {
	        if(fexist(CodeFile(i))) continue;
	        if(!fexist(CodeFile(i)))
	        {
	            if(i >= MAX_CODES) return SendClientMessage(playerid, -1, ""#Red"!לא ניתן ליצור עוד קודים");
				new date[3];
				getdate(date[2], date[1], date[0]);
				DOF2_CreateFile(CodeFile(i));
				DOF2_SetString(CodeFile(i), "Code", CodeTAP);
				DOF2_SetString(CodeFile(i), "Code Name", CodeName(params[1]));
				DOF2_SetString(CodeFile(i), "Code Owner", "n.u.l.l");
				format(String, sizeof(String), "%02d/%02d/%02d",date[0]+params[0], date[1], date[2]);
				if(date[0]+params[0] > 31)
				{
					format(String, sizeof(String), "%02d/%02d/%02d",date[0]+params[0]-31, date[1], date[2]);
				}
				DOF2_SetString(CodeFile(i), "Code Days", String);
				DOF2_SetInt(CodeFile(i), "Code Type", params[1]);
				DOF2_SaveFile();
				format(codeInfo[i][cCode], 15, "%s", CodeTAP);
				format(codeInfo[i][codeName], 15, "%s", CodeName(params[1]));
				format(codeInfo[i][cOwner], MAX_PLAYER_NAME, "%s", "n.u.l.l");
				format(codeInfo[i][cDate], 15, "%s", DOF2_GetString(CodeFile(i),"Code Days"));
				codeInfo[i][cType] = params[1];
				CodePress = i;
				CodeYamin = params[0];
				SendFormatMessage(playerid, -1,""#Green"» עד התאריך "#Orange"%s "#AsO"%s "#Green"הקוד הוא "#Turkiz"%s "#Green"הכנת את הקוד",DOF2_GetString(CodeFile(i),"Code Days"), CodeTAP, CodeName(params[1]));
			}
	        break;
		}
		DontHave = true;
	}
	else return SendClientMessage(playerid, -1, ""#Red"/aCode [#Help]");
	return 1;
}
