public OnGameModeExit()
{
	for(new i = 1; i< MAX_CODES; i++) if(fexist(CodeFile(i)))
	{
	    if(fexist(CodeFile(i)))
	    {
			DOF2_SetString(CodeFile(i),"Code",codeInfo[i][cCode]);
			DOF2_SetString(CodeFile(i),"Code Name",codeInfo[i][codeName]);
			DOF2_SetString(CodeFile(i),"Code Owner",codeInfo[i][cOwner]);
			DOF2_SetString(CodeFile(i),"Code Days",codeInfo[i][cDate]);
			DOF2_SetInt(CodeFile(i),"Code Type",codeInfo[i][cType]);
			DOF2_SaveFile();
		}
    }
	for(new i = 1; i< MAX_TBANS; i++) if(fexist(TBanFile(i)))
	{
	    if(fexist(CodeFile(i)))
	    {
			DOF2_SetString(TBanFile(i),"Admin",tBan[i][tAdmin]);
			DOF2_SetString(TBanFile(i),"Reason",tBan[i][tReason]);
			DOF2_SetString(TBanFile(i),"Player",tBan[i][tPlayer]);
			DOF2_SetString(TBanFile(i),"Date",tBan[i][tDate]);
			DOF2_SaveFile();
		}
    }
	for(new i = 1; i < MAX_HOUSES; i++)
	{
	    if(fexist(hFile(i)))
	    {
	        DOF2_SetInt(hFile(i), "House ID", hInfo[i][hID]);
	        DOF2_SetString(hFile(i), "House Owner", hInfo[i][hOwn]);
	        DOF2_SetFloat(hFile(i), "House Pos1", hInfo[i][hPos][0]);
	        DOF2_SetFloat(hFile(i), "House Pos2", hInfo[i][hPos][1]);
	        DOF2_SetFloat(hFile(i), "House Pos3", hInfo[i][hPos][2]);
	        DOF2_SetBool(hFile(i), "House Lock", hInfo[i][hLock]);
	        DOF2_SetString(hFile(i), "House Key1", hInfo[i][hKeyID1]);
	        DOF2_SetString(hFile(i), "House Key2", hInfo[i][hKeyID2]);
	        DOF2_SetString(hFile(i), "House Key3", hInfo[i][hKeyID3]);
	        DOF2_SetInt(hFile(i), "House Bank", hInfo[i][hBank]);
	        DOF2_SetInt(hFile(i), "House Price", hInfo[i][hPrice]);
	        DOF2_SetInt(hFile(i), "House Size", hInfo[i][hSize]);
		}
	}
	DOF2_Exit();
	return 1;
}
public OnRconLoginAttempt(ip[], password[], success)
{
	printf("IP: %s has been connected!", ip);
	if(!success)
	{
        new pip[16];
        for(new i = GetPlayerPoolSize(); i != -1; --i)
        {
            GetPlayerIp(i, pip, sizeof(pip));
            if(!strcmp(ip, pip, true))
            {
				SendFormatMessageToAll(-1, ""#Red"DM_Guard: %s has been kicked from the server. [Reason: Rcon Failed]",pName[i]);
				Kick2(i);
			}
		}
	}
	return 1;
}

public OnPlayerEnterDynamicArea(playerid, areaid)
{
	if(areaid == puRect && puOn)
	{
	    puPlayers++;
		SendFormatMessage(playerid, -1, ""#Blue"נכנסת לאזור הפיקאפ! מספר שחקנים אשר משתתפים: "#White"%d",puPlayers);
		for(new i; i <= GetPlayerPoolSize(); i++) if(IsPlayerInArea(i, 2055.306, 969.2637, 2347.253, 1202.821)) SendFormatMessage(i, -1, ""#Orange">> "#White"(Players: %d)"#Purple"!הצטרף לאזור חיפוש הפיקאפ "#White"%s "#Purple"השחקן",puPlayers,pName[playerid]);
		if((puPlayers % 3 == 0 && puPlayers != 0) || puPlayers == 3)
		{
			switch(random(3))
			{
		    	case 0:SendFormatMessageToAll(-1, ""#Blue"» !כעת נמצאים באזור החיפוש: "#White"%d שחקנים "#Blue", להצטרפות השתגרו לטירה ליד "#White"לאס וונטוראס",puPlayers);
		    	case 1:SendFormatMessageToAll(-1, ""#Yellow"» !כעת נמצאים באזור החיפוש: "#White"%d שחקנים "#Yellow", להצטרפות השתגרו לטירה ליד "#White"לאס וונטוראס",puPlayers);
		    	case 2:SendFormatMessageToAll(-1, ""#Purple"» !כעת נמצאים באזור החיפוש: "#White"%d שחקנים "#Purple", להצטרפות השתגרו לטירה ליד "#White"לאס וונטוראס",puPlayers);
			}
		}
	}
	for(new i; i < sizeof(caInfo); i++)
	{
		if(areaid == caVar[i][caArea])
		{
			new str[50];
			format(str, sizeof (str), "Clans/%s.ini", caVar[i][caOwn]);
   			if(DOF2_GetInt(str, "CaptPlayers")) DOF2_SetInt(str, "CaptPlayers", DOF2_GetInt(str, "CaptPlayers")+1);
			if(!strcmp(caVar[i][caOwn], "n.u.l.l", false)) SendFormatMessage(playerid, -1,""#Orange"»"#White" .נכנסת אל האזור {%s}%s",CAColor(i), caInfo[i][caName]);
			else SendFormatMessage(playerid, -1,""#Orange"»"#White" נכנסת לאזור {%s}%s {%s}%s "#White" אשר נשלט על ידי הקלאן",CAColor(i), caInfo[i][caName],GetHColor(rgba2hex(DOF2_GetInt(str,"R"), DOF2_GetInt(str, "G"), DOF2_GetInt(str, "B"),100)), caVar[i][caOwn]);
			InArea[playerid] = i;
		}
	}
	for(new i; i<MAX_HQ_RESCUE; i++)
	{
	    new file[40];
	    format(file ,40, "Clans/HQ/GangZones/%d.ini", i);
	    if(fexist(file))
	    {
			if(areaid == HQGangInfo[i][gRect])
			{
				InHQ[playerid] = true;
				format(InHQClanName[playerid],90,"%s",HQGangInfo[i][gClan]);
				new str[50];
				format(str, sizeof (str), "Clans/%s.ini", HQGangInfo[i][gClan]);
				if(!strcmp(GetClanName(playerid),HQGangInfo[i][gClan]) && DOF2_GetBool(PFile(playerid),"HaveClan") == true) SendClientMessage(playerid,-1,""#LGreen"!נכנסת למפקדת הקלאן שלך");
				else if(strcmp(GetClanName(playerid),HQGangInfo[i][gClan]))
				{
					SendFormatMessage(playerid,-1,""#Red"• !{%s}%s נכנסת אל מפקדת הקלאן",GetHColor(rgba2hex(DOF2_GetInt(str,"R"), DOF2_GetInt(str, "G"), DOF2_GetInt(str, "B"),100)),GetClanName(playerid));
					for(new hqzv; hqzv <= GetPlayerPoolSize(); hqzv++)
					{
					    if(!strcmp(GetClanName(hqzv),HQGangInfo[i][gClan]) && DOF2_GetBool(PFile(hqzv),"HaveClan") == true)
					    {
						    if(DOF2_GetBool(PFile(playerid),"HaveClan") == true)
						    {
