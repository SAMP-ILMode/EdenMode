CMD:join(playerid, params[])
{
    if(InAct[playerid]) return SendClientMessage(playerid, 0xff0000ff, ""#Red" .אתה כבר בפעילות");
    if(!ActStatus || ActStatus == 2) return SendClientMessage(playerid, 0xff0000ff, !ActStatus ? (""#Red".אין פעילות עובדת בשרת") : (""#Red" .הפעילות כבר החלה, נסה בפעם הבאה"));
	if(Acts[IsActOn][Max_Act_Players] < ActPlayers) return SendClientMessage(playerid, 0xff0000ff, ""#Red".כל המקומות בפעילות נתפסו");
	if(SCarIn[playerid]) return SendClientMessage(playerid, 0xff0000ff, ""#Red".עליך לצאת מהרכב המיוחד");
	if(GetPlayerInterior(playerid) != 0) return SendClientMessage(playerid, 0xff0000ff, ""#Red".לא ניתן לבצע פקודה זו במצבך הנוכחי");
	if(Y_GetPlayerMoney(playerid) < 200) return SendClientMessage(playerid, -1, ""#Red"** $מחיר הצטרפות לפעילות: 200");
    if(ActPlayers == Acts[IsActOn][Max_Act_Players]) SendFormatMessage(playerid,-1, ""#Red"»  !נתפסו המקומות לפעילות %d",Acts[IsActOn][ActName]);
	Y_GivePlayerMoney(playerid, -200);
	InAct[playerid] = true;
	ActPlayers++;
	if(ActPlayers == Acts[IsActOn][Max_Act_Players]) SendFormatMessageToAll(-1, ""#HBrown"»  !נתפסו המקומות לפעילות "#Brown"%d",Acts[IsActOn][ActName]);
	if(!FJoin)
	{
		Y_GivePlayerMoney(playerid, 4000);
		SendFormatMessageToAll(-1, ""#LGreen"+ (%d/%d)/Join :הצטרף לפעילות ראשון וזכה בסכום 4,000$. להצטרפות %s",Acts[IsActOn][Max_Act_Players],ActPlayers,pName[playerid]);
        FJoin = true;
	}
	else if(FJoin)
	{
		switch(Acts[IsActOn][Type])
		{
			case 2..3:SendFormatMessageToAll(-1, ""#LGreen"[+] !הצטרף אל המירוץ %s",pName[playerid]);
		}
	}
	SendFormatMessage(playerid, -1, ".נרשמת לפעילות "#TBlue"%s "#White"אתה משתתף מספר "#Red"%d/%d",Acts[IsActOn][ActName], Acts[IsActOn][Max_Act_Players], ActPlayers);
	return 1;
}
