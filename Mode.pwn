	}
	else if(!strcmp(Mera, "cancel", true, 6))
	{
		if(sscanf(params, "s[8]u", Mera, params[0])) return SendClientMessage(playerid, -1, ""#Red"/Tr Take [id]");
		if(!IsPlayerConnected(params[0])) return SendClientMessage(playerid, -1, ""#Red"!שחקן איננו מחובר");
		if(!IsReport[params[0]]) return SendClientMessage(playerid, -1, ""#Red"!שחקן זה לא מבקש רפורט");
		SendFormatMessage(playerid, -1, ""#Red"** !%s בטלת את הרפורט של השחקן",pName[params[0]]);
		SendFormatMessage(params[0], -1, ""#Red"** !ביטל את הרפורט שלך %s האדמין",pName[playerid]);
	}
	else if(!strcmp(Mera, "mute", true, 4))
	{
		if(sscanf(params, "s[8]u", Mera, params[0])) return SendClientMessage(playerid, -1, ""#Red"/Tr Mute [id]");
		if(!IsPlayerConnected(params[0])) return SendClientMessage(playerid, -1, ""#Red"!שחקן איננו מחובר");
		if(!IsReport[params[0]]) return SendClientMessage(playerid, -1, ""#Red"!שחקן זה לא מבקש רפורט");
		SendFormatMessageToAll(-1,"*** %s Has been muted by %s for 30 seconds. ["#Red"/A - סתמי"#White"]",pName[params[0]],pName[playerid]);
