stock EnglishToHebrew(text[])
{
    new TextString[256];
    for(new iPos, iLen = strlen(text); iPos < iLen; iPos++)
    {
        switch(text[iPos])
        {
            case '.': strcat(TextString, "/");
            case 'ש': strcat(TextString, "a");
            case 'נ': strcat(TextString, "b");
            case 'ב': strcat(TextString, "c");
            case 'ג': strcat(TextString, "d");
            case 'ק': strcat(TextString, "e");
            case 'כ': strcat(TextString, "f");
            case 'ע': strcat(TextString, "g");
            case 'י': strcat(TextString, "h");
            case 'ן': strcat(TextString, "i");
            case 'ח': strcat(TextString, "j");
            case 'ל': strcat(TextString, "k");
            case 'ך': strcat(TextString, "l");
            case 'צ': strcat(TextString, "m");
            case 'מ': strcat(TextString, "n");
            case 'ם': strcat(TextString, "o");
            case 'פ': strcat(TextString, "p");
            case '/': strcat(TextString, "q");
            case 'ר': strcat(TextString, "r");
            case 'ד': strcat(TextString, "s");
            case 'א': strcat(TextString, "t");
            case 'ו': strcat(TextString, "u");
            case 'ה': strcat(TextString, "v");
            case ''': strcat(TextString, "w");
            case 'ס': strcat(TextString, "x");
            case 'ט': strcat(TextString, "y");
            case 'ז': strcat(TextString, "z");
            case ' ', 0..9: strcat(TextString, text[iPos]);
        }
    }
    return TextString;
}

stock PSIK(zsnumber)
{
    new num[50]; valstr(num, zsnumber);
    new revnum[50]; for(new i = 0; i < strlen(num); i++) format(revnum, sizeof(revnum), "%s%c",                   revnum, num[strlen(num)-(i+1)]);
    num=""; for(new n = 0; n < strlen(revnum); n++)
    {
        format(num, sizeof(num), "%s%c", num, revnum[n]);
        if((n+1)%3==0 && (n+1!=strlen(revnum))) format(num, sizeof(num), "%s,", num);
    }
    new newnum[50]; for(new c = 0; c < strlen(num); c++) format(newnum, sizeof(newnum), "%s%c",         newnum, num[strlen(num)-(c+1)]);
    return newnum;
}

stock Zero(num, slots = 13)
{
    new Zstring[256];
    format(Zstring, sizeof(Zstring), "%d", num);
    for(new i = 0; i <= slots - strlen(Zstring); i++)
    {
        strins(Zstring, "0", 0);
    }
    return Zstring;
}

