	{
		if(IsPlayerConnected(i) && InAFK[i])
		{
			++afklist;
			format(str,sizeof(str),"%s"#LBlue"#%d)"#White" %s [ID: "#LBlue"%d "#White" | Time: "#LBlue"%s "#White"]\n",str,afklist,pName[i],i, zSTime(TimeInAFK[i]-120));
			ShowPlayerDialog(playerid,5123,DIALOG_STYLE_MSGBOX,""#AsO"» AFK List",str,"אישור","");
		}
	}
	if(!afklist) return ShowPlayerDialog(playerid,1345,DIALOG_STYLE_MSGBOX,""#Green":רשימת לא נמצאים",""#TBlue".אין שחקנים במצב לא נמצא","אישור","");
	return 1;
}
