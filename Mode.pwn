public OnPlayerDeath(playerid, killerid, reason)
{
//=================================[ActsS]======================================
	if(InAct[playerid] && ActStatus == 2)
	{
		ActPlayers--;
		InAct[playerid] = false;
		DisablePlayerRaceCheckpoint(playerid);
		InActCP[playerid] = 0;
		RaceTime[playerid] = 0;
		FJoin = false;
		HaveFirst = false;
		HaveSecond = false;
		HaveThree = false;
