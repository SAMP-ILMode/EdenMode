			SetPlayerCameraPos(playerid, -1918.311767, 239.839691, 57.635005);
			SetPlayerCameraLookAt(playerid, -1915.624267, 243.329452, 55.268791);
			InterpolateCameraPos(playerid, -1918.311767, 239.839691, 57.635005, -1887.151489, 269.467315, 43.424911, 5000);
			InterpolateCameraLookAt(playerid, -1915.624267, 243.329452, 55.268791, -1891.780517, 270.636291, 41.939849, 4000);
			return InCSamera[playerid] = 2;
		}
		return 1;
	}
	if(InAFK[playerid])
