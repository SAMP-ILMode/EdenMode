CMD:mytime(playerid, params[])
{
	new Option[4];
	if(sscanf(params, "s[4] ", Option)) return SendClientMessage(playerid, -1, ""#Red"/MyTime [ Set / Del ]");
	if(!strcmp(Option, "set", true, 3))
	{
	    if(GameTime[playerid] != -1) return SendClientMessage(playerid, -1, ""#Red"/MyTime Del - הגדרת כבר שעה, על מנת לערוך אותה");
		if(sscanf(params, "s[4]i", Option, params[0])) return SendClientMessage(playerid, -1, ""#Red"/MyTime Set [hour]");
		if(params[0] > 24 || params[0] < 0) return SendClientMessage(playerid, -1, ""#Red"/MyTime Set [hour(24-00)]");
		SetPlayerTime(playerid, params[0], 00);
		SendFormatMessage(playerid, -1, ""#LGreen" ** שינית את השעה במשחק שלך: %d",params[0]);
		GameTime[playerid] = params[0];
	}
	else if(!strcmp(Option, "del", true, 3))
	{
	    if(GameTime[playerid] == -1) return SendClientMessage(playerid, -1, ""#Red"/MyTime Set - לא הגדרת שעה, על מנת להגדיר");
	    SetPlayerTime(playerid, random(24), random(60));
	    GameTime[playerid] = -1;
		SendClientMessage(playerid, -1, ""#LGreen" ** איפסת את השעה שלך");
	}
	else return SendClientMessage(playerid, -1, ""#Red"/MyTime [ Set / Del ]");
	return 1;
}
