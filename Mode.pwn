			{
				SendFormatMessage(i, -1,""#Green".הפעיל אנטי טים קיל בקלאן שלך %s הלידר",pName[playerid]);
				SetPlayerTeam(i, DOF2_GetInt(str, "N"));
			}
		}
		else if(!strcmp(edit, "Off", true))
		{
			new str[90];
			format(str,90,"Clans/%s.ini",GetClanName(playerid));
			if(DOF2_GetBool(str, "HaveTK") == false) return SendClientMessage(playerid,-1,""#Red".אין לך אנטי-טים קיל בקלאן");
			if(DOF2_GetBool(str, "TKOn") == false) return SendClientMessage(playerid,-1,""#Red".המערכת כבר מכובת");
			DOF2_SetBool(str, "TKOn", false);
            for(new i; i <= GetPlayerPoolSize(); i++) if(!strcmp(GetClanName(playerid), GetClanName(i), true)) SendFormatMessage(i, -1,""#Green".כיבה אנטי-טים קיל בקלאן %s הלידר",pName[playerid]);
