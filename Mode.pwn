CMD:totto(playerid, params[])
{
	if(TottoOn == false) return SendClientMessage(playerid, -1, ".המערכת הינה פועלת");
	if(IsTotto[playerid] == true) return SendClientMessage(playerid, -1, ".לא ניתן להמר שנית");
	if(Y_GetPlayerMoney(playerid) < TottoPrice) return SendClientMessage(playerid, -1, ".אין לך את הסכום הדרוש");
	if(sscanf(params,"i",params[0])) return SendClientMessage(playerid,-1, ""#Red"/Totto [1-6]");
	if(params[0] == TottoNum) TottoWinners++,IsTottoWinner[playerid] = true;
	IsTotto[playerid] = true;
	SendFormatMessage(playerid,-1,""#NoN"» ."#White"%d"#NoN" הימרת בהצלחה בהגרלה! המספר שבחרת הוא",params[0]);
	Y_GivePlayerMoney(playerid, -TottoPrice);
	return 1;
}
