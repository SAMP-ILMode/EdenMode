		SendClientMessageToAll(-1, ""#Blue"/ClanWar Spec - הקלאן וואר התחיל! לצפייה");
		new b = 0;
		HaveClanWar = true;
		DOF2_SetString(cwfile, "CWInvite", "n.u.l.l");
        for(new i; i <= GetPlayerPoolSize(); i++)
        {
			if(InCW[i])
			{
			    SetPlayerPos(i, CWSpawns[b][0], CWSpawns[b][1], CWSpawns[b][2]);
			    SetPlayerFacingAngle(i, CWSpawns[b][3]);
