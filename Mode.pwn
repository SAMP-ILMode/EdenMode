CMD:math(playerid,params[])
{
	new Ans;
	if(sscanf(params,"i",Ans)) return SendClientMessage(playerid, -1 , ""#Red"/Math [Ans]");
	if(Znumber[0] == 0 && Znumber[1] == 0)return SendClientMessage(playerid,-1,""#Red" .האתגר אינו פועל כעת");
	if(Znumber[2] != 0)
	{
		if(Ans == Znumber[0] + Znumber[1] - Znumber[2])
		{
			SendFormatMessageToAll(-1,""#Green"• .(Time: "#LGreen"00:%s"#Green") "#LGreen"5,000$ :ענה ראשון על התרגיל וזכה ב "#LGreen"%s"#Green" השחקן",zSTime(gettime()-MathStart),pName[playerid]);
			SendFormatMessageToAll(-1,""#Blue"• .%d :התשובה לתרגיל היתה",(Znumber[0] + Znumber[1] - Znumber[2]));
			DOF2_SetInt(PFile(playerid),"MathAns", DOF2_GetInt(PFile(playerid),"MathAns")+1);
			if(DOF2_GetInt(PFile(playerid),"BestMath") > DOF2_GetInt(SERVER_FILE, "BestActs"))
			{
				DOF2_SetString(SERVER_FILE,"BestMather",pName[playerid]);
				DOF2_SetInt(SERVER_FILE,"BestMath", DOF2_GetInt(PFile(playerid),"MathAns"));
				SendFormatMessageToAll(-1,""#Purple2"» "#LBlue"("#LYellow"%d:מספר תשובות"#LBlue")"#Orange"%s"#LBlue" :כרגע השחקן עם הכי הרבה תשובות למטמיקה הוא",DOF2_GetInt(PFile(playerid),"MathAns"),pName[playerid]);
			}
			KillTimer(TimerM);
			Y_GivePlayerMoney(playerid,5000);
			Znumber[0] = 0;
			Znumber[1] = 0;
			Znumber[2] = 0;
			return 1;
		}
		if(Ans != Znumber[0] + Znumber[1] - Znumber[2])
		{
			SendClientMessage(playerid, -1, ""#Red".תשובה שגויה, נסה שנית");
		}
		return 1;
	}
	if(Ans == Znumber[0] + Znumber[1])
	{
		SendFormatMessageToAll(-1,""#Green"• .(Time: "#LGreen"00:%s "#Green") "#LGreen"5,000$ :ענה ראשון על התרגיל וזכה ב "#LGreen"%s"#Green" השחקן",zSTime(gettime()-MathStart),pName[playerid]);
		SendFormatMessageToAll(-1,""#Blue"• .%d :התשובה לתרגיל היתה",(Znumber[0] + Znumber[1]));
		KillTimer(TimerM);
		Y_GivePlayerMoney(playerid,5000);
		Znumber[0] = 0;
		Znumber[1] = 0;
		Znumber[2] = 0;
		return 1;
	}
	if(Ans != (Znumber[0] + Znumber[1]))
	{
		SendClientMessage(playerid, -1, ""#Red".תשובה שגויה, נסה שנית");
	}
	return 1;
}
