CMD:hqs(playerid,params[])
{
	new String2[0x7D], DClans[5000], ABCD[5000], ClanName[0xFF], num = 0, Clans = 0;
	for(new w; w<MAX_CLANS; w++)
	{
		num++;
		format(String,sizeof(String),"Clan%d",num);
		ClanName = DOF2_GetString("Clans/ClanList.ini",String);
		if(DOF2_FileExists(ClanFile(ClanName)))
		{
			Clans++;
			if(Clans > 0)
			{
				format(String,sizeof(String),"{%s}%d) • %s (Have Hq: %s , Pickups: %d , Objects: %s , First Tele:%s)\n",DRGB2HEX(DOF2_GetInt(ClanFile(ClanName), "R"), DOF2_GetInt(ClanFile(ClanName), "G"), DOF2_GetInt(ClanFile(ClanName), "B"),100),Clans+0,ClanName, (DOF2_GetInt(ClanFile(ClanName),"HaveHQ")? ("יש"):("אין")), DOF2_GetInt(ClanFile(ClanName),"HQObjects"), DOF2_GetString(ClanFile(ClanName),"HQPickups"), DOF2_GetString(ClanFile(ClanName),"HQRTele"));
				strcat(DClans,String);
			}
			else
			{
				ShowPlayerDialog(playerid, 158949+2, DIALOG_STYLE_MSGBOX, ""#HPurple"רשימת מפקדות", "{CBD2D2}- .לא נמצאו מפקדות בשרת -", "אישור", "");
			}
		}
	}
	format(String2, sizeof(String2), "{CBD2D2}- /Clans :לרשימת הקלאנים בשרת -", Clans);
	format(ABCD, sizeof(ABCD), "%s\n\n%s", String2, DClans);
	ShowPlayerDialog(playerid, 158949+2, DIALOG_STYLE_MSGBOX, ""#HPurple"רשימת מפקדות", ABCD, "סגור", "");
	return 1;
}
