CMD:call(playerid, params[])
{
	if(GetPlayerState(playerid) == PLAYER_STATE_DRIVER )return SendClientMessage(playerid,-1,""#Red" .שנוהגים - לא מטלפנים");
	new id;
	if(sscanf(params,"u",id)) return SendClientMessage(playerid,-1,"/Call [ID]");
	if(!IsPlayerConnected(id)) return SendClientMessage(playerid,-1,""#Red" האיידי אינו מחובר");
	if(id==playerid)return SendClientMessage(playerid,-1,""#Red" .לא ניתן לשלוח לעצמך הודעה");
	if(InCall[playerid] == 1) return SendClientMessage(playerid,-1,""#Red" .אתה כבר נמצא בשיחה");
    if(InCall[id] == 1) return SendClientMessage(playerid,-1,""#Red" .שחקן זה בשיחה,אנא המתן שתגמר השיחה");
	if(DOF2_GetBool(PFile(id),"BlockCalls") == true) return SendClientMessage(playerid,-1,""#Red".שחקן זה חסם שיחות נכנסות");
	SendFormatMessage(playerid,-1,""#Blue"...אנא המתן למענה \"%s\" מבצע שיחה לשחקן",pName[id]);
	SendFormatMessage(id,-1,""#Yellow",מתקשר אליך \"%s\" השחקן",pName[playerid]);
	SendClientMessage(id,-1,""#LGreen"/Anscall -"#White" בכדי לענות לשיחה הקלד");
	SendClientMessage(id,-1,""#NoN"/Ignore -"#White" בכדי לדחות את השיחה הקלד");
	InCall[playerid]=0,InCall[id]=0;
	GetCall[playerid]=0,GetCall[id]=1;
	gc=id; sender=playerid;
	return 1;
}
//==============================================================================
CMD:ignore(playerid, params[])
{
	if(GetCall[playerid]==0)return SendClientMessage(playerid,-1,""#Red" .לא קבלת שום שיחה");
	SendFormatMessage(gc,-1,""#NoN".%s "#White":דחית את השיחה של השחקן",pName[sender]);
	SendFormatMessage(sender,-1,""#White" .לא ענה לשיחה "#LGreen"%s "#White":השחקן",pName[gc]);
	InCall[sender]=0,InCall[gc]=0;
	GetCall[sender]=0,GetCall[gc]=0;
	return 1;
}
//==============================================================================
CMD:anscall(playerid, params[])
{
	if(GetCall[playerid]==0)return SendClientMessage(playerid,-1,""#Red" .לא קבלת שום שיחה");
	if(InCall[playerid]==1)return SendClientMessage(playerid,-1,""#Red" .אתה כבר בשיחה");
	if(GetPlayerState(playerid) == PLAYER_STATE_DRIVER)return SendClientMessage(playerid,-1,""#Red" .שנוהגים - לא מטלפנים");
	SendFormatMessage(gc,-1,""#LGreen"%s "#White":ענית לשיחה של השחקן",pName[sender]);
	SendFormatMessage(sender,-1,""#White" .ענה לשיחה,כעת תוכלל דבר איתו "#LGreen"%s "#White":השחקן",pName[gc]);
	InCall[sender]=1;
	InCall[gc]=1;
	GetCall[sender]=0;
	GetCall[gc]=0;
	return 1;
}
//==============================================================================
CMD:hangup(playerid, params[])
{
	if(InCall[gc]==0)return SendClientMessage(gc,-1,""#Red" .אתה לא בשום שיחה כעת");
	if(gc==playerid)
	{
		SendFormatMessage(gc,-1,""#NoN".%s "#White":נתקת את השיחה לשחקן",pName[sender]);
		SendFormatMessage(sender,-1,""#White".ניתק את השיחה "#NoN"%s "#White":השחקן",pName[gc]);
		SetPlayerSpecialAction(gc,SPECIAL_ACTION_STOPUSECELLPHONE);
		SetPlayerSpecialAction(sender,SPECIAL_ACTION_STOPUSECELLPHONE);
		InCall[gc]=0,GetCall[gc]=0; InCall[sender]=0,GetCall[sender]=0;
	}
	if(sender==playerid)
	{
		SendFormatMessage(gc,-1,""#NoN".%s "#White":נתקת את השיחה לשחקן",pName[gc]);
		SendFormatMessage(sender,-1,""#White".ניתק את השיחה "#NoN"%s "#White":השחקן",pName[sender]);
		SetPlayerSpecialAction(gc,SPECIAL_ACTION_STOPUSECELLPHONE);
		SetPlayerSpecialAction(sender,SPECIAL_ACTION_STOPUSECELLPHONE);
		InCall[gc]=0,GetCall[gc]=0; InCall[sender]=0,GetCall[sender]=0;
	}
	return 1;
}
