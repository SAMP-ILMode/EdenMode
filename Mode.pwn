CMD:tree(playerid, params[])
{
	if(MoneyTree == false) return SendClientMessage(playerid, 0xb01111FF, ""#Red".האיזור אינו פתוח כעת");
	if(!InMoneyTree[playerid])
	{
		if(IsPlayerInAnyVehicle(playerid)) return SendClientMessage(playerid, 0xb01111FF, ""#Red".אינך יכול להיכנס לאזור זה עם רכב");
