CMD:tl(playerid, params[])
{
    new Array[15];
	if(sscanf(params, "s[15] ", Array)) return SendClientMessage ( playerid, -1, "/TL [Buy/Sell/Goto/Get/Accept/Deny]" );
	if(!strcmp(Array, "Buy", true))
	{
		if(DOF2_GetBool(PFile(playerid),"HaveTL") == true) return SendClientMessage(playerid,-1,""#Red"! יש ברשותך מערכת השתגרויות");
		if(Y_GetPlayerMoney(playerid) < 800000) return SendClientMessage(playerid,-1,""#Red"$800,000 :מחיר קניית השתגרויות");
		DOF2_SetBool(PFile(playerid),"HaveTL",true);
		SendFormatMessageToAll(-1,""#HRed"» "#Green"!קנה מערכת השתגרויות "#Blue"\"%s\" "#Green" השחקן",pName[playerid]);
		Y_GivePlayerMoney(playerid, -800000);
	}
	else if(!strcmp(Array, "Sell", true))
	{
		if(DOF2_GetBool(PFile(playerid),"HaveTL") == false) return SendClientMessage(playerid,-1,""#Red"! אין ברשותך מערכת השתגרויות");
		DOF2_SetBool(PFile(playerid),"HaveTL",true);
		SendFormatMessageToAll(-1,""#HRed"» "#Green"!מכר את מערכת ההשתגרויות שלו וקיבל חצי מהסכום "#Blue"\"%s\" "#Green" השחקן",pName[playerid]);
		Y_GivePlayerMoney(playerid, 400000);
	}
	else if(!strcmp(Array, "Goto", true))
	{
		if(DOF2_GetBool(PFile(playerid),"HaveTL") == false) return SendClientMessage(playerid,-1,""#Red"! אין ברשותך מערכת השתגרויות");
		if(sscanf(params,"s[15]u",Array,params[0])) return SendClientMessage(playerid,-1,"/TL Goto ID");
		if(IsInviteGOTO[params[0]] == true) return SendClientMessage(playerid,-1,""#Red".שחקן זה מוזמן להשתגרות");
	    SendFormatMessage(playerid,-1,""#HRed"» "#Green".להשתגר אליו "#Blue"\"%s\" "#Green" הזמנת את השחקן",pName[params[0]]);
	    SendFormatMessage(params[0],-1,""#HRed"» "#Green"/tl deny לביטול • /tl accept מזמין אותך להשתגר אליך, לאישור "#Blue"\"%s\" "#Green" השחקן",pName[params[0]]);
	    IsInviteGOTO[params[0]] = true;
	    IdToInviteGOTO[params[0]] = playerid;
	 	IsInviteTL[params[0]] = true;
    }
	else if(!strcmp(Array, "Get", true))
	{
		if(DOF2_GetBool(PFile(playerid),"HaveTL") == false) return SendClientMessage(playerid,-1,""#Red"! אין ברשותך מערכת השתגרויות");
		if(sscanf(params,"s[15]u",Array,params[0])) return SendClientMessage(playerid,-1,"/TL Get ID");
		if(IsInviteGOTO[params[0]] == true) return SendClientMessage(playerid,-1,""#Red".שחקן זה מוזמן להשתגרות");
	    SendFormatMessage(playerid,-1,""#HRed"» "#Green".להשתגר אליך "#Blue"\"%s\" "#Green" הזמנת את השחקן",pName[playerid]);
	    SendFormatMessage(params[0],-1,""#HRed"» "#Green"/tl deny לביטול • /tl accept מזמין אותך להשתגר אליו, לאישור "#Blue"\"%s\" "#Green" השחקן",pName[params[0]]);
	    IsInviteGET[params[0]] = true;
	    IdToInviteGET[params[0]] = playerid;
		IsInviteTL[params[0]] = true;
	}
	else if(!strcmp(Array, "Accept", true))
	{
		if(IsInviteTL[playerid] == false) return SendClientMessage(playerid,-1,""#Red".אינך מוזמן להשתגרות");
		if(!IsPlayerConnected(IdToInviteGET[playerid])) return SendClientMessage(playerid,-1,""#Red".שחקן זה אינו מחובר לשרת");
		new Float:zPos[3];
		GetPlayerPos(IdToInviteGET[playerid], zPos[0], zPos[1], zPos[2]);
		new Float:rPos[3];
		GetPlayerPos(playerid, rPos[0], rPos[1], rPos[2]);
		if(IsInviteGOTO[playerid] == true) return SetPlayerPos(playerid,zPos[0], zPos[1], zPos[2]);
		else if(IsInviteGOTO[playerid] == true) return SetPlayerPos(IdToInviteGET[playerid],rPos[0], rPos[1], rPos[2]);
		SendFormatMessage(IdToInviteGET[playerid],-1,""#HRed"» "#Green"!אישר את הזמנת ההשתגרות שלך "#Blue"\"%s\" "#Green"השחקן",pName[playerid]);
		SendClientMessage(playerid,-1,""#LGreen".אישרת את ההזמנה בהצלחה");
	    IsInviteGET[params[0]] = false;
	    IdToInviteGET[params[0]] = -1;
	    IsInviteGOTO[params[0]] = false;
	    IdToInviteGOTO[params[0]] = -1;
		IsInviteTL[params[0]] = false;
	}
	return 1;
}

