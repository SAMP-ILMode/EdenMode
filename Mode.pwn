CMD:pickupguns(playerid, params[])
{
	if(GetPlayerState(playerid) != PLAYER_STATE_ONFOOT) return SendClientMessage(playerid, 0xE0E0E0FF, ""#Red".לא ניתן לבצע פקודה ברכב");
	new f = MAX_DROPGUN+1;
	for(new a = 0; a < MAX_DROPGUN; a++)
	{
		if(IsPlayerInRangeOfPoint(playerid, 5.0, dGunData[a][ObjPos][0], dGunData[a][ObjPos][1], dGunData[a][ObjPos][2]))
		{
			f = a;
			break;
		}
	}
	if(f > MAX_DROPGUN) return SendClientMessage(playerid, 0x33AA3300, ""#Red".אתה לא ליד נשק ולכן ולא תוכל להרים אותו");
	DestroyObject(dGunData[f][ObjID]);
	Delete3DTextLabel(TextGun[playerid]);
	GivePlayerWeapon(playerid, dGunData[f][ObjData][0], dGunData[f][ObjData][1]);
	dGunData[f][ObjPos][0] = 0.0;
	dGunData[f][ObjPos][1] = 0.0;
	dGunData[f][ObjPos][2] = 0.0;
	dGunData[f][ObjID] = -1;
//	dGunData[f][ObjData][0] = 0;
	dGunData[f][ObjData][1] = 0;
	SendFormatMessage(playerid,-1,""#Yellow"» "#White"%s"#Purple2" :הרמת את הנשק", GunNames[dGunData[f][ObjData][0]]);
	return 1;
}
//==============================================================================
CMD:dropguns(playerid, params[])
{
	if(GetPlayerState(playerid) != PLAYER_STATE_ONFOOT) return SendClientMessage(playerid, 0xE0E0E0FF, ""#Red".לא ניתן לבצע פקודה ברכב");
	new GunID = GetPlayerWeapon(playerid);
	new GunAmmo = GetPlayerAmmo(playerid);
	if(GunID > 0 && GunAmmo != 0)
	{
		new f = MAX_DROPGUN+1;
		for(new a = 0; a < MAX_DROPGUN; a++)
		{
			if(dGunData[a][ObjPos][0] == 0.0)
			{
				f = a;
				break;
			}
		}
		if(f > MAX_DROPGUN) return SendClientMessage(playerid, 0x33AA3300, ""#Red".אין לך נשקים ולכן לא תוכל להפיל נשקים");
		RemovePlayerWeapon(playerid, GunID);
		dGunData[f][ObjData][0] = GunID;
		dGunData[f][ObjData][1] = GunAmmo;
		GetPlayerPos(playerid, dGunData[f][ObjPos][0], dGunData[f][ObjPos][1], dGunData[f][ObjPos][2]);
		dGunData[f][ObjID] = CreateObject(GunObjects[GunID], dGunData[f][ObjPos][0], dGunData[f][ObjPos][1], dGunData[f][ObjPos][2]-1, 93.7, 120.0, 120.0);
		new str[128];
		format(str,sizeof(str),""#Grey"••• "#Purple2" /PickupGuns :בכדי להרים את הנשק "#Grey"•••\n"#Grey"••• "#Purple2" %s :סוג הנשק "#Purple2"", GunNames[dGunData[f][ObjData][0]]);
		TextGun[playerid] = CreateDynamic3DTextLabel(str,-1,dGunData[f][ObjPos][0], dGunData[f][ObjPos][1], dGunData[f][ObjPos][2], 50.0, INVALID_PLAYER_ID, INVALID_VEHICLE_ID, 0, 0, 0, -1, 15.0 );
		SendFormatMessage(playerid,-1,""#LOrange"%s"#Brown" :הפלת את הנשק", GunNames[dGunData[f][ObjData][0]]);
	}
	return 1;
}
