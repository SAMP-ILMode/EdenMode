CMD:orgs(playerid,params[])
{
	new String2[0x7D], DClans[5000], ABCD[5000], OrgName[0xFF], num = 0, Orgs = 0;
	for(new w; w<MAX_ORGS; w++)
	{
		num++;
		format(String,sizeof(String),"Org%d",num);
		OrgName = DOF2_GetString("Orgs/OrgsList.ini",String);
		if(fexist(OrgFile(OrgName)))
		{
			Orgs++;
			if(Orgs > 0)
			{
				format(String,sizeof(String),""#TBlue"%d) • %s (Founder: %s | Players: %s)\n",Orgs+0,OrgName, DOF2_GetInt(OrgFile(OrgName),"Founder"), DOF2_GetString(OrgFile(OrgName),"Players"));
				strcat(DClans,String);
			}
			else
			{
				ShowPlayerDialog(playerid, 1949+2, DIALOG_STYLE_MSGBOX, ""#HPurple"רשימת ארגונים", "{CBD2D2}- .לא נמצאו ארגונים בשרת -", "אישור", "");
			}
		}
	}
	format(String2, sizeof(String2), "{CBD2D2}- %d :מספר הקלאנים הקיימים בשרת-", Orgs);
	format(ABCD, sizeof(ABCD), "%s\n\n%s", String2, DClans);
	ShowPlayerDialog(playerid, 58949+2, DIALOG_STYLE_MSGBOX, ""#HPurple"רשימת ארגונים", ABCD, "סגור", "");
	return 1;
}
