				PlayerCD(i, 5);
		        DOF2_SetInt(cwfile, "CWPlayers", b);
		        DOF2_SetInt(zfile, "CWPlayers", b);
			}
		}
	}
	else if(!strcmp(Option, "Cancel", true))
	{
		new
		    cwfile[32],
		    zfile[32]
		;
		format(cwfile, 32, "Clans/%s.ini", GetClanName(playerid));
		format(zfile, 32, "Clans/%s.ini", DOF2_GetString(cwfile, "CWInvite"));
		if(DOF2_GetBool(PFile(playerid),"HaveClan") == false) return SendClientMessage(playerid,-1,""#Red".אינך נמצא בקלאן");
		if(DOF2_GetInt(PFile(playerid),"ClanLevel") < 4) return SendClientMessage(playerid,-1,""#Red".אינך לידר בקלאן");
