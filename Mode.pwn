strcomp(const str1[], const str2[])
{
	new TimesCorrect;
	for(new i = 0; i < strlen(str1); i++)
	{
		if(str1[i] == str2[i])TimesCorrect++;
	}
	if(TimesCorrect == strlen(str1)&&TimesCorrect == strlen(str2)) return true;
	else return false;
}

