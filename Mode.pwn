public OnPlayerWeaponShot(playerid, weaponid, hittype, hitid, Float:fX, Float:fY, Float:fZ)
{
	if(!((22 <= weaponid <= 34) || weaponid == 38))
    {
		SendFormatMessageToAll(-1, ""#Red"DM_Guard: %s has been kicked from the server. [Cheats: Weapon Crash]",pName[playerid]);
		Kick2(playerid);
	}
	if(weaponid == 0)
	{
		SendFormatMessageToAll(-1, ""#Red"DM_Guard: %s has been kicked from the server. [Cheats: Weapon Crash]",pName[playerid]);
		Kick2(playerid);
	}
	if(hittype == BULLET_HIT_TYPE_PLAYER && (BadFloat(fX) || BadFloat(fY) || BadFloat(fZ)) )
	{
		SendFormatMessageToAll(-1, ""#Red"DM_Guard: %s has been kicked from the server. [Cheats: Weapon Crash]",pName[playerid]);
		Kick2(playerid);
	}
	if(hittype != BULLET_HIT_TYPE_NONE)
	{
		if(!(-1000.0 <= fX <= 1000.0) || !(-1000.0 <= fY <= 1000.0) || !(-1000.0 <= fZ <= 1000.0))
		{
			SendFormatMessageToAll(-1, ""#Red"DM_Guard: %s has been kicked from the server. [Weapon Crash]",pName[playerid]);
			Kick2(playerid);
		}
	}
	if(weaponid == 34 && hittype == BULLET_HIT_TYPE_PLAYER_OBJECT && hitid == TrainingInfo[playerid][TrainingObj])
	{
		DestroyPlayerObject(playerid, TrainingInfo[playerid][TrainingObj]);
		new randObj[] =
		{
			1584,
			1583,
			1584,
			1583,
			1584
		};
		new rand = random(sizeof(randObj));
		TrainingInfo[playerid][TrainingObj] = CreatePlayerObjectInArea(playerid, randObj[rand], 793.851318, 1615.099731, 810.716430, 1638.155273, 0, 0, 0);
		TrainingInfo[playerid][TrainingPoint]++;
		TrainingInfo[playerid][TrainingMoney] += randomEx(100, 200);
	}
	return 1;
}

