CMD:stopacts(playerid, params[])
{
	if(IsAdmin(playerid) < 1 && IsActer(playerid) < 1) return 0;
	if(!ActStatus) return SendClientMessage(playerid, 0xff0000ff, "אין פעילויות עובדות כרגע");
    new reason[30];
    if(sscanf(params, "z", reason)) return 1;
    if(isnull(reason)) reason = "האדמין לא הקליד סיבה";
	SendFormatMessageToAll(-1,""#Red":סגר את הפעיליות %s האדמין",pName[playerid]);
	SendFormatMessageToAll(-1,""#Red"(%s)",reason);
	KillTimer(ActTimer);
	KillTimer(ActTimer2);
	IsActOn = 0;
	ActStatus = 0;
	ActPlayers = 0;
	if(ActRezef != 0)
	{
	    if(ActRezef == 0) return 0;
		new best = -9999;
  		new id = -1;
		for(new i; i <= GetPlayerPoolSize(); i++)
		{
			if(RezefPoints[i] && RezefPoints[i] >= best)
			{
				best = RezefPoints[i];
				id = i;
			}
		}
		new money = random(5000);
		SendClientMessageToAll(-1,""#LOrange"---------- [ Activity Strike ] ----------");
		SendClientMessageToAll(-1,""#Orange"• !רצף הפעיליות הסתיים כי האדמין סגר את הפעיליות");
		SendFormatMessageToAll(-1,""#Yellow"• %d :מספר הפעיליות שהופעלו",ActRezef);
		if(id != -1)
		{
			SendFormatMessageToAll(-1,""#zYellow"• %s :השחקן אשר עם הכי הרבה נקודות",pName[id]);
			SendFormatMessageToAll(-1,""#zYellow"• %d :כמות פעיליות אשר השחקן זכה",best);
			SendFormatMessageToAll(-1,""#zYellow"• $%s :לכן הוא זכה בסכום",PSIK(money));
		}
		else SendClientMessageToAll(-1,""#zYellow"• - אין שחקן אשר זכה ברצף -");
		SendClientMessageToAll(-1,""#LYellow"----------------------------------------------");
	    ActRezef = 0;
		ActRAmount = 0;
		foreach(new i: Player) RezefPoints[i] = 0;
		SendRconCommand("hostname " SERVER_HOST"");
	}
	for(new i; i <= GetPlayerPoolSize(); i++)
	{
		if(InAct[i])
		{
			InAct[i] = false;
			if(ActStatus == 2)
			{
				OnPlayerSpawn(i);
				InAct[i] = false;
				DisablePlayerRaceCheckpoint(i);
				SetPlayerVirtualWorld(i,0);
				InActCP[i] = 0;
				KillTimer(ActTimer3[i]);
				RaceTime[i] = 0;
			}
		}
	}
	for(new i; i<sizeof(ActVehicle); i++) DestroyVehicle(ActVehicle[i]), RemoveVehicleMissiles(ActVehicle[i]);
	return 1;
}
