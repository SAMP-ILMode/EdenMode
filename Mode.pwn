CMD:sawn(playerid, params[])
{
//==============================================================================
	if(!InSawn[playerid])
	{
		switch(SawnPlayers)
		{
			case 0:
			{
				SawnPlayers++;
				InSawn[playerid] = true;
				SetPlayerPosEx(playerid, SawnBattleSpawns[0][0], SawnBattleSpawns[0][1], SawnBattleSpawns[0][2]);
				SetPlayerFacingAngle(playerid, SawnBattleSpawns[0][3]);
				ResetPlayerWeapons(playerid);
				GivePlayerWeapon(playerid, 26, 9999);
				GivePlayerWeapon(playerid, 22, 15);
				GivePlayerWeapon(playerid, 28, 15);
				SetPlayerHealth(playerid, 100);
				SetPlayerArmour(playerid, 100);
				SawnPID = playerid;
				SawnWorld = randomEx(1, 10000);
				SetPlayerInterior(playerid,0);
				SetPlayerVirtualWorld(playerid, SawnWorld);
				TogglePlayerControllable(playerid, 0);
				SetCameraBehindPlayer(playerid);
				for(new i; i <= GetPlayerPoolSize(); i++) if(DOF2_GetBool(PFile(i),"SawnMessages"))
				{
					SendFormatMessage(i,-1, ""#TBlue" (/Settings: כיבוי הודעות)/Sawn - היצטרף לקרב סאון אוטומטי, להיצטרפות{%06x}%s",GetPlayerColor(playerid) >>> 8, pName[playerid]);
				}
			}
			case 1:
			{
				SawnPlayers++;
				InSawn[playerid] = true;
				SetPlayerPosEx(playerid, SawnBattleSpawns[1][0], SawnBattleSpawns[1][1], SawnBattleSpawns[1][2]);
				SetPlayerFacingAngle(playerid, SawnBattleSpawns[1][3]);
				SawnEID[playerid] = SawnPID;
				SawnEID[SawnPID] = playerid;
				ResetPlayerWeapons(playerid);
				GivePlayerWeapon(playerid, 26, 9999);
				GivePlayerWeapon(playerid, 22, 15);
				GivePlayerWeapon(playerid, 28, 15);
				SetPlayerHealth(playerid, 100);
				SetPlayerArmour(playerid, 100);
				SetPlayerInterior(playerid,0);
				SetPlayerVirtualWorld(playerid, SawnWorld);
				TogglePlayerControllable(playerid, 0);
				SetCameraBehindPlayer(playerid);
				for(new i; i <= GetPlayerPoolSize(); i++) if(DOF2_GetBool(PFile(i),"SawnMessages"))
				{
					SendFormatMessage(i,-1, ""#TBlue" (/Settings: כיבוי הודעות)!{%06x}%s "#TBlue"היצטרף לקרב סאון של השחקן {%06x}%s",GetPlayerColor(SawnPID) >>> 8, pName[SawnPID],GetPlayerColor(playerid) >>> 8, pName[playerid]);
				}
				SawnPlayers = 0;
				PlayerCD(playerid, 3);
				PlayerCD(SawnPID, 3);
			}
		}
	}
	return 1;
}

