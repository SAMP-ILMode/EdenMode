//==================================[MOrgs]=====================================
CMD:createorg(playerid,params[])
{
	if(!IsMClan(playerid)) return 0;
	new orgname[256];
	if(sscanf(params,"s[256]",orgname)) return SendClientMessage(playerid, -1, "/CreateOrg [Org] ");
	new file[50];
	format(file, sizeof(file), "Orgs/%s.ini", orgname);
	DOF2_CreateFile(file);
	DOF2_SetString(file, "Founder", "None");
	DOF2_SetInt(file, "Players",0);
 	SendFormatMessageToAll(-1,""#Orange">• "LYellow"%s"#Grey" פתח את הארגון %s מנהל הקלאנים", orgname, pName[playerid]);
	if(!DOF2_FileExists("Orgs/OrgsList.ini")) DOF2_CreateFile("Orgs/OrgsList.ini");
	for(new i=1; i<MAX_ORGS; i++)
	{
		format(String, 0x7D, "Org%d", i);
		if(!DOF2_IsSet("Orgs/OrgsList.ini", String))return DOF2_SetString("Orgs/OrgsList.ini", String, orgname);
	}
	return 1;
}
//==============================================================================
CMD:delorg(playerid,params[])
{
	if(!IsMClan(playerid)) return 0;
	new orgname[256],str[256];
	if(sscanf(params,"s[256]",orgname)) return SendClientMessage(playerid, -1, "/DelOrg [Org] ");
	format(ofile, sizeof(ofile), "Orgs/%s.ini", orgname);
	if(!DOF2_FileExists(ofile)) return SendClientMessage(playerid, 0xFF0000C8, "!ארגון זה איננו קיים בשרת");
	format(str, sizeof(str), "Org%d = %s", DOF2_GetInt(ofile, "N") , orgname);
	DOF2_Unset("/Orgs/OrgsList.ini",str);
	SendFormatMessageToAll(-1,""#Orange">•"#LGreen" .מרשימת הקלאנים "LYellow"%s"#Grey" מחק את הקלאן %s האדמין", orgname, pName[playerid]);
	for(new i; i <= GetPlayerPoolSize(); i++)
	{
		if(DOF2_GetBool(PFile(i),"HaveOrg") == true && !strcmp(orgname,GetOrgName(i),true))
		{
			DOF2_SetBool(PFile(i),"HaveOrg",false);
			DOF2_SetString(PFile(i), "Org","אין");
			DOF2_SetInt(PFile(i), "OrgLevel",0);
		}
	}
	DOF2_RemoveFile(ofile);
	return 1;
}
