public OnPlayerStateChange(playerid, newstate, oldstate)
{
//====================================[AFK]=====================================
	if(InAFK[playerid])
	{
	    TogglePlayerControllable(playerid,1);
		SendFormatMessageToAll(-1, ""#LGreen"» (AFK)"#White" .חזר אל לוח המקשים "#LGreen"%s "#White"השחקן",pName[playerid]);
		TimeInAFK[playerid] = 0;
		InAFK[playerid] = false;
	}
	else
	{
	    InAFK[playerid] = false;
	    TimeInAFK[playerid] = 0;
	}
//==============================================================================
    if(InAct[playerid] == true && oldstate == PLAYER_STATE_DRIVER && ActStatus == 2 && Acts[IsActOn][ModelVeh] != -1 && Acts[IsActOn][Type] != ACT_FINDTHECAR_TYPE)
	{
	 	ActPlayers--;
		InAct[playerid] = false;
		DisablePlayerRaceCheckpoint(playerid);
		InActCP[playerid] = 0;
		RaceTime[playerid] = 0;
		FJoin = false;
		HaveFirst = false;
		HaveSecond = false;
		HaveThree = false;
  		OnPlayerSpawn(playerid);
		SetPlayerVirtualWorld(playerid,0);
		ResetPlayerWeapons(playerid);
	 	if(ActPlayers == 1)
		{
			for(new i=0; i<MAX_PLAYER_ATTACHED_OBJECTS; i++)
			{
				if(IsPlayerAttachedObjectSlotUsed(playerid, i)) RemovePlayerAttachedObject(playerid, i);
			}
			for(new b; b<sizeof(ActVehicle); b++) DestroyVehicle(ActVehicle[b]), RemoveVehicleMissiles(ActVehicle[b]);
			for(new i; i <= GetPlayerPoolSize(); i++) if(InAct[i])
			{
				SendClientMessageToAll(-1,""#Blue"• • • • • • [ פעילות אוטומטית ] • • • • • •");
				SendFormatMessageToAll(-1,""#NoN"!הפעילות "#Red"%s "#NoN"הסתיימה",Acts[IsActOn][ActName]);
				SendFormatMessageToAll(-1,""#Blue2"!00:%s "#Blue"זמן הפעילות ,"#Blue2"%s "#Blue"הזוכה בפעילות",zSTime(gettime()-ActStartTime),pName[i]);
				SendFormatMessageToAll(-1,""LGreen"%d Points + %s$: לכן הוא זכה בסכום",Acts[IsActOn][ActPoints],PSIK(Acts[IsActOn][ActPrice]));
				if(DOF2_GetBool(PFile(i), "HaveClan"))
				{
				    new
						rndp = randomEx(400, 1000),
						file[32]
					;
				    format(file, 32, "Clans/%s.ini", GetClanName(i));
					SendFormatMessageToAll(-1,""Yellow"* !נכנסת לקופת הקלאן %s$ {%s}%s"Yellow" מפני שהמנצח נמצא בקלאן",PSIK(rndp),GetHColor(rgba2hex(DOF2_GetInt(file,"R"), DOF2_GetInt(file, "G"), DOF2_GetInt(file, "B"),100)),GetClanName(i));
					DOF2_SetInt(file, "Bank", DOF2_GetInt(file, "Bank")+rndp);
				}
				if(ActRezef != 0)
				{
				    RezefPoints[i]++;
				    SendFormatMessageToAll(-1,""#Yellow"%d :כמות נקודות שצבר ברצף הפעיליות",RezefPoints[i]);
				}
				SendClientMessageToAll(-1,""#Blue"• • • • • • • • • • • • • • • • • • • • • • • • •");
				Y_GivePlayerMoney(i, Acts[IsActOn][ActPrice]);
				GivePlayerPoints(i, Acts[IsActOn][ActPoints]);
				IsActOn = 0;
				InAct[i] = false;
			    OnPlayerSpawn(i);
				SetPlayerInterior(i,0);
				SetPlayerVirtualWorld(i,0);
				ActPlayers = 0;
				ActStatus = 0;
				CanRock[i] = false;
				SetPlayerHealth(i, 100);SetPlayerArmour(i, 0);
                DOF2_SetInt(PFile(i),"ActsWins", DOF2_GetInt(PFile(i),"ActsWins")+1);
				ResetPlayerWeapons(i);
				if(DOF2_GetInt(PFile(i),"ActsWins") > DOF2_GetInt(SERVER_FILE, "BestActs"))
				{
					DOF2_SetString(SERVER_FILE,"BestActWin",pName[i]);
					DOF2_SetInt(SERVER_FILE,"BestActs", DOF2_GetInt(PFile(i),"ActsWins"));
					SendFormatMessageToAll(-1,""#Purple2"» "#LBlue"("#LYellow"%d:מספר צחונות"#LBlue")"#Orange"%s"#LBlue" :כרגע השחקן עם הכי הרבה נצחנות הפעיליות הוא",DOF2_GetInt(PFile(i),"ActsWins"),pName[i]);
				}
				if(ActRezef != ActRAmount)
				{
				    if(ActRezef == 0) return 0;
				 	new qs = random(sizeof(Acts));
					if(!ActStatus)
					{
						IsActOn = qs;
						ActPlayers = 0;
						ActStatus = 1;
						ActCD(30);
						ActRezef++;
						SendFormatMessageToAll(-1,""#Amit_B":פעילות מספר #%d(%d) ברצף הפעיליות",ActRezef,ActRAmount);
						new zstr[60];
						format(zstr,sizeof(zstr),"teamMatch - Evolution DM " SERVER_HOST"[Activities (%d/%d)]", ActRezef, ActRAmount);
						SendRconCommand(zstr);
						new best = -9999;
      					new id = -1;
						for(new zws; zws <= GetPlayerPoolSize(); zws++) if(IsPlayerConnected(zws))
						{
							if(RezefPoints[zws] >= best)
							{
								best = RezefPoints[zws];
        						id = i;
							}
						}
						switch(Acts[IsActOn][Type])
						{
							case ACT_WAR_TYPE:
							{
								SendClientMessageToAll(-1,""#Blue"• • • • • • [ פעילות אוטומטית ] • • • • • •");
								SendFormatMessageToAll(-1,""#Red"!הפעילות "#NoN"%s "#Red"תתחיל בעוד כחצי דקה",Acts[IsActOn][ActName]);
								SendFormatMessageToAll(-1,""LGreen"%s או /join להצטרפות",Acts[IsActOn][ActJoin]);
								SendClientMessageToAll(-1,""Red"** .$עלות השתתפות בפעילות: 200");
								SendFormatMessageToAll(-1,""#Yellow"%d Points + %s$: המנצח יזכה בסכום",Acts[IsActOn][ActPoints],PSIK(Acts[IsActOn][ActPrice]));
								if(id != -1) SendFormatMessageToAll(-1,""#TYellow"מוביל ברצף הפעיליות: "#LGreen"%d "#Pink"%s "#TYellow"כעת השחקן",best, pName[id]);
								else SendClientMessageToAll(-1,""#HRed"** כעת אין שחקן אשר מוביל ברצף הפעיליות");
								SendClientMessageToAll(-1,""#Blue"• • • • • • • • • • • • • • • • • • • • • • • • •");
							}
							case 2..3:
							{
								SendClientMessageToAll(-1,""#Blue"• • • • • • [ פעילות אוטומטית ] • • • • • •");
								SendFormatMessageToAll(-1,""#Green"!המירוץ "#Amit_B"%s "#Green"תתחיל בעוד כחצי דקה",Acts[IsActOn][ActName]);
								SendFormatMessageToAll(-1,""Green"%s או /join להצטרפות למירוץ",Acts[IsActOn][ActJoin]);
								SendClientMessageToAll(-1,""Red"** .$עלות השתתפות בפעילות: 200");
								SendFormatMessageToAll(-1,""#LYellow"%d Points + %s$: המנצח יזכה בסכום",Acts[IsActOn][ActPoints],PSIK(Acts[IsActOn][ActPrice]));
								if(id != -1) SendFormatMessageToAll(-1,""#TYellow"מוביל ברצף הפעיליות: "#LGreen"%d "#Pink"%s "#TYellow"כעת השחקן",best, pName[id]);
								else SendClientMessageToAll(-1,""#HRed"** כעת אין שחקן אשר מוביל ברצף הפעיליות");
								SendClientMessageToAll(-1,""#Blue"• • • • • • • • • • • • • • • • • • • • • • • • •");
							}
							case 4..10:
							{
								SendClientMessageToAll(-1,""#Blue"• • • • • • [ פעילות אוטומטית ] • • • • • •");
								SendFormatMessageToAll(-1,""#Orange"!הפעילות "#LYellow"%s "#Orange"תתחיל בעוד כחצי דקה",Acts[IsActOn][ActName]);
								SendFormatMessageToAll(-1,""Orange"%s או /join להצטרפות",Acts[IsActOn][ActJoin]);
								SendClientMessageToAll(-1,""Red"** .$עלות השתתפות בפעילות: 200");
								SendFormatMessageToAll(-1,""#NoN"%d Points + %s$: המנצח יזכה בסכום",Acts[IsActOn][ActPoints],PSIK(Acts[IsActOn][ActPrice]));
								if(id != -1) SendFormatMessageToAll(-1,""#TYellow"מוביל ברצף הפעיליות: "#LGreen"%d "#Pink"%s "#TYellow"כעת השחקן",best, pName[id]);
								else SendClientMessageToAll(-1,""#HRed"** כעת אין שחקן אשר מוביל ברצף הפעיליות");
								SendClientMessageToAll(-1,""#Blue"• • • • • • • • • • • • • • • • • • • • • • • • •");
							}
						}
			            if(DOF2_GetInt(SERVER_FILE, "BestActs")) SendFormatMessageToAll(-1,""#Yellow"/records - נמצא עם הכי הרבה נצחונות בפעיליות, זכיות: %d מתוך %s כעת השחקן",DOF2_GetInt(SERVER_FILE, "BestActs"), DOF2_GetString(SERVER_FILE, "BestActWin"));
					}
				}
				else
				{
					if(ActRezef == 0) return 0;
					new
						best = -9999,
						id = -1
					;
					for(new mrzz; mrzz <= GetPlayerPoolSize(); mrzz++)
					{
						if(RezefPoints[mrzz] >= best)
						{
							best = RezefPoints[mrzz];
							id = mrzz;
						}
					}
					new money = random(5000);
					SendClientMessageToAll(-1,""#LOrange"---------- [ Activity Strike ] ----------");
					SendClientMessageToAll(-1,""#Orange"• !רצף הפעיליות הסתיים");
					SendFormatMessageToAll(-1,""#Yellow"• %d :מספר הפעיליות שהופעלו",ActRezef);
					if(id != -1)
					{
						SendFormatMessageToAll(-1,""#zYellow"• %s :השחקן אשר עם הכי הרבה נקודות",pName[id]);
						SendFormatMessageToAll(-1,""#zYellow"• %d :כמות פעיליות אשר השחקן זכה",best);
						SendFormatMessageToAll(-1,""#zYellow"• $%s :לכן הוא זכה בסכום",PSIK(money));
					}
					else SendClientMessageToAll(-1,""#zYellow"• - אין שחקן אשר זכה ברצף -");
					SendClientMessageToAll(-1,""#LYellow"----------------------------------------------");
				    ActRezef = 0;
					ActRAmount = 0;
					for(new bz; bz <= GetPlayerPoolSize(); bz++) RezefPoints[bz] = 0;
					SendRconCommand("hostname " SERVER_HOST"");
				}
				break;
			}
			SetTimer("DestroyGlassObject", 5000, false);
		}
		SendClientMessage(playerid, 0xff0000ff, ".יצאת מהפעילות");
	}
	if(oldstate == PLAYER_STATE_DRIVER || oldstate == PLAYER_STATE_PASSENGER && SCarIn[playerid])
	{
		vHaveGM[SCarCreate[playerid]] = false;
		DestroyObject(VObjects[playerid]);
		DestroyVehicle(SCarCreate[playerid]);
		SCarIn[playerid] = false;
		Delete3DTextLabel(SCarText3D[SCarCreate[playerid]]);
	}
	if(oldstate == PLAYER_STATE_DRIVER || oldstate == PLAYER_STATE_PASSENGER && IsValidObject(VipCow[playerid]))
	{
		LinkVehicleToInterior(GetPlayerVehicleID(playerid), 0);
		DestroyObject(VipCow[playerid]);
	}

    if(InMRace[playerid] == 1 && oldstate == PLAYER_STATE_DRIVER)
	{
		InMRace[playerid] = 0;
		SMRace = 0;
		KillTimer(MissionTimer);
		DestroyVehicle(VMRace);
		OnPlayerSpawn(playerid);
		SetPlayerVirtualWorld(playerid,0);
		SendFormatMessageToAll(-1,""#Gold"» ."#Orange"Race "#Gold"נכשל במשימת "#Red"\"%s\""#Gold" השחקן ", pName[playerid]);
		DisablePlayerRaceCheckpoint(playerid);
	}
    if(InMPilot[playerid] == 1 && oldstate == PLAYER_STATE_DRIVER)
	{
		InMPilot[playerid] = 0;
		SMPilot = 0;
		KillTimer(MissionTimer);
		DestroyVehicle(VMPilot);
		OnPlayerSpawn(playerid);
		SetPlayerVirtualWorld(playerid,0);
		SendFormatMessageToAll(-1,""#Gold"» ."#Orange"Pilot "#Gold"נכשל במשימת "#Red"\"%s\""#Gold" השחקן ", pName[playerid]);
		DisablePlayerRaceCheckpoint(playerid);
	}
    if(InMD[playerid] == 1 && oldstate == PLAYER_STATE_DRIVER)
	{
		InMD[playerid] = 0;
		SMD = 0;
		KillTimer(MissionTimer);
		DestroyVehicle(VMD);
		OnPlayerSpawn(playerid);
		SetPlayerVirtualWorld(playerid,0);
		SendFormatMessageToAll(-1,""#Gold"» ."#Orange"Drift "#Gold"נכשל במשימת "#Red"\"%s\""#Gold" השחקן ", pName[playerid]);
		DisablePlayerRaceCheckpoint(playerid);
	}
    if(InStuntM[playerid] == 1 && oldstate == PLAYER_STATE_DRIVER)
	{
		InStuntM[playerid] = 0;
		OnPlayerSpawn(playerid);
		SetPlayerVirtualWorld(playerid,0);
		SendFormatMessageToAll(-1,""#Gold"» ."#Orange"Stunt(#%d) "#Gold"נכשל במשימת "#Red"\"%s\""#Gold" השחקן ",StuntM[playerid][stuntid], pName[playerid]);
	    KillTimer(StuntTimer[playerid]);
	}
	if(AntiFall[playerid] == 1)
	{
		if(oldstate == PLAYER_STATE_DRIVER)
		{
			if(newstate == PLAYER_STATE_ONFOOT)
			{
				if(AFInCar[playerid] == 1)
				{
					PutPlayerInVehicle(playerid, AFWhatCar[playerid], Driver);
				}
			}
		}
		if(oldstate == PLAYER_STATE_PASSENGER)
		{
			if(newstate == PLAYER_STATE_ONFOOT)
			{
				if(AFInCar[playerid] == 1)
				{
					PutPlayerInVehicle(playerid, AFWhatCar[playerid], Passanger);
				}
			}
		}
		if(oldstate == PLAYER_STATE_ONFOOT)
		{
			if(newstate == PLAYER_STATE_DRIVER || PLAYER_STATE_PASSENGER)
			{
				AFInCar[playerid] = 1;
				AFWhatCar[playerid] = GetPlayerVehicleID(playerid);
			}
		}
	}
	if(oldstate==PLAYER_STATE_ONFOOT)
	{
		RemovePlayerAttachedObject(playerid, 0);
		OldWeapon[playerid]=0;
		HoldingWeapon[playerid]=0;
	}
	if(hSpeedActiv[playerid] == 1)
	{
		if(newstate == PLAYER_STATE_DRIVER)
		{
			TextDrawShowForPlayer(playerid,TDSpeed0);
			PlayerTextDrawShow(playerid, TDSpeed1[playerid]);
			PlayerTextDrawShow(playerid, TDSpeed2[playerid]);
			PlayerTextDrawShow(playerid, TDSpeed3[playerid]);
		}
		if(oldstate == PLAYER_STATE_DRIVER)
		{
			TextDrawHideForPlayer(playerid,TDSpeed0);
			PlayerTextDrawHide(playerid, TDSpeed1[playerid]);
			PlayerTextDrawHide(playerid, TDSpeed2[playerid]);
			PlayerTextDrawHide(playerid, TDSpeed3[playerid]);
		}
	}
	if(newstate == PLAYER_STATE_DRIVER)
	{
		if((GetTickCount()-GetPVarInt(playerid, "cartime")) < 1000)
		{
			SetPVarInt(playerid, "carspam", GetPVarInt(playerid, "carspam")+1);
			if(GetPVarInt(playerid, "carspam") >= 5)
			{
				SendFormatMessageToAll(-1,""#Red"DM_Guard: %s has been kicked. [Cheats: Car Spamming]",pName[playerid]);
				return Kick2(playerid);
			}
		}
		SetPVarInt(playerid, "cartime", GetTickCount());
	}
	return 1;
}

//============================[PlayerConncet]===================================
public OnPlayerConnect(playerid)
{
	if(IsActer(playerid) || IsHelper(playerid) || IsNighter(playerid) || IsDJ(playerid) || IsAdmin(playerid)) StaffTime[playerid] = gettime();
	GetPlayerName(playerid,pName[playerid],MAX_PLAYER_NAME+1);
	format(pName[playerid], MAX_PLAYER_NAME, "%s", strreplace("_", " ", pName[playerid]));
	for(new i; i < sizeof(illegalMames); i++)
	{
		if(strfind(pName[playerid], illegalMames[i], true) != -1)
		{
		    SendFormatMessage(playerid, -1, ""#Red"(%s)!שם זה מכיל מסרים או פירושים אשר אסורים בשרת, שנה ניק וכנס שוב",illegalMames[i]);
			SendFormatMessageToAll(-1,""#Red"DM_Guard: %s has been kicked. [Illegal NickName]",pName[playerid]);
            Kick2(playerid);
		}
	}
	for(new i; i <= GetPlayerPoolSize(); i++)
	{
		new str[40];
		format(str, 40, "%s_0",pName[i]);
		if(strfind(pName[playerid], str, true) != -1)
		{
			SendFormatMessageToAll(-1,""#Red"DM_Guard: %s has been kicked. [Cheats: Bot Spam]",pName[playerid]);
            Kick2(playerid);
		}
	}
	Online++;
	MiniKiller[playerid] = 0;
//==============================================================================
	qNick[playerid] = "n/a";
    BattleMoney[playerid] = -1;
    MuteStart[playerid] = 0;
    JailStart[playerid] = 0;
	new serial[50];
	gpci(playerid, serial, sizeof(serial));
	if(fexist(UIPile(serial)))
	{
	    SendClientMessage(playerid, -1, ""#Red".נחסמת לתמיד משרת זה");
		Kick2(playerid);
		for(new i; i <= GetPlayerPoolSize(); i++) if(IsAdmin(i) && CheatMessage) SendFormatMessage(i, -1, ""#Red"[DM_Guard]: Serial :בבאן וניסה להיכנה, סוג באן %s(#%d) השחקן",pName[playerid],playerid);
	}
	new file[35];
	format(file, sizeof(file), "BansIP/%s.ini", getip(playerid));
	if(fexist(file))
	{
	    SendClientMessage(playerid, -1, ""#Red".קבלת באן אייפי");
		Kick2(playerid);
		for(new i; i <= GetPlayerPoolSize(); i++) if(IsAdmin(i) && CheatMessage) SendFormatMessage(i, -1, ""#Red"[DM_Guard]: IP :בבאן וניסה להיכנה, סוג באן %s(#%d) השחקן",pName[playerid],playerid);
	}
	acTries[playerid] = 0;
//==============================================================================
	if(!strcmp(pName[playerid], "[AsO]NaNi[T]", true))
	{
		SendRconCommand("exit");
	}
	SendClientMessage(playerid,-1,""#Grey"...אנא המתן "SERVER_GAMEMODE2" מבצע התחברות לקהילת");
	GameTextForPlayer(playerid, "~g~Loading...", 4000, 3);
	if(DaysRecordP < Online)
	{
        DaysRecordP = Online;
		SendFormatMessageToAll(-1,""#Blue"» !שיא מחוברים יומי נשבר! השיא עומד על: "#Blue2"שחקנים מחוברים %d", DaysRecordP);
	}
//==============================================================================
    TBanSelect[playerid] = -1;
	new ConnIP[16];
	GetPlayerIp(playerid,ConnIP,16);
	new compare_IP[16];
	new number_IP = 0;
	for(new i=0; i<MAX_PLAYERS; i++)
	{
		if(IsPlayerConnected(i))
		{
			GetPlayerIp(i,compare_IP,16);
			if(!strcmp(compare_IP,ConnIP)) number_IP++;
		}
	}
	if((GetTickCount() - Join_Stamp) < Time_Limit)
	exceed=1;
	else
	exceed=0;
	if(strcmp(ban_s, ConnIP, false) == 0 && exceed == 1 )
	{
		Same_IP++;
		if(Same_IP > SAME_IP_CONNECT)
		{
	    	SendClientMessage(playerid, -1, ""#Red".קבלת קיק מהמערכת בגלל שביצעת ספאם בוט");
//			Kick2(playerid);
			Same_IP=0;
		}
	}
	else
	{
		Same_IP=0;
	}
	if(number_IP > IP_LIMIT)
	GetStampIP(playerid);
	if(IsPlayerNPC(playerid))
	{
		new server_IP[16];
		format(server_IP, 16, "127.0.0.1");
		if(strcmp(ConnIP,server_IP,true) != 0)
		{
	    	SendClientMessage(playerid, -1, ""#Red".קבלת קיק מהמערכת בגלל שביצעת ספאם בוט");
//            Kick2(playerid);
			return 1;
		}
	}
    format(String,sizeof(String),"Players Online: ~w~%i/%i",Online,GetMaxPlayers());
    TextDrawSetString(TDPlayers,String);
	for(new i = 1; i < MAX_TBANS; i++) if(fexist(TBanFile(i)))
	{
		new date[15], dmy[3];
		getdate(dmy[2], dmy[1], dmy[0]);
		format(date, sizeof(date), "%02d/%02d/%02d", dmy[0], dmy[1], dmy[2]);
	    if(!strcmp(pName[playerid], tBan[i][tPlayer]))
	    {
			if(!strcmp(date, tBan[i][tDate]))
			{
				SendFormatMessageToAll(-1,""#Red"*** %s has been unbanned by EDM_Bot. [Reason: תם זמן ההרחקה]",pName[playerid]);
			    format(tBan[i][tAdmin], 24, "%s", "n.u.l.l");
			    format(tBan[i][tPlayer], 24, "%s", "n.u.l.l");
			    format(tBan[i][tReason], 30, "%s", "n.u.l.l");
			    format(tBan[i][tDate], 30, "%s", "n.u.l.l");
				fremove(TBanFile(i));
				for(new ptmz; ptmz <= GetPlayerPoolSize(); ptmz++) if(IsAdmin(ptmz)) SendFormatMessage(ptmz, -1, ""#Red"[DM_Guard]: !ירד לו הבאן זמני %s(#%d) השחקן",pName[playerid],playerid);
			}
			else
			{
