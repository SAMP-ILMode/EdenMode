			{
				if(!strcmp(GetClanName(playerid), DOF2_GetString(PFile(i), "Clan"), true))
				{
					SetPlayerTeam(playerid, random(500));
				}
			}
		}
		else if(!strcmp(edit, "Buy", true))
		{
			new str[90];
			format(str,90,"Clans/%s.ini",GetClanName(playerid));
			if(DOF2_GetBool(str, "HaveTK") == true) return SendClientMessage(playerid,-1,""#Red".יש לך אנטי-טים קיל בקלאן");
			if(Y_GetPlayerMoney(playerid) < 125000) return SendClientMessage(playerid,-1,""#Red"120,000$ :מחיר אנטי טים קיל לקלאן");
