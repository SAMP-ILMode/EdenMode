stock SetPlayerRandomColor(playerid)
{
	new r = random(256),g = random(256),b = random(256);
	while(r > 200 && g > 200 && b > 200)
	{
		r = random(256),g = random(256),b = random(256);
	}
	SetPlayerColor(playerid,rgba2hex(r,g,b,100));
	if(DOF2_GetBool(PFile(playerid), "HaveClan") == true)
	{
		new str[256];
		format(str ,sizeof(str), "/Clans/%s.ini", GetClanName(playerid));
		SetPlayerColor(playerid,rgba2hex(DOF2_GetInt(str,"R"), DOF2_GetInt(str, "G"), DOF2_GetInt(str, "B"),100));
	}
	return 1;
}
